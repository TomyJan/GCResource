[
  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "ActivityAbility_Endora_Handler",
      "isDynamicAbility": true,
      "abilityMixins": [
        {
          "$type": "AttachToStateIDMixin",
          "stateIDs": ["ShootBS", "Shoot", "FastAim", "FastAimShoot"],
          "modifierName": "AimHideEndora"
        }, //瞄准射击时隐藏
        {
          "$type": "StageReadyMixin",
          "onStageReady": [
            {
              "$type": "RemoveUniqueModifier",
              "target": "Team",
              "doOffStage": true,
              "modifierName": "UNIQUE_ActivityAbility_Endora_Handler"
            },
            {
              "$type": "AttachModifier",
              "target": "Team",
              "doOffStage": true,
              "modifierName": "UNIQUE_ActivityAbility_Endora_Handler"
            }
          ]
        }
      ],
      "onAdded": [
        {
          "$type": "RemoveUniqueModifier",
          "target": "Team",
          "doOffStage": true,
          "modifierName": "UNIQUE_ActivityAbility_Endora_Handler"
        },
        {
          "$type": "AttachModifier",
          "target": "Team",
          "doOffStage": true,
          "modifierName": "UNIQUE_ActivityAbility_Endora_Handler"
        }
      ],
      "onAvatarIn": [
        {
          "$type": "AttachModifier",
          "target": "Team",
          "doOffStage": true,
          "modifierName": "UNIQUE_ActivityAbility_Endora_Handler"
        }
      ],

      "modifiers": {
        "UNIQUE_ActivityAbility_Endora_Handler": {
          "stacking": "Unique",
          "isUnique": true,
          "onAdded": [
            {
              "$type": "RemoveUniqueModifier",
              "doOffStage": true,
              "modifierName": "UNIQUE_ActivityAbility_Endora_RemoveEffect"
            },
            {
              "$type": "RemoveUniqueModifier",
              "doOffStage": true,
              "modifierName": "UNIQUE_ActivityAbility_Endora_Effect"
            },
            {
              "$type": "ApplyModifier",
              "doOffStage": true,
              "modifierName": "UNIQUE_ActivityAbility_Endora_Effect"
            }
          ],
          "onRemoved": [
            {
              "$type": "ApplyModifier",
              "doOffStage": true,
              "modifierName": "UNIQUE_ActivityAbility_Endora_RemoveEffect"
            }
          ]
        },
        "UNIQUE_ActivityAbility_Endora_Effect": {
          "stacking": "Unique",
          "isUnique": true,
          "onAdded": [
            {
              "$type": "AttachEffect",
              "doOffStage": true,
              "effectPattern": "Eff_Activity_Endora"
            }
          ]
        },
        "UNIQUE_ActivityAbility_Endora_RemoveEffect": {
          "duration": 2,
          "isUnique": true,
          "onAdded": [
            {
              "$type": "SendEffectTrigger",
              "parameter": "Die",
              "effectPattern": "Eff_Activity_Endora"
            }
          ],
          "onRemoved": [
            {
              "$type": "RemoveUniqueModifier",
              "doOffStage": true,
              "modifierName": "UNIQUE_ActivityAbility_Endora_Effect"
            }
          ]
        },
        "AimHideEndora": {
          "onAdded": [
            {
              "$type": "SendEffectTrigger",
              "parameter": "Die",
              "target": "Team",
              "effectPattern": "Eff_Activity_Endora"
            }
          ],
          "onRemoved": [
            {
              "$type": "SendEffectTrigger",
              "parameter": "Show",
              "target": "Team",
              "effectPattern": "Eff_Activity_Endora"
            }
          ]
        }
      }
    }
  }, //ActivityAbility_Endora_Handler_纯水精灵显示逻辑 （精灵是绑定在队伍上的）

  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "ActivityAbility_Endora_SwtichSkill",
      "isDynamicAbility": true,
      "onAbilityStart": [
        {
          "$type": "AttachModifier",
          "doOffStage": true,
          "modifierName": "UNIQUE_ActivityAbility_Endora_AddSkillInfo",
          "target": "CurTeamAvatars"
        }
      ],
      "modifiers": {
        "UNIQUE_ActivityAbility_Endora_AddSkillInfo": {
          "isUnique": true,
          "stacking": "Refresh",
          "onAdded": [
            {
              "$type": "AddAvatarSkillInfo",
              "doOffStage": true,
              "skillID": 20020
            },
            {
              "$type": "AvatarSkillStart",
              "skillID": 20020,
              "cdRatio": 0.05
            }, //AvatarSkillStart _ 这个是为了切换时个小CD，使得UI看上去有个技能可以用的效果提示
            {
              "$type": "SendEffectTrigger",
              "target": "Team",
              "type": "Bool",
              "parameter": "SwitchSkill",
              "value": 1,
              "effectPattern": "Eff_Activity_Endora"
            },
            {
              "$type": "ApplyModifier",
              "doOffStage": true,
              "modifierName": "SwitchSkill"
            }
          ],
          "onRemoved": [
            {
              "$type": "SendEffectTrigger",
              "target": "Team",
              "type": "Bool",
              "parameter": "SwitchSkill",
              "value": 0,
              "effectPattern": "Eff_Activity_Endora"
            },
            {
              "$type": "RemoveModifier",
              "doOffStage": true,
              "modifierName": "SwitchSkill"
            },
            {
              "$type": "RemoveAvatarSkillInfo",
              "doOffStage": true,
              "skillID": 20020
            }
          ]
        },
        "SwitchSkill": {
          "stacking": "Refresh",
          "onAdded": [
            {
              "$type": "ChangePlayMode",
              "muteRemoteAction": true,
              "doOffStage": true,
              "toPlayMode": "EndoraState"
            }
          ],
          "onRemoved": [
            {
              "$type": "ChangePlayMode",
              "muteRemoteAction": true,
              "doOffStage": true,
              "toPlayMode": "Normal"
            }
          ],

          "modifierMixins": [
            {
              "$type": "SwitchSkillIDMixin",
              "priority": "Playmode",
              "skillIndex": 1,
              "skillID": 20020
            }
          ]
        }
      }
    }
  }, //ActivityAbility_Endora_SwtichSkill（切换技能，start由客户端使用道具时调用）

