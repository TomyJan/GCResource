  //////////////////////////////////////////////////////////////
  //
  // 诺艾尔Coop用岩元素含量高的碎石堆
  //
  //////////////////////////////////////////////////////////////
  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "SceneObj_Environment_Rock_StonePileQuest_Hard",
      "onAdded": [
        {
          "$type": "AttachModifier",
          "modifierName": "BeHitBlunt"
        }
      ],
      "modifiers": {
        "BeHitBlunt": {
          "elementType": "Rock",
          "elementDurability": 100,
          "modifierMixins": [
            {
              "$type": "ShieldBarMixin"
            }
          ],
          "onBeingHit": [
            {
              "$type": "FireEffect",
              "effectPattern": "Eff_SceneObj_StoneWall_01_Broken",
              "born": {
                "$type": "ConfigBornByTarget"
              },
              "predicates": [
                {
                  "$type": "ByNot",
                  "predicates": [
                    {
                      "$type": "ByHitStrikeType",
                      "strikeType": "Default"
                    },
                    {
                      "$type": "ByHitStrikeType",
                      "strikeType": "Pierce"
                    },
                    {
                      "$type": "ByHitStrikeType",
                      "strikeType": "None"
                    }
                  ]
                }
              ]
            }
          ],
          "onRemoved": [
            {
              "$type": "FireEffect",
              "effectPattern": "Eff_SceneObj_StoneWall_01_Destroy",
              "born": {
                "$type": "ConfigBornByTarget"
              }
            },
            {
              "$type": "KillSelf"
            }
          ]
        }
      }
    }
  },
  ////////////////////////////////////////////////////////////////////////////////////
  //
  // 2.0 宵宫线——灌木
  //
  ////////////////////////////////////////////////////////////////////////////////////
  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "SceneObj_Area_Dq_Property_Ani_Foliage_Shrub_01_Ability",
      "onAdded": [
        {
          "$type": "ApplyModifier",
          "modifierName": "GrassMaterial"
        },
        {
          "$type": "SetGlobalValue",
          "key": "_Bush_Attack_Count",
          "value": 0
        }
      ],
      "modifiers": {
        "GrassMaterial": {
          "elementType": "Wood",
          "elementDurability": 30,
          "onAdded": [
            {
              //燃烧时免疫其余燃烧攻击
              "$type": "AddGlobalValue",
              "key": "_ABILITY_BURNING_IGNITE_ONLY",
              "value": 1,
              "useLimitRange": true,
              "maxValue": 1,
              "minValue": 1
            }
          ],
          "onRemoved": [
            {
              "$type": "Predicated",
              "target": "Self",
              "targetPredicates": [
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "_Bush_Death_Type",
                  "value": 1,
                  "compareType": "Equal"
                }
              ],
              "successActions": [
                {
                  "$type": "FireEffect",
                  "effectPattern": "Eff_Quest_Foliage_Shrub_01_Burnt",
                  "born": {
                    "$type": "ConfigBornType",
                    "offset": {
                      "x": 0.0,
                      "y": -0.2,
                      "z": 0.0
                    },
                    "onGround": true
                  }
                }
              ],
              "failActions": [
                {
                  "$type": "FireEffect",
                  "effectPattern": "Eff_Quest_Foliage_Shrub_01_Destroy",
                  "born": {
                    "$type": "ConfigBornType",
                    "offset": {
                      "x": 0.0,
                      "y": -0.2,
                      "z": 0.0
                    },
                    "onGround": true
                  }
                }
              ]
            },
            {
              "$type": "KillSelf"
            }
          ],
          "onBeingHit": [
            {
              //爆炸立刻摧毁
              "$type": "Predicated",
              "target": "Self",
              "targetPredicates": [
                {
                  "$type": "ByAttackTags",
                  "attackTags": ["ExplodeDamage"]
                }
              ],
              "successActions": [
                {
                  "$type": "FireEffect",
                  "effectPattern": "Eff_Quest_Foliage_Shrub_01_Destroy"
                },
                {
                  "$type": "KillSelf"
                }
              ]
            },
            {
              "$type": "AddGlobalValue",
              "key": "_Bush_Death_Type",
              "value": 0,
              "useLimitRange": true,
              "maxValue": 1,
              "minValue": 0
            },
            {
              //增加击打计数
              "$type": "AddGlobalValue",
              "key": "_Bush_Attack_Count",
              "value": 1
            },
            {
              "$type": "SetAnimatorTrigger",
              "triggerID": "Shake"
            },
            {
              "$type": "Predicated",
              "target": "Self",
              "targetPredicates": [
                {
                  "$type": "ByHitElementDurability",
                  "element": "Fire",
                  "durability": 10,
                  "compareType": "MoreThan"
                }
              ],
              "successActions": [
                {
                  //点燃标记
                  "$type": "AddGlobalValue",
                  "key": "_Bush_Death_Type",
                  "value": 1,
                  "useLimitRange": true,
                  "maxValue": 1,
                  "minValue": 0
                }
              ],
              "failActions": [
                {
                  //击打标记
                  "$type": "RemoveModifier",
                  "modifierName": "GrassMaterial",
                  "predicates": [
                    {
                      "$type": "ByTargetGlobalValue",
                      "key": "_Bush_Attack_Count",
                      "value": 2,
                      "compareType": "Equal"
                    }
                  ]
                }
              ]
            }
          ]
        }
      }
    }
  },
