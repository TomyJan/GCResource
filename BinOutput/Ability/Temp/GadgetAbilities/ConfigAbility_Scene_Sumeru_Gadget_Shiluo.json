  //////////////////////////////////////////////////////////////////////////////////////////////////
  //
  //	显示屏_失落之森WQ 70350436
  //
  //////////////////////////////////////////////////////////////////////////////////////////////////
  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "SceneObj_Area_Xm_Property_Prop_Display_01_Ability",
      "onAdded": [
        {
          //机器人模式是模式1：Attack
          "$type": "SetGlobalValue",
          "key": "RobotMode",
          "value": 0
        },
        {
          //记录按键次数
          "$type": "SetGlobalValue",
          "key": "GV_ButtonTimes",
          "value": 0
        }
      ],
      "abilityMixins": [
        {
          "$type": "AttachToGadgetStateMutexMixin",
          "gadgetStates": [0, 201],
          "modifierNames": [["PowerOn"], ["PowerOff"]]
        },
        {
          //次数逻辑放在模板上
          "$type": "UIInteractMixin",
          "ui_message_id": "OpenPage",
          "actionList": [
            {
              "$type": "ApplyModifier",
              "modifierName": "ScreenEffectOn"
            }
          ]
        },
        {
          //次数逻辑放在模板上
          "$type": "UIInteractMixin",
          "ui_message_id": "ClosePage",
          "actionList": [
            {
              "$type": "ApplyModifier",
              "modifierName": "ScreenEffectOff"
            }
          ]
        },
        {
          //次数逻辑放在模板上
          "$type": "UIInteractMixin",
          "ui_message_id": "v3_0_RobotControl_ButtonDown",
          "actionList": [
            {
              "$type": "ApplyModifier",
              "modifierName": "PressButtonModifier"
            }
          ]
        },
        {
          //次数逻辑放在模板上
          "$type": "AttachModifierToSelfGlobalValueNoInitMixin",
          "globalValueKey": "GV_ButtonTimes",
          "valueSteps": [0.5, 1.5, 2.5, 3.5],
          "modifierNameSteps": ["FirstClick", "SecondClick", "ThirdClick"]
        }
      ],
      "modifiers": {
        "PowerOff": {
          "onAdded": [
            {
              "$type": "EnableGadgetInteeMp",
              "enable": false
            },
            {
              "$type": "SetAnimatorInt",
              "intID": "ScreenOn",
              "value": 0,
              "MPTriggerOnRemote": true
            }
          ]
        },
        "PowerOn": {
          "onAdded": [
            {
              "$type": "EnableGadgetInteeMp",
              "enable": true
            },
            {
              "$type": "SetAnimatorInt",
              "intID": "ScreenOn",
              "value": 0,
              "MPTriggerOnRemote": true
            }
          ]
        },
        "ScreenEffectOn": {
          "duration": 0.1,
          "onAdded": [
            {
              "$type": "SetAnimatorInt",
              "intID": "ScreenOn",
              "value": 1,
              "MPTriggerOnRemote": true
            },
            {
              //每次按时会增加1格
              "$type": "SetGlobalValue",
              "key": "GV_ButtonTimes",
              "value": 0
            },
            {
              //Trigger
              "$type": "ServerLuaTriggerEvent",
              "luaCallType": "FromGroup",
              "sourceName": "OpenScreen"
            }
          ]
        },
        "ScreenEffectOff": {
          "duration": 0.1,
          "onAdded": [
            {
              "$type": "SetAnimatorInt",
              "intID": "ScreenOn",
              "value": 0,
              "MPTriggerOnRemote": true
            },
            {
              //Trigger
              "$type": "ServerLuaTriggerEvent",
              "luaCallType": "FromGroup",
              "sourceName": "CloseScreen"
            }
          ]
        },
        "PressButtonModifier": {
          //要防玩家疯狂点击
          "duration": 0.1,
          "onAdded": [
            {
              //每次按时会增加1格
              "$type": "AddGlobalValue",
              "key": "GV_ButtonTimes",
              "useLimitRange": true,
              "value": 1,
              "maxValue": 4,
              "minValue": 0
            }
          ]
        },
        "FirstClick": {
          "onAdded": [
            {
              "$type": "Predicated",
              "target": "AllPlayerAvatars",
              "targetPredicates": [
                {
                  "$type": "ByAvatarIsHost",
                  "target": "Target",
                  "isHost": true
                }
              ],
              "successActions": [
                {
                  "$type": "AttachModifier",
                  "modifierName": "NotifyUI01",
                  "target": "Target"
                }
              ]
            }
          ]
        },
        "SecondClick": {
          "onAdded": [
            {
              "$type": "Predicated",
              "target": "AllPlayerAvatars",
              "targetPredicates": [
                {
                  "$type": "ByAvatarIsHost",
                  "target": "Target",
                  "isHost": true
                }
              ],
              "successActions": [
                {
                  "$type": "AttachModifier",
                  "modifierName": "NotifyUI02",
                  "target": "Target"
                }
              ]
            }
          ]
        },
        "ThirdClick": {
          "onAdded": [
            {
              "$type": "Predicated",
              "target": "AllPlayerAvatars",
              "targetPredicates": [
                {
                  "$type": "ByAvatarIsHost",
                  "target": "Target",
                  "isHost": true
                }
              ],
              "successActions": [
                {
                  //Trigger
                  "$type": "ServerLuaTriggerEvent",
                  "luaCallType": "FromGroup",
                  "sourceName": "TitanDriveFinish"
                },
                {
                  "$type": "AttachModifier",
                  "modifierName": "NotifyUI03",
                  "target": "Target"
                }
              ]
            }
          ]
        },
        "NotifyUI01": {
          "onAdded": [
            {
              //应显示文本 “诶，怎么好像没什么反应”
              "$type": "NotifyUI",
              "uiContextName": "RobotControlPage",
              "message": "ShowReminder",
              "intParam": 33040100
            }
          ]
        },
        "NotifyUI02": {
          "onAdded": [
            {
              //应显示文本 “啊有点吓人你不要乱按搞坏了”
              "$type": "NotifyUI",
              "uiContextName": "RobotControlPage",
              "message": "ShowReminder",
              "intParam": 33040101
            }
          ]
        },
        "NotifyUI03": {
          "onAdded": [
            {
              //关闭机器人控制界面
              "$type": "NotifyUI",
              "uiContextName": "RobotControlPage",
              "message": "Close"
            }
          ]
        }
      }
    }
