  //////////////////////////////////
  //
  //  昼夜转换机关
  //
  //////////////////////////////////
  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "SceneObj_Area_Dq_Property_Ani_Prop_CircleConsole_BaiHe_01",
      "abilityMixins": [
        //白天锁定
        {
          "$type": "AttachToGadgetStateMixin",
          "gadgetState": 200,
          "modifierName": "DayLocked"
        },
        //夜晚状态
        {
          "$type": "AttachToGadgetStateMixin",
          "gadgetState": 300,
          "modifierName": "NightLocked"
        },

        //白天状态
        {
          "$type": "AttachToGadgetStateMixin",
          "gadgetState": 202,
          "modifierName": "DayState"
        },
        //夜晚状态
        {
          "$type": "AttachToGadgetStateMixin",
          "gadgetState": 302,
          "modifierName": "NightState"
        },
        //转换到白天状态
        {
          "$type": "AttachToGadgetStateMixin",
          "gadgetState": 212,
          "modifierName": "ChangeToDayCountdown"
        },
        //转换到夜晚状态
        {
          "$type": "AttachToGadgetStateMixin",
          "gadgetState": 312,
          "modifierName": "ChangeToNightCountdown"
        }
      ],

      "modifiers": {
        //锁定状态
        "DayLocked": {
          "onAdded": [
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_Item_SpecialTreasureBox_LockIdle",
              "born": {
                "$type": "ConfigBornByAttachPoint",
                "attachPointName": "LockEffect"
              }
            }
          ],
          "onRemoved": [
            {
              "$type": "FireEffect",
              "effectPattern": "Eff_Item_SpecialTreasureBox_Unlock",
              "born": {
                "$type": "ConfigBornByAttachPoint",
                "attachPointName": "LockEffect"
              }
            }
          ]
        },
        "NightLocked": {
          "onAdded": [
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_Item_SpecialTreasureBox_LockIdle",
              "born": {
                "$type": "ConfigBornByAttachPoint",
                "attachPointName": "LockEffect"
              }
            }
          ],
          "onRemoved": [
            {
              "$type": "FireEffect",
              "effectPattern": "Eff_Item_SpecialTreasureBox_Unlock",
              "born": {
                "$type": "ConfigBornByAttachPoint",
                "attachPointName": "LockEffect"
              }
            }
          ]
        },

        //转换到白天的过渡状态
        "ChangeToDayCountdown": {
          "duration": 2,
          "onAdded": [
            {
              "$type": "DebugLog",
              "content": "EnvState: 开始转换到白天"
            },
            {
              "$type": "FireEffect",
              "effectPattern": "Eff_ScenObj_CircleConsole_ChangeToDay",
              "born": {
                "$type": "ConfigBornByAttachPoint",
                "attachPointName": "RootNode",
                "offset": {
                  "x": 0,
                  "y": -0.5,
                  "z": 0
                }
              }
            }
          ],
          "onRemoved": [
            {
              "$type": "DebugLog",
              "content": "EnvState: 转换到白天完毕"
            },

            //找到自己的group，发送SLC，修改LevelTag，并恢复选项
            {
              "$type": "ServerLuaCall",
              "funcName": "SLC_Change_Day_Night",
              "luaCallType": "FromGroup"
            },
            {
              "$type": "ExecuteGadgetLua",
              "param1": 202
            }
          ]
        },

        //转换到夜晚的过渡状态
        "ChangeToNightCountdown": {
          "duration": 2,
          "onAdded": [
            {
              "$type": "DebugLog",
              "content": "EnvState: 开始转换到黑夜"
            },
            {
              "$type": "FireEffect",
              "effectPattern": "Eff_ScenObj_CircleConsole_ChangeToNight",
              "born": {
                "$type": "ConfigBornByAttachPoint",
                "attachPointName": "RootNode",
                "offset": {
                  "x": 0,
                  "y": -0.5,
                  "z": 0
                }
              }
            }
          ],
          "onRemoved": [
            {
              "$type": "DebugLog",
              "content": "EnvState: 转换到黑夜完毕"
            },

            //找到自己的group，发送SLC，修改LevelTag，并恢复选项
            {
              "$type": "ServerLuaCall",
              "funcName": "SLC_Change_Day_Night",
              "luaCallType": "FromGroup"
            },
            {
              "$type": "ExecuteGadgetLua",
              "param1": 302
            }
          ]
        },
        "DayState": {
          "onAdded": [
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_ScenObj_CircleConsole_Day",
              "born": {
                "$type": "ConfigBornByAttachPoint",
                "attachPointName": "RootNode",
                "offset": {
                  "x": 0,
                  "y": 0.3,
                  "z": 0
                }
              }
            }
          ]
        },
        "NightState": {
          "onAdded": [
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_ScenObj_CircleConsole_Night",
              "born": {
                "$type": "ConfigBornByAttachPoint",
                "attachPointName": "RootNode",
                "offset": {
                  "x": 0,
                  "y": 0.3,
                  "z": 0
                }
              }
            }
          ]
        }
      }
    }
  },
