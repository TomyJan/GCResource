  //{
  //_BREAKOUT_BOSS_HP
  //_BREAKOUT_DAMAGE
  //- onHit: actionList
  //}
  //
  //ActivityBreakoutClearSpawnedBricks,
  // suiteIdList

  // DamageByAttackValue
  // AttachModifierToHPPercentMixinV2
  //_BREAKOUT_BOSS_STATE     boss状态gv
  //	打砖块boss 海乱鬼
  //////////////////////////////////////////////////////////////////////
  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "BrickBreaker_Boss01Samurai",
      "abilitySpecials": {
        "TeleportTime": 0.5,
        "BlockToPoint": 1,
        "BlockBackPoint": 1,
        "BossHp": 40
      },
      "onAdded": [
        {
          "$type": "ApplyModifier",
          "modifierName": "Controller"
        },
        {
          "$type": "SetGlobalValue",
          "key": "_BREAKOUT_BOSS_HP",
          "value": "%BossHp"
        },
        {
          "$type": "SetGlobalValue",
          "key": "_BREAKOUT_BOSS_HP_UI_CELL",
          "value": 4
        },
        {
          "$type": "SetGlobalValue",
          "key": "BLOCK_CONTROL",
          "value": 0
        },
        {
          "$type": "SetGlobalValue",
          "key": "IN_SKILL",
          "value": 0
        },
        {
          "$type": "SetGlobalValue",
          "key": "COMMON_SKILL_TRIGGER",
          "value": 0
        },
        {
          "$type": "SetGlobalValue",
          "key": "BLOCK_SKILL_TRIGGER",
          "value": 0
        },
        {
          "$type": "SetGlobalValue",
          "key": "ATTACK_SKILL_TRIGGER",
          "value": 1
        },
        {
          "$type": "SetGlobalValue",
          "key": "CAN_BLOCK",
          "value": 1
        },
        {
          "$type": "SetGlobalValue",
          "key": "IN_SKILL_BLOCK",
          "value": 0
        },
        {
          "$type": "SetGlobalValue",
          "key": "AttackSkillType",
          "value": 0
        },
        {
          "$type": "FireEffect",
          "effectPattern": "Eff_SceneObj_BrickBreaker_Boss02FatuusMaiden_Born"
        }
      ],
      "onKill": [
        {
          "$type": "FireEffect",
          "effectPattern": "Eff_SceneObj_BrickBreaker_Boss02FatuusMaiden_Die"
        }
      ],
      "modifiers": {
        "Controller": {
          "modifierMixins": [
            {
              "$type": "AttachModifierToSelfGlobalValueMixin",
              "globalValueKey": "_BREAKOUT_BOSS_HP",
              "valueSteps": [-1, "0.33*%BossHp", "0.66*%BossHp", "%BossHp+1"],
              "defaultGlobalValueOnCreate": "%BossHp",
              "modifierNameSteps": [
                "BlockControl3",
                "BlockControl2",
                "BlockControl1"
              ]
            },

            {
              "$type": "ActivityBreakoutBossMixin",
              "damageKey": "_BREAKOUT_DAMAGE",
              "stateKey": "_BREAKOUT_BOSS_STATE",
              "hpKey": "_BREAKOUT_BOSS_HP",
              "onHit": [
                //客户端自己更改gv给自己扣血

                //如果boss可以进行架势则开启架势
                {
                  "$type": "Predicated",
                  "targetPredicates": [
                    {
                      "$type": "ByTargetGlobalValue",
                      "key": "CAN_BLOCK",
                      "value": 1,
                      "compareType": "Equal"
                    }
                  ],
                  "successActions": [
                    {
                      "$type": "SetGlobalValue",
                      "key": "BLOCK_SKILL_TRIGGER",
                      "value": 1
                    },
                    {
                      "$type": "SetGlobalValue",
                      "key": "CAN_BLOCK",
                      "value": 0
                    },
                    {
                      "$type": "ApplyModifier",
                      "modifierName": "SkillStarter"
                    }
                  ]
                },
                //正面收到一次攻击后解除架势状态
                {
                  "$type": "Predicated",
                  "target": "Self",
                  "targetPredicates": [
                    {
                      "$type": "ByTargetGlobalValue",
                      "key": "_BREAKOUT_DAMAGE",
                      "value": 0,
                      "compareType": "Equal"
                    }
                  ],
                  "successActions": [
                    {
                      "$type": "RemoveModifier",
                      "modifierName": "BlockSkill"
                    },
                    {
                      "$type": "DebugLog",
