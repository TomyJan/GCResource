  ////////////////////////////////////////////////////////////////////
  //
  // 群玉阁副本-海底古神Control（72000006）
  // Controld的mark_flag必须为0，五个触手依次为2,4,8,16,32
  // 三台归终机为128,256,512。落雷控制器为64。
  //
  ////////////////////////////////////////////////////////////////////
  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "SceneObj_Monster_Eldritch_Tentacle_Control_Ability",
      "abilityMixins": [
        {
          "$type": "AttachToGadgetStateMixin", //归终机射击流程(古神无受击)
          "gadgetState": 201,
          "modifierName": "BeHitPhaseSmall"
        },
        {
          "$type": "AttachToGadgetStateMixin", //古神攻击动作
          "gadgetState": 202,
          "modifierName": "AttackPhase01"
        },
        {
          "$type": "AttachToGadgetStateMixin", //归终机点名
          "gadgetState": 901,
          "modifierName": "PreparePhaseRandomOne"
        },
        {
          "$type": "AttachToGadgetStateMixin", //归终机全亮
          "gadgetState": 902,
          "modifierName": "PreparePhaseALL"
        },
        {
          "$type": "AttachToGadgetStateMixin", //归终机发射，带古神受击
          "gadgetState": 903,
          "modifierName": "BeHitPhaseBig"
        },
        {
          "$type": "AttachToGadgetStateMixin",
          "gadgetState": 0,
          "modifierName": "GadgetZeroModifier"
        }
      ],
      "modifiers": {
        //5个触手进入攻击状态
        "AttackPhase01": {
          "onAdded": [
            {
              "$type": "SetAnimatorTrigger",
              "triggerID": "AttackTrigger",
              "otherTargets": {
                "$type": "SelectTargetsBySelfGroup",
                "operation": "OR",
                "value": 33,
                "compareType": "Lesser"
              }
            },
            {
              "$type": "ApplyModifier",
              "modifierName": "RevertStateToZero_After2Second"
            }
          ]
        },

        //切换到归终机的准备阶段
        "PreparePhaseRandomOne": {
          "onAdded": [
            {
              "$type": "ApplyModifier", //指挥归终机蓄力
              "modifierName": "StateTo201_Now",
              "otherTargets": {
                "$type": "SelectTargetsBySelfGroup",
                "operation": "OR",
                "value": 127,
                "compareType": "Greater",
                "topLimit": 1,
                "sortType": "Random"
              }
            },
            {
              "$type": "ApplyModifier",
              "modifierName": "RevertStateToZero_After2Second"
            }
          ]
        },

        //切换到归终机的准备阶段
        "PreparePhaseALL": {
          "onAdded": [
            {
              "$type": "ApplyModifier", //指挥全部归终机蓄力
              "modifierName": "StateTo201_Now",
              "otherTargets": {
                "$type": "SelectTargetsBySelfGroup",
                "operation": "OR",
                "value": 127,
                "compareType": "Greater"
              }
            },
            {
              "$type": "ApplyModifier",
              "modifierName": "RevertStateToZero_After2Second"
            }
          ]
        },

        //所有归终机一齐发射，同时5个触手进入受击状态
        "BeHitPhaseSmall": {
          "duration": 4.0,
          "onAdded": [
            {
              "$type": "ApplyModifier", //指挥归终机开炮
              "modifierName": "StateTo202_Now",
              "otherTargets": {
                "$type": "SelectTargetsBySelfGroup",
                "operation": "OR",
                "value": 127,
                "compareType": "Greater"
              }
            },
            {
              "$type": "ApplyModifier", //受击特效
              "modifierName": "AdeptusBallista_Hit",
              "otherTargets": {
                "$type": "SelectTargetsBySelfGroup",
                "operation": "OR",
                "value": 2,
                "compareType": "Equal"
              }
            }
          ],
          "onRemoved": [
            {
              "$type": "ApplyModifier",
              "modifierName": "RevertStateToZero_After2Second"
            }
          ]
        },

        "AdeptusBallista_Hit": {
          "duration": 2.5,
          "onRemoved": [
            {
              "$type": "FireEffect",
              "effectPattern": "Eff_SceneObj_AdeptusBallista_Hit",
              "born": {
                "$type": "ConfigBornBySelf",
                "offset": {
                  "x": 0,
                  "y": 125,
                  "z": 0
                },
                "bornRandom": {
                  "$type": "BornRandomFromCenter",
                  "minRandomRange": 0,
                  "maxRandomRange": 50
                }
              }
            }
          ]
        },

        //所有归终机一齐发射，同时5个触手进入受击状态
        "BeHitPhaseBig": {
          "duration": 3.0,
          "onAdded": [
            {
              "$type": "ApplyModifier", //指挥归终机开炮
              "modifierName": "StateTo202_Now",
              "otherTargets": {
                "$type": "SelectTargetsBySelfGroup",
                "operation": "OR",
                "value": 127,
                "compareType": "Greater"
              }
            },
            {
              "$type": "ApplyModifier", //受击特效
              "modifierName": "AdeptusBallista_MultipleHit",
              "otherTargets": {
                "$type": "SelectTargetsBySelfGroup",
                "operation": "OR",
                "value": 2,
                "compareType": "Equal"
              }
            }
          ],
          "onRemoved": [
            {
              "$type": "ApplyModifier",
              "modifierName": "RevertStateToZero_After2Second"
            },
            {
              "$type": "SetAnimatorTrigger",
              "triggerID": "BeHitTrigger",
              "otherTargets": {
                "$type": "SelectTargetsBySelfGroup",
                "operation": "OR",
                "value": 33,
                "compareType": "Lesser"
              }
            }
          ]
        },

        "AdeptusBallista_MultipleHit": {
          "duration": 3.0,
          "onRemoved": [
            {
              "$type": "FireEffect",
              "effectPattern": "Eff_SceneObj_AdeptusBallista_MultipleHit",
              "born": {
                "$type": "ConfigBornBySelf",
                "offset": {
                  "x": 0,
                  "y": 125,
                  "z": 0
                }
              }
            }
          ]
        },

        //5秒后将GadgetState切换为0
        "RevertStateToZero_After2Second": {
          "duration": 2,
          "onRemoved": [
            {
              "$type": "ExecuteGadgetLua",
              "param1": 0
            }
          ]
        },

        //立刻将GadgetState切换为201
        "StateTo201_Now": {
          "duration": 0.1,
          "onAdded": [
            {
              "$type": "ExecuteGadgetLua",
              "param1": 201
            }
          ]
        },

        //立刻将GadgetState切换为202
        "StateTo202_Now": {
          "duration": 0.1,
          "onAdded": [
            {
              "$type": "ExecuteGadgetLua",
              "param1": 202,
              "predicates": [
                {
                  "$type": "ByTargetGadgetState",
                  "gadgetState": 201
                }
              ]
            }
          ]
        },

        "GadgetZeroModifier": {}
      }
    }
  },
