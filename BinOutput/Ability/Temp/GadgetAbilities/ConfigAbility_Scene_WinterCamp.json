  /////////////////////////////////////////////////////////////////
  //
  //  雪山冬令营 跑酷 大型浮冰（复用雪山浮冰机制）
  //
  /////////////////////////////////////////////////////////////////
  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "SceneObj_Area_Common_Property_Ani_Prop_LargeFloatingIce_01",
      "abilityMixins": [
        {
          "$type": "AttachToGadgetStateMixin",
          "gadgetState": 0,
          "modifierName": "Default"
        },
        {
          "$type": "AttachToGadgetStateMixin",
          "gadgetState": 201,
          "modifierName": "phase1"
        },
        {
          "$type": "AttachToGadgetStateMixin",
          "gadgetState": 202,
          "modifierName": "phase2"
        }
      ],
      "modifiers": {
        "Default": {
          "thinkInterval": 0.5,
          "onAdded": [
            {
              "$type": "AddGlobalValue",
              "key": "TRIGGERED",
              "value": 1,
              "useLimitRange": true,
              "maxValue": 1,
              "minValue": 0
            }
          ],
          "onThinkInterval": [
            //每次计算前先清零
            {
              "$type": "AddGlobalValue",
              "key": "TOTAL_WEIGHT",
              "value": 0,
              "useLimitRange": true,
              "maxValue": 0,
              "minValue": 0
            },
            {
              "$type": "SumTargetWeightToSelfGlobalValue",
              "key": "TOTAL_WEIGHT",
              "otherTargets": {
                "$type": "SelectTargetsByShape",
                "shapeName": "RectW2L5_FloatIce",
                "centerBasedOn": "Self",
                "campTargetType": "AllExceptSelf",
                "campBasedOn": "Self"
              }
            },
            {
              "$type": "Predicated",
              "target": "Caster",
              "targetPredicates": [
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "TOTAL_WEIGHT",
                  "value": 19,
                  "compareType": "MoreThan"
                },
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "TRIGGERED",
                  "value": 0,
                  "compareType": "MoreThan"
                }
              ],
              "successActions": [
                {
                  "$type": "ExecuteGadgetLua",
                  "param1": 201
