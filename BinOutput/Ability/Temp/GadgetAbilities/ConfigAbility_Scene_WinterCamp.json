  //////////////////////////////////////////////////////
  //
  //	雪山纹章-普通
  //
  //////////////////////////////////////////////////////
  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "SceneObj_Area_Ljxs_Property_Ani_Prop_TriathlonCoin_01",
      "abilityMixins": [
        {
          "$type": "AttachToGadgetStateMixin",
          "gadgetState": 201, //冰冻锁定状态
          "modifierName": "CoinLocked"
        },
        {
          "$type": "AttachToGadgetStateMixin",
          "gadgetState": 0,
          "modifierName": "CoinDefault"
        }
      ],
      "onFieldEnter": [
        {
          "$type": "Predicated",
          "target": "Self",
          "targetPredicates": [
            {
              //金币在非锁定状态可以被吃
              "$type": "ByTargetGlobalValue",
              "key": "TriathlonCoin_Locked",
              "target": "Self",
              "value": 0,
              "compareType": "Equal"
            }
          ],
          "successActions": [
            {
              //金币被吃特效 通用
              "$type": "FireEffect",
              "target": "Self",
              "effectPattern": "Eff_SceneObj_GlideChampCoin_Hit"
            },
            {
              //统计纹章。0普通 1限时
              "$type": "ServerLuaCall",
              "luaCallType": "CurChallengeGroup",
              "funcName": "SLC_WinterCampCoinGet",
              "paramNum": 1,
              "param1": 0
            },
            {
              "$type": "ApplyModifier",
              "modifierName": "AddCoin",
              "target": "Target"
            },
            {
              "$type": "KillSelf",
              "target": "Self"
            }
          ]
        }
      ],
      "modifiers": {
        "AddCoin": {
          "duration": 1.0,
          "onAdded": [
            {
              //吃金币特效 通用
              "$type": "FireEffect",
              "effectPattern": "Eff_SceneObj_GetGlideChampCoin"
            }
          ]
        },
        "FindPlayerAfterBreak": {
          "thinkInterval": 0.1,
          "onThinkInterval": [
            {
              //切出状态，立即搜一下附近有没有玩家
              "$type": "Predicated",
              "otherTargets": {
                "$type": "SelectTargetsByShape",
                "shapeName": "CircleR10H10",
                "centerBasedOn": "Self",
                "campTargetType": "AllExceptSelf",
                "entityTypePriority": ["Avatar"],
                "campBasedOn": "Self",
                "sortType": "Nearest"
              },
              "targetPredicates": [
                {
                  "$type": "ByEntityTypes",
                  "entityTypes": ["Avatar"]
                }
              ],
              "successActions": [
                {
                  //统计纹章。0普通 1限时
                  "$type": "ServerLuaCall",
                  "luaCallType": "CurChallengeGroup",
                  "funcName": "SLC_WinterCampCoinGet",
                  "paramNum": 1,
                  "param1": 0
                },
                {
                  "$type": "ApplyModifier",
                  "modifierName": "AddCoin",
                  "target": "Other"
                },
                {
                  "$type": "KillSelf",
                  "target": "Self"
                }
              ]
            }
          ]
        },
        "CoinLocked": {
          "onAdded": [
            {
              "$type": "SetGlobalValue",
              "key": "TriathlonCoin_Locked",
              "target": "Self",
              "value": 1
            }
          ],
          "onRemoved": [
            {
              "$type": "ApplyModifier",
              "modifierName": "FindPlayerAfterBreak"
            }
          ]
        },
        "CoinDefault": {
          "onAdded": [
            {
              "$type": "SetGlobalValue",
              "key": "TriathlonCoin_Locked",
              "target": "Self",
              "value": 0
            }
          ]
        }
      }
    }
  },
