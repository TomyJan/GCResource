          ],
          "onRemoved": [
            {
              "$type": "SetGlobalValue",
              "doOffStage": true,
              "doAfterDie": true,
              "key": "WinterCamp_RocketJump",
              "target": "Self",
              "value": 0
            }
          ]
        }
      }
    }
  },
