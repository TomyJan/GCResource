  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "SceneObj_Gear_Common_Property_Prop_JumpUp_01",
      "onFieldEnter": [
        {
          "$type": "ApplyModifier",
          "target": "TargetTeam",
          "modifierName": "UNIQUE_WinterCamp_RocketJump_GV"
