        {
          "$type": "AttachModifier",
          "target": "Self",
          "modifierName": "EffectBall"
        }
      ],
      "modifiers": {
        "EffectBall": {
          "onAdded": [
            {
              //球待机特效
              "$type": "AttachEffect",
              "effectPattern": "Eff_SceneObj_JumpUp_01"
