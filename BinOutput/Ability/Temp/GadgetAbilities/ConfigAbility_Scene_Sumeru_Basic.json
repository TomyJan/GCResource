          "modifierNameSteps": ["ExitDeathZone"]
        }
      ],
      "modifiers": {
        "ExitDeathZone": {
          "duration": 3,
          "modifierMixins": [
            {
              //因Trigger而死
              "$type": "DeathZoneMixin",
              "disactiveHaloTrigger": [
                {
                  "$type": "SetGlobalValue",
                  "key": "SelfDestory",
                  "value": 1
                }
              ]
            }
          ],
          "onRemoved": [
            {
              "$type": "SetGlobalValue",
              "key": "SelfDestory",
              "value": 1
            }
          ]
        },
        "BeHitState": {
          //被草种子攻击
          "onBeingHit": [
            {
              "$type": "SetGlobalValue",
              "key": "SelfDestory",
              "value": 1,
              "predicates": [
                {
                  //被草种子或大型草种子攻击时
                  "$type": "ByAttackTags",
                  "target": "Target",
                  "attackTags": ["GrassSeed", "GrassSeedBig"]
                },
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "CanDestoryBySeed",
                  "value": 1,
                  "compareType": "NoneOrEqual"
                }
              ]
            }
          ]
        }
      }
    }
