          "onBeingHit": [
            {
              "$type": "Predicated",
              "target": "Self",
              "targetPredicates": [
                {
                  "$type": "ByHitElementDurability",
                  "element": "Grass",
                  "durability": 10,
                  "compareType": "MoreThan"
                }
              ],
              "successActions": [
                {
                  //表现层立刻处理
                  "$type": "SetGlobalValue",
                  "key": "GrassState",
                  "value": 0
                },
                {
                  "$type": "ChangeGadgetState",
                  "state": 0
                }
              ]
            }
          ]
        },
        //负责值传递和表现，GadgetState=301
        "DeathDestory": {
          "onAdded": [
            {
              "$type": "SetGlobalValue",
              "key": "GrassState",
              "value": 301
            },
            {
              //若不在死域中保底切回正常态
              "$type": "SetGlobalValue",
              "key": "GrassState",
              "value": 0,
              "predicates": [
                {
                  //不在死域中
                  "$type": "ByTargetGlobalValue",
                  "key": "SGV_DeathZoneState",
                  "value": 0,
                  "compareType": "NoneOrEqual"
                }
              ]
