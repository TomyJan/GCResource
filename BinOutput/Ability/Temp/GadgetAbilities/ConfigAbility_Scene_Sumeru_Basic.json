  //////////////////////////////////////////////////////////////////////////////////////////////////
  //
  //	三态物件基础效果
  //  TODO:增加阻断配置（ ExcitedToNormal
  //
  //////////////////////////////////////////////////////////////////////////////////////////////////
  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "GrassGadget_ThreeState",
      "abilitySpecials": {
        "ExcitedDuration": 1.0 //激活态转正常态的时间，可以根据GV值复写
      },
      "onAdded": [
        {
          //防冰冻
          "$type": "AttachAbilityStateResistance",
          "resistanceListID": 14005
        },
        {
          //防冻结
          "$type": "AttachAbilityStateResistance",
          "resistanceListID": 14002
        },
        {
          //改变发生反应时元素消耗的比例
          "$type": "AttachModifier",
          "modifierName": "GrassStateManage"
        },
        {
          "$type": "SetGlobalValue",
          "key": "GV_ExcitedDuration",
          "value": 1.0
        },
        {
          //根据AbilitySpecial修改ExcitedDuration（激化态时间）
          "$type": "ApplyModifier",
          "modifierName": "ChangeExcitedDuration"
        },
        {
          //初始GadgetState切换用，可以非死域环境中使物件以消亡态的方式创生
          "$type": "ApplyModifier",
          "modifierName": "GoToDestoryState",
          "predicates": [
            {
              //GadgetState == 202
              "$type": "ByTargetGadgetState",
              "gadgetState": 202
            },
            {
              //不能在死域中
              "$type": "ByTargetGlobalValue",
              "key": "SGV_DeathZoneState",
              "value": 0,
              "compareType": "NoneOrEqual"
            }
          ]
        },
        {
          //避开大世界落雨影响
          "$type": "DisableAffectedByWorld",
          "disableWeatherEffect": true
        }
      ],
      "abilityMixins": [
        {
          "$type": "AttachToGadgetStateMixin",
          "gadgetState": 0,
          "modifierName": "Normal"
        },
        {
          "$type": "AttachToGadgetStateMixin",
          "gadgetState": 200,
          "modifierName": "Excited"
        },
        {
          "$type": "AttachToGadgetStateMixin",
          "gadgetState": 300,
          "modifierName": "Destory"
        },
        {
          "$type": "AttachToGadgetStateMixin",
          "gadgetState": 301,
          "modifierName": "DeathDestory"
        },
        {
          //次数逻辑放在模板上
          "$type": "AttachModifierToSelfGlobalValueNoInitMixin",
          "globalValueKey": "GrassState",
          "defaultGlobalValueOnCreate": 1,
          "valueSteps": [-10, 10, 210, 310],
          "modifierNameSteps": ["NormalShow", "ExcitedShow", "DestoryShow"]
        },
        {
          //三态物件处于死域时的逻辑
          "$type": "AttachModifierToSelfGlobalValueNoInitMixin",
          "globalValueKey": "SGV_DeathZoneState",
          "valueSteps": [0.5, 1.5],
          "modifierNameSteps": ["EnterDeathZone"]
        }
      ],
      "modifiers": {
        //LD布设的时候如果需要默认消亡态可用
        "GoToDestoryState": {
          "onAdded": [
            {
              //表现层立刻处理
              "$type": "SetGlobalValue",
              "key": "GrassState",
              "value": 300
            },
            {
              "$type": "ChangeGadgetState",
              "state": 300
            }
          ]
        },
        "EnterDeathZone": {
          //如果SGV为1，则立刻进入死域态。
          "onAdded": [
            {
              //表现层立刻处理
              "$type": "SetGlobalValue",
              "key": "GrassState",
              "value": 300
            },
            {
              //切死域状态
              "$type": "ChangeGadgetState",
              "state": 301
            }
          ],
          "onRemoved": [
            //移除时将三态物件逻辑切换为正常态。派蒙退后使逻辑正常。
            {
              "$type": "ChangeGadgetState",
              "state": 0
            }
          ]
        },
        "GrassStateManage": {
          "modifierMixins": [
            {
              "$type": "ModifyElementDecrateMixin",
              "group": "ThreeStateGrass"
            }
          ]
        },
        "ChangeExcitedDuration": {
          "duration": 0.1,
          "onRemoved": [
            {
              "$type": "SetGlobalValueToOverrideMap",
              "target": "Self",
              "globalValueKey": "GV_ExcitedDuration",
              "overrideMapKey": "ExcitedDuration"
            }
          ]
        },
        //负责值传递和表现，GadgetState=0
        "Normal": {
          "onAdded": [
            {
              //当三态物件从死域态进入正常态时，GrassState不立刻切换。
              "$type": "Predicated",
              "target": "Self",
              "targetPredicates": [
                {
                  "$type": "ByNot",
                  "predicates": [
                    {
                      "$type": "ByTargetGlobalValue",
                      "key": "GrassState",
                      "value": 301,
                      "compareType": "Equal"
                    }
                  ]
                }
              ],
              "successActions": [
                {
                  "$type": "SetGlobalValue",
                  "key": "GrassState",
                  "value": 0
                }
              ]
            }
          ],
          "modifierMixins": [
            {
              "$type": "DoActionBySelfElementReactionMixin",
              "reactionTypes": ["Overdose"],
              "actionQueue": [
                {
                  //表现层立刻处理
                  "$type": "SetGlobalValue",
                  "key": "GrassState",
                  "value": 200
                },
                {
                  "$type": "ChangeGadgetState",
                  "state": 200
                }
              ]
            },
            {
              "$type": "DeathZoneMixin",
              "disactiveHaloTrigger": [
                {
                  "$type": "SetGlobalValue",
                  "key": "GrassState",
                  "value": 0
                }
              ]
            }
          ],
          "onBeingHit": [
            {
              "$type": "Predicated",
              "target": "Self",
              "targetPredicates": [
                {
                  "$type": "ByHitElementDurability",
                  "element": "Fire",
                  "durability": 0,
                  "compareType": "MoreThan"
                }
              ],
              "successActions": [
                {
                  //表现层立刻处理
                  "$type": "SetGlobalValue",
                  "key": "GrassState",
                  "value": 300
                },
                {
                  "$type": "ChangeGadgetState",
                  "state": 300
                }
              ]
            }
          ]
        },
        //负责值传递和表现，GadgetState=200
        "Excited": {
          "duration": "%ExcitedDuration",
          "onAdded": [
            {
              "$type": "SetGlobalValue",
              "key": "GrassState",
              "value": 200
            }
          ],
          "onRemoved": [
            {
              //重置指挥冲击波的触发
              "$type": "SetGlobalValue",
              "key": "EnableWisdomImpact",
              "value": 0
            },
            {
              //通过GadgetState==200保证其是因为自身Duration结束后切换的
              "$type": "Predicated",
              "target": "Self",
              "targetPredicates": [
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "GrassState",
                  "value": 200,
                  "compareType": "Equal"
                },
                {
                  "$type": "ByTargetGadgetState",
                  "gadgetState": 200
                }
              ],
              "successActions": [
                {
                  //表现层立刻处理
                  "$type": "SetGlobalValue",
                  "key": "GrassState",
                  "value": 0
                },
                {
                  "$type": "ChangeGadgetState",
                  "state": 0
                }
              ]
            }
          ]
        },
        //负责值传递和表现，GadgetState=300
        "Destory": {
          "onAdded": [
            {
              "$type": "SetGlobalValue",
              "key": "GrassState",
              "value": 300
