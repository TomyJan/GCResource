  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "SceneObj_Area_Desert_Property_Ani_Prop_SandWorm_01",
      "abilitySpecials": {
        "Damage_Coefficient_Monster": 10,
        "Damage_Coefficient_Player": 1.5
      },
      "abilityMixins": [
        {
          "$type": "AttachModifierToGlobalValueMixin",
          "globalValueKey": "_is_attack",
          "globalValueTarget": "Self",
          "valueSteps": [-0.5, 0.5, 1.5],
          "modifierNameSteps": ["controller_stop", "controller_attack"]
        }
      ],
      "onAdded": [
        {
          "$type": "SetGlobalValue",
          "key": "_is_attack",
          "value": 0
        },
        {
          "$type": "AttachModifier",
          "modifierName": "controller_attack_callback"
        },
        {
          "$type": "SetSelfAttackTarget",
          "otherTargets": {
            "$type": "SelectTargetsByShape",
            "centerBasedOn": "Self",
            "campBasedOn": "CurLocalAvatar",
            "campTargetType": "SelfCamp",
            "entityTypePriority": ["Avatar"],
            "shapeName": "CircleR140",
            "topLimit": 1
          },
          "predicates": [
            {
              "$type": "ByEntityTypes",
              "entityTypes": ["Avatar"]
            }
          ]
        },
        //沙虫本体创生时，向玩家发一颗子弹
        {
          "$type": "TriggerBullet",
          "bulletID": 70290438,
          "lifeByOwnerV2": true,
          "ownerIs": "Self",
          "trackTarget": "SelfAttackTarget",
          "born": {
            "$type": "ConfigBornBySelf",
            "offset": {
              "x": 0,
              "y": 2,
              "z": 0
            },
            "onGround": true,
            "alongGround": true
          }
        }
      ],
      "onRemoved": [
        {
          "$type": "RemoveModifier",
          "modifierName": "controller_attack"
        }
      ],
      "modifiers": {
        "controller_attack_callback": {
          "onHittingOther": [
            {
              "$type": "ServerLuaCall",
              "luaCallType": "FromGroup",
              "funcName": "SLC_Sandworm_Attack_Hit_Avatar",
              "paramNum": 0
            }
          ]
        },
        "controller_stop": {},
        "controller_attack": {
          "onAdded": [
            {
              "$type": "SetGlobalPos",
              "key": "_avatar_pos",
              "target": "SelfAttackTarget",
              "born": {
                "$type": "ConfigBornByTarget"
              },
              "setTarget": false
            },
            {
              "$type": "FireEffect",
              "effectPattern": "Eff_SceneObj_Sandworm_Attack_Alert_Baihe",
              "born": {
                "$type": "ConfigBornByGlobalValue",
                "positionKey": "_avatar_pos",
                "offset": {
                  "x": 0.0,
                  "y": 0.0,
                  "z": 0.0
                }
              }
            }
          ],
          "onRemoved": [
            {
              "$type": "DebugLog",
              "content": "沙虫测试：开始打攻击特效和攻击盒"
            },
            {
              "$type": "FireEffect",
              "effectPattern": "Eff_SceneObj_Sandworm_AttackOut_Baihe",
              "born": {
                "$type": "ConfigBornByGlobalValue",
                "positionKey": "_avatar_pos",
                "offset": {
                  "x": 0.0,
                  "y": 0.0,
                  "z": 0.0
                }
              }
            },
            {
              "$type": "TriggerAttackEvent",
              "targetType": "Enemy",
              "attackEvent": {
                "attackPattern": {
                  "$type": "ConfigAttackSphere",
                  "triggerType": "TriggerNoRepeat",
                  "radius": 3,
                  "born": {
                    "$type": "ConfigBornByGlobalValue",
                    "positionKey": "_avatar_pos",
                    "offset": {
                      "x": 0.0,
                      "y": 1.0,
                      "z": 0.5
                    }
                  },
                  "ignoreMassive": true
                  // "checkHitLayerType": "Both",
                },
                "attackInfo": {
                  "attackTag": "SandwormAttack",
                  "attackProperty": {
                    "damageExtra": "%LevelEntity_AvatarDot_SystemValue*%Damage_Coefficient_Player",
                    "strikeType": "Blunt",
                    "trueDamage": true,
                    "elementType": "None",
                    "ignoreAttackerProperty": true,
                    "enBreak": 999999
                  },
                  "hitPattern": {
                    "onHitEffectName": "",
                    "hitImpulseType": "LV6",
                    "hitHaltTime": 0.0,
                    "hitHaltTimeScale": 0.0,
                    "muteHitText": false
                  },
                  "forceCameraShake": false
                }
              }
            },
            {
              "$type": "TriggerAttackEvent",
              "targetType": "SelfCamp",
              "attackEvent": {
                "attackPattern": {
                  "$type": "ConfigAttackSphere",
                  "triggerType": "TriggerNoRepeat",
                  "radius": 3,
                  "born": {
                    "$type": "ConfigBornByGlobalValue",
                    "positionKey": "_avatar_pos",
