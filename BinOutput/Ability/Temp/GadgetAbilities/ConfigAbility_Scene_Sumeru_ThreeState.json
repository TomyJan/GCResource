  //////////////////////////////////////////////////////////////////////////////////////////////////
  //
  //	三态物件_须弥毒气蘑菇（原炸弹蘑菇）
  //
  //////////////////////////////////////////////////////////////////////////////////////////////////
  {
    //炸弹蘑菇本体
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "SceneObj_Area_Xm_Property_Ani_Prop_XuMiBombmushroom_01_Ability",
      "onAdded": [
        {
          //初始化，激化态时间
          "$type": "SetGlobalValue",
          "key": "GV_ExcitedDuration",
          "value": 6.0
        },
        {
          //初始化，智慧冲击波等级
          "$type": "SetGlobalValue",
          "key": "WisdomImpactLevel",
          "value": 2.0
        },
        {
          //初始化技能CD
          "$type": "SetGlobalValue",
          "key": "GasCD",
          "value": 1
        }
      ],
      "abilityMixins": [
        {
          //GrassState由模板控制
          "$type": "AttachModifierToSelfGlobalValueNoInitMixin",
          "globalValueKey": "GrassState",
          "defaultGlobalValueOnCreate": 1,
          "valueSteps": [-10, 10, 210, 310],
          "modifierNameSteps": ["NormalState", "ExcitedState", "DestoryState"]
        },
        {
          //CD计算器
          "$type": "AttachModifierToSelfGlobalValueNoInitMixin",
          "globalValueKey": "GasCD",
          "defaultGlobalValueOnCreate": 1,
          "valueSteps": [-0.5, 0.5, 1.5],
          "modifierNameSteps": ["TryTrigger", "CDTimer"],
          "removeAppliedModifier": false
        },
        {
          "$type": "FieldEntityCountChangeMixin",
          "simpleCountGlobaValueKey": "AvatarCount",
          "campTargetType": "All",
          "targetPredicates": [
            {
              "$type": "ByEntityTypes",
              "entityTypes": ["Avatar"]
            }
          ]
        },
        {
          //维持在ActiveLoop时持续毒气攻击
          "$type": "AttachToStateIDMixin",
          "stateIDs": ["Excited"],
          "modifierName": "PoisonGasLoop"
        }
      ],
      "modifiers": {
        "CDTimer": {
          //暂定3秒CD
          "duration": 2.9,
          "onRemoved": [
            {
              "$type": "SetGlobalValue",
              "key": "GasCD",
              "value": 0
            }
          ]
        },
        "TryTrigger": {
          //每次CD结束时会尝试触发。否则就静默等待HasAvatar补触发
          "duration": 0.1,
          "onRemoved": [
            {
              "$type": "Predicated",
              "target": "Self",
              "targetPredicates": [
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "AvatarCount",
                  "value": 1,
                  "compareType": "MoreThanAndEqual"
                },
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "GrassState",
                  "value": 0,
                  "compareType": "Equal"
                }
              ],
              "successActions": [
                {
                  "$type": "SetAnimatorTrigger",
                  "triggerID": "Attack",
                  "MPTriggerOnRemote": true
                },
                {
                  "$type": "SetGlobalValue",
                  "key": "GasCD",
                  "value": 1
                }
              ]
            }
          ]
        },
        "NormalState": {
          "modifierMixins": [
            {
              "$type": "AttachModifierToSelfGlobalValueNoInitMixin",
              "globalValueKey": "AvatarCount",
              "valueSteps": [-0.5, 0.5, 4.5],
              "modifierNameSteps": ["NoAvatar", "HasAvatar"]
            }
          ],
          "onAdded": [
            {
              "$type": "SetAnimatorInt",
              "intID": "GrassState",
              "value": 0,
              "MPTriggerOnRemote": true
            }
          ]
        },
        "NoAvatar": {
          //正常态附近无玩家时
          "onAdded": [
            {
              "$type": "SetAnimatorInt",
              "intID": "FieldEnter",
              "value": 0,
              "MPTriggerOnRemote": true
            },
            {
              "$type": "ResetAnimatorTrigger",
              "triggerID": "Attack"
            }
          ]
        },
        "HasAvatar": {
          //正常态附近有玩家时
          "onAdded": [
            {
              "$type": "SetAnimatorInt",
              "intID": "FieldEnter",
              "value": 1,
              "MPTriggerOnRemote": true
            },
            {
              //第一次进入时会触发
              "$type": "Predicated",
              "target": "Self",
              "targetPredicates": [
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "GasCD",
                  "value": 0,
                  "compareType": "Equal"
                }
              ],
              "successActions": [
                {
                  "$type": "SetAnimatorTrigger",
                  "triggerID": "Attack",
                  "MPTriggerOnRemote": true
                },
                {
                  "$type": "SetGlobalValue",
                  "key": "GasCD",
                  "value": 1
                }
              ]
            }
          ]
        },
        "ExcitedState": {
          "onAdded": [
            {
              "$type": "SetAnimatorInt",
              "intID": "GrassState",
              "value": 200,
              "MPTriggerOnRemote": true
            }
          ]
        },
        "DestoryState": {
          "onAdded": [
            {
              "$type": "SetAnimatorInt",
              "intID": "GrassState",
              "value": 300,
              "MPTriggerOnRemote": true
            },
            {
              "$type": "ResetAnimatorTrigger",
              "triggerID": "Attack"
            }
          ]
        },
        "PoisonGasLoop": {
          //持续在这个状态下，会不停打出攻击盒
          "onAdded": [
            {
              "$type": "DebugLog",
              "content": "TD_Test: Attach PoisonGasLoop!!!!!!!!!!!"
            },
            {
              "$type": "FireEffect",
              "effectPattern": "Eff_SceneObj_XuMiBombmushroom_02",
              "born": {
                "$type": "ConfigBornByAttachPoint",
                "attachPointName": "RootNode"
              }
            },
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_SceneObj_XuMiBombmushroom_02_Loop",
              "born": {
                "$type": "ConfigBornByAttachPoint",
                "attachPointName": "RootNode"
              }
            },
            {
              "$type": "TriggerAbility",
              "abilityName": "SceneObj_Area_Xm_Property_Ani_Prop_XuMiBombmushroom_01_Attack02"
            }
          ],
          "modifierMixins": [
            {
              //ThinkInterval性能优化
              "$type": "ThinkIntervalMixin",
              "thinkInterval": 0.5,
              "onThinkInterval": [
                {
                  "$type": "TriggerAbility",
                  "abilityName": "SceneObj_Area_Xm_Property_Ani_Prop_XuMiBombmushroom_01_Attack02"
                }
              ]
            }
          ]
        }
      }
    }
  },
