            {
              "$type": "FireEffect",
              "effectPattern": "Eff_SceneObj_LudiReboundShooter_01_Destroy_01"
            },
            {
              "$type": "TriggerSetRenderersEnable",
              "doOffStage": true,
              "renderNames": ["Property_Ani_Prop_LudiReboundShooter_01_Vo"],
              "setEnable": false
            },
            {
              "$type": "TriggerSetRenderersEnable",
              "doOffStage": true,
              "renderNames": ["Property_Ani_Prop_LudiReboundShooter_02_Vo"],
              "setEnable": true
            },
            {
              "$type": "TriggerSetRenderersEnable",
              "doOffStage": true,
              "renderNames": ["Property_Ani_Prop_LudiReboundShooter_03_Vo"],
