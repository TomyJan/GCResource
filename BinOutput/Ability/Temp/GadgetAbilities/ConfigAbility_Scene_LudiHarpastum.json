            },
            {
              "$type": "KillSelf"
            }
          ]
        },

        "AddSpeed": {
          "properties": {
            "Actor_BulletMoveSpeedRatio": "%ExtraSpeed"
          }
        },

        "Timer": {
          "duration": 7,
          "stacking": "Refresh",
          "onRemoved": [
            {
              "$type": "SetGlobalValue",
              "key": "GV_Init",
              "value": 1
            }
          ]
        },

        "Shoot": {
          "modifierMixins": [
            {
              "$type": "DoActionByCreateGadgetMixin",
              "doOffStage": true,
              "actionQueue": [
                {
                  "$type": "ApplyModifier",
                  "doOffStage": true,
                  "modifierName": "AddSpeed",
                  "target": "Target"
                }
              ]
            }
          ],
          "onBeingHit": [
            {
              "$type": "ApplyModifier",
              "modifierName": "Timer"
            },
            {
              "$type": "Predicated",
              "target": "Target",
              "targetPredicates": [
                {
                  "$type": "ByAttackTags",
                  "attackTags": ["Activity_LudiHarpastum_ParabolaBall_Hit"]
                }
              ],
              "successActions": [
                {
                  "$type": "Predicated",
                  "target": "Target",
                  "targetPredicates": [
                    {
                      "$type": "ByTargetGlobalValue",
                      "key": "GV_Is_PlayerBall",
                      "value": 0,
                      "compareType": "NoneOrEqual"
                    }
                  ],
                  "successActions": [
                    {
                      "$type": "SetSelfAttackTarget",
                      "target": "CurLocalAvatar"
                    },
                    {
                      "$type": "SetGlobalValueToOverrideMap",
                      "globalValueKey": "GV_BulletSpeed",
                      "overrideMapKey": "ExtraSpeed"
                    },

                    {
                      "$type": "TriggerBullet",
                      "bulletID": 70350210,
                      "lifeByOwnerIsAlive": true,
                      "ownerIs": "Caster",
                      "propOwnerIs": "Caster",
                      "born": {
                        "$type": "ConfigBornByAttachPoint",
                        "attachPointName": "ShootPoint",
                        "direction": {
                          "$type": "ConfigDirectionByAttackTarget",
                          "randomAngleHor": 0
                        }
                      }
                    },
                    {
                      "$type": "AddElementDurability",
                      "modifierName": "HitController",
                      "target": "Self",
                      "value": "%PerHitDamage",
                      "elementType": "None",
                      "useLimitRange": true,
                      "minValue": 0.0,
                      "maxValue": 100.0
                    },
                    {
                      "$type": "AddGlobalValue",
                      "key": "GV_BulletSpeed",
                      "value": "%SpeedDelta",
                      "useLimitRange": true,
                      "minValue": 1,
                      "maxValue": "%MaxTimes"
                    }
                  ],
                  "failActions": [
                    {
                      "$type": "Predicated",
                      "target": "Self",
                      "targetPredicates": [
                        {
                          "$type": "ByTargetGlobalValue",
                          "key": "GV_Is_HitByPlayerBall",
                          "value": 0,
                          "compareType": "NoneOrEqual"
                        }
                      ],
                      "successActions": [
                        {
                          "$type": "SetSelfAttackTarget",
                          "target": "CurLocalAvatar"
                        },
                        {
                          "$type": "SetGlobalValueToOverrideMap",
                          "globalValueKey": "GV_BulletSpeed",
                          "overrideMapKey": "ExtraSpeed"
                        },

                        {
                          "$type": "TriggerBullet",
                          "bulletID": 70350210,
                          "lifeByOwnerIsAlive": true,
                          "ownerIs": "Caster",
                          "propOwnerIs": "Caster",
                          "born": {
                            "$type": "ConfigBornByAttachPoint",
                            "attachPointName": "ShootPoint",
                            "direction": {
                              "$type": "ConfigDirectionByAttackTarget",
                              "randomAngleHor": 0
                            }
                          }
                        },
                        {
                          "$type": "SetGlobalValue",
                          "key": "GV_Is_HitByPlayerBall",
                          "value": 1
                        }
                      ]
                    }
                  ]
                }
              ]
            }
          ],
          "onAdded": [
            {
              "$type": "SetGlobalValue",
              "key": "GV_Is_HitByPlayerBall",
              "value": 0
            },
            {
              "$type": "SetGlobalValue",
