            {
              "$type": "ShieldBarMixin"
            },
            {
              "$type": "AttachModifierToElementDurabilityMixin",
              "valueSteps": [0.5, 50, 80, 100.5],
              "modifierNameSteps": ["FireEff2", "FireEff1", "Normal"]
