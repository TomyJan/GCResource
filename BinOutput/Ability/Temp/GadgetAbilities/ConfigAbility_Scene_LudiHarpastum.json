            },
            {
              "$type": "TriggerSetRenderersEnable",
              "doOffStage": true,
              "renderNames": ["Property_Ani_Prop_LudiReboundShooter_02_Vo"],
              "setEnable": false
            },
            {
              "$type": "TriggerSetRenderersEnable",
