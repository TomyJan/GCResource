            },
            {
              "$type": "SetGlobalValue",
              "key": "GV_Is_HitByPlayerBall",
              "value": 0
            },
            {
              "$type": "AddElementDurability",
              "modifierName": "HitController",
              "value": "%MaxHP",
              "elementType": "None",
              "useLimitRange": true,
              "minValue": 0.0,
              "maxValue": "%MaxHP"
            },
            {
              "$type": "FireEffect",
              "effectPattern": "Eff_SceneObj_LudiReboundShooter_01_Born"
            }
          ],
          "onRemoved": [
            {
              "$type": "SetGlobalValue",
              "key": "GV_Init",
              "value": 0
            }
          ]
        },

        "HitController": {
          "elementType": "None",
          "elementDurability": "%MaxHP",
