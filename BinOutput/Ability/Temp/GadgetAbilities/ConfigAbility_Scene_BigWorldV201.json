  //////////////////////////////////////////////////////////////////////////////////////////////////
  //
  // 海神之心 洞窟大门 特效点
  // 功能简述，共有0 201两个state，0时attach一个旧特效，切换至201时立即attach新特效，并延迟3秒销毁旧特效
  //////////////////////////////////////////////////////////////////////////////////////////////////
  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "SceneObj_Area_Dq_Property_Prop_WataGate_01_Point_Ability",
      "onAdded": [
        {
          "$type": "ApplyModifier",
          "modifierName": "Effect1"
        }
      ],
      "abilityMixins": [
        {
          "$type": "AttachToGadgetStateMixin",
          "gadgetState": 0,
          "modifierName": "DefaultState"
        },
        {
          "$type": "AttachToGadgetStateMixin",
          "gadgetState": 201,
          "modifierName": "ActiveState"
        }
      ],
      "modifiers": {
        "DefaultState": {
          "onAdded": [
            {
              "$type": "SetGlobalValue",
              "key": "needEff",
              "value": 1
            }
          ]
        },
        "ActiveState": {
          "onAdded": [
            {
              "$type": "Predicated",
              "target": "Self",
              "targetPredicates": [
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "needEff",
                  "target": "Self",
                  "value": 1,
                  "compareType": "Equal"
                }
              ],
              //变量为1，说明进入state201前为state0，需要播放开门特效
              "successActions": [
                {
                  "$type": "DebugLog",
                  "content": "suc"
                },
                {
                  "$type": "ApplyModifier",
                  "modifierName": "TimerToKillEffect1"
                },
                {
                  //播放新特效
                  "$type": "AttachEffect",
                  "effectPattern": "Eff_SceneObj_WataGate_Open",
                  "born": {
                    "$type": "ConfigBornByAttachPoint",
                    "attachPointName": "attachPoint"
                  }
                },
                {
                  "$type": "SetGlobalValue",
                  "key": "needEff",
                  "value": 0
                }
              ],
              //变量为0，说明加载时直接就是state201，不播放开门特效且直接关空气墙
              "failActions": [
                {
                  "$type": "DebugLog",
                  "content": "fail"
                },
                {
                  "$type": "RemoveModifier", //移除旧特效
                  "modifierName": "Effect1"
                },
                {
                  "$type": "EnableSceneTransformByName", //关掉空气墙
                  "transformNames": ["AirWallCollider"],
                  "setEnable": false
                },
                {
                  "$type": "KillSelf"
                }
              ]
            }
          ]
        },
        "TimerToKillEffect1": {
          "duration": 3,
          "onRemoved": [
            {
              "$type": "DebugLog",
              "content": "timer"
            },
            {
              "$type": "RemoveModifier", //移除旧特效
              "modifierName": "Effect1"
            },
            {
              "$type": "EnableSceneTransformByName", //关掉空气墙
              "transformNames": ["AirWallCollider"],
              "setEnable": false
            },
            {
              "$type": "KillSelf"
            }
          ]
        },
        //默认状态下的Effect1
        "Effect1": {
          "onAdded": [
            {
              //播放特效
              "$type": "AttachEffect",
              "effectPattern": "Eff_SceneObj_WataGate_Close",
              "born": {
                "$type": "ConfigBornByAttachPoint",
                "attachPointName": "attachPoint"
              }
            }
          ]
        }
      }
    }
  },
