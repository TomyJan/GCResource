  //////////////////////////////////////////////////////////////////////////////////////////////////
  //
  // 3.0 织梦之森 最终核心 飞弹发射器
  //
  //////////////////////////////////////////////////////////////////////////////////////////////////
  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "SceneObj_Area_Common_Property_Prop_FinalCore_01_Lanuch",
      "abilitySpecials": {
        "SkillCD": 10,
        "FireRate": 0.5
      },
      "abilityMixins": [
        {
          "$type": "AttachToGadgetStateMutexMixin",
          "gadgetStates": [0, 201],
          "modifierNames": [["Standby"], ["Fire"]]
        },
        {
          //CD监视Mxin
          "$type": "AttachModifierToSelfGlobalValueMixin",
          "globalValueKey": "_MaranaMissle_CD",
          "valueSteps": [-0.5, 0.8, 1.5],
          "modifierNameSteps": ["ShootingCD", "IntervalShooting"]
        },
        {
          //Effect显示Mixin
          "$type": "AttachModifierToSelfGlobalValueMixin",
          "globalValueKey": "_MaranaMissle_CD",
          "valueSteps": [0, 10],
          "modifierNameSteps": ["ShowBall"]
        },
        {
          //计算在LCTrigger内的玩家
          "$type": "FieldEntityCountChangeMixin",
          "campTargetType": "AllExceptSelf",
          "targetPredicates": [
            {
              "$type": "ByEntityTypes",
              "entityTypes": ["Avatar"]
            }
          ],
          "onFieldEnter": [
            {
              "$type": "AddGlobalValue",
              "key": "EntityCount",
              "value": 1,
              "target": "Self"
            }
          ],
          "onFieldExit": [
            {
              "$type": "AddGlobalValue",
              "key": "EntityCount",
              "value": -1,
              "target": "Self"
            }
          ]
        }
      ],
      "onAbilityStart": [
        {
          "$type": "ApplyModifier",
          "modifierName": "FireBullet"
        }
      ],
      "modifiers": {
        "Standby": {},
        "Fire": {
          "onAdded": [
            {
              "$type": "SendEffectTrigger",
              "parameter": "Stage",
              "type": "Int",
              "value": 1,
              "effectPattern": "Eff_SceneObj_POI_Xm_JRZB_Finalcore_01"
            }
          ]
        },
        "ShootingCD": {
          //CD重新计时
          "thinkInterval": "%SkillCD/2",
          "onThinkInterval": [
            {
              //时间到,技能CD结束
              "$type": "AddGlobalValue",
              "key": "_MaranaMissle_CD",
              "value": 0.5
            }
          ]
        },
        "ShowBall": {
          //显示能量球特效
          "onAdded": [
            /*
						{
							"$type": "AttachEffect",
							"effectPattern": "Eff_SceneObj_MaranaMissle_Idle_Size4",
						},
						*/
          ]
        },
        "IntervalShooting": {
          //每{FireRate}秒尝试发射死域飞弹
          "thinkInterval": "%FireRate",
          "onAdded": [
            {
              "$type": "TriggerAbility",
              "abilityName": "SceneObj_Area_Common_Property_Prop_FinalCore_01_Lanuch",
              "target": "Self"
            }
          ],
          "onThinkInterval": [
            {
              "$type": "TriggerAbility",
              "abilityName": "SceneObj_Area_Common_Property_Prop_FinalCore_01_Lanuch",
              "target": "Self"
            }
          ]
        },
        "FireBullet": {
          "duration": 0.1,
          "onAdded": [
            {
              //仅当有Team在范围内,会尝试发射
              "$type": "Predicated",
              "target": "Self",
              "targetPredicates": [
                {
                  //CD已结束
                  "$type": "ByTargetGlobalValue",
                  "key": "_MaranaMissle_CD",
                  "value": 1,
                  "compareType": "Equal"
                },
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "EntityCount",
                  "value": 1,
                  "compareType": "MoreThanAndEqual"
                }
              ],
              "successActions": [
                {
                  //随机选择范围内合法攻击目标
                  "$type": "SetSelfAttackTarget",
                  "otherTargets": {
                    "$type": "SelectTargetsByLCTrigger",
                    "campTargetType": "Enemy",
                    "sortType": "Random",
                    "topLimit": 1
                  },
                  "predicates": [
                    {
                      "$type": "ByEntityTypes",
                      "entityTypes": ["Avatar"]
                    }
                  ]
                },
                {
                  //根据Field随机选择1个目标发射70290512子弹
                  "$type": "TriggerBullet",
                  "bulletID": 70290493, //死域专属飞弹
                  "born": {
                    "$type": "ConfigBornByAttachPoint",
                    "attachPointName": "EffectPoint",
                    "offset": {
                      "x": 0.0,
                      "y": 0.0,
                      "z": 0.0
                    }
                  },
                  "predicates": [
                    {
                      "$type": "ByHasAttackTarget"
                    }
                  ]
                },
                {
                  //子弹发射成功,CD重新计
                  "$type": "SetGlobalValue",
                  "key": "_MaranaMissle_CD",
                  "value": 0,
                  "predicates": [
                    {
                      "$type": "ByHasAttackTarget"
                    }
                  ]
                }
              ]
            }
          ]
        }
      }
    }
  },
