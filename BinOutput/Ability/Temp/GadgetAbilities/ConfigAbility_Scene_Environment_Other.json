  /////////////////////////////////////////////////////////
  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "SceneObj_Environment_Other_AreaGuardian",
      "onAdded": [
        {
          "$type": "AttachModifier",
          "modifierName": "GuardianStart"
        }
      ],
      "modifiers": {
        "GuardianStart": {
          "onBeingHit": [
            /*{
							"$type": "ExecuteGroupTrigger",
							"sourceName": "GuardianStart",
							"param1": 0,
						},*/
          ]
        }
      }
    }
  }
]
