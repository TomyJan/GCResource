  /*
	s1641530 【3.6 backlog内】沙漠须弥物件-激光炮-TD
	https://www.tapd.cn/22963631/prong/stories/view/1122963631001641530
	*/
  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "SceneObj_Area_Common_Property_Ani_Prop_LaserTower_01",
      "onAdded": [
        {
          "$type": "SetGlobalValue",
          "key": "_Enemy_Nums",
          "value": 0,
          "useLimitRange": true,
          "minValue": 0,
          "maxValue": 10
        }
      ],
      "onFieldEnter": [
        {
          "$type": "AddGlobalValue",
          "key": "_Enemy_Nums",
          "value": 1,
          "minValue": 0,
          "maxValue": 10
        }
      ],
      "onFieldExit": [
        {
          "$type": "AddGlobalValue",
          "key": "_Enemy_Nums",
          "value": -1,
          "minValue": 0,
          "maxValue": 10
        }
      ],
      "abilityMixins": [
        {
          "$type": "AttachModifierToGlobalValueMixin",
          "globalValueTarget": "Self",
          "globalValueKey": "_Enemy_Nums",
          "valueSteps": [-0.5, 0.5, 100],
          "modifierNameSteps": ["LaserOff", "LaserOn"]
        }
      ],
      "modifiers": {
        "LaserOff": {
          "onAdded": [
            {
              "$type": "EnablePartControl",
              "partRootNames": ["ShootControl"],
              "enable": false
            }
          ]
        },
        //激光塔打开，在玩家脚下创一个追踪子弹
        "LaserOn": {
          "onAdded": [
            //开始追踪
            {
              "$type": "EnablePartControl",
              "enable": true,
              "partRootNames": ["ShootControl"]
            },
            {
              "$type": "AttachModifier",
              "modifierName": "LaserSearchTarget"
            },
            {
              "$type": "AttachModifier",
              "modifierName": "LaserTracking"
            }
          ],
          "thinkInterval": 8,
          "onThinkInterval": [
            {
              "$type": "ApplyModifier",
              "modifierName": "LaserSearchTarget"
            },
            {
              "$type": "ApplyModifier",
              "modifierName": "LaserTracking"
            }
          ]
        },
        //激光寻找跟踪子弹作为激光打击的目标
        "LaserSearchTarget": {
          "duration": 1,
          "onAdded": [
            {
              "$type": "Predicated",
              "otherTargets": {
                "$type": "SelectTargetsByShape",
                "shapeName": "CircleR25",
                "centerBasedOn": "Self",
                "campTargetType": "All",
                "campBasedOn": "Self",
                "sortType": "Nearest",
                "topLimit": 1,
                "entityTypePriority": ["Avatar"]
              },
              "targetPredicates": [
                {
                  "$type": "ByEntityTypes",
                  "entityTypes": ["Avatar"]
                }
              ],
              "successActions": [
                {
                  "$type": "ApplyModifier",
                  "modifierName": "LaserTrackSetPos",
                  "target": "Target"
                },
                {
                  "$type": "SetPartControlTarget",
                  "target": "Target",
                  "partRootNames": ["ShootControl"]
                },
