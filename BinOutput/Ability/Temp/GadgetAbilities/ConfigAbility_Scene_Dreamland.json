                "offset": {
                  "x": 0,
                  "y": 0,
                  "z": 0
                }
              }
            }
          ]
        },
        "SwitchState_OnPlayerHit_MuteTimer": {
          "duration": 1,
          "onAdded": [
            {
              "$type": "SetGlobalValue",
              "key": "Can_Reaction_OnHit",
              "value": 0
            }
          ],
          "onRemoved": [
            {
              "$type": "SetGlobalValue",
              "key": "Can_Reaction_OnHit",
              "value": 1
            }
          ]
        },
        "SwitchState_OnPlayerHit": {
          "onBeingHit": [
            {
              "$type": "Predicated",
              "target": "Target",
              "targetPredicates": [
                {
                  "$type": "ByNot",
                  "predicates": [
                    {
                      "$type": "ByAttackTags",
                      "attackTags": ["BurningDamage", "ShockDamage"]
                    }
                  ]
                },
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "Can_Reaction_OnHit",
                  "target": "Self",
                  "value": 1,
                  "compareType": "Equal"
                },
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "SGV_Constellation_Target",
                  "target": "Self",
                  "value": 0,
                  "compareType": "MoreThan"
                }
              ],
              "successActions": [
                {
                  "$type": "Predicated",
                  "target": "Self",
                  "targetPredicates": [
                    {
                      "$type": "ByTargetGadgetState",
                      "gadgetState": 0
                    }
                  ],
                  "successActions": [
                    {
                      "$type": "ApplyModifier",
                      "target": "Self",
                      "modifierName": "SwitchState_OnPlayerHit_MuteTimer"
                    },
                    {
                      "$type": "ServerLuaCall",
                      "funcName": "SLC_Constellation_TryConnect",
                      "luaCallType": "FromGroup",
                      "predicates": [
                        {
                          "$type": "ByTargetGlobalValue",
                          "value": 0,
                          "compareType": "MoreThan",
                          "key": "SGV_Constellation_Mark"
                        }
                      ]
                    }
                  ],
                  "failActions": [
                    {
                      "$type": "ApplyModifier",
                      "target": "Self",
                      "modifierName": "SwitchState_OnPlayerHit_MuteTimer"
                    },
                    {
                      "$type": "ServerLuaCall",
                      "funcName": "SLC_Constellation_CloseConnect",
                      "luaCallType": "FromGroup"
                    }
                  ]
                }
              ]
            }
          ]
        }
      }
    }
  },
