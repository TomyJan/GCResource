  //	2.8辛焱岛 音符植物 逻辑层 查找周围关联物件
  //
  //////////////////////////////////////////////////////////////////////////////////////////////////
  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "SceneObj_Area_Common_Property_Prop_NotePlantShoot_01",
      "abilityMixins": [],
      "modifiers": {
        "NotePlantShoot_UnavailableMark": {
          "duration": 1.5,
          "stacking": "Unique",
          "onAdded": [
            {
              "$type": "SetGlobalValue", //标记自己不可再次被选
              "key": "NotePlant_MuteMark",
              "value": 1
            }
          ],
          "onRemoved": [
            {
              "$type": "SetGlobalValue",
              "key": "NotePlant_MuteMark",
              "value": 0
            }
          ]
        },
        "NotePlantShoot_Delay": {
          "duration": 0.6,
          "onRemoved": [
            {
              "$type": "Predicated",
              "otherTargets": {
                //r=4.8m
                "$type": "SelectTargetsByShape",
                "shapeName": "CircleTransferFlower",
                "centerBasedOn": "Self",
                "campTargetType": "SelfCamp",
                "campBasedOn": "Self",
                "sortType": "Random"
              },
              "predicatesForeach": [
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "NotePlant_Can_Track",
                  "value": 1, //是否可被音符植物追踪，这个值由目标物件自己维护
                  "compareType": "Equal"
                },
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "NotePlant_MuteMark",
                  "value": 0, //不找重复的物件
                  "compareType": "NoneOrEqual"
                }
              ],
              "successActions": [
                {
                  "$type": "ApplyModifier",
                  "modifierName": "NotePlantShoot_UnavailableMark", //标记自己不可再次被选
                  "target": "Target"
                },
                {
                  "$type": "TriggerAbility",
                  "target": "Target",
                  "abilityName": "SceneObj_Area_Common_Property_Prop_TransferFlowerShoot_01"
                }
              ]
            }
          ]
        }
      },
      "onAbilityStart": [
        {
          "$type": "ApplyModifier",
          "modifierName": "NotePlantShoot_Delay"
        }
      ]
    }
  },
