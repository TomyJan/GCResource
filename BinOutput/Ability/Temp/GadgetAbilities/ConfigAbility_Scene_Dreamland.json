          "thinkInterval": 2, //创生不保序 onAdd时连线对象可能不在
          "onThinkInterval": [
            {
              "$type": "SetGlobalValueToOverrideMap",
              "globalValueKey": "SGV_Constellation_Target",
              "overrideMapKey": "Constellation_Target"
            },
            {
              "$type": "AttachModifier",
              "modifierName": "Constellation_ConnectLine",
              "otherTargets": {
                "$type": "SelectTargetsBySelfGroup",
                "operation": "OR",
                "value": 0,
                "compareType": "Equal"
              },
              "predicatesForeach": [
                {
                  "$type": "ByTargetGlobalValue",
