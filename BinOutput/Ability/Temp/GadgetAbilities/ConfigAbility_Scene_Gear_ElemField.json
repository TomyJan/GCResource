[
  ///////////////////////////////////////
  //
  // 火元素场阶段切换器
  //
  ////////////////////////////////////
  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "SceneObj_Gear_ElementField_FireController",
      "abilityMixins": [
        {
          "$type": "AttachToGadgetStateMixin",
          "gadgetState": 0,
          "modifierName": "Default"
        },
        {
          "$type": "AttachToGadgetStateMixin",
          "gadgetState": 201,
          "modifierName": "Element_Handle"
        }
      ],
      "modifiers": {
        "Default": {
          "onAdded": []
        },

        "Element_Handle": {
          "thinkInterval": 0.5,
          "onThinkInterval": [
            {
              "$type": "ApplyModifier",
              "modifierName": "Fire_Summonor",
              "doOffStage": true,
              //"$type": "KillSelf",
              "otherTargets": {
                "$type": "SelectTargetsByShape",
                "shapeName": "CircleR100",
                "centerBasedOn": "Self",
                "campTargetType": "All",
                "campBasedOn": "Self"
              },
              "predicates": [
                {
                  "$type": "ByEntityTypes",
                  "entityTypes": ["Avatar"]
                }
              ]
            }
          ],
          "onAdded": [
            {
              "$type": "ApplyModifier",
              "modifierName": "Fire_Summonor",
              "doOffStage": true,
              //"$type": "KillSelf",
              "otherTargets": {
                "$type": "SelectTargetsByShape",
                "shapeName": "CircleR100",
                "centerBasedOn": "Self",
                "campTargetType": "All",
                "campBasedOn": "Self"
              },
              "predicates": [
                {
                  "$type": "ByEntityTypes",
                  "entityTypes": ["Avatar"]
                }
              ]
            }
          ],
          "onRemoved": [
            {
              "$type": "ApplyModifier",
              "modifierName": "Fire_AntiSummonor",
              "doOffStage": true,
              //"$type": "KillSelf",
              "otherTargets": {
                "$type": "SelectTargetsByShape",
                "shapeName": "CircleR100",
                "centerBasedOn": "Self",
                "campTargetType": "All",
                "campBasedOn": "Self"
              },
              "predicates": [
                {
                  "$type": "ByEntityTypes",
                  "entityTypes": ["Avatar"]
                }
              ]
            }
          ]
        },

        /////////////////////////////
        //
        //火焰召唤
        //
        /////////////////////////////
        "Fire_Summonor": {
          "duration": 0.2,
          "onAdded": [
            {
              "$type": "AddGlobalValue",
              "key": "FIRE_CHARGES",
              "value": 1,
              "useLimitRange": true,
              "minValue": 0,
              "maxValue": 100,
              "target": "CurLocalAvatar"
            },

            //刷新叠加数值
            {
              "$type": "SetGlobalValueToOverrideMap",
              "globalValueKey": "FIRE_CHARGES",
              "overrideMapKey": "CounterStage",
              "isFromOwner": false
            },

            //添火
            {
              "$type": "AddElementDurability",
              "modifierName": "Fire_Damge",
              "doOffStage": true,
              "target": "CurTeamAvatars",
              "value": 50,
              "useLimitRange": true,
              "maxValue": 50.0,
              "minValue": 0.0
            },
            //在固定层数刷新特效
            {
              "$type": "ApplyModifier",
              "modifierName": "Fire_Damge",
              "doOffStage": true,
              "predicates": [
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "HAS_FIRE_DAMAGE",
                  "value": 0,
                  "compareType": "NoneOrEqual"
                }
              ]
            },
            {
              "$type": "RemoveModifier",
              "modifierName": "Fire_Damge",
              "doOffStage": true,
              "predicates": [
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "FIRE_CHARGES",
                  "value": 31,
                  "compareType": "Equal"
                }
              ]
            },
            {
              "$type": "ApplyModifier",
              "modifierName": "Fire_Damge",
              "doOffStage": true,
              "predicates": [
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "FIRE_CHARGES",
                  "value": 31,
                  "compareType": "Equal"
                }
              ]
            },
            {
              "$type": "RemoveModifier",
              "modifierName": "Fire_Damge",
              "doOffStage": true,
              "predicates": [
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "FIRE_CHARGES",
                  "value": 61,
                  "compareType": "Equal"
                }
              ]
            },
            {
              "$type": "ApplyModifier",
              "modifierName": "Fire_Damge",
              "doOffStage": true,
              "predicates": [
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "FIRE_CHARGES",
                  "value": 61,
                  "compareType": "Equal"
                }
              ]
            }
          ]
        },

        "Fire_AntiSummonor": {
          "duration": 0.2,
          "onAdded": [
            {
              "$type": "AddGlobalValue",
              "key": "FIRE_CHARGES",
              "value": 0,
              "useLimitRange": true,
              "minValue": 0,
              "maxValue": 0,
              "target": "CurTeamAvatars"
            },
            {
              "$type": "RemoveModifier",
              "modifierName": "Fire_Damge",
              "target": "CurTeamAvatars"
            }
          ]
        },
        //在后台每2秒移除一层灼烧
        "Remove_Fire": {
          "thinkInterval": 2,
          "onThinkInterval": [
            {
              "$type": "AddGlobalValue",
              "key": "FIRE_CHARGES",
              "value": -10,
              "useLimitRange": true,
              "minValue": 0,
              "maxValue": 100,
              "doOffStage": true
            }
          ]
        },

        "Fire_Damge": {
          "duration": 1,
          "elementType": "Fire",
          "elementDurability": 50,
          "doOffStage": true,
          "onAvatarOut": [
            {
              "$type": "ApplyModifier",
              "modifierName": "Remove_Fire",
              "doOffStage": true
            }
          ],
          "onAvatarIn": [
            {
              "$type": "RemoveModifier",
              "doOffStage": true,
              "modifierName": "Remove_Fire"
            }
          ],
          "onAdded": [
            {
              "$type": "AddGlobalValue",
              "key": "HAS_FIRE_DAMAGE",
              "value": 1,
              "useLimitRange": true,
              "minValue": 0,
              "maxValue": 1,
              "doOffStage": true
            },
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_Element_Fire_Level_01",
              "predicates": [
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "FIRE_CHARGES",
                  "value": 30,
                  "compareType": "LessAndEqual"
                }
              ]
            },
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_Element_Fire_Level_02",
              "predicates": [
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "FIRE_CHARGES",
                  "value": 30,
                  "compareType": "MoreThan"
                },
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "FIRE_CHARGES",
                  "value": 60,
                  "compareType": "LessAndEqual"
                }
              ]
            },
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_Element_Fire_Level_03",
              "predicates": [
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "FIRE_CHARGES",
                  "value": 60,
                  "compareType": "MoreThan"
                }
              ]
            },
            {
              "$type": "DungeonFogEffects",
              "muteRemoteAction": true,
              "enable": true,
              "cameraFogEffectName": "Eff_Camera_Fire",
              "playerFogEffectName": "",
              "localOffset": {
                "x": 0,
                "y": 0,
                "z": 0
              }
            }
          ],
          "onRemoved": [
            {
              "$type": "AddGlobalValue",
              "key": "HAS_FIRE_DAMAGE",
              "value": -1,
              "useLimitRange": true,
              "minValue": 0,
              "maxValue": 1,
              "doOffStage": true
            },
            {
              "$type": "DungeonFogEffects",
              "muteRemoteAction": true,
              "enable": false,
              "cameraFogEffectName": "Eff_Camera_Fire",
              "playerFogEffectName": "",
              "localOffset": {
                "x": 0,
                "y": 0,
                "z": 0
              }
            }
          ],
          "thinkInterval": 2,
          "onThinkInterval": [
            {
              "$type": "DamageByAttackValue",
              "target": "CurLocalAvatar",
              "attacker": "Self",
              "attackInfo": {
                "attackProperty": {
                  "damagePercentage": 0.0,
                  "strikeType": "None",
                  "elementType": "Fire",
                  "elementDurability": 50,
                  "enBreak": 0.0,
                  "damageExtra": "%CounterStage"
                },
                "hitPattern": {
                  "onHitEffectName": "none",
                  "hitImpulseType": "LV2",
                  "overrideHitImpulse": {
                    "hitLevel": "Light",
                    "hitImpulseX": 600,
                    "hitImpulseY": 800
                  },
                  "reboundRatio": 0,
                  "canBeDefenceHalt": false,
                  "hitHaltTime": 0.0,
                  "hitHaltTimeScale": 0.05,
                  "muteHitText": false
                }
              }
            }
          ]
        }
      },
      "abilitySpecials": {
        "CounterStage": 0.0
      }
    }
  },

  ///////////////////////////////////////
  //
  // 冰元素场阶段切换器
  //
  ////////////////////////////////////
  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "SceneObj_Gear_ElementField_IceController",
      "abilityMixins": [
        {
          "$type": "AttachToGadgetStateMixin",
          "gadgetState": 0,
          "modifierName": "Default"
