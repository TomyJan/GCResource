      ],
      "modifiers": {
        "Default": {
          "onAdded": []
        },
        "Element_Handle": {
          "onAdded": [
            {
              "$type": "ApplyModifier",
              "modifierName": "UNIQUE_Ice_Summonor", //召唤寒冰围绕
              "target": "Team",
              "doOffStage": true
            },
            {
              "$type": "CreateGadget", //创建随主体一起死亡的子弹
              "gadgetID": 70330016,
              "lifeByOwnerIsAlive": true
            }
          ],
          "onRemoved": [
            {
              "$type": "RemoveModifier",
              "modifierName": "UNIQUE_Ice_Summonor",
