  //
  //////////////////////////////////////////////////////////////////////////////////////////////////
  //////////////////////////////////////////////////////////////////////////////////////////////////
  //
  // 死域玩法_死域核心(地上型)
  //
  //////////////////////////////////////////////////////////////////////////////////////////////////
  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "SceneObj_Area_Xm_Property_Ani_Prop_DeathZoneCore_01_Ability",
      "abilityMixins": [
        {
          //状态0时,就是很老实的状态
          "$type": "AttachToGadgetStateMixin",
          "gadgetState": 0,
          "modifierName": "IdleState"
        },
        {
          //状态1时,主要是处理喷雾
          "$type": "AttachToGadgetStateMixin",
          "gadgetState": 1,
          "modifierName": "AppearState"
        },
        {
          //201时玩家与其交互,或者攻击。物件死亡演出,同时向Group发ServerLuaCall通知状态改变
          "$type": "AttachToGadgetStateMixin",
          "gadgetState": 201,
          "modifierName": "CoreState"
        },
        {
          //202,死核死亡
          "$type": "AttachToGadgetStateMixin",
          "gadgetState": 202,
          "modifierName": "DeathState"
        },
        {
          "$type": "AttachToAnimatorStateIDMixin",
          "stateIDs": ["KeepClose"],
          "modifierName": "CoreClose"
        },
        {
          //与其交互时
          "$type": "WidgetMpSupportMixin",
          "succActions": [
            {
              "$type": "ApplyModifier",
              "target": "Self",
              "modifierName": "CoreInteeEffect",
              "predicates": [
                {
                  "$type": "ByTargetGadgetState",
                  "target": "Self",
                  "gadgetState": 201
                }
              ]
            }
          ]
        }
      ],
      "modifiers": {
        "CoreClose": {
          //核心关闭时的表现（在0和1状态时）
          "onAdded": [
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_SceneObj_MaranaCore_01_Idle"
            }
          ]
        },
        "CoreInteeEffect": {
          //交互后附加的Modifier
          "stacking": "Unique",
          "duration": 0.5,
          "onAdded": [
            {
              //Trigger
              "$type": "ServerLuaTriggerEvent",
              "luaCallType": "FromGroup",
              "sourceName": "DF_CallCamera"
            }
          ],
          "onRemoved": [
            {
              //Trigger
              "$type": "ServerLuaTriggerEvent",
              "luaCallType": "FromGroup",
              "sourceName": "DF_CoreDie"
            }
          ]
        },
        "IdleState": {
          //待机模式
          "onAdded": [
            {
              "$type": "AttachModifier",
              "target": "Self",
              "modifierName": "DisableIntee"
            },
            {
              "$type": "SetAnimatorInt",
              "intID": "IsOpen",
              "value": 0,
              "MPTriggerOnRemote": true
            }
          ]
        },
        "AppearState": {
          //吐烟模式
          "onAdded": [
            {
              "$type": "AttachModifier",
              "target": "Self",
              "modifierName": "DisableIntee"
            },
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_SceneObj_MaranaCore_WhiteMist_01"
            },
            {
              "$type": "SetAnimatorInt",
              "intID": "IsOpen",
              "value": 0,
              "MPTriggerOnRemote": true
            }
          ]
        },
        "CoreState": {
          //交互可用。攻击时也可触发
          "onAdded": [
            {
              "$type": "EnableGadgetInteeMp",
              "enable": true
            },
            {
              "$type": "EnableHitBoxByName",
              "hitBoxNames": ["HitBox"],
              "setEnable": true
            },
            {
              "$type": "SetAnimatorInt",
              "intID": "IsOpen",
              "value": 1,
              "MPTriggerOnRemote": true
            },
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_SceneObj_MaranaCore_01_Unlock"
            }
          ]
        },
        "DeathState": {
          "duration": 2.6,
          "onAdded": [
            {
              "$type": "AttachModifier",
              "target": "Self",
              "modifierName": "DisableIntee"
            },
            {
              "$type": "FireEffect",
              "effectPattern": "Eff_SceneObj_MaranaCore_01_Disappear"
            },
            {
              //根消失
              "$type": "SetMaterialParamFloatByTransform",
              "matName": "Root",
              "patternName": "_DissolveVal",
              "useCurve": true,
              "lerpTime": 2.5,
              "lerpCurveIndex": 0,
              "value": 1
            },
            {
              //核心消失
              "$type": "SetMaterialParamFloatByTransform",
              "matName": "Core",
              "patternName": "_DissolveVal",
              "useCurve": true,
              "lerpTime": 2.5,
              "lerpCurveIndex": 0,
              "value": 1.15
            },
            {
              //花消失
              "$type": "SetMaterialParamFloatByTransform",
              "matName": "Flower",
              "patternName": "_DissolveVal",
              "useCurve": true,
              "lerpTime": 2.5,
              "lerpCurveIndex": 0,
              "value": 1.8
            }
          ],
          "onRemoved": [
            {
              "$type": "KillSelf",
              "predicates": [
                {
                  "$type": "ByTargetGadgetState",
                  "gadgetState": 202
                }
              ]
            }
          ]
        },
        "DisableIntee": {
          //不可交互功能封装
          "onAdded": [
            {
              "$type": "EnableGadgetInteeMp",
              "enable": false
            },
            {
              "$type": "EnableHitBoxByName",
              "hitBoxNames": ["HitBox"],
              "setEnable": false
            }
          ]
        }
      }
    }
  },
