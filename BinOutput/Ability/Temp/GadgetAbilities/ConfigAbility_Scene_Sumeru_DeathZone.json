  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "SceneObj_Area_Xm_Property_Ani_Prop_DeathZonePoint_01_Ability",
      "onAdded": [
        {
          //Load时立刻死亡
          "$type": "KillSelf",
          "predicates": [
            {
              "$type": "ByTargetGadgetState",
              "gadgetState": 202
            }
          ]
        }
      ],
      "abilityMixins": [
        {
          "$type": "AttachToGadgetStateMixin",
          "gadgetState": 0,
          "modifierName": "Alive"
        },
        {
          //接收到来自GrassSeedChecker_Destory的GV
          "$type": "AttachModifierToSelfGlobalValueNoInitMixin",
          "globalValueKey": "SelfDestory",
          "valueSteps": [0.5, 1.5],
          "modifierNameSteps": ["DeathCall"]
        },
        {
          //测试用
          "$type": "AttachToGadgetStateMixin",
          "gadgetState": 901,
          "modifierName": "SetSummonTo1"
        },
        {
          //测试用
          "$type": "AttachToGadgetStateMixin",
          "gadgetState": 902,
          "modifierName": "SetSummonTo2"
        },
        {
          //测试用
          "$type": "AttachToGadgetStateMixin",
          "gadgetState": 903,
          "modifierName": "SetSummonTo3"
        }
      ],
      "modifiers": {
        "Alive": {
          "onAdded": [
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_SceneObj_MaranaNexus_Idle"
            },
            {
              "$type": "SetMaterialParamFloatByTransform",
              "matName": "Root",
              "patternName": "_DissolveVal",
              "value": 0,
              "canBeHandledOnRecover": true
            },
            {
              "$type": "SetMaterialParamFloatByTransform",
              "matName": "Core",
              "patternName": "_DissolveVal",
              "value": 0,
              "canBeHandledOnRecover": true
            },
            {
              "$type": "SetGlobalValue",
              "key": "SelfDestory",
              "value": 0
            }
          ]
        },
        "DeathCall": {
          //亡语必须保持很晚消失，给AttackLanded留时间
          "onAdded": [
            {
              "$type": "AttachModifier",
              "modifierName": "DistanceSummon"
            },
            {
              //死域节点切状态到202
              "$type": "ChangeGadgetState",
              "state": 202
            }
          ]
        },
        "DeathZone_ExtraDamage": {
          //死域重击效果=>此处应激发玩家ability
          "duration": 0.1,
          "onAdded": [
            {
              //角色身上特殊特效表现
              "$type": "SetGlobalValue",
              "key": "DeathZoneGV_ExtraAttack",
              "target": "Team",
              "value": 1
            }
          ]
        },
        "DistanceSummon": {
          //生效时获取玩家与节点之间的位置，然后根据位置信息处理不同亡语效果
          "onAdded": [
            {
              "$type": "SetSelfAttackTarget",
              "otherTargets": {
                "$type": "SelectTargetsByShape",
                "shapeName": "CircleR50",
                "centerBasedOn": "Self",
                "campTargetType": "Enemy",
                "campBasedOn": "Self",
                "topLimit": 1,
                "sortType": "Nearest"
              },
              "predicates": [
                {
                  "$type": "ByEntityTypes",
                  "entityTypes": ["Avatar"]
                }
              ]
            },
            //获取最近目标的位置
            {
              "$type": "Predicated",
              "targetPredicates": [
                {
                  "$type": "ByHasAttackTarget"
                },
                {
                  "$type": "ByTargetPositionToSelfPosition",
                  "target": "SelfAttackTarget",
                  "compareType": "XZ",
                  "value": 5,
                  "logic": "LesserOrEqual"
                }
              ],
              "successActions": [
                {
                  "$type": "DebugLog",
                  "content": "DeathZonePoint Trigger DeathCry_01 !!!"
                },
                {
                  "$type": "AttachModifier",
                  "modifierName": "DeathCry_01"
                }
              ],
              "failActions": [
                {
                  "$type": "Predicated",
                  "targetPredicates": [
                    {
                      "$type": "ByHasAttackTarget"
                    },
                    {
                      "$type": "ByTargetPositionToSelfPosition",
                      "target": "SelfAttackTarget",
                      "compareType": "XZ",
                      "value": 8,
                      "logic": "LesserOrEqual"
                    }
                  ],
                  "successActions": [
                    {
                      "$type": "DebugLog",
                      "content": "DeathZonePoint Trigger DeathCry_02 !!!"
                    },
