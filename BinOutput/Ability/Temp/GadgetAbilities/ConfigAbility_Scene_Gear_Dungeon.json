  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "SceneObj_Level_Common_Property_Ani_Prop_StageFireBasket_01",
      //基础：0，无特效；
      //101：小火；
      //201：临时大火，5秒后转回101；
      //202：临时最大火，5秒后转到203
      //203：最大火，持续
      "abilityMixins": [
        {
          "$type": "AttachToGadgetStateMixin",
          "gadgetState": 0,
          "modifierName": "DefaultEffect"
        },
        {
          "$type": "AttachToGadgetStateMixin",
          "gadgetState": 101,
          "modifierName": "BurnEffect"
        },
        {
          "$type": "AttachToGadgetStateMixin",
          "gadgetState": 201,
          "modifierName": "SuperBurnEffect"
        },
        {
          "$type": "AttachToGadgetStateMixin",
          "gadgetState": 202,
          "modifierName": "TempSuperBurnEffect"
        },
        {
          "$type": "AttachToGadgetStateMixin",
          "gadgetState": 203,
          "modifierName": "PersistentSuperBurnEffect"
        }
      ],
      "modifiers": {
        //gadgetstate = 0
        "DefaultEffect": {},
        //gadgetstate = 101
        "BurnEffect": {
          "onAdded": [
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_SceneObj_Fire_01_StageBasin_Large"
            }
          ]
        },
        //gadgetstate = 201
        //大火特效，持续5秒以后切回201
        "SuperBurnEffect": {
          "duration": 5,
          "onAdded": [
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_SceneObj_Fire_02_LargeFireBasket"
            }
          ],
          "onRemoved": [
            {
              "$type": "ExecuteGadgetLua",
              "target": "Self",
              "param1": 101
            }
          ]
        },
        //gadgetstate = 202
        //大火特效，持续5秒以后切回201
        "TempSuperBurnEffect": {
          "duration": 5,
          "onAdded": [
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_SceneObj_Fire_02_LargeFireBasket"
            }
          ],
          "onRemoved": [
            {
              "$type": "ExecuteGadgetLua",
              "target": "Self",
              "param1": 203
            }
          ]
        },
        //gadgetstate = 203
        //大火特效，持续
        "PersistentSuperBurnEffect": {
          "onAdded": [
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_SceneObj_Fire_01_StageBasin_LargeHigh"
