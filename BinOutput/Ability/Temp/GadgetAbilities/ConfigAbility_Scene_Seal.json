  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "Test_LightLock_Bullet",
      "onAdded": [
        {
          "$type": "AddGlobalValue",
          "target": "Self",
          "key": "_IS_LIGHTLOCK_BULLET",
          "value": 1
        },
        {
          "$type": "ApplyModifier",
          "target": "Self",
          "modifierName": "LightLock_Bullet"
        }
      ],
      "modifiers": {
        "LightLock_Bullet": {
          "thinkInterval": 1,
          "onAdded": [
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_Stages_Item_Clue01_FollowPlayer"
            }
          ],
          "onThinkInterval": [
            {
              "$type": "Predicated",
              "target": "Owner",
              "targetPredicates": [
                {
                  "$type": "ByTargetPositionToSelfPosition",
                  "compareType": "XZ",
                  "value": 120,
                  "logic": "LesserOrEqual"
                }
              ],
              "successActions": [
                {
                  "$type": "SendEffectTrigger",
                  "parameter": "Out_Danger"
                }
              ],
              "failActions": [
                {
                  "$type": "SendEffectTrigger",
                  "parameter": "In_Danger"
                }
              ]
            }
          ],
          "onKill": [
            {
              "$type": "SendEffectTrigger",
              "parameter": "Clue_Bullet_Clear"
            },
            {
              "$type": "AddGlobalValue",
              "target": "Team",
              "key": "_LIGHTLOCK_BULLET_AVATAR_COUNT",
              "value": -3,
              "useLimitRange": true,
              "maxValue": 3,
              "minValue": 0
            },
            {
              "$type": "AddGlobalValue",
              "target": "Team",
              "key": "_LIGHTLOCK_BULLET_LOST",
              "value": 1,
              "useLimitRange": true,
              "maxValue": 1,
              "minValue": -1
            },
            {
              "$type": "AddGlobalValue",
              "target": "Team",
              "key": "_LIGHTLOCK_BULLET_AVATAR_EXIST",
              "value": 0,
              "useLimitRange": true,
              "maxValue": 0,
              "minValue": 0
            },
            {
              "$type": "KillSelf"
            }
          ]
        }
      }
    }
  },
