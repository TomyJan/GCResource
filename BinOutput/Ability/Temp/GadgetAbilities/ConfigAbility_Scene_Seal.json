  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "Test_LightLock_Point",
      "onAdded": [
        {
          "$type": "ApplyModifier",
          "modifierName": "Eff_Material"
        }
      ],
      "onFieldEnter": [
        {
          "$type": "Predicated",
          "target": "Self",
          "targetPredicates": [
            {
              "$type": "ByNot",
              "predicates": [
                {
                  "$type": "ByTargetGadgetState",
                  "target": "Self",
                  "gadgetState": 202
                }
              ]
            }
          ],
          "successActions": [
            {
              "$type": "ApplyModifier",
              "target": "Self",
              "modifierName": "Notice_LightLock"
            },
            {
              "$type": "AddGlobalValue",
              "target": "Team",
              "key": "_LIGHTLOCK_BULLET_AVATAR_COUNT",
              "value": 1,
              "useLimitRange": true,
              "maxValue": 3,
              "minValue": 0
            }
          ]
        }
      ],
      "abilityMixins": [
        {
          "$type": "AttachToGadgetStateMixin",
          "gadgetState": 0,
          "modifierName": "Default_0"
        },
        {
          "$type": "AttachToGadgetStateMixin",
          "gadgetState": 202,
          "modifierName": "Locked"
        },
        {
          "$type": "AttachToGadgetStateMixin",
          "gadgetState": 201,
          "modifierName": "LightCross_Die"
        }
      ],
      "modifiers": {
        "Eff_Material": {
          "onAdded": [
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_Stages_Item_Clue01"
            }
          ]
        },
        "Default_0": {
          "onAdded": []
        },
        "Locked": {
          "onAdded": [
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_Item_SpecialTreasureBox_LockIdle",
              "born": {
                "$type": "ConfigBornByAttachPoint",
                "attachPointName": "RootNode",
                "offset": {
                  "x": 0.0,
                  "y": 0.5,
                  "z": 0.0
                }
              }
            }
          ],
          "onRemoved": [
            {
              "$type": "FireEffect",
              "effectPattern": "Eff_Item_SpecialTreasureBox_Unlock",
              "born": {
                "$type": "ConfigBornByAttachPoint",
                "attachPointName": "RootNode",
                "offset": {
                  "x": 0.0,
                  "y": 0.5,
                  "z": 0.0
                }
              }
            }
          ]
        },
        "Notice_LightLock": {
          "onAdded": [
            {
              "$type": "ExecuteGadgetLua",
              "param1": 1
            }
          ]
        },
        "LightCross_Die": {
          "onAdded": [
            {
              "$type": "SendEffectTrigger",
              "effectPattern": "Eff_Stages_Item_Clue01",
              "parameter": "Clue_Die"
            },
            {
              "$type": "KillSelf"
            }
          ]
        }
