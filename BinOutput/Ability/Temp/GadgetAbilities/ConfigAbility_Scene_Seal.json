  //
  //	魔改光锁逻辑
  //
  //////////////////////////////////////////////////////
  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "Test_LightLock_Seal_Logic",
      "onAdded": [
        /*{
					"$type": "AddGlobalValue",
					"target": "Team",
					"key": "_LIGHTLOCK_BULLET_AVATAR_EXIST",
					"value": 0,
					"useLimitRange": true,
					"maxValue": 0,
					"minValue": 0,
				},*/
        {
          "$type": "AddGlobalValue",
          "target": "Self",
          "key": "_LIGHTLOCK_ACTIVE_FORBID",
          "value": 0,
          "useLimitRange": true,
          "maxValue": 0,
          "minValue": 0
        },
        {
          "$type": "AddGlobalValue",
          "target": "Self",
          "key": "_IS_LIGHTLOCK",
          "value": 1,
          "useLimitRange": true,
          "maxValue": 1,
          "minValue": 1
        }
      ],
      "abilityMixins": [
        //禁止回收子弹的状态
        {
          "$type": "AttachToGadgetStateMixin",
          "gadgetState": 204,
          "modifierName": "Active_Forbid"
        },
        {
          "$type": "AttachToStateIDMixin",
          "stateIDs": ["Seal_Collect"],
          "modifierName": "Check_Distance"
        },
        //给玩家添加子弹的状态
        {
          "$type": "AttachToGadgetStateMixin",
          "gadgetState": 103,
          "modifierName": "Bullet_Create"
        }
      ],
      "modifiers": {
        "Active_Forbid": {
          "thinkInterval": 1,
          "onAdded": [
            {
              "$type": "AddGlobalValue",
              "target": "Self",
              "key": "_LIGHTLOCK_ACTIVE_FORBID",
              "value": 1,
              "useLimitRange": true,
              "maxValue": 1,
              "minValue": 0
            }
          ],
          "onThinkInterval": [
            {
              "$type": "AddGlobalValue",
              "target": "Self",
              "key": "_LIGHTLOCK_ACTIVE_FORBID",
              "value": 1,
              "useLimitRange": true,
              "maxValue": 1,
              "minValue": 0
            }
          ],
          "onRemoved": [
            {
              "$type": "AddGlobalValue",
              "target": "Self",
              "key": "_LIGHTLOCK_ACTIVE_FORBID",
              "value": -1,
              "useLimitRange": true,
              "maxValue": 1,
              "minValue": 0
            }
          ]
        },
        //用于检测玩家是否在光锁半径160米内
        "Check_Distance": {
          "onAdded": [
            {
              "$type": "AddGlobalValue",
              "target": "Team",
              "key": "_LIGHTLOCK_BULLET_LOST",
              "value": 0,
              "useLimitRange": true,
              "maxValue": 1,
              "minValue": -1
            },
            {
              "$type": "ApplyModifier",
              "target": "Self",
              "modifierName": "Check_Bullet_Lost"
            },
            {
              "$type": "ApplyModifier",
              "target": "Self",
              "modifierName": "Initial_Check"
            }
          ]
        },
        //比对玩家计数与封印计数，若不一致说明玩家出界了
        "Initial_Check": {
          "duration": 0.5,
          "onRemoved": [
            {
              "$type": "ApplyModifier",
              "modifierName": "Group_Reset",
              "predicates": [
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "_LIGHTLOCK_BULLET_COUNT",
                  "value": 0,
                  "compareType": "MoreThan"
                }
              ]
            },
            {
              "$type": "AddGlobalValue",
              "target": "Self",
              "key": "_LIGHTLOCK_BULLET_COUNT",
              "value": -3,
              "useLimitRange": true,
              "maxValue": 3,
              "minValue": 0,
              "predicates": [
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "_LIGHTLOCK_BULLET_COUNT",
                  "value": 0,
                  "compareType": "MoreThan"
                }
              ]
            }
          ]
        },
        //判断子弹消失时出界/吸附
        "Check_Bullet_Lost": {
          "thinkInterval": 1,
          "onThinkInterval": [
            //每秒判断是否有玩家出界引起的子弹消失问题
            {
              "$type": "Predicated",
              "target": "Team",
              "targetPredicates": [
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "_LIGHTLOCK_BULLET_LOST",
                  "value": 1,
                  "compareType": "Equal"
                }
              ],
              "successActions": [
                {
                  "$type": "ApplyModifier",
                  "modifierName": "Point_Refresh",
                  "target": "Caster"
                }
              ],
              "failActions": [
                {
                  "$type": "AddGlobalValue",
                  "target": "Team",
                  "key": "_LIGHTLOCK_BULLET_LOST",
                  "value": -1,
                  "useLimitRange": true,
                  "maxValue": 1,
                  "minValue": 0
                }
              ]
            },
            //每秒判断玩家是否进入到10米内的吸附区
            {
              "$type": "ApplyModifier",
              "target": "Caster",
              "modifierName": "LightLock_Attract",
              "predicates": [
                {
                  "$type": "ByTargetPositionToSelfPosition",
                  "target": "Team",
                  "compareType": "XYZ",
                  "value": 10,
                  "logic": "LesserOrEqual"
                },
                {
                  "$type": "ByTargetGlobalValue",
                  "target": "Self",
                  "key": "_LIGHTLOCK_ACTIVE_FORBID",
                  "value": 0,
                  "compareType": "Equal"
                }
              ]
            }
          ]
        },
        "LightLock_Attract": {
          "duration": 1,
          "onAdded": [
            //防止onkill引起的group重置
            {
              "$type": "AddGlobalValue",
              "target": "Team",
              "key": "_LIGHTLOCK_BULLET_LOST",
              "value": -1,
              "useLimitRange": true,
              "maxValue": 1,
              "minValue": -1
            },
            {
              "$type": "Predicated",
              "target": "Team",
              "targetPredicates": [
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "_LIGHTLOCK_BULLET_AVATAR_COUNT",
                  "value": 1,
                  "compareType": "Equal"
                }
              ],
              "successActions": [
                {
                  "$type": "AddGlobalValue",
                  "target": "Self",
                  "key": "_LIGHTLOCK_BULLET_COUNT",
                  "value": 1,
                  "useLimitRange": true,
                  "maxValue": 1,
                  "minValue": 0
                }
              ]
            },
            {
              "$type": "Predicated",
              "target": "Team",
              "targetPredicates": [
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "_LIGHTLOCK_BULLET_AVATAR_COUNT",
                  "value": 2,
                  "compareType": "Equal"
                }
              ],
              "successActions": [
                {
                  "$type": "AddGlobalValue",
                  "target": "Self",
                  "key": "_LIGHTLOCK_BULLET_COUNT",
                  "value": 2,
                  "useLimitRange": true,
                  "maxValue": 2,
                  "minValue": 0
                }
              ]
            },
            {
              "$type": "Predicated",
              "target": "Team",
              "targetPredicates": [
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "_LIGHTLOCK_BULLET_AVATAR_COUNT",
                  "value": 3,
                  "compareType": "Equal"
                }
              ],
              "successActions": [
                {
                  "$type": "AddGlobalValue",
                  "target": "Self",
                  "key": "_LIGHTLOCK_BULLET_COUNT",
                  "value": 3,
                  "useLimitRange": true,
                  "maxValue": 3,
                  "minValue": 0
                }
              ]
            },
            {
              "$type": "ApplyModifier",
              "modifierName": "LightLock_Bullet_Attract",
              "otherTargets": {
                "$type": "SelectTargetsByShape",
                "shapeName": "CircleR15",
                "centerBasedOn": "Self",
                "campTargetType": "All",
                "campBasedOn": "Self"
              },
              "predicates": [
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "_IS_LIGHTLOCK_BULLET",
                  "value": 1,
                  "compareType": "Equal"
                }
              ]
            },
            //根据子弹计数决定封印的状态转换情况
            {
              "$type": "ExecuteGadgetLua",
              "param1": 1,
              "predicates": [
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "_LIGHTLOCK_BULLET_COUNT",
                  "value": 1,
                  "compareType": "Equal"
                }
              ]
            },
            {
              "$type": "ExecuteGadgetLua",
              "param1": 2,
              "predicates": [
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "_LIGHTLOCK_BULLET_COUNT",
                  "value": 2,
                  "compareType": "Equal"
                }
              ]
            },
            {
              "$type": "ExecuteGadgetLua",
              "param1": 3,
              "predicates": [
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "_LIGHTLOCK_BULLET_COUNT",
                  "value": 3,
                  "compareType": "Equal"
                }
              ]
            },
            //减少封印的count计数
            {
              "$type": "AddGlobalValue",
              "target": "Caster",
              "key": "_LIGHTLOCK_BULLET_COUNT",
              "value": -3,
              "useLimitRange": true,
              "maxValue": 3,
              "minValue": 0
