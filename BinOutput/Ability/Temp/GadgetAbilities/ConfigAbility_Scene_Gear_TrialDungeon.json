[
  //////////////////////////////////
  //
  //  试玩活动——追踪机关（基础）
  //
  //////////////////////////////////
  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "SceneObj_Level_Common_Property_Ani_Prop_TrackingOrgan_01_Ability",
      "abilitySpecials": {
        "CDTime": 60
      },
      "onAdded": [
        {
          "$type": "AttachAbilityStateResistance",
          "resistanceListID": 11001
        },
        {
          "$type": "SetGlobalValue",
          "key": "_Gear_State", // -1 => Stop/ 0=> Close / 1=>Start
          "value": 0
        },
        {
          "$type": "SetGlobalValue",
          "key": "_Avatar_Enter_Field", // 1 => Enter/ 0=> Exit
          "value": 0
        }
      ],
      "onFieldEnter": [
        {
          "$type": "Predicated",
          "target": "Target",
          "targetPredicates": [
            {
              "$type": "ByEntityTypes",
              "entityTypes": ["Avatar"]
            },
            {
              "$type": "ByIsLocalAvatar"
            }
          ],
          "successActions": [
            {
              "$type": "AddGlobalValue",
              "target": "Self",
              "key": "_Avatar_Enter_Field", // >1 => Enter/ 0=> Exit
              "value": 1
            },
            {
              "$type": "SetGlobalValue",
              "target": "Self",
              "key": "_Gear_State",
              "value": 1,
              "predicates": [
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "_Gear_State",
                  "value": 0,
                  "compareType": "Equal"
                }
              ]
            }
          ]
        }
      ],
      "onFieldExit": [
        {
          "$type": "Predicated",
          "target": "Target",
          "targetPredicates": [
            {
              "$type": "ByEntityTypes",
              "entityTypes": ["Avatar"]
            },
            {
              "$type": "ByIsLocalAvatar"
            }
          ],
          "successActions": [
            {
              "$type": "AddGlobalValue",
              "target": "Self",
              "key": "_Avatar_Enter_Field",
              "value": -1,
              "useLimitRange": true,
              "minValue": 0
            }
          ]
        }
      ],
      "abilityMixins": [
        {
          // -1 => Stop/ 0=> Close / 1=>Start
          "$type": "AttachModifierToSelfGlobalValueMixin",
          "globalValueKey": "_Gear_State",
          "valueSteps": [-1.5, -0.5, 0.5, 1.5],
          "modifierNameSteps": ["Gear_Stop", "Gear_Close", "Gear_Start"]
        }
      ],
      "modifiers": {
        "Gear_Stop": {
          "duration": "%CDTime",
          "onAdded": [
            {
              "$type": "SetAnimatorInt",
              "intID": "State",
              "value": 0
            },
            {
              "$type": "ExecuteGadgetLua",
              "param1": 202
            }
          ],
          "onRemoved": [
            {
              "$type": "SetGlobalValue",
              "target": "Self",
              "key": "_Gear_State",
              "value": 0,
              "predicates": [
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "_Avatar_Enter_Field",
                  "value": 0,
                  "compareType": "Equal"
                }
              ]
            },
            {
              "$type": "SetGlobalValue",
              "target": "Self",
              "key": "_Gear_State",
              "value": 1,
              "predicates": [
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "_Avatar_Enter_Field",
                  "value": 1,
                  "compareType": "MoreThanAndEqual"
                }
              ]
            }
          ]
        },
        "Gear_Close": {
          "onAdded": [
            {
              "$type": "SetAnimatorInt",
              "intID": "State",
              "value": 0
            },
            {
              "$type": "ExecuteGadgetLua",
              "param1": 0
            }
          ]
        },
        "Gear_Start": {
          "onAdded": [
            {
              "$type": "SetAnimatorInt",
              "intID": "State",
              "value": 201
            },
            {
              "$type": "ExecuteGadgetLua",
              "param1": 201
            }
          ]
        }
      }
    }
  },
