  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "SceneObj_Area_Dq_Property_Ani_Prop_CircleConsole_01",
      "abilityMixins": [
        //白天锁定
        {
          "$type": "AttachToGadgetStateMixin",
          "gadgetState": 200,
          "modifierName": "DayLocked"
        },
        //夜晚状态
        {
          "$type": "AttachToGadgetStateMixin",
          "gadgetState": 300,
          "modifierName": "NightLocked"
        },

        //白天状态
        {
          "$type": "AttachToGadgetStateMixin",
          "gadgetState": 202,
          "modifierName": "DayState"
        },
        //夜晚状态
        {
          "$type": "AttachToGadgetStateMixin",
          "gadgetState": 302,
          "modifierName": "NightState"
        },
        //转换到白天状态
        {
          "$type": "AttachToGadgetStateMixin",
          "gadgetState": 212,
          "modifierName": "ChangeToDayCountdown"
        },
        //转换到夜晚状态
        {
          "$type": "AttachToGadgetStateMixin",
          "gadgetState": 312,
          "modifierName": "ChangeToNightCountdown"
        }
      ],

      "onFieldEnter": [
        //昼夜转换机关教学
        {
          "$type": "DoWatcherSystemAction",
          "watcherId": 1133,
          "target": "Target",
          "authorityOnly": true,
          "predicates": [
            {
              "$type": "ByNot",
              "predicates": [
                {
                  "$type": "ByAny",
                  "predicates": [
                    {
                      "$type": "ByTargetGadgetState",
                      "target": "Self",
                      "gadgetState": 200
                    },
                    {
                      "$type": "ByTargetGadgetState",
                      "target": "Self",
                      "gadgetState": 300
                    }
                  ]
                }
              ]
            }
          ]
        }
      ],
      "modifiers": {
        //锁定状态
        "DayLocked": {
          "onAdded": [
            {
              "$type": "SetAnimatorInt",
              "intID": "Glow",
              "value": 0
            }
          ]
        },
        "NightLocked": {
          "onAdded": [
            {
              "$type": "SetAnimatorInt",
              "intID": "Glow",
              "value": 0
            }
          ]
