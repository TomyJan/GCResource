            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_SceneObj_CircleConsole_01_Activated",
              "effectTempleteID": 101,
              "scale": 1,
              "born": {
                "$type": "ConfigBornByAttachPoint",
                "attachPointName": "EffectPoint",
                "useRotation": true
              }
            }
          ],
          "onRemoved": [
            {
              "$type": "DebugLog",
              "content": "EnvState: 转换到白天完毕"
            },
            {
              "$type": "ExecuteGadgetLua",
              "param1": 202
