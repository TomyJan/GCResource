          "onAdded": [
            {
              "$type": "SetAnimatorInt",
              "intID": "Glow",
              "value": 101
            },
            {
              "$type": "SetAnimatorInt",
              "intID": "Color",
              "value": 103
            },
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_SceneObj_CircleConsole_01_Stay",
              "effectTempleteID": 101,
              "born": {
                "$type": "ConfigBornByAttachPoint",
