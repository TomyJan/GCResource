                      "target": "Applier",
                      "key": "SGV_Lock",
                      "value": 0,
                      "compareType": "Equal"
                    },
                    {
                      "$type": "ByTargetGadgetState",
                      "target": "Applier",
                      "gadgetState": 0
                    }
                  ]
                }
              ]
            }
          ]
        },
        //未激活，微弱发光
        "DefaultState": {
          "onAdded": [
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_SceneObj_TeleportRingOfTokoyo_Idle",
              "born": {
                "$type": "ConfigBornByAttachPoint",
                "attachPointName": "RootNode"
              }
            },
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_SceneObj_TeleportRingOfTokoyo_Blue_Light",
              "effectTempleteID": 100,
              "born": {
                "$type": "ConfigBornByAttachPoint",
                "attachPointName": "EffectPoint"
              }
            }
          ]
        },
        "Active": {
          "onAdded": [
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_SceneObj_TeleportRingOfTokoyo_Idle",
              "born": {
                "$type": "ConfigBornByAttachPoint",
                "attachPointName": "RootNode"
              }
            },
            {
              "$type": "FireEffect",
              "effectPattern": "Eff_SceneObj_TeleportRingOfTokoyo_Blue_Active",
              "born": {
                "$type": "ConfigBornByAttachPoint",
                "attachPointName": "EffectPoint"
              }
            },
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_SceneObj_TeleportRingOfTokoyo_Blue_Hard",
              "effectTempleteID": 100,
              "born": {
                "$type": "ConfigBornByAttachPoint",
                "attachPointName": "EffectPoint"
              }
            }
          ]
        },
        "Success": {
          //"duration": 3,
          "onAdded": [
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_SceneObj_TeleportRingOfTokoyo_Active",
              "born": {
                "$type": "ConfigBornByAttachPoint",
                "attachPointName": "RootNode"
              }
            },
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_SceneObj_TeleportRingOfTokoyo_Blue_Hard",
              "effectTempleteID": 100,
              "born": {
                "$type": "ConfigBornByAttachPoint",
