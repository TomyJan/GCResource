          "onAdded": [
            {
              "$type": "FireEffect",
              "effectPattern": "Eff_SceneObj_Ballon_03_Shoot" //B.被击杀死亡临时特效(被波及的)
            },
            //高分气球
            {
              "$type": "ApplyModifier",
              "modifierName": "HighCall",
              "predicates": [
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "IS_HIGHBALLOON",
                  "value": 1,
                  "compareType": "Equal"
                }
              ]
            },
            //低分气球
            {
              "$type": "ApplyModifier",
