  {
    //70800231	3.1捕猎活动_捕网陷阱[投掷生成]  数量上限处理
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "SceneObj_Level_Activity_Hunting_Trap_02_Limit_Handler",
      "onAbilityStart": [
        //数量上限管理器
        {
          "$type": "SetGlobalValue",
          "key": "_Hunt_Trap_Count",
          "value": 0
        },
        {
          //先查场上有几个
          "$type": "Predicated",
          "otherTargets": {
            "$type": "SelectTargetsByTag",
            "tag": "_Hunt_Trap",
            "entityTypePriority": ["Gadget"]
          },
          "successActions": [
            {
              "$type": "AddGlobalValue",
              "key": "_Hunt_Trap_Count",
              "value": 1
            }
          ]
        },
        {
          //如果超过3个(这个value不包括自己)
          "$type": "ApplyModifier",
          "modifierName": "Action_KillSelf_Predicate_Handler",
          "predicates": [
            {
              "$type": "ByTargetGlobalValue",
              "key": "_Hunt_Trap_Count",
              "compareType": "MoreThan",
              "value": 2
            }
          ]
        }
      ],
      "onAdded": [
        {
          "$type": "SetGlobalValue",
          "key": "_Hunt_Trap_Order",
          "value": 0
        },
        {
          //物件order修改
          "$type": "Predicated",
          "otherTargets": {
            "$type": "SelectTargetsByTag",
            "tag": "_Hunt_Trap",
            "entityTypePriority": ["Gadget"]
          },
          "successActions": [
            {
              "$type": "ApplyModifier",
              "modifierName": "Action_Add_Order",
              "target": "Target"
            }
          ]
        },
        {
          //各自检测是否要killself
          "$type": "Predicated",
          "otherTargets": {
            "$type": "SelectTargetsByTag",
            "tag": "_Hunt_Trap",
            "entityTypePriority": ["Gadget"]
          },
          "successActions": [
            {
              "$type": "TriggerAbility",
              "abilityName": "SceneObj_Level_Activity_Hunting_Trap_02_Limit_Handler",
              "target": "Target"
            }
          ]
        },
        {
          "$type": "ChangeTag",
          "isAdd": true,
          "tag": "_Hunt_Trap"
        }
      ],
      "modifiers": {
        "Action_KillSelf_Predicate_Handler": {
          //数量超过3时每个物件都会跑这个
          "duration": 0.1,
          "onAdded": [
            {
              "$type": "SetGlobalValueToOverrideMap",
              "globalValueKey": "_Hunt_Trap_Order",
              "overrideMapKey": "_Hunt_Trap_Order"
            },
            {
              "$type": "SetGlobalValue",
              "key": "_Is_Order_Largest",
              "value": 1
            },
            {
              "$type": "Predicated",
              "otherTargets": {
                "$type": "SelectTargetsByTag",
                "tag": "_Hunt_Trap",
                "entityTypePriority": ["Gadget"]
              },
              "targetPredicates": [
                {
                  //只要任意一个比我大，就不用killself
                  "$type": "ByTargetGlobalValue",
                  "key": "_Hunt_Trap_Order",
                  "compareType": "MoreThan",
                  "value": "%_Hunt_Trap_Order"
                }
              ],
              "successActions": [
                {
                  "$type": "SetGlobalValue",
                  "key": "_Is_Order_Largest",
                  "value": 0
                }
              ]
            },
            {
              "$type": "KillSelf",
              "hideEntity": true,
              "predicates": [
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "_Is_Order_Largest",
                  "compareType": "Equal",
                  "value": 1
                }
              ]
            }
          ]
        },
        "Action_Add_Order": {
          "duration": 0.1,
          "onAdded": [
            {
              "$type": "AddGlobalValue",
              "key": "_Hunt_Trap_Order",
              "value": 1
            }
          ]
        }
      },
      "abilitySpecials": {
        "_Hunt_Trap_Order": 0
      }
    }
  },
