  //////////////////////////////////////////////////////
  //
  //	鹤观 雷鸟雕像
  //
  //////////////////////////////////////////////////////
  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "SceneObj_Area_Dq_Property_Build_ReginStatue_01",
      "onAdded": [
        //标记自己是雷鸟羽毛交互物件
        {
          "$type": "SetGlobalValue",
          "target": "Self",
          "key": "GV_ReginGadget",
          "value": 1
        },
        //标记用来监听发送消息的GV
        {
          "$type": "SetGlobalValue",
          "target": "Self",
          "key": "GV_ReginSignal",
          "value": 0
        }
      ],
      "abilityMixins": [
        {
          "$type": "AttachToGadgetStateMixin",
          "gadgetState": 0
        },
        //雷鸟雕像在201状态才会挂监听消息的Modifier
        {
          "$type": "AttachToGadgetStateMixin",
          "gadgetState": 201,
          "modifierName": "ReginListener"
        }
      ],
      "modifiers": {
        "SLC_CallReginStatueGroup": {
          "onAdded": [
            {
              "$type": "DebugLog",
              "content": "### Tsurumi Call Group"
            },
            {
              "$type": "ServerLuaCall",
              "luaCallType": "FromGroup",
              "funcName": "SLC_CallTsurumiGroup",
              "target": "Self",
              "isTarget": true
            }
          ]
        },

        "ReginListener": {
          "modifierMixins": [
            {
              "$type": "AttachModifierToSelfGlobalValueMixin",
              "globalValueKey": "GV_ReginSignal",
              "valueSteps": [0.5, 20],
              "modifierNameSteps": ["SLC_CallReginStatueGroup"]
            }
          ]
        }
      }
    }
  },

