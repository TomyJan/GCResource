  ///////////////////////////////////////////////////////////////////////////////////////
  //
  //   2.8海岛大活动 旋涡-大
  //
  /////////////////////////////////////////////////////////////////////////////////////////////
  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "SceneObj_Area_Common_Property_Prop_Whirlpool_03",
      "onAdded": [
        {
          "$type": "ApplyModifier",
          "modifierName": "SphereOne"
        },
        {
          "$type": "ApplyModifier",
          "modifierName": "SphereTwo"
        },
        {
          "$type": "ApplyModifier",
          "modifierName": "SphereThree"
        }
      ],
      "onFieldEnter": [
        {
          "$type": "ApplyModifier",
          "modifierName": "SelfDie",
          "target": "Target",
          "predicates": [
            {
              "$type": "ByEntityTypes",
              "entityTypes": ["Vehicle"]
            }
          ]
        },
        {
          "$type": "ApplyModifier",
          "modifierName": "Drowning",
          "target": "Target",
          "predicates": [
            {
              "$type": "ByEntityTypes",
              "entityTypes": ["Avatar"]
            },
            {
              "$type": "ByTargetGlobalValue",
              "key": "_ABILITY_Avatar_InSkiff_Mark",
              "value": 0,
              "compareType": "NoneOrEqual"
            },
            {
              //不吸冲刺状态的船
              "$type": "ByTargetGlobalValue",
              "key": "_ABILITY_Skiff_SprintMark",
              "value": 0,
              "compareType": "NoneOrEqual"
            }
          ]
        }
      ],
      "onFieldExit": [
        {
          "$type": "RemoveModifier",
          "modifierName": "Drowning",
          "target": "Target",
          "predicates": [
            {
              "$type": "ByEntityTypes",
              "entityTypes": ["Avatar"]
            }
          ]
        },
        {
          "$type": "RemoveModifier",
          "modifierName": "SelfDie",
          "target": "Target",
          "predicates": [
            {
              "$type": "ByEntityTypes",
              "entityTypes": ["Vehicle"]
            }
          ]
        }
      ],
      "modifiers": {
        "LoseStamina": {
          "stacking": "Refresh",
          "duration": 0.4,
          "modifierMixins": [
            {
              "$type": "CostStaminaMixin",
              "costStaminaDelta": 15
            }
          ]
        },
        "SelfDie": {
          "modifierMixins": [
            {
              "$type": "CostStaminaMixin",
              "costStaminaDelta": 15
            }
          ],
          "thinkInterval": 1,
          "onThinkInterval": [
            {
              "$type": "LoseHP",
              "target": "Self",
              "amountByTargetMaxHPRatio": 0.2, //每秒低温的扣血百分比
              "amount": 150,
              "lethal": true, //低温扣血的致死性
              "disableWhenLoading": true
            }
          ]
        },
        "Drowning": {
          "thinkInterval": 0.2,
          "onThinkInterval": [
            {
              "$type": "ApplyModifier",
              "modifierName": "LoseStamina",
              "target": "Self"
            },
            {
              "$type": "RemoveModifier",
              "modifierName": "Drowning",
              "target": "Self",
              "predicates": [
                {
                  "$type": "ByNot",
                  "predicates": [
                    {
                      "$type": "ByEntityIsAlive",
                      "target": "Applier"
                    }
                  ]
                }
              ]
            },
            {
              "$type": "RemoveModifier",
              "modifierName": "Drowning",
              "target": "Self",
              "predicates": [
                {
                  "$type": "ByTargetPositionToSelfPosition",
                  "target": "Applier",
                  "compareType": "XZ",
                  "value": 3,
                  "logic": "Greater"
                }
              ]
            },
            {
              //氮气冲刺解除状态
              "$type": "RemoveModifier",
              "modifierName": "Drowning",
              "target": "Self",
              "predicates": [
                {
                  "$type": "ByTargetGlobalValue",
                  "target": "Self",
                  "key": "_ABILITY_Skiff_SprintMark",
                  "value": 1,
                  "compareType": "Equal"
                }
              ]
            }
          ]
        },
        "SphereOne": {},
        "SphereTwo": {
          "modifierMixins": [
            {
              "$type": "WindZoneMixin",
              "shapeName": "CircleR6H3",
              "strength": 2.8,
              "innerRadius": 1,
              "attenuation": 3,
              "targetType": "All",
              "offset": {
                "x": 0.0,
                "y": -1.4,
                "z": 0.0
              },
              "predicates": [
                {
                  "$type": "ByEntityTypes",
                  "entityTypes": ["Avatar"]
                  /*"reject" : true,*/
                }
              ]
            }
          ]
        },
        "SphereThree": {
          "modifierMixins": [
            {
              "$type": "WindZoneMixin",
              "shapeName": "CircleR6H3",
              "strength": 380,
              "innerRadius": 1,
              "attenuation": 3,
              "targetType": "All",
              "offset": {
                "x": 0.0,
                "y": -1.4,
                "z": 0.0
              },
              "predicates": [
                {
                  "$type": "ByEntityTypes",
                  "entityTypes": ["Vehicle"]
                  /*"reject" : true,*/
                }
              ]
            }
          ]
        }
      }
    }
  },
