[
  ////////////////////////////////////////////////////////////////
  //
  //  12301 铁影阔剑
  //
  //  生命值低于rate%时，提升分支攻击时的霸体值和伤害   ok
  //
  ////////////////////////////////////////////////////////////////
  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "Weapon_Claymore_LowHPAddExtraAttackEndura",
      "isDynamicAbility": true,
      "abilityMixins": [
        {
          "$type": "AttachModifierToPredicateMixin",
          "modifierName": "UNIQUE_Claymore_LowHPAddExtraAttackEndura_Handler",
          "onEvent": "HP",
          "predicates": [
            {
              "$type": "ByTargetHPRatio",
              "target": "Self",
              "HPRatio": "%HPRate",
              "logic": "LesserOrEqual"
            }
          ]
        }
      ],
      "modifiers": {
        "UNIQUE_Claymore_LowHPAddExtraAttackEndura_Handler": {
          "modifierMixins": [
            {
              "$type": "AttachToStateIDMixin",
              "stateIDs": [
                "ExtraAttack",
                "ExtraAttack1",
                "ExtraAttack2",
                "ExtraAttack3"
              ], //由于命名问题，实际上extraattack就是BS起手阶段，然后ExtraAttack数字最大的会作为收尾动作。
              "target": "Self",
              "modifierName": "UNIQUE_Claymore_LowHPAddExtraAttackEndura"
            }
          ],
          "stacking": "Unique",
          "isUnique": true
        },
        "UNIQUE_Claymore_LowHPAddExtraAttackEndura": {
          "stacking": "Unique",
          "isUnique": true,
