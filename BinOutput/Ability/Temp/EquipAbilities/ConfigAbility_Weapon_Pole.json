  ///////////////////////////////////////////////////////////////
  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "Weapon_Pole_Dual",
      "isDynamicAbility": true,
      "onAdded": [
        {
          "$type": "AttachModifier",
          "modifierName": "UNIQUE_Pole_Dual_Handler"
        }
      ],
      "abilityMixins": [
        {
          "$type": "AttachModifierToSelfGlobalValueMixin",
          "globalValueKey": "_ABILITY_Weapon_Pole_Dual_Enemy",
          "valueSteps": [-0.5, "%Count-0.5", 999],
          "removeAppliedModifier": true,
          "modifierNameSteps": [
            "UNIQUE_Pole_Dual_Atk",
            "UNIQUE_Pole_Dual_AtkDef"
          ]
        }
      ],
      "modifiers": {
        "UNIQUE_Pole_Dual_Handler": {
          "isUnique": true,
          "thinkInterval": 1, //1秒实测反应挺快的。可以慢一些不影响效果。
          "onThinkInterval": [
            {
              "$type": "Predicated",
              "target": "Self",
              // "targetPredicates":
              // [
              // 	{
              // 		"$type":"ByIsCombat",
              // 	},
              // ],
              "successActions": [
                {
                  "$type": "SetGlobalValue",
                  "key": "_ABILITY_Weapon_Pole_Dual_Enemy_Count",
                  "value": 0
                },
                {
                  //程序优化一下，用一个aciton获取targets数量。然后compare就行了
                  "$type": "ApplyModifier",
                  "modifierName": "Pole_Dual_Add",
                  "otherTargets": {
                    "$type": "SelectTargetsByShape",
                    "shapeName": "CircleR8",
                    "centerBasedOn": "Self",
                    "campTargetType": "Enemy",
                    "campBasedOn": "Self",
                    "sortType": "Nearest" //从近开始选
                  },
                  "predicates": [
                    {
                      "$type": "ByEntityTypes",
                      "entityTypes": ["Monster"]
                    },
                    {
                      "$type": "ByTargetGlobalValue",
                      "key": "_ANIMAL_ENTITY_TYPE",
                      "value": 0,
                      "compareType": "NoneOrEqual"
                    }
                  ]
                },
                {
                  "$type": "ApplyModifier",
                  "modifierName": "UNIQUE_Pole_Dual_Timer"
                }
              ]
            }
          ]
        },
        //通过给周围的怪加，然后给角色获得count。 类lisa逻辑。
        "Pole_Dual_Add": {
          "duration": 0.1,
          "stacking": "Multiple",
          "onAdded": [
            {
              "$type": "ApplyModifier",
              "modifierName": "Pole_Dual_AddOnAvatar",
              "target": "Caster"
            }
          ]
        },
        //跨remote只能通过ApplyModifier进行通信
        "Pole_Dual_AddOnAvatar": {
          "duration": 0.1,
          "stacking": "Multiple",
          "onAdded": [
            {
