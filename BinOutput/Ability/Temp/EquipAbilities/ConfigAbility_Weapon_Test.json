  ////////////////////////////////////////////////////////////////
  //
  //  武器刀光变色测试   用addability会无法测试globaKey相关功能，因为加的key没有名字。
  //
  ////////////////////////////////////////////////////////////////
  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "Weapon_Sword_ChangeColor",
      "onAbilityStart": [
        {
          "$type": "AddGlobalValue",
          "key": "_ABILITY_WEAPONSKILL_ChangeColor_COUNT",
          "value": 1
          // "useLimitRange":true,
          // "maxValue":7,
          // "minValue":0,
        }
      ],
      "abilityMixins": [],
      "onAdded": [
        {
          "$type": "AttachModifier",
          "modifierName": "handler"
        }
      ],

      "modifiers": {
        "handler": {
          //用addability时这里用abilitymixin有点问题。。只能改用modifierability
          "modifierMixins": [
            {
              "$type": "AttachModifierToSelfGlobalValueMixin",
              "globalValueKey": "_ABILITY_WEAPONSKILL_ChangeColor_COUNT",
              "defaultGlobalValueOnCreate": 0,
              "valueSteps": [-0.5, 0.5, 1.5, 2.5, 3.5, 4.5, 5.5, 6.5, 100],
              "removeAppliedModifier": true,
              "modifierNameSteps": [
                "No_Color",
                "Water",
                "Fire",
                "Ice",
                "Wind",
                "Electric",
                "Rock",
                "Reset"
              ]
            }
          ]
        },
        "No_Color": {
          "elementType": "None",
          "elementDurability": 20,
          "overrideWeaponElement": true
        },
        "Water": {
          "elementType": "Water",
          "elementDurability": 20,
          "overrideWeaponElement": true
        },
        "Fire": {
          "elementType": "Fire",
          "elementDurability": 20,
          "overrideWeaponElement": true
        },
        "Ice": {
          "elementType": "Ice",
          "elementDurability": 20,
          "overrideWeaponElement": true
        },
        "Wind": {
          "elementType": "Wind",
          "elementDurability": 20,
          "overrideWeaponElement": true
        },
        "Electric": {
          "elementType": "Electric",
          "elementDurability": 20,
          "overrideWeaponElement": true
        },
        "Rock": {
          "elementType": "Rock",
          "elementDurability": 20,
          "overrideWeaponElement": true
        },
        "Reset": {
          "onAdded": [
            {
              "$type": "AddGlobalValue",
              "key": "_ABILITY_WEAPONSKILL_ChangeColor_COUNT",
              "value": -10,
              "useLimitRange": true,
              // "maxValue":7,
              "minValue": 0
            }
          ]
        }
      }
    }
  },

