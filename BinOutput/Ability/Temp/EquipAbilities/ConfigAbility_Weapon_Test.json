  ////////////////////////////////////////////////////////////////
  //
  //  备用 每次攻击附带%rate固定伤害和%rate2百分比角色伤害。   OK
  //
  ////////////////////////////////////////////////////////////////
  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "Weapon_Claymore_ExtraPhyDamagePerHit",
      "abilityMixins": [],
      "onAdded": [
        {
          "$type": "AttachModifier",
          "modifierName": "UNIQUE_Claymore_ExtraPhyDamagePerHit_Handler"
        }
      ],
      "modifiers": {
        "UNIQUE_Claymore_ExtraPhyDamagePerHit_Handler": {
          "stacking": "Unique",
          "isUnique": true,
          "onAttackLanded": [
            {
              "$type": "Predicated",
              "target": "Target",
              "targetPredicates": [
                // {// 限制普攻，因为被火烧的时候还会加伤害T T。不限制类型就炸了。
                // 	"$type" : "ByAttackTags",
                // 	"attackTags": ["NormalAttack_01","NormalAttack_02","NormalAttack_03","NormalAttack_04","NormalAttack_05","NormalAttack_06",]
                // },
                {
                  "$type": "ByEntityTypes",
                  "entityTypes": ["Monster"] //不限制普攻的话，至少限制别烧自己……
                },
                {
                  "$type": "ByHitDamage", //例如风压剑这种，额外造成伤害的话不太合适，并且玩家难以理解。（虽然巨剑没有风压剑）
                  "damage": 0,
                  "logic": "Greater"
                },
                {
                  "$type": "ByNot",
                  "predicates": [
                    {
                      "$type": "ByAttackTags",
                      "attackTags": ["WeaponSkill"]
                    }
                  ]
                }
              ],
              "successActions": [
                {
                  "$type": "DamageByAttackValue", //这个效果格挡后伤害仍然会给出，但是打元素盾后伤害不会出现，因为元素盾主人是锁血状态。
                  "born": {
                    "$type": "ConfigBornByAttachPoint",
                    "attachPointName": "RootNode",
                    "offset": {
                      "x": 0,
                      "y": 0,
                      "z": 0
                    }
                  },
                  "target": "Target",
                  "attackInfo": {
                    "attackTag": "WeaponSkill",
                    "attackProperty": {
                      "damagePercentage": "%Rate2",
                      "damageExtra": "%Rate",
                      "elementType": "None",
                      "strikeType": "Default",
                      "enBreak": 25
                    },
                    "hitPattern": {
                      "onHitEffectName": "default",
                      "hitImpulseType": "Default",
                      "reboundRatio": 0,
                      "hitHaltTime": 0.0,
                      "hitHaltTimeScale": 0.01,
                      "muteHitText": false
                    },
                    "forceCameraShake": false
                  }
                }
              ]
            }
          ]
        }
      },
      "abilitySpecials": {
        "Rate": 2,
        "Rate2": 0
      }
    }
  },
