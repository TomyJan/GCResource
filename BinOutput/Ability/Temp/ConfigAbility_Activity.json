  //////////////////////////////////////////////////////////////////////////////////////////////
  ///
  ///    连续弹跳蘑菇-玩家
  ///
  //////////////////////////////////////////////////////////////////////////////////////////////
  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "Continuous_Trampoline",
      "isDynamicAbility": true,
      "onAdded": [
        {
          //team上
          "$type": "AttachModifier",
          "modifierName": "CheckTrampolineFall"
        },
        {
          "$type": "AttachModifier",
          "modifierName": "CheckIsJumping_Team"
          // "target": "CurLocalAvatar",
        },
        {
          "$type": "SetGlobalValue",
          "key": "_ABILITY_Activity_Trampoline_Jump",
          "target": "Team",
          "useLimitRange": true,
          "value": 0,
          "maxValue": 1,
          "minValue": 0
        },
        {
          "$type": "SetGlobalValue",
          "key": "_ABILITY_Activity_Trampoline_JumpTimes",
          "target": "Team",
          "useLimitRange": true,
          "value": 0,
          "maxValue": 1,
          "minValue": 0
        }
      ],
      "abilityMixins": [
        {
          "$type": "CurLocalAvatarMixinV2",
          "modifierName": "CheckTrampolineJump"
        },
        {
          "$type": "CurLocalAvatarMixinV2",
          "modifierName": "UNIQUE_OnAvatarKilled"
        },
        {
          "$type": "ServerUpdateGlobalValueMixin",
          "key": "_ABILITY_Activity_Trampoline_JumpTimes",
          "minValue": 0,
          "maxValue": 1,
          "useLimitRange": true
        }
      ],
      "modifiers": {
        "UNIQUE_OnAvatarKilled": {
          //记录角色死亡，一直存在于curLocalAvatar上
          "isUnique": true,
          "stacking": "Unique",
          "onAdded": [
            {
              //如果是因为切换角色导致这个modifier上身，也发一次失败
              "$type": "ServerLuaCall",
              "doOffStage": true,
              "funcName": "SLC_Fall_Onto_Ground",
              "luaCallType": "CurChallengeGroup"
            }
          ],
          "onKill": [
            {
              "$type": "DebugLog",
              "content": "ContinuousTrampoline: avatar is killed"
            },
            {
              //fail
              "$type": "ServerLuaCall",
              "doOffStage": true,
              "funcName": "SLC_Fall_Onto_Ground",
              "luaCallType": "CurChallengeGroup"
            }
          ]
        },
        //team上
        "CheckTrampolineJump": {
          //检查玩家是否开始了蘑菇超级跳
          "modifierMixins": [
            {
              "$type": "DoActionOnGlobalValueChangeMixin",
              "globalValueKey": "SuccessTriggerTrampolineJump",
              "actions": [
                {
                  "$type": "ApplyModifier",
                  "modifierName": "UNIQUE_Jump_Succeed_Once",
                  "predicates": [
                    {
                      //必须是主机玩家才能发slc
                      "$type": "ByAvatarIsHost",
                      "target": "CurLocalAvatar",
                      "isHost": true
                    },
                    {
                      //GV大于等于1
                      "$type": "ByTargetGlobalValue",
                      "target": "CurLocalAvatar",
                      "key": "SuccessTriggerTrampolineJump",
                      "value": 1,
                      "compareType": "MoreThanAndEqual"
                    }
                  ]
                }
              ]
            }
          ]
          /*
                    "thinkInterval":0.1,
                    "onThinkInterval": 
                    [
                        {//玩家起跳之后添加一个unique modifier
                            "$type":"ApplyModifier",
                            "modifierName": "UNIQUE_Jump_Succeed_Once",//从这里发slc
                            "target": "CurLocalAvatar",
                            "predicates": 
                            [
                                {
                                    "$type": "ByHasModifier",
                                    "modifierName": "UNIQUE_Trampoline_Jump_Unable",
                                },
                                {//必须是主机玩家才能发slc
                                    "$type" : "ByAvatarIsHost",
                                    "isHost" : true,
                                },
                            ]
                        },
                        {
                            "$type": "RemoveModifier",
                            "modifierName": "UNIQUE_Jump_GV_Set_Zero",
                            "target": "CurLocalAvatar",
                            "predicates": 
                            [
                                {
                                    "$type": "ByHasModifier",
                                    "modifierName": "UNIQUE_Trampoline_Jump_Unable",
                                },
                                {//必须是主机玩家才能发slc
                                    "$type" : "ByAvatarIsHost",
                                    "isHost" : true,
                                },
                            ]
                        }
                    ],
                    */
        },
        "CheckIsJumping_Team": {
          "modifierMixins": [
            {
              "$type": "CurLocalAvatarMixinV2",
              "modifierName": "CheckIsJumping"
            }
          ],
          "onReconnect": [
            {
              //一旦重连立刻失败
              "$type": "ApplyModifier",
              "modifierName": "UNIQUE_Fall_Onto_Ground"
            }
          ]
        },
        "CheckIsJumping": {
          //检查玩家是否在超级跳跃状态中，avatar身上
          "modifierMixins": [
            {
              //蘑菇跳允许的几个state，但有时候感觉FallOntoGround的modifier上身没跑onAdded导致没失败
              "$type": "AttachToAnimatorStateIDMixin",
              "stateIDs": [
                "Jump",
                "DropDown",
                "Fly",
                "FlyStart",
                "JumpForRun",
                "JumpForWalk",
                "JumpForSprint"
                // "FallOnGround", "FallOnGroundLit", "FallToGroundRun", "FallToGroundRunHard", "FallToGroundSprint",
              ],
              "modifierName": "UNIQUE_Trampoline_Jump_Process"
            }
          ]
        },
        "CheckTrampolineFall": {
          //team上
          "modifierMixins": [
            {
              "$type": "AttachModifierToSelfGlobalValueMixin",
              "globalValueKey": "_ABILITY_Activity_Trampoline_Jump",
              "valueSteps": [-0.5, 0.5],
              "removeAppliedModifier": true,
              "modifierNameSteps": ["UNIQUE_Fall_Onto_Ground"]
            }
          ]
        },
        "UNIQUE_Jump_Succeed_Once": {
          //avatar上
          "isUnique": true,
          "stacking": "Unique",
          "duration": 0.05,
          "onAdded": [
            {
              //jump times ++
              "$type": "Predicated",
              "target": "Team",
              "targetPredicates": [
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "_ABILITY_Activity_Trampoline_JumpTimes",
                  "target": "Team",
                  "value": 0,
                  "compareType": "Equal"
                }
              ],
              "successActions": [
                {
                  "$type": "ServerLuaCall",
                  "doOffStage": true,
                  "funcName": "SLC_Jump_Succeed_Once",
                  "luaCallType": "CurChallengeGroup"
                },
                {
                  "$type": "SetGlobalValue",
                  "key": "_ABILITY_Activity_Trampoline_JumpTimes",
                  "target": "Team",
                  "useLimitRange": true,
                  "value": 1,
                  "maxValue": 1,
                  "minValue": 0
                }
              ]
            }
          ],
          "onRemoved": [
            {
              //防止极短时间快速跳SuccessTriggerTrampolineJump
              "$type": "RemoveModifier",
              "modifierName": "UNIQUE_Jump_GV_Set_Zero",
              "predicates": [
                {
                  //必须是主机玩家才能发slc
                  "$type": "ByAvatarIsHost",
                  "target": "CurLocalAvatar",
                  "isHost": true
                },
                {
                  //GV大于等于1
                  "$type": "ByTargetGlobalValue",
                  "target": "CurLocalAvatar",
                  "key": "SuccessTriggerTrampolineJump",
                  "value": 1,
                  "compareType": "MoreThanAndEqual"
                }
              ]
            },
            {
              //remove的时候判一次当前是否在合法动作里，如果不在就再判一次，还不在就直接失败
              "$type": "Predicated",
              "target": "Self",
              "targetPredicates": [
                {
                  //在非法状态里
                  "$type": "ByNot",
                  "predicates": [
                    {
                      "$type": "ByHasModifier",
                      "modifierName": "UNIQUE_Trampoline_Jump_Process"
                    }
                  ]
                }
              ],
              "successActions": [
                {
                  //等0.5s再判一次
                  "$type": "ApplyModifier",
                  "modifierName": "CheckSecondTime"
                }
              ],
              "failActions": [
                {
                  "$type": "DebugLog",
                  "content": "ContinuousTrampoline avatar has process modifier"
                }
              ]
            }
          ]
        },
        "CheckSecondTime": {
          //avatar上
          "duration": 0.5,
          "onRemoved": [
            {
              //remove的时候判第二次当前是否在合法动作里
              "$type": "Predicated",
              "target": "Self",
              "targetPredicates": [
                {
                  //在非法状态里
                  "$type": "ByNot",
                  "predicates": [
                    {
                      "$type": "ByHasModifier",
                      "modifierName": "UNIQUE_Trampoline_Jump_Process"
                    }
                  ]
                }
              ],
              "successActions": [
                {
                  "$type": "ServerLuaCall",
                  "doOffStage": true,
                  "funcName": "SLC_Fall_Onto_Ground",
                  "luaCallType": "CurChallengeGroup"
                }
              ]
            }
          ]
        },
        "UNIQUE_Fall_Onto_Ground": {
          //team上
          "isUnique": true,
          "stacking": "Unique",
          "onAdded": [
            {
              //fail
              "$type": "ServerLuaCall",
              "doOffStage": true,
              "funcName": "SLC_Fall_Onto_Ground",
              "luaCallType": "CurChallengeGroup"
            }
          ]
        },
        "UNIQUE_Trampoline_Jump_Process": {
          //local avatar上
          "isUnique": true,
          "stacking": "Unique",
          "onAdded": [
            {
              "$type": "SetGlobalValue",
              "key": "_ABILITY_Activity_Trampoline_Jump",
              "target": "Team",
              "value": 1
            }
          ],
          "onRemoved": [
            {
              "$type": "ApplyModifier",
              "modifierName": "UNIQUE_Jump_GV_Set_Zero",
              "predicates": [
                //不是主机就不能把这个值设回0
                {
                  "$type": "ByAvatarIsHost",
                  "isHost": true
                }
              ]
            }
          ]
        },
        "UNIQUE_Jump_GV_Set_Zero": {
          //出了超级跳允许的几个状态，判断一下出了之后过了多久，超过1s就要设为0了，1s内不用设为0
          "isUnique": true,
          "stacking": "Unique",
          "duration": 0.7,
          "thinkInterval": 0.6,
          "onThinkInterval": [
            {
              "$type": "SetGlobalValue",
              "key": "_ABILITY_Activity_Trampoline_Jump",
              "target": "Team",
              "value": 0
            }
          ]
        }
      }
    }
  },
