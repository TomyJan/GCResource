        {
          "$type": "SkillButtonHoldChargeMixin",
          "skillID": 10013,
          "beforeStateIDs": ["LiquidStrike_BS"],
          "chargeLoopStateIDs": [
            "LiquidStrike_BS1",
            "LiquidStrike_Move",
            "LiquidStrike_MoveStandby"
          ],
          "chargeLoopDurations": [0.01, 60],
          "nextLoopTriggerID": "NextLoopTrigger",
          "endHoldTrigger": "EndHoldTrigger"
        },
        {
          "$type": "AttachToStateIDMixin",
          "stateIDs": ["LiquidStrike_Move"],
          "modifierName": "LiquidStrike_SetAnimatorTrigger"
        }
        //解释下这里为啥要加这么一个modifier（结合状态机看一下）：水激流前摇分了两个state，如果在第一个state 的时候体力空了，服务器发了对应的消息过来后，会强行发一个EndHoldTrigger
        //由于这个时候还在第一个state，跳转到第二个state的时候这个 Trigger 就被清掉了，会一直在 moveloop 里出不去，所以需要再进 moveloop 时根据体力手动补个 trigger
      ],
      "onAdded": [],
      "onAbilityStart": [
        {
          "$type": "AvatarSkillStart",
          "skillID": 10013,
          "costStaminaRatio": "%StaminaStartCost"
        }
      ],
      "modifiers": {
        "LiquidStrike_SetAnimatorTrigger": {
          "onAdded": [
            {
              "$type": "SetAnimatorTrigger",
              "triggerID": "EndHoldTrigger",
              "predicates": [
                {
                  "$type": "ByLocalAvatarStamina",
                  "stamina": 1,
                  "logic": "LesserOrEqual"
                }
              ]
            }
          ]
        }
      },
      "abilitySpecials": {
        "StaminaStartCost": 0
      }
    }
