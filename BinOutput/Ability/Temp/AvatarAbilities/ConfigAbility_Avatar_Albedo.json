            },
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_Avatar_Boy_Sword_Albedo_FuriousMonolith_Aim_Line"
            }
          ]
        },
        //长按蓄力时角色自己身上的循环特效
        "AimingAlertFX": {
          "onAdded": [
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_Avatar_Boy_Sword_Albedo_FuriousMonolith_AimAlert"
            },
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_Avatar_Boy_Sword_Albedo_FuriousMonolith_AimAlert_Hand"
            }
          ]
        }
      }
    }
  },
