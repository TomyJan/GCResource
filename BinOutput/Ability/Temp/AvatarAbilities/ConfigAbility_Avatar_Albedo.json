  ////////////////////////////////////////////////////////////////
  //
  //  Albedo_FuriousMonolith
  //
  ////////////////////////////////////////////////////////////////
  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "SkillObj_Albedo_FuriousMonolith", //底座生命和创建平台相关功能
      "abilityMixins": [
        {
          "$type": "DoActionByTeamStatusMixin",
          "actions": [
            {
              "$type": "KillSelf",
              "predicates": [
                {
                  "$type": "ByCurTeamHasFeatureTag",
                  "featureTagID": 10038,
                  "number": 0,
                  "logic": "LesserOrEqual"
                }
              ]
            }
          ]
        }
      ],
      "onAdded": [
        //一个底座对应一个平台，重复触发不会反复创建，只是同一个反复再利用
        //底座创建时就预创建平台，提前加载以对抗延迟
        //角色站在底座上，此时会reset平台位置并升起，如果已经升起平台会在此reset平台位置等待下一次升起
        {
          "$type": "AttachModifier",
          "modifierName": "UNIQUE_DelayCreateMovingPlatform_Handler"
        },
        {
          "$type": "AttachModifier",
          "modifierName": "UNIQUE_MonolithLife"
        },
        {
          "$type": "AttachEffect",
          "effectPattern": "Eff_Avatar_Boy_Sword_Albedo_FuriousMonolith" //模型特效
        },
        {
          "$type": "FireEffect",
          "effectPattern": "Eff_Avatar_Boy_Sword_Albedo_FuriousMonolith_Earthquake" //创建时的震荡波特效
        },
        /////////////////////////临   时   方   案/////////////////
        {
          "$type": "ApplyModifier",
          "modifierName": "FuriousMonolith_RockDurability_Handler"
        },
        {
          "$type": "TriggerAttackEvent",
          "targetType": "Enemy",
          "attackEvent": {
            "attackPattern": {
              "$type": "ConfigAttackCircle",
              "triggerType": "TriggerNoRepeat",
              "height": 4.0,
              "fanAngle": 360.0,
              "detectDirection": "CenterToSide",
              "radius": 5.0,
              "born": {
                "$type": "ConfigBornBySelf",
                "offset": {
                  "x": 0.0,
                  "y": -2.0,
                  "z": 0.0
                }
              }
              // "checkHitLayerType": "Both",
            },
            "attackInfo": {
              "canBeModifiedBy": "OriginOwner",
              "attackTag": "Elemental_Art",
              "attackProperty": {
                "damagePercentage": "%OpeningDamage",
                "strikeType": "Blunt",
                "attackType": "Melee",
                "elementType": "Rock",
                "elementDurability": 25,
                "enBreak": 50
              },
              "hitPattern": {
                // "onHitEffectName": "default",
                "onHitEffectName": "Eff_Avatar_Boy_Sword_Albedo_FuriousMonolith_Earthquake_Behit",
                "hitImpulseType": "LV4",
                "reboundRatio": 0,
                "hitHaltTime": 0.0,
                "hitHaltTimeScale": 0.02,
                "muteHitText": false,
                "retreatType": "ByHitDirection"
              },
              "forceCameraShake": true,
              "cameraShake": {
                "shakeType": "Center",
                "shakeRange": 0.4,
                "shakeTime": 0.12,
                "shakeDistance": 60,
                "extension": {
                  "baseOnCamera": true
                }
              }
            }
          }
        }
      ],
      "onAbilityStart": [
        {
          "$type": "Predicated",
          "target": "Target",
          "targetPredicates": [
            {
              "$type": "ByEntityTypes",
              "entityTypes": ["Avatar"] //角色才能升平台
            },
            {
              "$type": "ByTargetGlobalValue",
              "key": "_ABILITY_Albedo_Monolith_Field_AvatarStateMark", //部分状态无法升平台
              "value": 0,
              "compareType": "NoneOrEqual"
            },
            {
              "$type": "ByTargetGlobalValue",
              "key": "_ABILITY_Albedo_Monolith_Field_AvatarInDelay", //如果角色已经在延迟计时，则无事发生
              "value": 0,
              "compareType": "NoneOrEqual"
            }
          ],
          "successActions": [
            {
              "$type": "ApplyModifier",
              "modifierName": "Albedo_Monolith_Field_AvatarCheckDelay_Intro", //key不为1，代表未进行延迟/延迟后升平台机会已经被使用，手动设置升平台的延迟
              "target": "Target",
              "predicates": [
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "_ABILITY_Albedo_Monolith_Field_AvatarCheckDelay", //关于角色延迟升起平台的状态，key为1代表已经delay过，可以升平台
                  "value": 0,
                  "compareType": "NoneOrEqual",
                  "target": "Target"
                }
              ]
            }
          ]
        },
        {
          "$type": "Predicated",
          "target": "Target",
          "targetPredicates": [
            {
              "$type": "ByEntityTypes",
              "entityTypes": ["Avatar"] //角色才能升平台
            },
            {
              "$type": "ByTargetGlobalValue",
              "key": "_ABILITY_Albedo_Monolith_Field_AvatarStateMark", //部分状态无法升平台
              "value": 0,
              "compareType": "NoneOrEqual"
            },
            {
              "$type": "ByTargetGlobalValue",
              "key": "_ABILITY_Albedo_Monolith_Field_AvatarCheckDelay", //角色升平台延迟结束
              "value": 1,
              "compareType": "Equal"
            },
            {
              "$type": "ByTargetGlobalValue",
              "key": "_ABILITY_Albedo_Monolith_ElevatorCD", //没在CD中才能升平台
              "value": 0,
              "compareType": "NoneOrEqual",
              "target": "Caster"
            }
          ],
          "successActions": [
            //一个底座对应一个平台，重复触发不会反复创建，只是同一个反复再利用
            //底座创建时就预创建平台，提前加载以对抗延迟
            //角色站在底座上，此时TryTriggerPlatformStartMove会reset平台位置并升起，如果已经升起平台会在此reset平台位置等待下一次升起
            {
              "$type": "TryTriggerPlatformStartMove",
              "detectHeight": 6, //安全区域检测
              "detectWidth": 1.5, //安全区域检测
              //不安全时，播放特效
              "failActions": [
                {
                  "$type": "ApplyModifier",
                  "modifierName": "CreateElevator_FailFx"
                }
              ]
            },
            {
              "$type": "DebugLog",
              "content": "!!!!!!!!!!!TryTriggerPlatformStartMove!!!!!!!!!!",
              "doOffStage": true
            },
            {
              "$type": "ApplyModifier",
              "modifierName": "Albedo_Monolith_Field_AvatarCheckDelay_Outro", //平台升起后重置key
              "target": "Target"
            }
          ]
        }
      ],
      "onKill": [
        {
          "$type": "Predicated",
          "target": "Self",
          "targetPredicates": [
            {
              "$type": "ByTargetGlobalValue",
              "key": "_ABILITY_Albedo_Monolith_DieFx_Mark",
              "value": 1,
              "compareType": "Equal"
            }
          ],
          "successActions": [
            {
              "$type": "FireEffect",
              "effectPattern": "Eff_Avatar_Boy_Sword_Albedo_FuriousMonolith_Broken"
            }
          ],
          "failActions": [
            {
              "$type": "FireEffect",
              "effectPattern": "Eff_Avatar_Boy_Sword_Albedo_FuriousMonolith_Die"
            }
          ]
        },
        {
          "$type": "ApplyModifier",
          "modifierName": "UNIQUE_Albedo_FuriousMonolith_Elevator_Killer",
          "otherTargets": {
            "$type": "SelectTargetsByChildren",
            "configID": 41038002,
            "sortType": "Default"
          }
        }
      ],
      "modifiers": {
        //延迟创建平台，避免平台把别的岩创生物干掉（是bug，但是暂时没有好的处理方法
        "UNIQUE_DelayCreateMovingPlatform_Handler": {
          "isUnique": true,
          "stacking": "Unique",
          "duration": 1.0,
          "thinkInterval": 0.3,
          "onThinkInterval": [
            {
              "$type": "ApplyModifier",
              "modifierName": "UNIQUE_DelayCreateMovingPlatform"
            }
          ]
        },
        "UNIQUE_DelayCreateMovingPlatform": {
          "isUnique": true,
          "duration": 1.0,
          "onAdded": [
            {
              "$type": "RemoveUniqueModifier",
              "modifierName": "UNIQUE_DelayCreateMovingPlatform_Handler"
            },
            {
              "$type": "CreateMovingPlatform",
              "gadgetID": 41038002,
              "campID": 1001,
              "campTargetType": "Enemy",
              "born": {
                "$type": "ConfigBornBySelf"
              },
              "byServer": true, //平台需要服务器创建
              "lifeByOwnerIsAlive": true
            }
          ]
        },
        "Albedo_Monolith_Field_AvatarCheckDelay_Intro": {
          "duration": 0.1,
          "onAdded": [
            {
              "$type": "SetGlobalValue",
              "key": "_ABILITY_Albedo_Monolith_Field_AvatarCheckDelay", //先上给自己，保证时效
              "value": 0,
              "doOffStage": true
            },
            {
              "$type": "Predicated",
              "target": "Self",
              "targetPredicates": [
                {
                  "$type": "ByIsCombat" //根据是否在战斗中，手动设置升平台的延迟
                }
              ],
              "successActions": [
                {
                  "$type": "SetOverrideMapValue",
                  "overrideMapKey": "AvatarCheckDelay",
                  "value": 0.5
                }
              ],
              "failActions": [
                {
                  "$type": "SetOverrideMapValue",
                  "overrideMapKey": "AvatarCheckDelay",
                  "value": 0.25
                }
              ]
            },
            {
              "$type": "ApplyModifier",
              "modifierName": "UNIQUE_Albedo_Monolith_Field_AvatarCheckDelay", //全队计时
              "target": "CurTeamAvatars",
              "doOffStage": true
            }
          ]
        },
        "UNIQUE_Albedo_Monolith_Field_AvatarCheckDelay": {
          "isUnique": true,
          "duration": "%AvatarCheckDelay",
          "onAdded": [
            {
              "$type": "SetGlobalValue",
              "key": "_ABILITY_Albedo_Monolith_Field_AvatarCheckDelay",
              "value": 0,
              "doOffStage": true
            },
            {
              "$type": "SetGlobalValue",
              "key": "_ABILITY_Albedo_Monolith_Field_AvatarInDelay", //正在延迟中
              "value": 1,
              "doOffStage": true
            }
          ],
          "onRemoved": [
            {
              "$type": "SetGlobalValue",
              "key": "_ABILITY_Albedo_Monolith_Field_AvatarCheckDelay", //延迟结束后可以升平台
              "value": 1,
              "doOffStage": true
            },
            {
              "$type": "SetGlobalValue",
              "key": "_ABILITY_Albedo_Monolith_Field_AvatarInDelay",
              "value": 0,
              "doOffStage": true
            }
          ]
        },
        "Albedo_Monolith_Field_AvatarCheckDelay_Outro": {
          "duration": 0.1,
          "onAdded": [
            {
              "$type": "SetGlobalValue",
              "key": "_ABILITY_Albedo_Monolith_Field_AvatarCheckDelay",
              "value": 0,
              "target": "CurTeamAvatars",
              "doOffStage": true
            }
          ]
        },
        "UNIQUE_Albedo_FuriousMonolith_Elevator_Killer": {
          "duration": 1.0,
