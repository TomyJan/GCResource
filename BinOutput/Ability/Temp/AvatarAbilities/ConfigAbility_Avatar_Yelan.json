  ////////////////////////////////////////////////////////////////
  //
  //  Avatar_Yelan_Aim_Charge
  //  瞄准的蓄力过程
  //
  ////////////////////////////////////////////////////////////////
  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "Avatar_Yelan_ShootAimingArrow_Charge",
      "abilityMixins": [
        {
          "$type": "AttachToStateIDMixin",
          "stateIDs": ["Aim", "FastAim"],
          "modifierName": "Yelan_Arrow_FX"
        },
        {
          "$type": "AttachToStateIDMixin",
          "stateIDs": ["Aim", "FastAim"],
          "modifierName": "ShootAimingArrow_ChargeMark"
        },
        {
          "$type": "AttachToStateIDMixin",
          "stateIDs": ["FastAim", "FastAimShoot", "Aim", "AimShoot"],
          "modifierName": "ShowAimingEffect"
        },
        {
          "$type": "AttachToNormalizedTimeMixin",
          "stateID": "AimLoop",
          "modifierName": "Aim_ChangeAlpha",
          "normalizeStart": 0.0,
          "normalizeEnd": 1.0
        },
        {
          "$type": "AttachToNormalizedTimeMixin",
          "stateID": "FastAimLoop",
          "modifierName": "FastAim_ChangeAlpha",
          "normalizeStart": 0.0,
          "normalizeEnd": 1.0
        }
      ],
      //射出箭矢，移除特效并对GV初始化
      "onAbilityStart": [
        {
          "$type": "RemoveModifier",
          "modifierName": "ShootAimingArrow_ChargeMark"
        },
        {
          "$type": "RemoveModifier",
          "modifierName": "WaterMagic_Arrow_Glow_Once"
        },
        {
          "$type": "RemoveModifier",
          "modifierName": "WaterMagic_Arrow_Glow_WithoutWater_Once"
        },
        {
          "$type": "RemoveModifier",
          "modifierName": "WaterMagic_Arrow_Glow_Once_WithSneak"
        },
        {
          "$type": "SetGlobalValue",
          "key": "_Enchanted_Time",
          "value": 0
        }
      ],
      "modifiers": {
        //长按瞄准与R键瞄准时的弓箭特效
        "Yelan_Arrow_FX": {
          "onAdded": [
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_Avatar_Lady_Bow_Yelan_Arrow"
            }
          ]
        },
        "ShootAimingArrow_ChargeMark": {
          "onAdded": [
            {
              "$type": "SetGlobalValue",
              "key": "_Enchanted_Time",
              "value": 0
            }
          ],
          "thinkInterval": 0.3,
          "onThinkInterval": [
            {
              "$type": "AddGlobalValue", //普通状态蓄力
              "key": "_Enchanted_Time",
              "value": 1,
              "useLimitRange": true,
              "maxValue": 5,
              "minValue": 0,
              "predicates": [
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "_ABILITY_Yelan_IsSneak",
                  "value": 0,
                  "compareType": "Equal"
                }
              ]
            },
            {
              "$type": "AddGlobalValue", //隐匿状态蓄力，直接拉满蓄力过程
              "key": "_Enchanted_Time",
              "value": 5,
              "useLimitRange": true,
              "maxValue": 5,
              "minValue": 0,
              "predicates": [
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "_ABILITY_Yelan_IsSneak",
                  "value": 1,
                  "compareType": "Equal"
                }
              ]
            }
          ]
        },
        "Aim_ChangeAlpha": {
          "modifierMixins": [
            {
              "$type": "EffectChangeAlphaMixin",
              "target": "Team",
              "startDuration": 0.2,
              "endDuration": 0.3
            }
          ]
        },
        "FastAim_ChangeAlpha": {
          "modifierMixins": [
            {
              "$type": "EffectChangeAlphaMixin",
              "target": "Team",
              "startDuration": 0.2,
              "endDuration": 0.3
            }
          ]
        },
        //播放蓄力特效（Tick特效状态）
        "ShowAimingEffect": {
          "onAdded": [
            {
              "$type": "AttachModifier",
              "modifierName": "WaterMagic_Arrow_Glow_WithoutWater_Once"
            }
          ],
          "thinkInterval": 0.1,
          "onThinkInterval": [
            {
              "$type": "AttachModifier",
              "modifierName": "WaterMagic_Arrow_Glow_Once_WithSneak", //隐匿状态下满蓄
              "predicates": [
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "_Enchanted_Time",
                  "value": 4,
                  "forceByCaster": true,
                  "compareType": "MoreThanAndEqual"
                },
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "_ABILITY_Yelan_IsSneak",
                  "value": 1,
                  "compareType": "Equal"
                }
              ]
            },
            {
              "$type": "AttachModifier",
              "modifierName": "WaterMagic_Arrow_Glow_Once", //非隐匿状态下满蓄
              "predicates": [
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "_Enchanted_Time",
                  "value": 4,
                  "forceByCaster": true,
                  "compareType": "MoreThanAndEqual"
                },
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "_ABILITY_Yelan_IsSneak",
                  "value": 0,
                  "compareType": "Equal"
                }
              ]
            },
            {
              "$type": "AttachModifier",
              "modifierName": "WaterMagic_Arrow_Glow_WithoutWater_Once", //未蓄力完毕
              "predicates": [
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "_Enchanted_Time",
                  "value": 1,
                  "forceByCaster": true,
                  "compareType": "Equal"
                }
              ]
            }
          ]
        },
        //不带属性的蓄力箭特效
        "WaterMagic_Arrow_Glow_WithoutWater_Once": {
          "stacking": "Unique",
          "onAdded": [
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_Yelan_Arrow_Glow_Once_01",
              //非隐匿状态下才会播这个效果
              "predicates": [
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "_ABILITY_Yelan_IsSneak",
                  "value": 0,
                  "compareType": "Equal"
                }
              ]
            },
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_Yelan_Arrow_Glow_Loop_01"
            }
          ]
        },
        //隐匿状态下的满蓄效果
        "WaterMagic_Arrow_Glow_Once_WithSneak": {
          "stacking": "Unique",
          "onAdded": [
            {
              "$type": "RemoveModifier",
              "modifierName": "WaterMagic_Arrow_Glow_Once"
            },
            {
              "$type": "RemoveModifier",
              "modifierName": "WaterMagic_Arrow_Glow_WithoutWater_Once"
            },
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_Yelan_Arrow_Glow_Once_02_Sneak"
            },
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_Yelan_Arrow_Glow_Loop_02"
            }
          ]
        },
        //非隐匿状态下的满蓄效果
        "WaterMagic_Arrow_Glow_Once": {
          "stacking": "Unique",
          "onAdded": [
            {
              "$type": "RemoveModifier",
              "modifierName": "WaterMagic_Arrow_Glow_Once_WithSneak"
            },
            {
              "$type": "RemoveModifier",
              "modifierName": "WaterMagic_Arrow_Glow_WithoutWater_Once"
            },
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_Yelan_Arrow_Glow_Once_02"
            },
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_Yelan_Arrow_Glow_Loop_02"
            }
          ]
        }
      }
    }
  },

