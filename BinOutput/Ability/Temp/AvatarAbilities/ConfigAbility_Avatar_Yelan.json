  ////////////////////////////////////////////////////////////////
  //
  //  命之座4 Constellation_4
  //  元素战技引爆时每标记一个敌方目标全队生命值上限提升XX%，至多提升XX%
  //
  ////////////////////////////////////////////////////////////////
  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "Avatar_Yelan_Constellation_4",
      "isDynamicAbility": true,
      "onAdded": [
        {
          "$type": "SetGlobalValue",
          "key": "_ABILITY_Yelan_Constellatin_4_Level",
          "value": 0
        },
        {
          "$type": "AttachModifier",
          "modifierName": "Avatar_Yelan_Constellatin_4_Handler"
        }
      ],
      "modifiers": {
        //关于Buff时点的触发器
        "Avatar_Yelan_Constellatin_4_Handler": {
          "modifierMixins": [
            {
              "$type": "AttachModifierToSelfGlobalValueMixin",
              "globalValueKey": "_ABILITY_Yelan_ElementalArt_IsTrigger",
              "valueSteps": [-0.5, 0.5, 1.5],
              "modifierNameSteps": ["Blank", "Trigger_Timer"],
              "actionQueues": [
                [
                  //对应Blank的list
                ],
                [
                  {
                    "$type": "SetGlobalValueToOverrideMap",
                    "globalValueKey": "_ABILITY_Yelan_QuietStep_HitCount", //这个值未命中目标时为0
                    "overrideMapKey": "Hit_Count",
                    "doOffStage": true
                  },
                  {
                    "$type": "Predicated",
                    "doOffStage": true,
                    "targetPredicates": [
                      {
                        "$type": "ByTargetGlobalValue",
                        "key": "_ABILITY_Yelan_QuietStep_HitCount",
                        "value": 0,
                        "compareType": "MoreThan"
                      }
                    ],
                    //若命中目标则刷新持续时间和Buff层数
                    "successActions": [
                      {
                        "$type": "RemoveModifier",
                        "modifierName": "Avatar_Yelan_Constellatin_4_Timer", //把非自己夜兰的计时器移除，只要转换了人就重置叠层
                        "target": "AllPlayerAvatars",
                        "doOffStage": true,
                        "predicates": [
                          {
                            "$type": "ByTargetType",
                            "targetType": "Caster",
                            "isTarget": false
                          },
                          {
                            "$type": "ByTargetPositionToSelfPosition",
                            "compareType": "XZ",
                            "value": 40,
                            "logic": "LesserOrEqual"
                          }
                        ]
                      },
                      {
                        "$type": "AttachModifier",
                        "modifierName": "Avatar_Yelan_Constellatin_4_Timer",
                        "doOffStage": true
                      },
                      {
                        "$type": "AddGlobalValue",
                        "key": "_ABILITY_Yelan_Constellatin_4_Level",
                        "value": "%Hit_Count",
                        "useLimitRange": true,
                        "minValue": 0,
                        "maxValue": "%Max_Level",
                        "doOffStage": true
                      },
                      {
                        "$type": "SetGlobalValueToOverrideMap",
                        "globalValueKey": "_ABILITY_Yelan_Constellatin_4_Level",
                        "overrideMapKey": "Buff_Level",
                        "doOffStage": true
                      },

                      //更新全队Buff生命值效果
                      {
                        "$type": "RemoveUniqueModifier",
                        "modifierName": "UNIQUE_Yelan_Constellation_4_HpBuff",
                        "target": "AllPlayerAvatars",
                        "doOffStage": true,
                        "predicates": [
                          {
                            "$type": "ByTargetPositionToSelfPosition",
                            "compareType": "XZ",
                            "value": 40,
                            "logic": "LesserOrEqual"
                          }
                        ]
                      },
                      {
                        "$type": "AttachModifier",
                        "modifierName": "UNIQUE_Yelan_Constellation_4_HpBuff",
                        "target": "AllPlayerAvatars",
                        "doOffStage": true,
                        "predicates": [
                          {
                            "$type": "ByTargetPositionToSelfPosition",
                            "compareType": "XZ",
                            "value": 40,
                            "logic": "LesserOrEqual"
                          }
                        ]
                      }
                    ]
                  }
                ]
              ]
            }
          ]
        },
        //计时器
        "Avatar_Yelan_Constellatin_4_Timer": {
          "duration": "%Duration",
          "onRemoved": [
            {
              "$type": "SetGlobalValue",
              "key": "_ABILITY_Yelan_Constellatin_4_Level",
              "value": 0,
              "doOffStage": true
            }
          ]
        },
        //真正的Buff效果
        "UNIQUE_Yelan_Constellation_4_HpBuff": {
          "isUnique": true,
          "stacking": "Unique",
          "duration": "%Duration",
          "isLimitedProperties": true,
          "properties": {
            "Actor_MaxHPRatio": "%Hp_Ratio*%Buff_Level"
          }
        },
        "Blank": {},
        //命4触发器回收
        "Trigger_Timer": {
          "duration": 0.2,
          "onRemoved": [
            {
              "$type": "SetGlobalValue",
              "key": "_ABILITY_Yelan_ElementalArt_IsTrigger", //短时清理一下Trigger
              "value": 0,
              "doOffStage": true
            }
          ]
        }
      },
      "onKill": [
        {
          "$type": "RemoveModifier",
          "modifierName": "Avatar_Yelan_Constellatin_4_Handler", //能在kill时尽快的处理掉自身的Buff加成效果
          "doOffStage": true
        }
      ],
      "abilitySpecials": {
        "Hit_Count": 0, //OverrideMapKey 不导表
        "Buff_Level": 0, //OverrideMapKey 不导表

        "Hp_Ratio": 0,
        "Duration": 0,
        "Max_Level": 0
      }
    }
  },

