          "stateIDs": ["ExtraAttack"],
          "modifierName": "ExtraAttack_FX"
        },
        {
          "$type": "AttachToStateIDMixin",
          "stateIDs": ["ExtraAttack_BS"],
          "modifierName": "ExtraAttack_BS_FX"
        },
        {
          "$type": "AttachToStateIDMixin",
