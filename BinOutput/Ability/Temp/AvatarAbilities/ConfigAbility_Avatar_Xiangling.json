  ////////////////////////////////////////////////////////////////
  //
  // 	 gadget技能：
  //
  //   Avatar_Xiangling_PandaFireAim 熊猫自主瞄准目标
  //
  ////////////////////////////////////////////////////////////////
  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "Avatar_Xiangling_PandaFireAim",
      "onAdded": [
        {
          "$type": "ApplyModifier",
          "modifierName": "FireAttack",
          "target": "Self"
        }
      ],
      "modifiers": {
        "FireAttack": {
          "thinkInterval": 1.6,
          "onThinkInterval": [
            {
              "$type": "SetSelfAttackTarget", //这些简单的gadget无法执行turndirection行为。eventPattern中的转向也是无效的。需要配合这个action才行。
              "turnToTargetImmediately": true,
              "otherTargets": {
                "$type": "SelectTargetsByShape",
                "shapeName": "CircleR8",
                "centerBasedOn": "Self",
                "campTargetType": "Enemy",
                "campBasedOn": "Self",
                "topLimit": 1,
                "sortType": "HigherScore" //从近开始选
              },
              "predicates": [
                {
                  "$type": "ByEntityTypes",
                  "entityTypes": ["Gadget", "Gear", "Monster"]
                }
              ]
            },
            {
              "$type": "ApplyModifier",
              "modifierName": "ElementFire"
            },
            {
              "$type": "SetAnimatorTrigger",
              "triggerID": "Panda_Attack_Trigger",
              "MPTriggerOnRemote": true
            }
          ]
        },
        "ElementFire": {
          "duration": 0.5,
          "elementType": "Fire", //用于让男主旋风卷起来的时候判定熊猫带火。
          "elementDurability": 20 //由于进水会把火属性清除，不能在重要modifier中写这个，而且这个modifier还需要持续上。
        },
        "GenerateElemBall_PandaFire": {
          "onAttackLanded": [
            {
              "$type": "Predicated",
              "target": "Target",
              "targetPredicates": [
                {
                  "$type": "ByEntityTypes",
                  "entityTypes": ["Monster"]
                },
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "_ANIMAL_ENTITY_TYPE",
                  "value": 0,
                  "compareType": "NoneOrEqual"
                }
              ],
              "successActions": [
                {
                  "$type": "AttachModifier",
                  "modifierName": "Ball",
                  "target": "Self"
                }
              ]
            },
            {
              "$type": "Predicated",
              "target": "Target",
              "targetPredicates": [
                {
                  "$type": "ByEntityTypes",
                  "entityTypes": ["Monster", "Avatar"]
                }
              ],
              "successActions": [
                {
                  "$type": "RemoveUniqueModifier",
                  "modifierName": "UNIQUE_Xiangling_Talent_231",
                  "target": "Target",
                  "predicates": [
                    {
                      "$type": "ByUnlockTalentParam", //这个默认判定自己。
                      "talentParam": "Xiangling_Talent_231"
                    }
                  ]
                },
                {
                  "$type": "ApplyModifier",
                  "modifierName": "UNIQUE_Xiangling_Talent_231",
                  "target": "Target",
                  "predicates": [
                    {
                      "$type": "ByUnlockTalentParam",
                      "talentParam": "Xiangling_Talent_231"
                    }
                  ]
                }
              ]
            }
          ]
        },
        "Ball": {
          "duration": 1,
          "stacking": "Unique",
          "onAdded": [
            {
              "$type": "Randomed",
              "chance": 1,
              "successActions": [
                {
                  "$type": "GenerateElemBall",
                  "configID": 2017,
                  "baseEnergy": 3,
                  "ratio": "%GetElementRatio",
                  "target": "SelfAttackTarget",
                  "born": {
                    "$type": "ConfigBornByTarget",
                    "offset": {
                      "x": 0.0,
                      "y": 0.0,
                      "z": 0.0
                    }
                  }
                }
              ]
            }
          ]
        },
        "UNIQUE_Xiangling_Talent_231": {
          "isUnique": true,
          "isLimitedProperties": true,
          "state": "DefenseDown",
          "stacking": "Refresh",
          "duration": "%Xiangling_Talent_231_P2_FireDefDownTime",
          "properties": {
            "Actor_FireSubHurtDelta": "%Xiangling_Talent_231_P1_FireDefDown"
          }
        }
      },
      "abilityMixins": [
        {
          "$type": "AttachToStateIDMixin",
          "stateIDs": ["Attack"],
          "modifierName": "GenerateElemBall_PandaFire",
          "target": "Self"
        }
      ],
      //天赋技能，生成一个可以被角色吃的gadget:辣椒
      "onKill": [
        {
          "$type": "Randomed",
          "chance": 1,
          "successActions": [
            {
              "$type": "CreateGadget",
              "gadgetID": 41023003,
              "campID": 1001,
              "campTargetType": "Enemy",
              "born": {
                "$type": "ConfigBornByTarget",
                "offset": {
                  "x": 0.0,
                  "y": 0.0,
                  "z": 0.0
                }
              }
            }
          ],
          "predicates": [
            {
              "$type": "ByUnlockTalentParam",
              "talentParam": "Xiangling_ProudSkill_24"
            }
          ]
        }
        // {
        // 	"$type":"FireEffect",
        // 	"effectPattern":"Eff_Avatar_Girl_Pole_Xiangling_Panda_Die"  //写到skillobj里了
        // }
      ],
      "abilitySpecials": {
        "DummyHp": 0.3,
        "GetElementRatio": 1.0,
        "Xiangling_Talent_231_P1_FireDefDown": 0,
        "Xiangling_Talent_231_P2_FireDefDownTime": 0
      }
    }
  }, //Avatar_Xiangling_PandaFireAim

