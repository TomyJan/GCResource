          //有时候其实根本没有配置BS，这个就需要是0，保证触发endhold。
          "chargeLoopStateIDs": [
            "Beidou_ThunderCounter_Loop1",
            "Beidou_ThunderCounter_Loop2",
            "Beidou_ThunderCounter_Loop3"
