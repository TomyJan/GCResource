  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "Avatar_Wanderer_WeaponVisible",
      "abilityMixins": [
        {
          "$type": "AttachModifierToSelfGlobalValueMixin",
          "globalValueKey": "_ABILITY_Avatar_Wanderer_WeaponVisible_Flag",
          "valueSteps": [-0.5, 0.5, 1.5],
          "removeAppliedModifier": true,
          "modifierNameSteps": ["Blank", "WeaponVisible_Handler"]
        },
        {
          "$type": "AttachToStateIDMixin",
          "stateIDs": [
            "ExtraAttack",
            "ExtraAttack_BS",
            "ExtraAttack_Charge",
            "ExtraAttack_AS",
            "Wanderer_ElementalBurst_Leap",
            "Wanderer_ElementalBurst_Land"
          ],
          "modifierName": "Handler"
        }
      ],
      "modifiers": {
        "Blank": {},
        "Handler": {
          "onAdded": [
            {
              "$type": "SetGlobalValue",
              "key": "_ABILITY_Avatar_Wanderer_WeaponVisible_Flag",
              "value": 1,
              "doOffStage": true
            }
          ],
          "onRemoved": [
            {
              "$type": "SetGlobalValue",
              "key": "_ABILITY_Avatar_Wanderer_WeaponVisible_Flag",
              "value": 0,
              "doOffStage": true
            }
          ]
        },
        "WeaponVisible_Handler": {
          "stacking": "Unique",
          "modifierMixins": [
            {
              "$type": "AttachActionToModifierMixin",
              "onAdded": [
                {
                  "$type": "TriggerHideWeapon",
                  "visible": false,
                  "partNames": ["equip01", "equip02", "equip03", "sheathPoint"], //隐藏原武器
                  "doOffStage": true,
                  "canBeHandledOnRecover": true
                }
              ],
              "onRemoved": [
                {
                  "$type": "TriggerHideWeapon",
                  "visible": true,
                  "partNames": ["equip01", "equip02", "equip03", "sheathPoint"], //显示原武器
                  "doOffStage": true,
                  "canBeHandledOnRecover": true
                }
              ]
            }
          ]
        }
      }
    }
  },

