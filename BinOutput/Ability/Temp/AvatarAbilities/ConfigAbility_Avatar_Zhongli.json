        {
          "$type": "ButtonHoldChargeMixin",
          "skillID": 10301,
          "chargeTime": 0.12,
          "secondChargeTime": 0.35,
          "OnBeginUncharged": [
            {
              "$type": "SetAnimatorTrigger",
              "triggerID": "DoSkill"
            },
            {
              "$type": "SetAnimatorInt",
