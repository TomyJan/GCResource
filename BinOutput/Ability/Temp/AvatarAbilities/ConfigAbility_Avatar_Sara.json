  ////////////////////////////////////////////////////////////////
  //
  //	蓄力逻辑 （含命之座1缩减蓄力时间的机制）
  //
  ////////////////////////////////////////////////////////////////
  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "Avatar_Sara_ShootAimingArrow_Charge",
      "abilityMixins": [
        {
          "$type": "AttachToStateIDMixin", //蓄力时记录蓄力的层数
          "stateIDs": ["Aim", "FastAim"],
          "modifierName": "ShootAimingArrow_Charge_Handler"
        },
        {
          "$type": "AttachToNormalizedTimeMixin",
          "stateID": "AimLoop",
          "modifierName": "Aim_ChangeAlpha",
          "normalizeStart": 0.0,
          "normalizeEnd": 1.0
        },
        {
          "$type": "AttachToNormalizedTimeMixin",
          "stateID": "FastAimLoop",
          "modifierName": "FastAim_ChangeAlpha",
          "normalizeStart": 0.0,
          "normalizeEnd": 1.0
        },
        {
          "$type": "AttachToStateIDMixin", //播放瞄准蓄力特效
          "stateIDs": ["Aim", "AimShoot"],
          "modifierName": "ShowAimingEffect"
        },
        {
          "$type": "AttachToStateIDMixin", //播放瞄准蓄力特效。
          "stateIDs": ["FastAim", "FastAimShoot"],
          "modifierName": "ShowAimingEffect"
        },
        {
          "$type": "AttachToStateIDMixin", //用于在离开蓄力状态时，清楚蓄力的特效及相关的标记
          "stateIDs": ["Aim", "FastAim", "FastAimShoot", "AimShoot"],
          "modifierName": "ShootAimingArrow_Charge_Remove"
        }
      ],
      "onAdded": [
        {
          //断线重连回复后保证数值的正确
          "$type": "ApplyModifier",
          "modifierName": "Avatar_Sara_ShootArrow_ResetGlobalValue",
          "target": "Self",
          "doOffStage": true
        }
      ],
      "onAbilityStart": [
        ///////////////////////射完了记得清一下标记及相关特效////////////////////////////////////
        {
          "$type": "AddGlobalValue",
          "key": "_Enchanted_Time",
          "value": -5,
          "useLimitRange": true,
          "maxValue": 5,
          "minValue": 0
        },
        {
          "$type": "RemoveModifier",
          "modifierName": "AimChargeMark"
        },
        {
          "$type": "RemoveModifier",
          "modifierName": "Magic_Arrow_Glow_01"
        },
        {
          "$type": "RemoveModifier",
          "modifierName": "Magic_Arrow_Glow_02"
        },
        {
          "$type": "RemoveModifier",
          "modifierName": "Sara_KarasShoot_Glow"
        }
        ////////////////////////////////////////////////////////////////////////////////////////
      ],
      "modifiers": {
        "ShootAimingArrow_Charge_Handler": {
          "thinkInterval": 0.3,
          "onThinkInterval": [
            {
              "$type": "ApplyModifier",
              "modifierName": "AimChargeMark"
            }
          ]
        },
        "Aim_ChangeAlpha": {
          "modifierMixins": [
            {
              "$type": "EffectChangeAlphaMixin",
              "target": "Team",
              "startDuration": 0.2,
              "endDuration": 0.3
            }
          ]
        },
        "FastAim_ChangeAlpha": {
          "modifierMixins": [
            {
              "$type": "EffectChangeAlphaMixin",
              "target": "Team",
              "startDuration": 0.2,
              "endDuration": 0.3
            }
          ]
        },
        "ShootAimingArrow_Charge_Remove": {
          "onAdded": [
            {
              "$type": "AddGlobalValue",
              "key": "_Enchanted_Time",
              "value": -5,
              "useLimitRange": true,
              "maxValue": 5,
              "minValue": 0
            }
          ],
          "onRemoved": [
            {
              "$type": "RemoveModifier",
              "modifierName": "AimChargeMark",
              "doOffStage": true
            }
          ]
        },
        "AimChargeMark": {
          "stacking": "MultipleRefreshNoRemove",
          "stackingOption": {
            "maxModifierNumForMultipleType": 5
          },
          "onAdded": [
            {
              "$type": "AddGlobalValue",
              "key": "_Enchanted_Time",
              "value": 1,
              "useLimitRange": true,
              "maxValue": 5,
              "minValue": 0
            }
          ]
        },
        "ShowAimingEffect": {
          "onAdded": [
            {
              "$type": "AttachModifier",
              "modifierName": "Magic_Arrow_Glow_01"
            }
          ],
          "thinkInterval": 0.1,
          "onThinkInterval": [
            {
              "$type": "Predicated",
              "target": "Self",
              "targetPredicates": [
                //判断自身是否在鸦羽箭状态下
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "_ABILITY_Sara_ElementalArt_COUNT",
                  "value": 1,
                  "forceByCaster": true,
                  "compareType": "MoreThanAndEqual"
                }
              ],
              "successActions": [
                {
                  //播放鸦羽箭状态下的蓄力完成特效
                  "$type": "AttachModifier",
                  "modifierName": "Sara_KarasShoot_Glow",
                  "predicates": [
                    {
                      "$type": "ByTargetGlobalValue",
                      "key": "_Enchanted_Time",
                      "value": "%Enchanted_Time*%Sara_AimShoot_Enchanted_Ratio",
                      "forceByCaster": true,
                      "compareType": "MoreThanAndEqual"
                    }
                  ]
                }
              ],
              "failActions": [
                {
                  //鸦羽箭时间到了，清除瞄准状态下的鸦羽特效
                  "$type": "RemoveModifier",
                  "modifierName": "Sara_KarasShoot_Glow"
                },
                {
                  //播放正常的蓄力特效
                  "$type": "AttachModifier",
                  "modifierName": "Magic_Arrow_Glow_02",
                  "predicates": [
                    {
                      "$type": "ByTargetGlobalValue",
                      "key": "_Enchanted_Time",
                      "value": 4,
                      "forceByCaster": true,
                      "compareType": "MoreThanAndEqual"
                    }
                  ]
                }
              ]
            },
            {
              //蓄力完成前的特效
              "$type": "AttachModifier",
              "modifierName": "Magic_Arrow_Glow_01",
              "predicates": [
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "_Enchanted_Time",
                  "value": 1,
                  "forceByCaster": true,
                  "compareType": "Equal"
                }
              ]
            }
          ]
        },
        "Magic_Arrow_Glow_02": {
          "stacking": "Unique",
          "onAdded": [
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_Avatar_Lady_Bow_Sara_ExtraAttack_Glow_Once_02"
            },
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_Avatar_Lady_Bow_Sara_ExtraAttack_Glow_Loop_02"
            }
          ]
        },
        "Magic_Arrow_Glow_01": {
          "stacking": "Unique",
          "onAdded": [
            {
              "$type": "SetGlobalValue",
              "key": "_ABILITY_Sara_AimShoot_Enchanted_Ratio",
              "value": "%Sara_AimShoot_Enchanted_Ratio",
              "useLimitRange": true,
              "maxValue": 1,
              "minValue": 0,
              "doOffStage": true,
              "predicates": [
                //判断自身是否在鸦羽箭状态下
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "_ABILITY_Sara_ElementalArt_COUNT",
                  "value": 1,
                  "forceByCaster": true,
                  "compareType": "MoreThanAndEqual"
                }
              ]
            },
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_Avatar_Lady_Bow_Sara_ExtraAttack_Glow_Once_01",
              "predicates": [
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "_ABILITY_Sara_AimShoot_Enchanted_Ratio",
                  "value": 1,
                  "compareType": "Equal"
                }
              ]
            },
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_Avatar_Lady_Bow_Sara_ExtraAttack_Glow_Loop_01",
              "predicates": [
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "_ABILITY_Sara_AimShoot_Enchanted_Ratio",
                  "value": 1,
                  "compareType": "Equal"
                },
                //判断自身是否在鸦羽箭状态下
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "_ABILITY_Sara_ElementalArt_COUNT",
                  "value": 0,
                  "forceByCaster": true,
                  "compareType": "NoneOrEqual"
                }
              ]
            },
            //鸦羽箭状态时的瞄准蓄力特效
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_Avatar_Lady_Bow_Sara_KarasShoot_Glow_Loop_01",
              "predicates": [
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "_ABILITY_Sara_AimShoot_Enchanted_Ratio",
                  "value": 1,
                  "compareType": "Equal"
                },
                //判断自身是否在鸦羽箭状态下
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "_ABILITY_Sara_ElementalArt_COUNT",
                  "value": 1,
                  "forceByCaster": true,
                  "compareType": "MoreThanAndEqual"
                }
              ]
            },
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_Avatar_Lady_Bow_Sara_ExtraAttack_Glow_Once_01_Talent",
              "predicates": [
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "_ABILITY_Sara_AimShoot_Enchanted_Ratio",
                  "value": 0.9,
                  "compareType": "LessAndEqual"
                }
              ]
            },
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_Avatar_Lady_Bow_Sara_ExtraAttack_Glow_Loop_01_Talent",
              "predicates": [
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "_ABILITY_Sara_AimShoot_Enchanted_Ratio",
                  "value": 0.9,
                  "compareType": "LessAndEqual"
                }
              ]
            }
          ],
          "onRemoved": [
            {
              //射完后需要重置这个值
              "$type": "SetGlobalValue",
              "key": "_ABILITY_Sara_AimShoot_Enchanted_Ratio",
              "value": 1,
              "useLimitRange": true,
              "maxValue": 1,
              "minValue": 0,
              "doOffStage": true
            }
          ]
        },
        "Sara_KarasShoot_Glow": {
          "stacking": "Unique",
          "onAdded": [
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_Avatar_Lady_Bow_Sara_KarasShoot_Glow_Burst"
            },
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_Avatar_Lady_Bow_Sara_KarasShoot_Glow_Loop"
            }
          ]
        },
        "Avatar_Sara_ShootArrow_ResetGlobalValue": {
          "onAdded": [
            {
              "$type": "AddGlobalValue",
              "key": "_ABILITY_Sara_AimShoot_Enchanted_Ratio",
              "value": 1.0,
              "useLimitRange": true,
              "maxValue": 1,
              "minValue": 0,
              "doOffStage": true,
              //"canBeHandledOnRecover" : true,
              "predicates": [
                //要在非鸦羽箭的状态下才重置GV
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "_ABILITY_Sara_ElementalArt_COUNT",
                  "value": 0,
                  "forceByCaster": true,
                  "compareType": "NoneOrEqual"
                }
              ]
            }
          ],
          "onReconnect": [
            {
              "$type": "SetGlobalValue",
              "key": "_ABILITY_Sara_AimShoot_Enchanted_Ratio",
              "value": 1.0,
              "useLimitRange": true,
              "maxValue": 1,
