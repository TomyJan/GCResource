  //按键相关逻辑

  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "Avatar_Kokomi_ExtraAttack",
      "abilityMixins": [
        {
          "$type": "ButtonHoldChargeMixin",
          "skillID": 10541,
          "chargeTime": 0.1,
          "secondChargeTime": 0.1,
          "OnBeginUncharged": [
            {
              "$type": "SetAnimatorTrigger",
              "triggerID": "DoSkill",
              "predicates": [
                {
                  "$type": "ByAnimatorInt",
                  "value": 0,
                  "parameter": "ComboCount",
                  "logic": "Equal"
                }
              ]
            },
            {
              "$type": "SetAnimatorInt",
              "intID": "SkillID",
              "value": 99,
              "predicates": [
                {
                  "$type": "ByAnimatorInt",
                  "value": 0,
                  "parameter": "ComboCount",
                  "logic": "Equal"
                }
              ]
            },
            {
              "$type": "SetAnimatorBool",
              "boolID": "IsCharging",
              "value": false
            }
          ],
          "OnReleaseUncharged": [
            {
              "$type": "SetAnimatorTrigger",
              "triggerID": "DoSkill"
            },
            {
              "$type": "SetAnimatorInt",
              "intID": "SkillID",
              "value": 1
            }
          ],
          "OnBeginCharged": [
            {
              "$type": "SetAnimatorBool",
              "boolID": "IsCharging",
              "value": true
            }
          ],
          "OnReleaseCharged": [
            {
              "$type": "SetAnimatorBool",
              "boolID": "IsCharging",
              "value": false
            },
            {
              "$type": "SetAnimatorTrigger",
              "triggerID": "DoSkill"
            },
            {
              "$type": "SetAnimatorInt",
              "intID": "SkillID",
              "value": 1
            }
          ],
          "OnBeginSecondCharged": [
            {
              "$type": "SetAnimatorTrigger",
              "triggerID": "DoSkill"
            },
            {
              "$type": "Predicated",
              "target": "Self",
              "targetPredicates": [
                {
                  "$type": "ByLocalAvatarStamina",
                  "stamina": "%ExtraAttack_Catalyst_Stamina_Cost",
                  "logic": "GreaterOrEqual"
                }
              ],
              "successActions": [
                {
                  "$type": "SetAnimatorInt",
                  "intID": "SkillID",
                  "value": 6
                }
              ],
              "failActions": [
                {
                  "$type": "SetAnimatorInt",
                  "intID": "SkillID",
                  "value": 1
                }
              ]
            },
            {
              "$type": "SetAnimatorBool",
              "boolID": "IsCharging",
              "value": false
            }
          ],
          "chargeStateIDs": [
            "Attack01",
            "Attack02",
            "Attack03",
            "ExtraAttack_BS",
            "ExtraAttack_Charge",
            "ExtraAttack",
            "ExtraAttack_AS"
          ]
        },
        {
          "$type": "AttachToStateIDMixin",
          "stateIDs": ["ExtraAttack_Charge"],
          "modifierName": "ExtraAttack_StaminaCost"
        }
      ],
      "modifiers": {
        "ExtraAttack_StaminaCost": {
          "onAdded": [
            {
              "$type": "AvatarSkillStart",
              "skillID": 10003,
              "costStaminaRatio": "%ExtraAttack_Catalyst_Stamina_Cost"
            }
          ]
        }
      },
      "abilitySpecials": {
        "ExtraAttack_Catalyst_Stamina_Cost": 0
      }
    }
  },

  //蓄力相关逻辑

  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "Avatar_Kokomi_ExtraAttack_Charge",
      "abilityMixins": [
        {
          "$type": "AttachToStateIDMixin",
          "stateIDs": ["ExtraAttack_BS", "ExtraAttack_Charge", "ExtraAttack"],
          "modifierName": "Avatar_Kokomi_ExtraAttack_BurstMark"
        }
      ],
      "onAbilityStart": [
        {
          "$type": "Predicated", //判定是否有攻击目标，走不同的释放逻辑
          "target": "Self",
          "targetPredicates": [
            {
              "$type": "ByHasAttackTarget"
            }
          ],
          "successActions": [
            {
              "$type": "ApplyModifier",
              "modifierName": "Avatar_Kokomi_ExtraAttack_Charge_HasTarget"
            }
          ],
          "failActions": [
            {
              "$type": "ApplyModifier",
              "modifierName": "Avatar_Kokomi_ExtraAttack_Charge_NoTarget"
            }
          ]
        }
      ],
      "modifiers": {
        //避免重击前摇过程中元素爆发时间正好结束导致的前摇和释放效果不一致。
        "Avatar_Kokomi_ExtraAttack_BurstMark": {
          //进入重击状态时，如果处于元素爆发状态内，则做一个标记施放强化版的重击。
          "onAdded": [
            {
              "$type": "SetGlobalValue",
              "key": "_ABILITY_Kokomi_ExtraAttack_BurstMark",
              "value": 0
            },
            {
              "$type": "SetGlobalValue",
              "key": "_ABILITY_Kokomi_ExtraAttack_BurstMark",
              "value": 1,
              "predicates": [
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "_ABILITY_Kokomi_ElementalBurst_Flag",
                  "value": 1,
                  "compareType": "Equal",
                  "target": "Caster"
                }
              ]
            },
            {
              "$type": "SetGlobalValue",
              "key": "_ABILITY_Kokomi_ElementalBurst_ExtraAttackHit_Flag",
              "target": "Self",
              "value": 0
            }
          ],
          //标记在离开重击状态后清除。
          "onRemoved": [
            {
              "$type": "SetGlobalValue",
              "key": "_ABILITY_Kokomi_ExtraAttack_BurstMark",
              "value": 0
            }
          ]
        },
        //有目标的逻辑
        "Avatar_Kokomi_ExtraAttack_Charge_HasTarget": {
          "duration": 0.5,
          "onAdded": [
            //记录重击的释放位置
            {
              "$type": "SetGlobalPos",
              "target": "SelfAttackTarget",
              "key": "_ABILITY_Kokomi_ExtraAttack_Pos",
              // "born":
              // {
              //     "$type": "ConfigBornByTarget",
              //     "onGround": true,
              // },
              "born": {
                "$type": "ConfigBornBySelectedPoint",
                "onGround": true,
                "offset": {
                  "x": 0.0,
                  "y": 0.0,
                  "z": 0.0
                }
              },
              "setTarget": false
            },

            //无重击标记时放普通版本特效
            {
              "$type": "FireEffect",
              "effectPattern": "Eff_Avatar_Girl_Catalyst_Kokomi_ExtraAttack_Alert",
              "born": {
                "$type": "ConfigBornByGlobalValue",
                "positionKey": "_ABILITY_Kokomi_ExtraAttack_Pos",
                "onGround": true
              },
              "predicates": [
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "_ABILITY_Kokomi_ExtraAttack_BurstMark",
                  "value": 0,
                  "compareType": "Equal",
                  "target": "Caster"
                }
              ]
            },

            //有重击标记时放强化版本特效
            {
              "$type": "FireEffect",
              "effectPattern": "Eff_Avatar_Girl_Catalyst_Kokomi_ExtraAttack_Alert_Burst",
              "born": {
                "$type": "ConfigBornByGlobalValue",
                "positionKey": "_ABILITY_Kokomi_ExtraAttack_Pos",
                "onGround": true
              },
              "predicates": [
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "_ABILITY_Kokomi_ExtraAttack_BurstMark",
                  "value": 1,
                  "compareType": "Equal",
                  "target": "Caster"
                }
              ]
            }
          ]
        },
        //无目标的逻辑
        "Avatar_Kokomi_ExtraAttack_Charge_NoTarget": {
          "duration": 0.5,
          "onAdded": [
            //记录重击的释放位置
            {
              "$type": "SetGlobalPos",
              "key": "_ABILITY_Kokomi_ExtraAttack_Pos",
              "born": {
                "$type": "ConfigBornBySelf",
                "offset": {
                  "x": 0.0,
                  "y": 0.0,
                  "z": 5.0
                },
                "onGround": true
              }
            },

            //无重击标记时放普通版本特效
            {
              "$type": "FireEffect",
              "effectPattern": "Eff_Avatar_Girl_Catalyst_Kokomi_ExtraAttack_Alert",
              "born": {
                "$type": "ConfigBornByGlobalValue",
                "positionKey": "_ABILITY_Kokomi_ExtraAttack_Pos",
                "onGround": true
              },
              "predicates": [
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "_ABILITY_Kokomi_ExtraAttack_BurstMark",
                  "value": 0,
                  "compareType": "Equal",
                  "target": "Caster"
                }
              ]
            },

            //有重击标记时放强化版本特效
            {
              "$type": "FireEffect",
              "effectPattern": "Eff_Avatar_Girl_Catalyst_Kokomi_ExtraAttack_Alert_Burst",
              "born": {
                "$type": "ConfigBornByGlobalValue",
                "positionKey": "_ABILITY_Kokomi_ExtraAttack_Pos",
                "onGround": true
              },
              "predicates": [
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "_ABILITY_Kokomi_ExtraAttack_BurstMark",
                  "value": 1,
                  "compareType": "Equal",
                  "target": "Caster"
                }
              ]
            }
          ]
        }
      }
    }
