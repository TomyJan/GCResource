[
  ////////////////////////////////////////////////////////////////
  //
  //  ExtraAttack
  //
  ////////////////////////////////////////////////////////////////
  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "Avatar_Kazuha_ExtraAttack",
      "abilityMixins": [
        {
          "$type": "ButtonHoldChargeMixin",
          "skillID": 10471,
          "chargeTime": 0.12,
          "secondChargeTime": 0.2,
          "OnBeginUncharged": [
            {
              "$type": "SetAnimatorTrigger",
              "triggerID": "DoSkill"
            },
            {
              "$type": "SetAnimatorInt",
              "intID": "SkillID",
              "value": 99
            },
            {
              "$type": "SetAnimatorBool",
              "boolID": "IsCharging",
              "value": false
            }
          ],
          "OnReleaseUncharged": [
            {
              "$type": "SetAnimatorInt",
              "intID": "SkillID",
              "value": 1
            }
          ],
          "OnBeginCharged": [
            {
              "$type": "SetAnimatorBool",
              "boolID": "IsCharging",
              "value": true
            }
          ],
          "OnReleaseCharged": [
            {
              "$type": "SetAnimatorBool",
              "boolID": "IsCharging",
              "value": false
            },
            {
              "$type": "SetAnimatorInt",
              "intID": "SkillID",
              "value": 1
            }
          ],
          "OnBeginSecondCharged": [
            {
              "$type": "SetAnimatorTrigger",
              "triggerID": "DoSkill",
              "predicates": [
                {
                  "$type": "ByLocalAvatarStamina",
                  "stamina": "%ExtraAttack_Sword_Stamina_Cost+%Flexible_Stamina",
                  "logic": "GreaterOrEqual"
                }
              ]
            },
            {
              "$type": "SetAnimatorInt",
              "intID": "SkillID",
              "value": 6,
              "predicates": [
                {
                  "$type": "ByLocalAvatarStamina",
                  "stamina": "%ExtraAttack_Sword_Stamina_Cost+%Flexible_Stamina",
                  "logic": "GreaterOrEqual"
                }
              ]
            },
            {
              "$type": "SetAnimatorBool",
              "boolID": "IsCharging",
              "value": false
            }
          ],
          "OnReleaseSecondCharged": [],
          "chargeStateIDs": [
            "Attack01",
            "Attack02",
            "Attack03",
            "Attack04",
            "Run"
          ]
        },
        {
          "$type": "AttachToStateIDMixin",
          "stateIDs": ["ExtraAttack"],
          "modifierName": "ExtraAttack_StaminaCost"
        }
      ],
      "modifiers": {
        "ExtraAttack_StaminaCost": {
          // "duration": 0.2,
          "onAdded": [
            {
              "$type": "AvatarSkillStart",
              "skillID": 10001,
              "costStaminaRatio": "%ExtraAttack_Sword_Stamina_Cost"
            }
          ]
        }
      },
      "abilitySpecials": {
        "ExtraAttack_Sword_Stamina_Cost": 0,
        "Flexible_Stamina": -2 //故意少判了一点点体力，为了顾及一部分网络延迟
      }
    }
  },

  ////////////////////////////////////////////////////////////////
  //
  //  普攻的一些Ability控制
  //
  ////////////////////////////////////////////////////////////////
  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "Avatar_Kazuha_NormalAttack_Handler",
      "abilityMixins": [
        {
          "$type": "AttachToNormalizedTimeMixin",
          "stateID": "Attack05",
          "modifierName": "Avatar_Kazuha_NormalAttack_05_Damage", //普攻最后一击次元斩
          "normalizeStart": 0.068,
          "normalizeEnd": 1.0
        },
        {
          "$type": "AttachToNormalizedTimeMixin",
          "stateID": "Attack05",
          "modifierName": "Avatar_Kazuha_Hide_Fx",
          "normalizeStart": 0.05,
          "normalizeEnd": 0.15
        },
        {
          "$type": "AttachToNormalizedTimeMixin",
          "stateID": "Attack05",
          "modifierName": "Avatar_Kazuha_Hide", //注意角色隐藏功能会叠加。如果隐藏2次会变-2，需要显示2次才能真显示出来。
          "normalizeStart": 0.06,
          "normalizeEnd": 0.16
        },
        {
          "$type": "AttachToNormalizedTimeMixin",
          "stateID": "ExtraAttack_AS",
          "modifierName": "Avatar_Kazuha_NormalAttack_MagicSheath", //重击收刀特效
          "normalizeStart": 0.46,
          "normalizeEnd": 0.66
        },
        {
          "$type": "AttachToNormalizedTimeMixin",
          "stateID": "ElementalArt_FallingAttack_AS",
          "modifierName": "Avatar_Kazuha_NormalAttack_MagicSheath", //特化空中攻击收刀特效
          "normalizeStart": 0.275,
          "normalizeEnd": 0.5
        }
      ],
      "modifiers": {
        //第五击自身消失Trigger
        "Avatar_Kazuha_Hide": {
          "onAdded": [
            {
              "$type": "TriggerSetVisible",
              "visible": false,
              "doOffStage": true
            },
            {
              "$type": "TriggerSetPassThrough",
              "passThrough": true,
              "doOffStage": true
            },
            {
              "$type": "EnableHDMesh",
              "enable": false,
              "hdMeshKey": "Default"
            }
          ],
          "onRemoved": [
            {
              "$type": "TriggerSetVisible",
              "visible": true,
              "doOffStage": true
            },
            {
              "$type": "TriggerSetPassThrough",
              "passThrough": false,
              "doOffStage": true,
              "canBeHandledOnRecover": true
            },
            {
              "$type": "EnableHDMesh",
              "enable": true,
              "hdMeshKey": "Default",
              "canBeHandledOnRecover": true
            }
          ]
        },
        //第五击自身消失速度线
        "Avatar_Kazuha_Hide_Fx": {
          "onAdded": [
            {
              "$type": "FireEffect",
              "effectPattern": "Eff_Avatar_Boy_Sword_Kazuha_Attack_HideSpark_05"
            }
          ],
          "onRemoved": [
            {
              "$type": "FireEffect",
              "effectPattern": "Eff_Avatar_Boy_Sword_Kazuha_Attack_ShowSpark_05"
            }
          ]
        },
        //第五击生成次元斩
        "Avatar_Kazuha_NormalAttack_05_Damage": {
          "onAdded": [
            {
              //指定Entity朝其当前位置打次元斩
              "$type": "CreateGadget",
              "gadgetID": 41047003,
              "target": "SelfAttackTarget",
              "campID": 1001,
              "campTargetType": "Enemy",
              // "born":
              // {
              // 	"$type": "ConfigBornByTarget",
              // 	"offset" :
              // 	{
              // 		"x": 0.0,
              // 		"y": 0.0,
              // 		"z": 0.0,
              // 	},
              // },
              "born": {
                "$type": "ConfigBornBySelectedPoint",
                "onGround": true,
                "offset": {
                  "x": 0.0,
                  "y": 0.0,
                  "z": 0.0
                }
              },
              "predicates": [
                {
                  "$type": "ByEntityTypes",
                  "entityTypes": [
                    "Gadget",
                    "Monster",
                    "GatherObject",
                    "Platform"
                  ]
                },
                {
                  "$type": "ByHasAttackTarget",
                  "target": "Self"
                }
              ]
            },
            {
              //非指定Entity朝前固定位置打次元斩
              "$type": "CreateGadget",
              "gadgetID": 41047003,
              "target": "SelfAttackTarget",
              "campID": 1001,
              "campTargetType": "Enemy",
              "born": {
                "$type": "ConfigBornBySelf",
                "offset": {
                  "x": 0.0,
                  "y": 0.0,
                  "z": 2.0
                }
              },
              "predicates": [
                {
                  "$type": "ByEntityTypes",
                  "entityTypes": [
                    "Gadget",
                    "Monster",
                    "GatherObject",
                    "Platform"
                  ],
                  "reject": true
                },
                {
                  "$type": "ByHasAttackTarget",
                  "target": "Self"
                }
              ]
            },
            {
              //无攻击目标朝前固定位置打次元斩
              "$type": "CreateGadget",
              "gadgetID": 41047003,
              "target": "Self",
              "campID": 1001,
              "campTargetType": "Enemy",
              "born": {
                "$type": "ConfigBornByTarget",
                "offset": {
                  "x": 0.0,
                  "y": 0.0,
                  "z": 2.0
                }
              },
              "predicates": [
                {
                  "$type": "ByNot",
                  "predicates": [
                    {
                      "$type": "ByHasAttackTarget",
                      "target": "Self"
                    }
                  ]
                }
              ]
            }
          ]
        },
        //普攻收刀特效
        "Avatar_Kazuha_NormalAttack_MagicSheath": {
          "onAdded": [
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_Avatar_Boy_Sword_Kazuha_Attack_Spark_Sheath"
            }
          ]
        }
      }
    }
  },

