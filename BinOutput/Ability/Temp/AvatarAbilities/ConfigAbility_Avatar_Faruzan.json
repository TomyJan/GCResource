  //////////////////////////////////////////////////////////////
  //
  //   蓄力期间特效表现:Avatar_Faruzan_ShootAimingArrow_Charge
  //
  //////////////////////////////////////////////////////////////
  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "Avatar_Faruzan_ShootAimingArrow_Charge",
      "abilityMixins": [
        {
          "$type": "AttachToStateIDMixin",
          "stateIDs": ["Aim", "FastAim"],
          "modifierName": "ShootAimingArrow_Charge_Handler"
        },
        {
          "$type": "AttachToNormalizedTimeMixin",
          "stateID": "AimLoop",
          "modifierName": "Aim_ChangeAlpha",
          "normalizeStart": 0.0,
          "normalizeEnd": 1.0
        },
        {
          "$type": "AttachToNormalizedTimeMixin",
          "stateID": "FastAimLoop",
          "modifierName": "FastAim_ChangeAlpha",
          "normalizeStart": 0.0,
          "normalizeEnd": 1.0
        },
        {
          "$type": "AttachToStateIDMixin", //播放瞄准蓄力特效
          "stateIDs": ["FastAim", "FastAimShoot"],
          "modifierName": "ShowAimingEffect"
        },
        {
          "$type": "AttachToStateIDMixin",
          "stateIDs": ["Aim", "FastAim", "FastAimShoot", "AimShoot"],
          "modifierName": "ShootAimingArrow_Charge_Remove"
        },
        {
          "$type": "AttachToStateIDMixin", //播放瞄准蓄力特效
          "stateIDs": ["Aim", "AimShoot"],
          "modifierName": "ShowAimingEffect"
        }
      ],
      "onAdded": [
        {
          //断线重连回复后保证数值的正确
          "$type": "ApplyModifier",
          "modifierName": "Avatar_Faruzan_ShootArrow_ResetGlobalValue",
          "target": "Self",
          "doOffStage": true
        }
      ],
      "onAbilityStart": [
        {
          "$type": "AddGlobalValue",
          "key": "_Enchanted_Time",
          "value": -5,
          "useLimitRange": true,
          "maxValue": 5,
          "minValue": 0
        },
        {
          "$type": "RemoveModifier",
          "modifierName": "AimChargeMark"
        },
        {
          "$type": "RemoveModifier",
          "modifierName": "WindMagic_Arrow_Glow_Once"
        },
        {
          "$type": "RemoveModifier",
          "modifierName": "WindMagic_Arrow_Glow_WithoutWind_Once"
        }
      ],
      "modifiers": {
        "ShootAimingArrow_Charge_Handler": {
          "thinkInterval": 0.3,
          "onThinkInterval": [
            {
              "$type": "AttachModifier",
              "modifierName": "AimChargeMark"
            }
          ]
        },
        "Aim_ChangeAlpha": {
          "modifierMixins": [
            {
              "$type": "EffectChangeAlphaMixin",
              "target": "Team",
              "startDuration": 0.2,
              "endDuration": 0.3
            }
          ]
        },
        "FastAim_ChangeAlpha": {
          "modifierMixins": [
            {
              "$type": "EffectChangeAlphaMixin",
              "target": "Team",
              "startDuration": 0.2,
              "endDuration": 0.3
            }
          ]
        },
        "ShootAimingArrow_Charge_Remove": {
          "onAdded": [
            {
              "$type": "AddGlobalValue",
              "key": "_Enchanted_Time",
              "value": -5,
              "useLimitRange": true,
              "maxValue": 5,
              "minValue": 0
            }
          ],
          "onRemoved": [
            {
              "$type": "RemoveModifier",
              "modifierName": "AimChargeMark"
            }
          ]
        },
        "AimChargeMark": {
          "stacking": "MultipleRefreshNoRemove",
          "stackingOption": {
            "maxModifierNumForMultipleType": 5
          },
          "onAdded": [
            {
              "$type": "AddGlobalValue",
              "key": "_Enchanted_Time",
              "value": 1,
              "useLimitRange": true,
              "maxValue": 5,
              "minValue": 0
            }
          ]
        },
        "ShowAimingEffect": {
          "onAdded": [
            {
              "$type": "AttachModifier",
              "modifierName": "WindMagic_Arrow_Glow_WithoutWind_Once"
            }
          ],
          "thinkInterval": 0.1,
          "onThinkInterval": [
            {
              "$type": "Predicated",
              "target": "Self",
              "targetPredicates": [
                //判断自身是否在强化箭状态下
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "_ABILITY_Faruzan_ElementalArt_COUNT",
                  "value": 1,
                  "forceByCaster": true,
                  "compareType": "MoreThanAndEqual"
                }
              ],
              "successActions": [
                {
                  //播放强化箭状态下的蓄力完成特效
                  "$type": "AttachModifier",
                  "modifierName": "Faruzan_KarasShoot_Glow",
                  "predicates": [
                    {
                      "$type": "ByTargetGlobalValue",
                      "key": "_Enchanted_Time",
                      "value": "%Enchanted_Time*%Faruzan_AimShoot_Enchanted_Ratio",
                      "forceByCaster": true,
                      "compareType": "MoreThanAndEqual"
                    }
                  ]
                }
              ],
              "failActions": [
                {
                  //强化箭时间到了，清除瞄准状态下的强化特效
                  "$type": "RemoveModifier",
                  "modifierName": "Faruzan_KarasShoot_Glow"
                },
                {
                  //播放正常时间的蓄力完成特效
                  "$type": "AttachModifier",
                  "modifierName": "WindMagic_Arrow_Glow_Once",
                  "predicates": [
                    {
                      "$type": "ByTargetGlobalValue",
                      "key": "_Enchanted_Time",
                      "value": 4,
                      "forceByCaster": true,
                      "compareType": "MoreThanAndEqual"
                    }
                  ]
                }
              ]
            },

            {
              "$type": "AttachModifier",
              "modifierName": "WindMagic_Arrow_Glow_WithoutWind_Once", //蓄力中特效判别（是不是强化箭状态）
              "predicates": [
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "_Enchanted_Time", //判断：开始蓄力了
                  "value": 1,
                  "forceByCaster": true,
                  "compareType": "Equal"
                }
              ]
            }
          ],
          "onRemoved": [
            {
              "$type": "RemoveModifier",
              "modifierName": "WindMagic_Arrow_Glow_Once"
            },
            {
              "$type": "RemoveModifier",
              "modifierName": "WindMagic_Arrow_Glow_WithoutWind_Once"
            }
          ]
        },

        //蓄力中特效判别（是不是强化箭状态）
        "WindMagic_Arrow_Glow_WithoutWind_Once": {
          "stacking": "Unique",
          "onAdded": [
            {
              "$type": "SetGlobalValue",
              "key": "_ABILITY_Faruzan_AimShoot_Enchanted_Ratio", //强化箭状态下的蓄力时间会减少
              "value": "%Faruzan_AimShoot_Enchanted_Ratio",
              "useLimitRange": true,
              "maxValue": 1,
              "minValue": 0,
              "doOffStage": true,
              "predicates": [
                //判断自身是否在强化箭状态下
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "_ABILITY_Faruzan_ElementalArt_COUNT",
                  "value": 1,
                  "forceByCaster": true,
                  "compareType": "MoreThanAndEqual"
                }
              ]
            },
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_WindMagic_Arrow_Glow_Once_05", //非强化箭的蓄力开始
              "predicates": [
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "_ABILITY_Faruzan_AimShoot_Enchanted_Ratio",
                  "value": 1,
                  "compareType": "Equal"
                }
              ]
            },
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_WindMagic_Arrow_Glow_Loop_05", //非强化箭的蓄力未完成状态
              "predicates": [
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "_ABILITY_Faruzan_AimShoot_Enchanted_Ratio",
                  "value": 1,
                  "compareType": "Equal"
                },
                //判断自身是否在强化箭状态下
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "_ABILITY_Faruzan_ElementalArt_COUNT",
                  "value": 0,
                  "forceByCaster": true,
                  "compareType": "NoneOrEqual"
                }
              ]
            },
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_WindMagic_Arrow_Glow_Loop_03", //强化箭状态时的瞄准蓄力特效（蓄力时间没减，但已经强化了）
              "predicates": [
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "_ABILITY_Faruzan_AimShoot_Enchanted_Ratio",
                  "value": 1,
                  "compareType": "Equal"
                },
                //判断自身是否在强化箭状态下
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "_ABILITY_Faruzan_ElementalArt_COUNT",
                  "value": 1,
                  "forceByCaster": true,
                  "compareType": "MoreThanAndEqual"
                }
              ]
            },
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_WindMagic_Arrow_Glow_Once_03", //强化箭状态下开始蓄力
              "predicates": [
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "_ABILITY_Faruzan_AimShoot_Enchanted_Ratio",
                  "value": 0.9,
                  "compareType": "LessAndEqual"
                }
              ]
            },
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_WindMagic_Arrow_Glow_Loop_03", //强化箭状态下的瞄准蓄力特效（蓄力时间已经减了）
              "predicates": [
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "_ABILITY_Faruzan_AimShoot_Enchanted_Ratio",
                  "value": 0.9,
                  "compareType": "LessAndEqual"
                }
              ]
            }
          ],
          "onRemoved": [
            {
              //射完后需要重置这个值
