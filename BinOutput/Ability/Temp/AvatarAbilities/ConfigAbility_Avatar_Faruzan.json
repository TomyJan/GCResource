              "maxValue": 1,
              "minValue": 0,
              "doOffStage": true,
              "predicates": [
                //要在非强化箭的状态下才重置GV
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "_ABILITY_Faruzan_ElementalArt_COUNT",
                  "value": 0,
