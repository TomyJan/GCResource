[
  //公子这个角色的特点是：近战、远程、印记伤害，算作普攻。
  //所以原本的普攻tag，在他身上需要重新分配：
  //远程普攻：NormalAttack_01
  //近战攻击：NormalAttack_02
  //远程瞄准/近战重击：ExtraAttack
  //印记远程：NormalAttack_03
  //印记近战：Elemental_Art
  //印记死亡：NormalAttack_05
  //印记收割：Elemental_Burst

  ////////////////////////////////////////////////////////////////
  //
  //  瞄准射击时，搭在弓上的箭特效
  //
  ////////////////////////////////////////////////////////////////
  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "Avatar_Tartaglia_Arrow_FX",
      "abilityMixins": [
        {
          "$type": "AttachToStateIDMixin",
          "stateIDs": ["Aim", "FastAim"],
          "modifierName": "Tartaglia_Arrow_FX"
        }
      ],
      "onAbilityStart": [],
      "modifiers": {
        "Tartaglia_Arrow_FX": {
          "onAdded": [
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_Avatar_Male_Bow_Tartaglia_Arrow"
            }
          ]
        }
      }
    }
  },

  ////////////////////////////////////////////////////////////////
  //
  //  公子的攻击键按键逻辑
  //
  ////////////////////////////////////////////////////////////////
  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "Avatar_Tartaglia_PressShoot",
      "abilityMixins": [
        {
          "$type": "AttachModifierToSelfGlobalValueMixin",
          "globalValueKey": "_ABILITY_Gandalfr_IsActivated",
          "valueSteps": [-0.5, 0.5, 1.5],
          "removeAppliedModifier": true,
          "modifierNameSteps": [
            "Tartaglia_Bow_Mode_Handler",
            "Tartaglia_Gandalfr_Mode_Handler"
          ]
        }
      ],
      "modifiers": {
        "Tartaglia_Bow_Mode_Handler": {
          "modifierMixins": [
            {
              "$type": "ButtonHoldChargeMixin",
              "skillID": 10331,
              "chargeTime": 0.2,
              "OnBeginUncharged": [
                {
                  "$type": "SetAnimatorTrigger",
                  "triggerID": "DoSkill"
                },
                {
                  "$type": "SetAnimatorInt",
                  "intID": "SkillID",
                  "value": 100
                }
              ],
              "OnReleaseUncharged": [
                {
                  "$type": "SetCameraLockTime",
                  "lockTime": 0.3
                },
                {
                  "$type": "EntityDoSkill",
                  "skillID": 10335,
                  "isHold": true
                }
              ],
              "OnBeginCharged": [
                {
                  "$type": "SetAnimatorTrigger",
                  "triggerID": "DoSkill"
                },
                {
                  "$type": "SetAnimatorInt",
                  "intID": "SkillID",
                  "value": 101
                }
              ],
              "OnReleaseCharged": [
                {
                  "$type": "AvatarSkillStart",
                  "skillID": 10331
                },
                {
                  "$type": "AvatarSkillStart",
                  "skillID": 10334
                },
                {
                  "$type": "EntityDoSkill",
                  "skillID": 10335,
                  "isHold": true
                }
                // {
                // 	"$type": "SetAnimatorTrigger",
                // 	"triggerID": "AimEnd",
                // },
              ],
              "chargeStateIDs": ["ShootBS", "Shoot", "FastAim", "FastAimShoot"]
            },
            {
              "$type": "AttachToStateIDMixin",
              "stateIDs": ["FastAim", "FastAimShoot"],
              "modifierName": "KeepFastAiming"
            },
            {
              //当快速瞄准射击结束时，会设置FastAimShootFinish，使得动画从FastAim状态出来，同时会通过FastAimShooting防止在射击过程中点击了瞄准按钮直接退出
              "$type": "AttachToStateIDMixin",
              "stateIDs": ["FastAimShoot"],
              "modifierName": "SetFastAimShooting"
            },
            {
              "$type": "AttachToStateIDMixin",
              "stateIDs": ["ShootBS", "FastAim", "FastAimShoot"],
              "modifierName": "SetNeedAimEndStates"
            }
          ],
          "onAdded": [
            {
              "$type": "TriggerSetRenderersEnable",
              "doOffStage": true,
              "renderNames": ["Body_Cloak"],
              "setEnable": true,
              "canBeHandledOnRecover": true
            },
            {
              "$type": "TriggerSetRenderersEnable",
              "doOffStage": true,
              "renderNames": ["Body_CloakEffectMesh"],
              "setEnable": false,
              "canBeHandledOnRecover": true
            },
            {
              "$type": "TriggerHideWeapon",
              "doOffStage": true,
              "visible": true,
              "partNames": ["equip01", "equip02", "equip03", "sheathPoint"],
              "canBeHandledOnRecover": true
            }
          ]
        },
        "Tartaglia_Gandalfr_Mode_Handler": {
          "modifierMixins": [
            {
              "$type": "ButtonHoldChargeMixin",
              "skillID": 10337,
              "chargeTime": 0.12,
              "secondChargeTime": 0.2,
              "OnBeginUncharged": [
                {
                  "$type": "SetAnimatorTrigger",
                  "triggerID": "DoSkill"
                },
                {
                  "$type": "SetAnimatorInt",
                  "intID": "SkillID",
                  "value": 11,
                  "predicates": [
                    {
                      "$type": "ByAny",
                      "predicates": [
                        {
                          "$type": "ByAnimatorInt",
                          "value": 0,
                          "parameter": "ComboCount",
                          "logic": "Equal"
                        },
                        {
                          "$type": "ByAnimatorInt",
                          "value": 6,
                          "parameter": "ComboCount",
                          "logic": "Equal"
                        }
                      ]
                    }
                  ]
                },
                {
                  "$type": "SetAnimatorBool",
                  "boolID": "IsCharging",
                  "value": false
                }
              ],
              "OnReleaseUncharged": [
                /*						{
									"$type": "SetAnimatorTrigger",
									"triggerID": "DoSkill",
								},*/
                {
                  "$type": "SetAnimatorInt",
                  "intID": "SkillID",
                  "value": 1
                }
              ],
              "OnBeginCharged": [
                {
                  "$type": "SetAnimatorBool",
                  "boolID": "IsCharging",
                  "value": true
                }
              ],
              "OnReleaseCharged": [
                {
                  "$type": "SetAnimatorBool",
                  "boolID": "IsCharging",
                  "value": false
                },
                /*						{
									"$type": "SetAnimatorTrigger",
									"triggerID": "DoSkill",
								},*/
                {
                  "$type": "SetAnimatorInt",
                  "intID": "SkillID",
                  "value": 1
                }
                /*						{
									"$type": "ResetAnimatorTrigger",
									"triggerID": "DoSkill",
								},*/
              ],
              "OnBeginSecondCharged": [
                {
                  "$type": "SetAnimatorTrigger",
                  "triggerID": "DoSkill",
                  "predicates": [
                    {
                      "$type": "ByLocalAvatarStamina",
                      "stamina": "%ExtraAttack_Sword_Stamina_Cost+%Flexible_Stamina",
                      "logic": "GreaterOrEqual"
                    }
                  ]
                },
                {
                  "$type": "SetAnimatorInt",
                  "intID": "SkillID",
                  "value": 6,
                  "predicates": [
                    {
                      "$type": "ByLocalAvatarStamina",
                      "stamina": "%ExtraAttack_Sword_Stamina_Cost+%Flexible_Stamina",
                      "logic": "GreaterOrEqual"
                    }
                  ]
                },
                {
                  "$type": "SetAnimatorBool",
                  "boolID": "IsCharging",
                  "value": false
                }
                /*						{
									"$type": "ResetAnimatorTrigger",
									"triggerID": "DoSkill",
								},*/
              ],
              "OnReleaseSecondCharged": [],
              "chargeStateIDs": [
                "Gandalfr_Attack01",
                "Gandalfr_Attack02",
                "Gandalfr_Attack03",
                "Gandalfr_Attack04",
                "Gandalfr_Attack05",
                "Gandalfr_Attack06",
                "Run"
              ]
            },
            {
              "$type": "AttachToStateIDMixin",
              "stateIDs": ["Gandalfr_ExtraAttack"],
              "modifierName": "ExtraAttack_StaminaCost"
            },
            {
              "$type": "AttachToStateIDMixin",
              "stateIDs": ["Aim", "FastAim"],
              "modifierName": "ExtraAttack_Mode_Error_Handler",
              "isCheckOnAttach": true
            }
          ],
