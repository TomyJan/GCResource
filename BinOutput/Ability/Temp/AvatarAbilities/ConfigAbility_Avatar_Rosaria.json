  ///罗莎莉亚元素战技，隐藏和显示，掉球等逻辑
  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "Avatar_Rosaria_ElementalArt", //镜头控制，播放由pattern播放。
      "onAdded": [
        {
          "$type": "AttachModifier",
          "modifierName": "GenerateElemBall_Rosaria"
        }
      ],
      "abilityMixins": [
        {
          "$type": "AttachToNormalizedTimeMixin",
          "stateID": "Rosaria_ElementalArt_BS",
          "normalizeStart": 0.0,
          "normalizeEnd": 1,
          "modifierName": "Rosaria_Check_Target"
        },
        {
          "$type": "AttachToNormalizedTimeMixin",
          "stateID": "Rosaria_ElementalArt_HasTarget",
          "modifierName": "Rosaria_ElementalArt_Hide1", //注意角色隐藏功能会叠加。如果隐藏2次会变-2，需要显示2次才能真显示出来。
          "normalizeStart": 0.0,
          "normalizeEnd": 0.3
        },
        {
          "$type": "AttachToNormalizedTimeMixin",
          "stateID": "Rosaria_ElementalArt_NoTarget",
          "modifierName": "Rosaria_ElementalArt_Hide2", //注意角色隐藏功能会叠加。如果隐藏2次会变-2，需要显示2次才能真显示出来。
          "normalizeStart": 0.0,
          "normalizeEnd": 0.3
        },
        {
          "$type": "AttachToNormalizedTimeMixin",
          "stateID": "Rosaria_ElementalArt_HasTarget", //art共56帧，位移在0.06也就是3帧多，0.1秒用时6帧，至少需要在20%动画之后才能保证位移到怪身后。而攻击点在33%动画。
          "modifierName": "Certain_Critcal_CheckWindow", //位移完成就算还没出手怪也可能转身，所以安排在25%，保证了掉帧动画延迟，又稍微早一点。
          "normalizeStart": 0.25,
          "normalizeEnd": 1
        },
        {
          "$type": "AttachToNormalizedTimeMixin",
          "stateID": "Rosaria_ElementalArt_NoTarget",
          "modifierName": "Certain_Critcal_CheckWindow",
          "normalizeStart": 0.25,
          "normalizeEnd": 1
        }
      ],
      "modifiers": {
        "Rosaria_Check_Target": {
          "onAdded": [
            {
              "$type": "SetAnimatorBool", //将bool设为0或1进入无目标和有目标2种动画效果。
              "boolID": "SkillHasTarget",
              "value": false,
              "predicates": [
                {
                  "$type": "ByNot",
                  "predicates": [
                    {
                      "$type": "ByHasAttackTarget"
                    }
                  ]
                }
              ]
            },
            {
              "$type": "SetAnimatorBool",
              "boolID": "SkillHasTarget",
              "value": true,
              "predicates": [
                {
                  "$type": "ByHasAttackTarget"
                }
              ]
            }
          ]
        },
        //有目标时
        "Rosaria_ElementalArt_Hide1": {
          "onAdded": [
            {
              "$type": "TriggerSetVisible",
              "visible": false,
              "doOffStage": true
            },
            {
              "$type": "TriggerSetPassThrough",
              "passThrough": true,
              "doOffStage": true
            },
            {
              "$type": "EnableHDMesh",
              "enable": false,
              "hdMeshKey": "Default"
            }
          ],
          "onRemoved": [
            {
              "$type": "TriggerSetVisible",
              "visible": true,
              "doOffStage": true
            },
            {
              "$type": "TriggerSetPassThrough",
              "passThrough": false,
              "doOffStage": true,
              "canBeHandledOnRecover": true
            },
            {
              "$type": "EnableHDMesh",
              "enable": true,
              "hdMeshKey": "Default",
              "canBeHandledOnRecover": true
            }
          ]
        },
        //没目标时
        "Rosaria_ElementalArt_Hide2": {
          "onAdded": [
            {
              "$type": "TriggerSetVisible",
              "visible": false,
              "doOffStage": true
            },
            {
              "$type": "TriggerSetPassThrough",
              "passThrough": true,
              "doOffStage": true
            },
            {
              "$type": "EnableHDMesh",
              "enable": false,
              "hdMeshKey": "Default"
            }
          ],
          "onRemoved": [
            {
              "$type": "TriggerSetVisible",
              "visible": true,
              "doOffStage": true
            },
            {
              "$type": "TriggerSetPassThrough",
              "passThrough": false,
              "doOffStage": true,
              "canBeHandledOnRecover": true
            },
            {
              "$type": "EnableHDMesh",
              "enable": true,
              "hdMeshKey": "Default",
              "canBeHandledOnRecover": true
            }
          ]
        },
        //注意描述说明这个效果只对怪物生效。
        "Certain_Critcal_CheckWindow": {
          "duration": 2,
          "onAdded": [
            {
              "$type": "Predicated",
              "target": "SelfAttackTarget",
              "targetPredicates": [
                {
                  "$type": "ByTargetForwardAndSelfPosition",
                  "target": "Target",
                  "value": 90,
                  "logic": "GreaterOrEqual"
                },
                {
                  "$type": "ByEntityTypes",
                  "entityTypes": ["Monster"]
                },
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "_ANIMAL_ENTITY_TYPE",
                  "value": 0,
                  "compareType": "NoneOrEqual"
                },
                {
                  "$type": "ByUnlockTalentParam", //这个自动判caster的
                  "talentParam": "Back_Critic"
                }
              ],
              "successActions": [
                {
                  "$type": "ApplyModifier",
                  "target": "Self",
                  "modifierName": "Extra_Critcal"
                }
              ],
              "failActions": []
            }
          ]
        },
        "Extra_Critcal": {
          // "modifierMixins":
          // [
          //     {
          //         "$type":"ModifyDamageMixin",
          //         "attackTags":["Elemental_Art",],
          //         "bonusCritical":"%Critical",
          //     },
          // ],
          "state": "AttackUp",
          "duration": "%Time",
          "properties": {
            "Actor_CriticalDelta": "%Critical"
          }
        },
        "GenerateElemBall_Rosaria": {
          "onAttackLanded": [
            {
              "$type": "Predicated",
              "target": "Target",
              "targetPredicates": [
                {
                  "$type": "ByEntityTypes",
                  "entityTypes": ["Monster"]
                },
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "_ANIMAL_ENTITY_TYPE",
                  "value": 0,
                  "compareType": "NoneOrEqual"
                },
                {
                  "$type": "ByAttackTags",
                  "attackTags": ["Elemental_Art"]
                },
                {
                  "$type": "ByHitStrikeType", //用slash判定第二下。
                  "strikeType": "Slash"
                }
              ],
              "successActions": [
                {
                  "$type": "AttachModifier",
                  "modifierName": "Ball",
                  "target": "Self"
                }
              ]
            },
            {
              "$type": "Predicated",
              "target": "Target",
              "targetPredicates": [
                {
                  "$type": "ByEntityTypes",
                  "entityTypes": ["Monster"]
                },
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "_ANIMAL_ENTITY_TYPE",
                  "value": 0,
                  "compareType": "NoneOrEqual"
                },
                {
                  "$type": "ByAttackTags",
                  "attackTags": ["Elemental_Art"]
                },
                {
                  "$type": "ByHitCritical"
                }
              ],
              "successActions": [
                {
                  "$type": "AttachModifier",
                  "modifierName": "Talent_ReviveEnergy",
                  "target": "Self"
                }
              ],
              "predicates": [
                {
                  "$type": "ByUnlockTalentParam", //解锁后开放回能逻辑，和前面拆2个的原因是这里需要判定暴击，没法写在一起。
                  "target": "Self",
                  "talentParam": "ReviveEnergy"
                }
              ]
            }
          ]
        },
        "Ball": {
          "duration": 0.6,
          "stacking": "Unique",
          "onAdded": [
            {
              "$type": "Randomed",
              "chance": 1,
              "successActions": [
                {
                  "$type": "GenerateElemBall",
                  "configID": 2022,
                  "baseEnergy": 9,
                  "ratio": "%GetElementRatio",
                  "born": {
                    "$type": "ConfigBornByTarget",
                    "offset": {
                      "x": 0.0,
                      "y": 0.0,
                      "z": 1.0
                    }
                  }
                }
              ]
            }
          ]
        },
        "Talent_ReviveEnergy": {
          "stacking": "Unique",
          "duration": 0.6, //太长会导致重置技能cd武器再触发时无法回能。
          "onAdded": [
            {
              "$type": "ReviveElemEnergy",
              "value": "%ReviveEnergy",
              "target": "Self"
            }
          ]
        }
      },
      "abilitySpecials": {
        "GetElementRatio": 1,
        "ReviveEnergy": 0,
        "Critical": 0,
        "Time": 0
      }
    }
  },

