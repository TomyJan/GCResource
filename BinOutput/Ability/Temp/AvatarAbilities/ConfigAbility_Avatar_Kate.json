  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "Test_StaticElecField",
      "onAbilityStart": [
        {
          "$type": "ApplyModifier",
          "modifierName": "StaticElec",
          "otherTargets": {
            "$type": "SelectTargetsByShape",
            "shapeName": "CircleR3",
            "centerBasedOn": "Target",
            "campTargetType": "Enemy",
            "campBasedOn": "Self"
          },
          "predicates": [
            {
              "$type": "ByEntityTypes",
              "entityTypes": ["Monster"]
            }
          ]
        },
        {
          "$type": "ApplyModifier",
          "modifierName": "UNIQUE_StaticElec_Effect",
          "otherTargets": {
            "$type": "SelectTargetsByShape",
            "shapeName": "CircleR3",
            "centerBasedOn": "Target",
            "campTargetType": "Enemy",
            "campBasedOn": "Self"
          },
          "predicates": [
            {
              "$type": "ByEntityTypes",
              "entityTypes": ["Monster"]
            }
          ]
        },
        {
          "$type": "ApplyModifier",
          "modifierName": "FlickerWave_Effect",
          "target": "Target",
          "predicates": [
            {
              "$type": "ByEntityTypes",
              "entityTypes": ["Monster"]
            }
          ]
        }
      ],
      "modifiers": {
        "FlickerWave_Effect": {
          "duration": 0.8,
          "onAdded": [
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_FlickerWave_Buff"
            }
          ]
        },
        "StaticElec": {
          "duration": 15,
          "stacking": "MultipleRefreshNoRemove",
          "stackingOption": {
            "maxModifierNumForMultipleType": 3
          },
          "onAdded": [
            {
              "$type": "AddGlobalValue",
              "key": "_ABILITY_StaticElec_COUNT",
              "value": 1,
              "useLimitRange": true,
              "maxValue": 3,
              "minValue": 0
            }
          ],
          "onRemoved": [
            {
              "$type": "AddGlobalValue",
              "key": "_ABILITY_StaticElec_COUNT",
              "value": -1,
              "useLimitRange": true,
              "maxValue": 3,
              "minValue": 0
            }
          ],
          "thinkInterval": 0.1,
          "onThinkInterval": [
            {
              "$type": "RemoveModifier",
              "modifierName": "StaticElec",
              "target": "Self",
              "predicates": [
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "_ABILITY_StaticElec_COUNT",
                  "value": 0,
                  "compareType": "Equal"
                }
              ]
            }
          ]
        },
        "UNIQUE_StaticElec_Effect": {
          "isUnique": true,
          "modifierMixins": [
            {
              "$type": "AttachModifierToSelfGlobalValueMixin", // BUFF增加和减少时，都触发这个modifier来检查和替换特效
              "globalValueKey": "_ABILITY_StaticElec_COUNT",
              "valueSteps": [-1, 0.5, 1.5, 2.5, 3.5],
              "modifierNameSteps": [
                "StaticElec_effect_0",
                "StaticElec_effect_1",
                "StaticElec_effect_2",
                "StaticElec_effect_3"
              ]
            }
          ]
        },
        "StaticElec_effect_0": {
          "onAdded": [
            {
              "$type": "RemoveModifier",
              "modifierName": "StaticElec_effect_1"
            },
            {
              "$type": "RemoveModifier",
              "modifierName": "StaticElec_effect_2"
            },
            {
              "$type": "RemoveModifier",
              "modifierName": "StaticElec_effect_3"
            }
          ]
        },
        "StaticElec_effect_1": {
          "onAdded": [
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_StaticElec_01"
            },
            {
              "$type": "RemoveModifier",
              "modifierName": "StaticElec_effect_0"
            },
            {
              "$type": "RemoveModifier",
              "modifierName": "StaticElec_effect_2"
            }
          ]
        },
        "StaticElec_effect_2": {
          "onAdded": [
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_StaticElec_02"
            },
            {
              "$type": "RemoveModifier",
              "modifierName": "StaticElec_effect_1"
            },
            {
              "$type": "RemoveModifier",
              "modifierName": "StaticElec_effect_3"
            }
          ]
        },
        "StaticElec_effect_3": {
          "onAdded": [
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_StaticElec_03"
            },
            {
              "$type": "RemoveModifier",
              "modifierName": "StaticElec_effect_2"
            }
          ]
        }
      }
    }
  },
