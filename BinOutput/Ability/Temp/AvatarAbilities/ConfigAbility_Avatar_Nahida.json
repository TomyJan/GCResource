          "onAdded": [
            {
              "$type": "Predicated",
              "targetPredicates": [
                {
                  "$type": "ByHasAttackTarget"
                }
              ],
              //有敌方目标
              "successActions": [
                {
                  "$type": "SetGlobalPos",
                  "key": "_ABILITY_Nahida_ExtraAttack_Pos", //确定重击施法位置
                  "target": "SelfAttackTarget",
                  "setTarget": false,
                  "born": {
                    "$type": "ConfigBornBySelectedPoint",
                    "onGround": true,
                    "offset": {
                      "x": 0.0,
                      "y": 0.0,
                      "z": 0.0
                    }
                  }
                }
              ],
              //没有敌方目标
              "failActions": [
                {
                  "$type": "SetGlobalPos",
                  "key": "_ABILITY_Nahida_ExtraAttack_Pos",
                  "born": {
                    "$type": "ConfigBornBySelf",
                    "offset": {
                      "x": 0.0,
                      "y": 0.0,
                      "z": 5.0
                    },
                    "onGround": true,
                    "onGroundRaycastUpDist": 4.0
                  }
                }
              ]
            },
            {
              "$type": "CreateGadget", //创建重击攻击盒
              "gadgetID": 41073001,
              "campID": 1001,
              "campTargetType": "Enemy",
              "born": {
                "$type": "ConfigBornByGlobalValue",
                "positionKey": "_ABILITY_Nahida_ExtraAttack_Pos",
                "offset": {
                  "x": 0.0,
                  "y": 1.0,
                  "z": 0.0
                },
                "onGround": true
              }
            }
          ]
        }
      }
    }
  },
