  //  接下来N次普攻/重击命中印记目标时可直接触发锁链脉冲攻击
  //
  ////////////////////////////////////////////////////////////////
  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "Avatar_Nahida_Constellation_6",
      "isDynamicAbility": true,
      "onAdded": [
        {
          "$type": "SetGlobalValue",
          "key": "_ABILITY_Nahida_Constellation_6_Num", //GV初始化
          "value": 0
        }
      ],
      "abilityMixins": [
        {
          "$type": "AttachModifierToSelfGlobalValueMixin",
          "globalValueKey": "_ABILITY_Nahida_Constellation_6_Num",
          "valueSteps": [-0.5, 0.5, 100],
          "modifierNameSteps": [
            "Blank",
            "Avatar_Nahida_Constellation_6_AttackState"
          ]
        },
        {
          "$type": "AttachToStateIDMixin",
          "stateIDs": ["Nahida_ElementalBurst_AS"],
          "modifierName": "Avatar_Nahida_Constellation_6_Trigger"
        }
      ],
      "modifiers": {
        //触发命6状态时点
        "Avatar_Nahida_Constellation_6_Trigger": {
          "onAdded": [
            {
              "$type": "SetGlobalValue",
              "key": "_ABILITY_Nahida_Constellation_6_Num",
              "value": "%Attack_Count"
            },
            {
              "$type": "AddElementDurability",
              "modifierName": "Avatar_Nahida_Constellation_6_Fx",
              "value": 100
            },
            {
              "$type": "ApplyModifier",
              "modifierName": "Avatar_Nahida_Constellation_6_Timer"
            }
          ]
        },
        //命6普攻触发连携减少计数器
        "Avatar_Nahida_Constellation_6_ReduceNum": {
          "duration": 0.1, //保证一次攻击行为所有目标都可触发的时间内
          "stacking": "Unique",
          "onRemoved": [
            {
              "$type": "AddGlobalValue",
              "key": "_ABILITY_Nahida_Constellation_6_Num", //需要保证次数为0.1s延迟触发，其他链接目标才能受到攻击
              "value": -1,
              "doOffStage": true
            }
          ],
          "thinkInterval": 0.1,
          "onThinkIntervalIsFixedUpdate": true,
          "onThinkInterval": [
            {
              "$type": "RemoveModifier",
              "modifierName": "Avatar_Nahida_Constellation_6_ReduceNum",
              "doOffStage": true
            }
          ]
        },
        //特殊状态计时器
        "Avatar_Nahida_Constellation_6_Timer": {
          "duration": "%Duration+1", //延迟补正
          "onRemoved": [
            {
              "$type": "SetGlobalValue",
              "key": "_ABILITY_Nahida_Constellation_6_Num",
              "value": 0,
              "doOffStage": true
            }
          ]
        },
        "Blank": {
          "onAdded": [
            {
              "$type": "ApplyModifier",
              "modifierName": "Delay_KillFx",
              "doOffStage": true
            }
          ]
        },
        //特殊模式：普攻命中锁链目标额外触发锁链伤害
        "Avatar_Nahida_Constellation_6_AttackState": {
          "onAdded": [
            {
              "$type": "ApplyModifier",
              "modifierName": "Avatar_Nahida_Constellation_6_Fx"
            }
          ],
          "modifierMixins": [
            {
              "$type": "AttackChainReceiverMixin", //接受怪物侧传来的Trigger信息
              "cd": 0.2,
              "attackChainReceiverKey": "Nahida_Constellation", //命6接收key
              "actionQueue": [
                {
                  "$type": "FireEffect",
                  "effectPattern": "Eff_Avatar_Loli_Catalyst_Nahida_ElementalArt_Ray_Hit",
                  "target": "Target",
                  "doOffStage": true
                },
                {
                  "$type": "DamageByAttackValue",
                  "target": "Target",
                  "doOffStage": true,
                  "born": {
                    "$type": "ConfigBornByAttachPoint",
                    "attachPointName": "RootNode",
                    "offset": {
                      "x": 0,
                      "y": 0.3,
                      "z": 0
                    }
                  },
                  "attackInfo": {
                    "attackTag": "Elemental_Art", //该标记用于记录锁链伤害
                    "attenuationTag": "Nahida_Chain_Constellation", //命6独立的元素衰减组
                    "attenuationGroup": "Nahida_Chain_Constellation",
                    "attackProperty": {
                      "damagePercentage": "%Constellation_6_Dmg_PerAtk",
                      "damageExtra": "%Constellation_6_Dmg_PerMastery*%FIGHT_PROP_ELEMENT_MASTERY",
                      "attackType": "Range",
                      "strikeType": "Default",
                      "elementType": "Grass",
                      "elementDurability": 25,
                      "enBreak": 30
                    },
                    "hitPattern": {
                      "onHitEffectName": "default",
                      "hitImpulseType": "LV2",
                      "reboundRatio": 0,
                      "hitHaltTime": 0.0,
                      "hitHaltTimeScale": 0.05,
                      "muteHitText": false
                    },
                    "forceCameraShake": false
                  }
                },
                {
                  "$type": "SendEffectTriggerToLineEffect",
                  "parameter": "Attack",
                  "type": "Trigger",
                  "effectPattern": "Eff_Avatar_Loli_Catalyst_Nahida_ElementalArt_Line",
                  "target": "Target",
                  "doOffStage": true
                },
                {
                  "$type": "ApplyModifier",
                  "modifierName": "Avatar_Nahida_Constellation_6_ReduceNum",
                  "target": "Caster",
                  "doOffStage": true
                }
              ],
              //判断抛事件的攻击者必须为自己，才会对链接目标进行脉冲攻击
              "attackerPredicates": [
                {
                  "$type": "ByTargetIsSelf",
                  "target": "Target",
                  "isSelf": true
                }
              ]
            }
          ]
        },
        //命6的状态特效Fx
        "Avatar_Nahida_Constellation_6_Fx": {
          "duration": 15, //比持续时间适当长点
          "elementDurability": 100,
          "onAdded": [
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_Avatar_Loli_Catalyst_Nahida_Constellation_6"
            }
          ]
        },
        "Delay_KillFx": {
          "duration": 0.5,
          "onAdded": [
            {
              "$type": "SendEffectTrigger",
              "parameter": "Die",
              "type": "Trigger",
              "effectPattern": "Eff_Avatar_Loli_Catalyst_Nahida_Constellation_6"
            }
          ],
          "onRemoved": [
            {
              "$type": "RemoveModifier",
              "modifierName": "Avatar_Nahida_Constellation_6_Fx",
              "doOffStage": true
            }
          ]
        }
      },
      "abilitySpecials": {
        "Constellation_6_Dmg_PerAtk": 0, //命6额外触发锁链伤害攻击倍率
        "Constellation_6_Dmg_PerMastery": 0, //命6额外触发锁链伤害精通倍率
        "Attack_Count": 0, //触发攻击次数
        "Duration": 0 //状态持续时间
      }
    }
  },

  ////////////////////////////////////////////////////////////////
