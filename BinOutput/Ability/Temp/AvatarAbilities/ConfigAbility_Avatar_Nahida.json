            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_Avatar_Loli_Catalyst_Nahida_Show01"
            }
          ],
          "onRemoved": [
            {
              "$type": "FireEffect",
              "effectPattern": "Eff_Avatar_Loli_Catalyst_Nahida_Show01_Disappear",
              "doOffStage": true
