  //点按/长按操作反馈区分
  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "Avatar_Nahida_ExtraAttack",
      "abilityMixins": [
        {
          "$type": "ButtonHoldChargeMixin",
          "skillID": 10731,
          "chargeTime": 0.12,
          "secondChargeTime": 0.12,
          "OnBeginUncharged": [
            {
              "$type": "SetAnimatorTrigger",
              "triggerID": "DoSkill",
              "predicates": [
                {
                  "$type": "ByAnimatorInt",
                  "value": 0,
                  "parameter": "ComboCount",
                  "logic": "Equal"
                }
              ]
            },
            {
              "$type": "SetAnimatorInt", //由于草神普攻逻辑特殊，这里的skillId配置与其他角色有一定差异，其他角色勿抄
              "intID": "SkillID",
              "value": 98
            },
            {
              "$type": "SetAnimatorInt",
              "intID": "SkillID",
              "value": 99,
              "predicates": [
                {
                  "$type": "ByAnimatorInt",
                  "value": 0,
                  "parameter": "ComboCount",
                  "logic": "Equal"
                }
              ]
            },
            {
              "$type": "SetAnimatorInt",
              "intID": "SkillID",
              "value": 1,
              "predicates": [
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "_ABILITY_Nahida_Is_AttackBS", //判定在BS动作中
                  "value": 1,
                  "compareType": "Equal"
                }
              ]
            },
            {
              "$type": "SetAnimatorBool",
              "boolID": "IsCharging",
              "value": false
            }
          ],
          "OnReleaseUncharged": [
            {
              "$type": "SetAnimatorTrigger",
              "triggerID": "DoSkill"
            },
            {
              "$type": "SetAnimatorInt",
              "intID": "SkillID",
              "value": 1
            }
          ],
          "OnBeginCharged": [
            {
              "$type": "SetAnimatorBool",
              "boolID": "IsCharging",
              "value": true
            }
          ],
          "OnReleaseCharged": [
            {
              "$type": "SetAnimatorBool",
              "boolID": "IsCharging",
              "value": false
            },
            {
              "$type": "SetAnimatorTrigger",
              "triggerID": "DoSkill"
            },
            {
              "$type": "SetAnimatorInt",
              "intID": "SkillID",
              "value": 1
            }
          ],
          "OnBeginSecondCharged": [
            {
              "$type": "SetAnimatorTrigger",
              "triggerID": "DoSkill"
            },
            {
              "$type": "SetAnimatorInt",
              "intID": "SkillID",
              "value": 6,
              "predicates": [
                {
                  "$type": "ByLocalAvatarStamina",
                  "stamina": "%ExtraAttack_Catalyst_Stamina_Cost*%Nahida_ExtraAttack_StaminaCost+%Flexible_Stamina",
                  "logic": "GreaterOrEqual"
                }
              ]
            },
            {
              "$type": "SetAnimatorInt",
              "intID": "SkillID",
              "value": 1,
              "predicates": [
                {
                  "$type": "ByLocalAvatarStamina",
                  "stamina": "%ExtraAttack_Catalyst_Stamina_Cost*%Nahida_ExtraAttack_StaminaCost+%Flexible_Stamina", //留出服务器误差体力
                  "logic": "Lesser"
                }
              ]
            },
            {
              "$type": "SetAnimatorBool",
              "boolID": "IsCharging",
              "value": false
            }
          ],
          "chargeStateIDs": [
            "Attack01",
            "Attack02",
            "Attack03",
            "ExtraAttack_BS",
            "ExtraAttack",
            "ExtraAttack_AS"
          ]
        },
        {
          "$type": "AttachToNormalizedTimeMixin",
          "stateID": "ExtraAttack",
          "normalizeStart": 0.163,
          "normalizeEnd": 1.0,
          "modifierName": "ExtraAttack_StaminaCost"
        },
        {
          "$type": "AttachToStateIDMixin",
          "stateIDs": ["ExtraAttack_BS"],
          "modifierName": "Avatar_Nahida_ExtraAttack_BS_Check"
        }
      ],
      "modifiers": {
        "ExtraAttack_StaminaCost": {
          "onAdded": [
            {
              "$type": "AvatarSkillStart",
              "skillID": 10003,
              "costStaminaRatio": "%Nahida_ExtraAttack_StaminaCost*%ExtraAttack_Catalyst_Stamina_Cost"
            }
          ]
        },
        //特殊_检测普攻BS动作
        "Avatar_Nahida_ExtraAttack_BS_Check": {
          "onAdded": [
            {
              "$type": "SetGlobalValue",
              "key": "_ABILITY_Nahida_Is_AttackBS",
              "value": 1,
              "doOffStage": true
            }
          ],
          "onRemoved": [
            {
              "$type": "SetGlobalValue",
              "key": "_ABILITY_Nahida_Is_AttackBS",
              "value": 0,
              "doOffStage": true
            }
          ]
        }
      },
      "abilitySpecials": {
        "Nahida_ExtraAttack_StaminaCost": 1,
        "ExtraAttack_Catalyst_Stamina_Cost": 0, //读表
        "Flexible_Stamina": -5 //因为延迟触发体力消耗，可适当放宽一点点
      }
    }
  },
