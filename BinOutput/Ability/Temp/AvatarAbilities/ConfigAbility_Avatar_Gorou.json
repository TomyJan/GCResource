          "stateIDs": ["Aim", "AimShoot"],
          "modifierName": "SetNeedAimEndStates"
        }
      ],
      "modifiers": {
        "KeepAiming": {
          "onAdded": [
            //将动画状态机设置为瞄准状态
            {
              "$type": "SetAnimatorBool",
              "boolID": "Aiming",
              "value": true
            },
            //进入瞄准镜头
            {
              "$type": "AvatarEnterFocus",
              "cameraFollowLower": {
                "x": 0.755,
                "y": 0.17,
                "z": -1.9
              },
              "cameraFollowUpper": {
                "x": 0.755,
                "y": 0.37,
                "z": -1.6
              },
              "cameraFastFocusMode": true,
              "faceToTarget": true,
              "faceToTargetAngleThreshold": 360,
              "assistance": {
                "ps4": {
                  "useFocusSticky": true,
                  "useGyro": false
                },
                "ps5": {
                  "useFocusSticky": true,
                  "useGyro": false
                },
                "other": {
                  "useFocusSticky": true,
                  "useGyro": true
                }
              },
              "canMove": true,
              "vcam_fov": 30
            },
            //如果处于大招状态下，把狗子的特效隐藏
            {
              "$type": "SendEffectTrigger",
              "parameter": "Hide",
              "effectPattern": "Eff_Avatar_Boy_Bow_Gorou_ElementalBurst_Shirou"
            }
          ],
          "onRemoved": [
            {
              "$type": "SetAnimatorBool",
              "boolID": "Aiming",
              "value": false
            },
            {
              "$type": "AvatarExitFocus",
              "keepRotation": true
            },
            //重新显示
            {
              "$type": "SendEffectTrigger",
              "parameter": "ExitHide",
              "effectPattern": "Eff_Avatar_Boy_Bow_Gorou_ElementalBurst_Shirou"
            }
          ]
        },
        "SetAimShooting": {
          "onAdded": [
            {
              "$type": "SetAnimatorBool",
              "boolID": "AimShooting",
              "value": true
            }
          ],
          "onRemoved": [
            {
              "$type": "SetAnimatorBool",
              "boolID": "AimShooting",
              "value": false
            }
          ]
        },
        "SetNeedAimEndStates": {
          "onAdded": [
            {
              "$type": "SetAnimatorBool",
              "boolID": "InStatesNeedAimEnd",
              "value": true
            }
          ],
          "onRemoved": [
            {
              "$type": "SetAnimatorBool",
              "boolID": "InStatesNeedAimEnd",
              "value": false
            }
          ]
        }
      }
    }
