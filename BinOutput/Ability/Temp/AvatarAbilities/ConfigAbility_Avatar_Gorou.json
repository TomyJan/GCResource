  ///////////////////////////////////////////////////////////////
  //
  //  蓄力期间特效表现
  //
  //////////////////////////////////////////////////////////////
  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "Avatar_Gorou_ShootAimingArrow_Charge",
      "abilityMixins": [
        {
          "$type": "AttachToStateIDMixin", //蓄力时记录蓄力的层数
          "stateIDs": ["Aim", "FastAim"],
          "modifierName": "ShootAimingArrow_Charge_Handler"
        },
        {
          "$type": "AttachToStateIDMixin", //播放瞄准蓄力特效
          "stateIDs": ["Aim", "AimShoot"],
          "modifierName": "ShowAimingEffect"
        },
        {
          "$type": "AttachToStateIDMixin", //播放瞄准蓄力特效。
          "stateIDs": ["FastAim", "FastAimShoot"],
          "modifierName": "ShowAimingEffect"
        },
        {
          "$type": "AttachToStateIDMixin", //用于在离开蓄力状态时，清楚蓄力的特效及相关的标记
          "stateIDs": ["Aim", "FastAim", "FastAimShoot", "AimShoot"],
          "modifierName": "ShootAimingArrow_Charge_Remove"
        },
        //用于处理瞄准状态下护盾特效半透
        {
          "$type": "AttachToNormalizedTimeMixin",
          "stateID": "AimLoop",
          "modifierName": "Aim_ChangeAlpha",
          "normalizeStart": 0.0,
          "normalizeEnd": 1.0
        },
        {
          "$type": "AttachToNormalizedTimeMixin",
          "stateID": "FastAimLoop",
          "modifierName": "FastAim_ChangeAlpha",
          "normalizeStart": 0.0,
          "normalizeEnd": 1.0
        }
      ],
      "onAbilityStart": [
        ////////////射完了记得清一下标记及相关特效////////////////////////////////////
        {
          "$type": "AddGlobalValue",
          "key": "_Enchanted_Time",
          "value": -5,
          "useLimitRange": true,
          "maxValue": 5,
          "minValue": 0
        },
        {
          "$type": "RemoveModifier",
          "modifierName": "AimChargeMark"
        },
        {
          "$type": "RemoveModifier",
          "modifierName": "RockMagic_Arrow_Glow_Once"
        },
        {
          "$type": "RemoveModifier",
          "modifierName": "RockMagic_Arrow_Glow_WithoutWind_Once"
        }
        //////////////////////////////////////////////////////////////////////////
      ],
      "modifiers": {
        "ShootAimingArrow_Charge_Handler": {
          "thinkInterval": 0.3,
          "onThinkInterval": [
            {
              "$type": "ApplyModifier",
              "modifierName": "AimChargeMark"
            }
          ]
        },
        "ShootAimingArrow_Charge_Remove": {
          "onAdded": [
            {
              "$type": "AddGlobalValue",
              "key": "_Enchanted_Time",
              "value": -5,
              "useLimitRange": true,
              "maxValue": 0,
              "minValue": 0
            }
          ],
          "onRemoved": [
            {
              "$type": "RemoveModifier",
              "modifierName": "AimChargeMark"
            }
          ]
        },
        "AimChargeMark": {
          "stacking": "MultipleRefreshNoRemove",
          "stackingOption": {
            "maxModifierNumForMultipleType": 5
          },
          "onAdded": [
            {
              "$type": "AddGlobalValue",
              "key": "_Enchanted_Time",
              "value": 1,
              "useLimitRange": true,
              "maxValue": 5,
              "minValue": 0
            }
          ]
        },
        "ShowAimingEffect": {
          "onAdded": [
            {
              "$type": "AttachModifier",
              "modifierName": "RockMagic_Arrow_Glow_WithoutWind_Once"
            }
          ],
          "thinkInterval": 0.1,
          //判断蓄力时间播不同的特效
          "onThinkInterval": [
            {
              "$type": "AttachModifier",
              "modifierName": "RockMagic_Arrow_Glow_Once",
              "predicates": [
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "_Enchanted_Time",
                  "value": 4,
                  "forceByCaster": true,
                  "compareType": "MoreThanAndEqual"
                }
              ]
            },
            {
              "$type": "AttachModifier",
              "modifierName": "RockMagic_Arrow_Glow_WithoutWind_Once",
              "predicates": [
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "_Enchanted_Time",
                  "value": 1,
                  "forceByCaster": true,
                  "compareType": "Equal"
                }
              ]
            }
          ],
          "onRemoved": [
            {
              "$type": "RemoveModifier",
              "modifierName": "RockMagic_Arrow_Glow_Once"
            },
            {
              "$type": "RemoveModifier",
              "modifierName": "RockMagic_Arrow_Glow_WithoutWind_Once"
            }
          ]
        },
        //带元素
        "RockMagic_Arrow_Glow_Once": {
          "stacking": "Unique",
          "onAdded": [
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_Avatar_Boy_Bow_Gorou_ExtraAttack_Glow_Once_02"
            },
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_Avatar_Boy_Bow_Gorou_ExtraAttack_Glow_Loop_02"
            }
          ]
        },
        //不带元素
        "RockMagic_Arrow_Glow_WithoutWind_Once": {
          "stacking": "Unique",
          "onAdded": [
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_Avatar_Boy_Bow_Gorou_ExtraAttack_Glow_Once_01"
            },
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_Avatar_Boy_Bow_Gorou_ExtraAttack_Glow_Loop_01"
            }
          ]
        },
        "Aim_ChangeAlpha": {
          "modifierMixins": [
            {
              "$type": "EffectChangeAlphaMixin",
              "target": "Team",
              "startDuration": 0.2,
              "endDuration": 0.3
            }
          ]
        },
        "FastAim_ChangeAlpha": {
          "modifierMixins": [
            {
              "$type": "EffectChangeAlphaMixin",
              "target": "Team",
              "startDuration": 0.2,
              "endDuration": 0.3
            }
          ]
        }
      }
    }
  }, //Avatar_Ambor_ShootAimingArrow_Charge

