          "modifierMixins": [
            {
              "$type": "AttachToNormalizedTimeMixin",
              "stateID": "CrimsonPlum_ExtraAttack",
              "modifierName": "CrimsonPlum_ExtraAttack_Invisible"
            },
            {
              "$type": "AttachToNormalizedTimeMixin",
              "stateID": "CrimsonPlum_ExtraAttack_AS",
              "modifierName": "CrimsonPlum_ExtraAttack_HideWeapon",
              "normalizeStart": 0.0,
              "normalizeEnd": 0.428
            },
            {
              "$type": "ReplaceEventPatternMixin",
