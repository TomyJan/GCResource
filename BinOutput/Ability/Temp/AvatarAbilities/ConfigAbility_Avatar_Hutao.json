                "shakeTime": 0.36,
                "shakeDistance": 30,
                "extension": {
                  "baseCycle": 0.08,
                  "cycleDamping": -0.04
                }
              }
            }
          }
        },
        {
          "$type": "ApplyModifier",
          "modifierName": "UNIQUE_Hutao_CrimsonPlum_Main_Handler",
          "target": "Self"
        },
        {
          "$type": "FireEffect",
          "effectPattern": "Eff_Avatar_Girl_Pole_Hutao_CrimsonPlum_Burst"
        },
        {
          "$type": "FireEffect",
          "effectPattern": "Eff_Avatar_Girl_Pole_Hutao_CrimsonPlum_Word"
        },
        {
          "$type": "LoseHP",
          "target": "Self",
          "amountByTargetCurrentHPRatio": "%CrimsonPlum_LifeCostRatio",
          "lethal": false
        },
        {
          "$type": "AvatarSkillStart",
          "skillID": 10462,
          "cdRatio": 1
