                "elementDurability": 0,
                "enBreak": 120
              },
              "hitPattern": {
                "onHitEffectName": "default",
                "hitImpulseType": "LV3",
                "retreatType": "ByHitDirection",
                "reboundRatio": 0,
                "hitHaltTime": 0.0,
                "canBeDefenceHalt": false,
                "hitHaltTimeScale": 0.05,
