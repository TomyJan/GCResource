      "onAdded": [
        //这里先remove一下这个modifier，确保后续能上的上来
        {
          "$type": "RemoveModifier",
          "modifierName": "UNIQUE_Activity_Sumo_Fever_Handler_Team",
          "target": "Team"
        },

