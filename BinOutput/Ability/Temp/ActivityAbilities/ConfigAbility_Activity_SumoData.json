              "$type": "AttachModifierToSelfGlobalValueMixin",
              "globalValueKey": "AVATAR_IN",
              "valueSteps": [0.5, 1.5],
              "removeAppliedModifier": true,
              "modifierNameSteps": ["UNIQUE_Avatar_In_Refresh_Buff"]
            }
          ]
        },

