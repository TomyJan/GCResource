  //烧符【水】
  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "Activity_RogueDungeon_Skill_Spell_Water",
      "isDynamicAbility": true,
      "abilitySpecials": {
        "BuffTime_Base": 5.5, //初始持续时间
        "HealHpOnce_Base": 4000, //初始治疗量
        "HealHpOnce_Base_Loop": 800, //初始循环治疗量
        "ReviveStamina_Base_Loop": 20, //初始循环恢复体力值

        //SR水烧符生效时间提升2秒
        "SR_Spell_Water_LifeTime": 2, //R提升持续时长
        "SR_Spell_Water_LifeTime_Stacking": 0,

        //R水烧符治疗量提升15%
        "R_Spell_Water_HealHpOnce_HealUp": 0.15, //R治疗量提升
        "R_Spell_Water_HealHpOnce_HealUpStacking": 0,

        //SR水烧符治疗量提升80%
        "SR_Spell_Water_HealHpOnce_HealUp": 0.8,
        "SR_Spell_Water_HealHpOnce_HealUpStacking": 0,

        //SSR 水烧符体力恢复提升，延长持续时间5秒
        "SSR_Spell_Water_LifeTime": 5, //R提升持续时长
        "SSR_Spell_Water_ReviveStamina": 10,
        "SSR_Spell_Water_Stacking": 0
      },
      "onAdded": [],
      "onAbilityStart": [
        //获取牌子信息情况
        {
          "$type": "SetGlobalValueToOverrideMap",
          "target": "Team",
          "globalValueKey": "SGV_Spell_Water_DurationUp_SR",
          "overrideMapKey": "SR_Spell_Water_LifeTime_Stacking"
        },
        {
          "$type": "SetGlobalValueToOverrideMap",
          "target": "Team",
          "globalValueKey": "SGV_Spell_Water_HealUp_R",
          "overrideMapKey": "R_Spell_Water_HealHpOnce_HealUpStacking"
        },
        {
          "$type": "SetGlobalValueToOverrideMap",
          "target": "Team",
          "globalValueKey": "SGV_Spell_Water_HealUp_SR",
          "overrideMapKey": "SR_Spell_Water_HealHpOnce_HealUpStacking"
        },
        {
          "$type": "SetGlobalValueToOverrideMap",
          "target": "Team",
          "globalValueKey": "SGV_Spell_Water_Up_SSR",
          "overrideMapKey": "SSR_Spell_Water_Stacking"
        },
        //控制式神放技能
        {
          "$type": "SendEffectTrigger",
          "parameter": "Skill02",
          "effectPattern": "Eff_Activity_Shikishogun"
        },
        //等动作表现完之后再播特效
        {
          "$type": "ApplyModifier",
          "modifierName": "RogueDungeon_Skill_Spell_Water_Delay",
          "target": "Team"
        }
      ],
      "modifiers": {
        "RogueDungeon_Skill_Spell_Water_Delay": {
          "duration": 0.8,
          "onRemoved": [
            //当前角色播特效
            {
              "$type": "FireEffect",
              "effectPattern": "Eff_Activity_Roguelike_Buff_Water",
              "target": "CurLocalAvatar"
            },
            //前后台角色都回复血量
            {
              "$type": "ApplyModifier",
              "modifierName": "RogueDungeon_Skill_Spell_Water",
              "target": "CurTeamAvatars"
            },
            //所有角色在接下来的X秒内回复血量
            {
              "$type": "ApplyModifier",
              "modifierName": "RogueDungeon_Skill_Spell_Water_Loop_Avatar_Heal",
              "target": "CurTeamAvatars"
            },
            //接下来的X秒内回复体力
            {
              "$type": "ApplyModifier",
              "modifierName": "RogueDungeon_Skill_Spell_Water_Loop_Avatar_Stamina",
              "target": "Team"
            }
          ]
        },
        "RogueDungeon_Skill_Spell_Water": {
          "duration": 0.1,
          "onAdded": [
            {
              "$type": "HealHP",
              "doOffStage": true,
              "target": "Self",
              "amount": "%HealHpOnce_Base+%HealHpOnce_Base*%R_Spell_Water_HealHpOnce_HealUp*%R_Spell_Water_HealHpOnce_HealUpStacking+%HealHpOnce_Base*%SR_Spell_Water_HealHpOnce_HealUp*%SR_Spell_Water_HealHpOnce_HealUpStacking"
            }
          ]
        },
        "RogueDungeon_Skill_Spell_Water_Loop_Avatar_Stamina": {
          "duration": "%BuffTime_Base+%SR_Spell_Water_LifeTime*%SR_Spell_Water_LifeTime_Stacking+%SSR_Spell_Water_LifeTime*%SSR_Spell_Water_Stacking",
          "stacking": "Refresh",
          "thinkInterval": 1,
          "onThinkInterval": [
            {
              "$type": "ApplyModifier",
              "modifierName": "RogueDungeon_Skill_Spell_Water_Loop_Avatar_ReviveStamina",
              "target": "CurLocalAvatar"
            }
          ]
        },
        "RogueDungeon_Skill_Spell_Water_Loop_Avatar_Heal": {
          "duration": "%BuffTime_Base+%SR_Spell_Water_LifeTime*%SR_Spell_Water_LifeTime_Stacking+%SSR_Spell_Water_LifeTime*%SSR_Spell_Water_Stacking",
          "stacking": "Refresh",
          "thinkInterval": 1,
          "onThinkInterval": [
            {
              "$type": "HealHP",
              "doOffStage": true,
              "target": "Self",
              "amount": "%HealHpOnce_Base_Loop+%HealHpOnce_Base_Loop*%R_Spell_Water_HealHpOnce_HealUp*%R_Spell_Water_HealHpOnce_HealUpStacking+%HealHpOnce_Base_Loop*%SR_Spell_Water_HealHpOnce_HealUp*%SR_Spell_Water_HealHpOnce_HealUpStacking"
