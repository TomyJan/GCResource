  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "Activity_ChannellerSlab_DangerArea",
      "onAdded": [
        {
          "$type": "ApplyModifier",
          "modifierName": "Activity_ChannellerSlab_DangerArea_CoolDown_Recover"
        }
      ],
      "abilityMixins": [
        //自身逻辑状态维护
        {
          "$type": "AttachModifierToSelfGlobalValueMixin",
          "globalValueKey": "_DANGERAREA_StateID",
          "defaultGlobalValueOnCreate": -1,
          "valueSteps": [-0.5, 0.5, 1.5],
          "removeAppliedModifier": true,
          "modifierNameSteps": [
            "Activity_ChannellerSlab_DangerArea_Active",
            "Activity_ChannellerSlab_DangerArea_CoolDown"
          ]
        },
        //范围圈特效状态维护，因为需要在CoolDown的时候就开始有创生表现
        {
          "$type": "AttachModifierToSelfGlobalValueMixin",
          "globalValueKey": "_DANGERAREA_ActiveEffect",
          "defaultGlobalValueOnCreate": 0,
          "valueSteps": [0.5, 1.5],
          "removeAppliedModifier": true,
          "modifierNameSteps": [
            "Activity_ChannellerSlab_DangerArea_ActiveEffect"
          ]
        },
        //局域内玩家数统计
        {
          "$type": "FieldEntityCountChangeMixin",
          "campTargetType": "Enemy",
          "targetPredicates": [
            {
              "$type": "ByEntityTypes",
              "entityTypes": ["Avatar"]
            }
          ],
          "onFieldEnter": [
            {
              "$type": "AddGlobalValue",
              "key": "_DANGERAREA_Avatar_Count",
              "value": 1,
              "target": "Self"
            }
          ],
          "onFieldExit": [
            {
              "$type": "AddGlobalValue",
              "key": "_DANGERAREA_Avatar_Count",
              "value": -1,
              "target": "Self"
            }
          ]
        }
      ],
      "modifiers": {
        "Activity_ChannellerSlab_DangerArea_Active": {
          // "modifierMixins":
          // [
          //     //根据区域内是否有人判断是否要切状态
          //     {
          //         "$type": "AttachModifierToSelfGlobalValueMixin",
          //         "globalValueKey": "_DANGERAREA_Avatar_Count",
          //         "valueSteps" : [-0.5,0.5, 99],
          //         "removeAppliedModifier":true,
          //         "modifierNameSteps" : ["Blank", "Activity_ChannellerSlab_CountDown"],
          //     },
          // ],
          "thinkInterval": 0.2,
          "onThinkInterval": [
            {
              "$type": "AttachModifier",
              "modifierName": "Activity_ChannellerSlab_CountDown",
              "predicates": [
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "_DANGERAREA_Avatar_Count",
                  "value": 0.5,
                  "compareType": "MoreThan"
                }
              ]
            },
            {
              "$type": "RemoveModifier",
              "modifierName": "Activity_ChannellerSlab_CountDown",
              "predicates": [
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "_DANGERAREA_Avatar_Count",
                  "value": 0.5,
                  "compareType": "LessAndEqual"
                }
              ]
            }
          ]
        },
        "Blank": {},
        "Activity_ChannellerSlab_CountDown": {
          "onAdded": [
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_Activity_ChannellerSlab_DangerArea_Alert",
              "predicates": [
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "_ACTIVIITY_ChannellerSlab_DangerArea_EnhancedTag",
                  "value": 0,
                  "compareType": "NoneOrEqual"
                }
              ]
            },
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_Activity_ChannellerSlab_DangerArea_Alert_Enhanced",
              "predicates": [
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "_ACTIVIITY_ChannellerSlab_DangerArea_EnhancedTag",
                  "value": 1,
                  "compareType": "Equal"
                }
              ]
            }
          ],
          "thinkInterval": 2,
          "onThinkIntervalIsFixedUpdate": true,
          "thinkIntervalIgnoreTimeScale": true,
          "onThinkInterval": [
            {
              "$type": "CreateGadget",
              "gadgetID": 49001018,
              "campID": 4001,
              "lifeByOwnerIsAlive": true,
              "born": {
                "$type": "ConfigBornBySelf",
                "offset": {
                  "x": 0.0,
                  "y": 0.0,
                  "z": 0.0
                },
                "direction": {
                  "$type": "ConfigDirectionByWorld",
                  "eularAngles": {
                    "x": 0.1,
                    "y": 0.1,
                    "z": 0.1
                  }
                },
                "onGround": true,
                "alongGround": true
              },
              "predicates": [
                {
                  "$type": "ByTargetHPRatio",
                  "target": "Self",
                  "HPRatio": 0,
                  "logic": "Greater"
                }
              ]
            },
            {
              "$type": "SetGlobalValue",
              "key": "_DANGERAREA_ActiveEffect",
              "value": 0
            },
            {
              "$type": "SetGlobalValue",
              "key": "_DANGERAREA_StateID",
              "value": 1
            }
          ]
        },
        "Activity_ChannellerSlab_DangerArea_CoolDown": {
          "duration": 6.5,
          "onRemoved": [
            {
              "$type": "ApplyModifier",
              "modifierName": "Activity_ChannellerSlab_DangerArea_CoolDown_Recover"
            }
          ]
        },
        "Activity_ChannellerSlab_DangerArea_CoolDown_Recover": {
          "duration": 1.5,
          "onAdded": [
            {
              "$type": "SetGlobalValue",
              "key": "_DANGERAREA_ActiveEffect",
              "value": 1
            }
          ],
          "onRemoved": [
            {
              "$type": "SetGlobalValue",
              "key": "_DANGERAREA_StateID",
              "value": 0
            }
          ]
        },
        "Activity_ChannellerSlab_DangerArea_ActiveEffect": {
          "modifierMixins": [
            //是否强化特效不一样
            {
              "$type": "AttachModifierToSelfGlobalValueMixin",
              "globalValueKey": "_ACTIVIITY_ChannellerSlab_DangerArea_EnhancedTag",
              "valueSteps": [-0.5, 0.5, 1.5],
              "removeAppliedModifier": true,
              "modifierNameSteps": [
                "DangerArea_ActiveEffect_Normal",
                "DangerArea_ActiveEffect_Enhanced"
              ]
            }
          ]
        },
        "DangerArea_ActiveEffect_Normal": {
          "onAdded": [
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_Activity_ChannellerSlab_DangerArea_Active"
            }
          ]
        },
        "DangerArea_ActiveEffect_Enhanced": {
          "onAdded": [
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_Activity_ChannellerSlab_DangerArea_Active_Enhanced"
            }
          ]
        }
      },
      "abilitySpecials": {
        "RandomLatency": 0.1
      }
    }
