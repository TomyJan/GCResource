  //3.星晶之舞:角色获得岩元素反应产生的晶片时，会释放小型冲击波，基于该角色的攻击力的500%对周围的敌人造成伤害。
  //此外，触发该效果后的10秒内，队伍中所有角色获得40%岩元素伤害加成。
  //该效果每5秒至多触发一次，队伍中所有角色共享冷却。
  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "ActivityAbility_EnergyDisk_Purple_PickupTriggerAOE_V2",
      "isDynamicAbility": true,
      "onAdded": [
        //全队共计CD触发器
        {
          "$type": "ApplyModifier",
          "modifierName": "UNIQUE_EnergyDisk_Purple_PickupTriggerAOE_Total",
          "target": "MPLevel"
        },
        {
          "$type": "ApplyModifier",
          "modifierName": "UNIQUE_EnergyDisk_Purple_PickupTriggerAOE",
          "target": "Self"
        }
      ],
      "modifiers": {
        "UNIQUE_EnergyDisk_Purple_PickupTriggerAOE_Total": {
          "isUnique": true,
          "stacking": "Unique",
          "modifierMixins": [
            {
              "$type": "AttachModifierToSelfGlobalValueMixin",
              "globalValueKey": "_Ability_PickupTriggerAOE_CD",
              "globalValueTarget": "MPLevel",
              "valueSteps": [-1.5, -0.5, 0.5],
              "removeAppliedModifier": true,
              "modifierNameSteps": [
                "UNIQUE_EnergyDisk_PickupTriggerAOE_Cd",
                "Blank"
              ]
            }
          ]
        },
        "Blank": {},

        "UNIQUE_EnergyDisk_PickupTriggerAOE_CD_Change_MPLEVEL": {
          "isUnique": true,
          "duration": 0.1,
          "onAdded": [
            {
              "$type": "SetGlobalValue",
              "key": "_Ability_PickupTriggerAOE_CD",
              "value": -1,
              "target": "MPLevel"
            }
          ]
        },

        "UNIQUE_EnergyDisk_Purple_PickupTriggerAOE": {
          "isUnique": true,
          "modifierMixins": [
            //因为没有针对元素结晶的Predicate，只能在Mixin当中遍历结晶的元素类型
            {
              "$type": "DoActionByGainCrystalSeedMixin",
              "elementTypes": [
                "Fire",
                "Water",
                "Grass",
                "Electric",
                "Ice",
                "Wind",
                "Rock"
              ],
              "actions": [
                {
                  "$type": "Predicated",
                  "target": "MPLevel",
                  "targetPredicates": [
                    {
                      "$type": "ByTargetGlobalValue",
                      "key": "_Ability_PickupTriggerAOE_CD",
                      "value": 0,
                      "compareType": "Equal"
                    }
                  ],
                  "successActions": [
                    {
                      "$type": "ApplyModifier",
                      "modifierName": "UNIQUE_EnergyDisk_Purple_PickupTriggerAOE_Effect",
                      "target": "Self"
                    },
                    {
                      "$type": "ApplyModifier",
                      "modifierName": "UNIQUE_EnergyDisk_PickupTriggerAOE_CD_Change_MPLEVEL",
                      "target": "MPLevel"
                    }
                    //进入CD
                  ]
                }
              ]
            }
          ]
        },
        "UNIQUE_EnergyDisk_Purple_PickupTriggerAOE_Effect": {
          "isUnique": true,
          "duration": 0.1,
          "onAdded": [
            // {
            //   "$type": "DebugLog",
            //   "content": "进入拾取后效果逻辑！",
            // },
            {
              "$type": "FireEffect",
              "effectPattern": "Eff_EnergyDisk_ElementalBurst_Wave_R40",
              "target": "Self"
            }
          ],
          "onRemoved": [
            {
              "$type": "TriggerAttackEvent",
              "targetType": "Enemy",
              "attackEvent": {
                "attackPattern": {
                  "$type": "ConfigAttackSphere", //攻击球
                  "triggerType": "TriggerNoRepeat",
                  "radius": 4.0, //半径
                  "born": {
                    "$type": "ConfigBornBySelf"
                  }
                },
                "attackInfo": {
                  //攻击伤害
                  "attackProperty": {
                    "damagePercentage": "%DamageRate", //伤害百分比
                    "strikeType": "Slash", //打击类型——挥砍
                    //"overrideByWeapon": true, //是否根据武器转化元素
                    "attackType": "Melee", //攻击类型
                    "elementType": "None", //元素类型
                    "elementDurability": 0,
                    "enBreak": 25 //破霸体值
                  },
                  //击中效果
                  "hitPattern": {
                    "onHitEffectName": "default",
                    "hitImpulseType": "LV1",
                    "canBeDefenceHalt": false,
                    "hitHaltTime": 0,
                    "hitHaltTimeScale": 0.02,
                    "muteHitText": false
                  }
                }
              }
            },
            {
              "$type": "ApplyModifier",
              "modifierName": "UNIQUE_EnergyDisk_Purple_PickupTriggerAOE_BuffAvatarEffect",
              "target": "AllPlayerAvatars"
            }
          ]
        },
        //队友BUFF提升岩元素伤害
        "UNIQUE_EnergyDisk_Purple_PickupTriggerAOE_BuffAvatarEffect": {
          "isUnique": true,
          "stacking": "Refresh",
          "duration": "%Bufftime",
          "properties": {
            "Actor_RockAddHurtDelta": "%RockAddHurt"
          }
        },
        "UNIQUE_EnergyDisk_PickupTriggerAOE_Cd": {
          "isUnique": true,
          "thinkInterval": "%CDTime",
          "onThinkInterval": [
            {
              "$type": "SetGlobalValue",
              "key": "_Ability_PickupTriggerAOE_CD",
              "target": "MPLevel",
              "value": 0
            }
          ]
        }
      },
      "abilitySpecials": {
        "DamageRate": 0, //攻击倍率
        "CDTime": 0,
        "RockAddHurt": 0, //岩元素伤害加成
        "Bufftime": 0 //伤害加成时长
      }
    }
