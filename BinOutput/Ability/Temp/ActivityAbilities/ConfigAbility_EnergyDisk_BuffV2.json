    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "ActivityAbility_EnergyDisk_Purple_HealthyDamage_V2",
      "isDynamicAbility": true,
      "onAdded": [
        {
          "$type": "AttachModifier",
          "modifierName": "UNIQUE_EnergyDisk_Purple_HealthyDamage"
        },
        {
          "$type": "SetSystemValueToOverrideMap",
          "doOffStage": true,
          "key": "DamageSystemValue",
          "type": "ElementLevelCo"
        }
      ],
      "modifiers": {
        "UNIQUE_EnergyDisk_Purple_HealthyDamage": {
          "isUnique": true,
          "onAttackLanded": [
            {
              "$type": "Predicated",
              "target": "Other",
              "targetPredicates": [
                {
                  "$type": "ByEntityTypes",
                  "entityTypes": ["Monster"]
                },
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "_EnergyDisk_Purple_HealthyDamage_CD",
                  "target": "Team",
                  "value": 0,
                  "compareType": "NoneOrEqual"
                }
              ],
              "successActions": [
                {
                  "$type": "Predicated",
                  "target": "Other",
                  "targetPredicates": [
                    {
                      "$type": "ByTargetHPRatio",
                      "target": "Caster",
                      "HPRatio": 0.99,
                      "logic": "Lesser"
                    }
                  ],
                  "successActions": [
                    {
                      "$type": "AttachModifier",
                      "modifierName": "UNIQUE_EnergyDisk_Purple_HealthyDamage_AOE_LowHp",
                      "target": "Other"
                    },
                    {
                      "$type": "AttachModifier",
                      "modifierName": "UNIQUE_EnergyDisk_Purple_HealthyDamage_CDHandler",
                      "target": "Team"
                    }
                  ],
                  "failActions": [
                    {
                      "$type": "AttachModifier",
                      "modifierName": "UNIQUE_EnergyDisk_Purple_HealthyDamage_AOE_HighHp",
                      "target": "Other"
                    },
                    {
                      "$type": "AttachModifier",
                      "modifierName": "UNIQUE_EnergyDisk_Purple_HealthyDamage_CDHandler",
                      "target": "Team"
                    }
                  ]
                }
              ]
            }
          ]
        },

        "UNIQUE_EnergyDisk_Purple_HealthyDamage_AOE_HighHp": {
          "isUnique": true,
          "duration": 0.1,
          "onAdded": [
            {
              "$type": "FireEffect",
              "effectPattern": "Eff_EnergyDisk_ElementalBurst_Wave_R40",
              "target": "Self"
            },
            {
              "$type": "TriggerAttackEvent",
              "target": "Caster",
              "targetType": "Enemy",
              "attackEvent": {
                "attackPattern": {
                  "$type": "ConfigAttackSphere", //攻击球
                  "triggerType": "TriggerNoRepeat",
                  "radius": 4.0, //半径
                  "born": {
                    "$type": "ConfigBornBySelf"
                  }
                },
                "attackInfo": {
                  "canBeModifiedBy": "OriginOwner",
                  "attackProperty": {
                    "damageExtra": "%DamageHigh*%DamageSystemValue",
                    "ignoreAttackerProperty": true,
                    "ignoreModifyDamage": true,
                    "trueDamage": true,
                    "attackType": "Melee",
                    "elementType": "None",
                    "elementDurability": 0,
                    "enBreak": 0
                  },
                  //击中效果
                  "hitPattern": {
                    "onHitEffectName": "default",
                    "hitImpulseType": "LV1",
                    "reboundRatio": 0,
                    "canBeDefenceHalt": false,
                    "hitHaltTime": 0,
                    "hitHaltTimeScale": 0.02,
                    "muteHitText": false
                  }
                }
              }
            }
          ]
        },

        "UNIQUE_EnergyDisk_Purple_HealthyDamage_AOE_LowHp": {
          "isUnique": true,
          "duration": 0.1,
          "onAdded": [
            {
              "$type": "FireEffect",
              "effectPattern": "Eff_EnergyDisk_ElementalBurst_Wave_R40",
              "target": "Self"
            },
            {
              "$type": "TriggerAttackEvent",
              "target": "Caster",
              "targetType": "Enemy",
              "attackEvent": {
                "attackPattern": {
                  "$type": "ConfigAttackSphere", //攻击球
                  "triggerType": "TriggerNoRepeat",
                  "radius": 4.0, //半径
                  "born": {
                    "$type": "ConfigBornBySelf"
                  }
                },
                "attackInfo": {
                  "canBeModifiedBy": "OriginOwner",
                  "attackProperty": {
                    "damageExtra": "%DamageLow*%DamageSystemValue",
                    "ignoreAttackerProperty": true,
                    "ignoreModifyDamage": true,
                    "trueDamage": true,
                    "attackType": "Melee",
                    "elementType": "None",
                    "elementDurability": 0,
                    "enBreak": 0
                  },
                  //击中效果
                  "hitPattern": {
                    "onHitEffectName": "default",
                    "hitImpulseType": "LV1",
                    "reboundRatio": 0,
                    "canBeDefenceHalt": false,
                    "hitHaltTime": 0,
                    "hitHaltTimeScale": 0.02,
                    "muteHitText": false
                  }
                }
              }
            }
          ]
