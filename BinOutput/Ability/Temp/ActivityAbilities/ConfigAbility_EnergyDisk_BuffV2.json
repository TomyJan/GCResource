        "UNIQUE_EnergyDisk_Purple_HealthyDamage_CDHandler": {
          "isUnique": true,
          "duration": "%CDTime",
          "onAdded": [
            {
              "$type": "SetGlobalValue",
              "key": "_EnergyDisk_Purple_HealthyDamage_CD",
              "target": "Team",
              "value": -1
