      },
      "abilitySpecials": {
        "DamageLow": 0,
        "DamageHigh": 0,
        "DamageSystemValue": 0,
        "CDTime": 0
      }
    }
  },

  //2.剑刃之舞:角色拥有5层「剑舞」效果，每层剑舞效果会使角色的普通攻击造成的伤害提高15%。
  //拥有5层效果时，普通攻击速度提高15%。角色受到伤害时，会失去1层剑舞，每秒最多因此失去1层剑舞。
  //角色在场上时，每10秒会获得一层剑舞，直到拥有5层剑舞为止。每个角色单独计算剑舞效果的层数与获得间隔时间。
  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "ActivityAbility_EnergyDisk_Purple_Berserker_V2",
      "isDynamicAbility": true,
      "onAdded": [
        {
          "$type": "AttachModifier",
          "modifierName": "UNIQUE_EnergyDisk_Purple_Berserker"
        },
        //随时间恢复层数
        {
          "$type": "AttachModifier",
          "modifierName": "UNIQUE_EnergyDisk_Purple_Berserker_RecoveryLayers"
        }
      ],
      "abilityMixins": [
        //iscombat再触发
      ],
      "modifiers": {
        "UNIQUE_EnergyDisk_Purple_Berserker_RecoveryLayers": {
          "isUnique": true,
          "thinkInterval": "%ReviveTime",
          "onThinkInterval": [
            {
              "$type": "AddGlobalValue",
              "key": "_ABILITY_EnergyDisk_Purple_AttackBuffLevel",
              "value": 1,
              "useLimitRange": true,
              "maxValue": 5
            }
          ]
        },

        "UNIQUE_EnergyDisk_Purple_Berserker": {
          "isUnique": true,
          "modifierMixins": [
            {
              "$type": "AttachModifierToSelfGlobalValueMixin",
              "globalValueKey": "_ABILITY_EnergyDisk_Purple_AttackBuffLevel",
              "defaultGlobalValueOnCreate": 5,
              "valueSteps": [-1.5, -0.5, 0.5, 1.5, 2.5, 3.5, 4.5, 5.5],
              "removeAppliedModifier": true,
              "modifierNameSteps": [
                "UNIQUE_EnergyDisk_Purple_Berserker_ResetTimer",
                "UNIQUE_EnergyDisk_Purple_BerserkerBuff0",
                "UNIQUE_EnergyDisk_Purple_BerserkerBuff1",
                "UNIQUE_EnergyDisk_Purple_BerserkerBuff2",
                "UNIQUE_EnergyDisk_Purple_BerserkerBuff3",
                "UNIQUE_EnergyDisk_Purple_BerserkerBuff4",
                "UNIQUE_EnergyDisk_Purple_BerserkerBuff5"
              ]
            }
          ],
          "onAdded": [
            {
              "$type": "AttachModifier",
              "modifierName": "UNIQUE_EnergyDisk_Purple_Berserker_Handler",
              "target": "Self"
            }
          ]
        },

        "UNIQUE_EnergyDisk_Purple_Berserker_Handler": {
          "isUnique": true,
          "onBeingHit": [
            {
              "$type": "ApplyModifier",
              "modifierName": "UNIQUE_EnergyDisk_Purple_Berserker_ResetCD",
              "target": "Self",
              "predicates": [
                {
                  "$type": "ByHitDamage",
                  "damage": 0,
                  "logic": "Greater"
                }
              ]
            }
          ]
        },
        "UNIQUE_EnergyDisk_Purple_Berserker_ResetCD": {
          "duration": 1, //1秒内不会掉层
          "isUnique": true,
          "stacking": "Unique",
          "onAdded": [
            {
              "$type": "AddGlobalValue",
              "key": "_ABILITY_EnergyDisk_Purple_AttackBuffLevel",
              "value": -1,
              "useLimitRange": true,
              "maxValue": 5,
              "minValue": -1 //需要保证一直受击的话永远不会开始计时。
            },
            {
              "$type": "DebugLog",
              "content": "受击触发globalvalue变更"
            }
          ]
        },
        "UNIQUE_EnergyDisk_Purple_Berserker_ResetTimer": {
          "isUnique": true,
          "thinkInterval": 0.3,
          "onThinkInterval": [
            /*
          {
            "$type":"AddGlobalValue",
            "key":"_ABILITY_EnergyDisk_Purple_AttackBuffLevel",
            "value":1,
            "useLimitRange":true,
            "maxValue":5,
          },*/
          ]
        },

        "UNIQUE_EnergyDisk_Purple_BerserkerBuff0": {
          "isUnique": true,
          "onAdded": [
            {
              "$type": "SetGlobalValue",
              "key": "_ABILITY_EnergyDisk_Purple_AttackBuffLevel",
              "value": 0
            }
          ]
        },
        "UNIQUE_EnergyDisk_Purple_BerserkerBuff1": {
          "isUnique": true,
          "onAdded": [
            {
              "$type": "FireEffect",
              "effectPattern": "Eff_EnergyDisk_ExtraAtkKill01", //特效替换
