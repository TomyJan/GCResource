  //4.交织之舞：队伍中任意角色施放元素爆发后，将产生「元素圆舞」效果，使队伍中所有角色的元素精通提高100点。
  //该效果持续20秒，至多叠加3次。叠加3次后在持续期间内，若再次产生该效果，将释放冲击波，造成范围伤害。
  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "ActivityAbility_EnergyDisk_Purple_ElementBurst_Count3Wave_V2",
      "isDynamicAbility": true,
      "onAdded": [
        {
          "$type": "ApplyModifier",
          "modifierName": "UNIQUE_EnergyDisk_Purple_ElementBurst_Count3Wave_MPLevel_Init",
          "target": "MPLevel"
        },
        //BUFF层数记在MPLevel上
        {
          "$type": "SetSystemValueToOverrideMap", //设置一些系统参数到OverrideMap里
          "key": "LevelEntity_Count3Wave_SystemValue",
          "type": "ElementLevelCo" //SystemValue类型
        },
        {
          "$type": "ApplyModifier",
          "modifierName": "UNIQUE_Purple_ElementBurst_Count3Wave_Handler"
        }
      ],
      "modifiers": {
        "UNIQUE_EnergyDisk_Purple_ElementBurst_Count3Wave_MPLevel_Init": {
          "isUnique": true,
          "onAdded": [
            {
              "$type": "SetGlobalValue",
              "target": "MPLevel",
              "key": "_ABILITY_ElementBurst_Layer",
              "value": 0
            }
          ]
        },
        "UNIQUE_Purple_ElementBurst_Count3Wave_Handler": {
          "isUnique": true,
          "modifierMixins": [
            {
              "$type": "OnAvatarUseSkillMixin",
              "clearSkillIdDelay": 0.2,
              "onTriggerUltimateSkill": [
                {
                  "$type": "ApplyModifier",
                  "modifierName": "UNIQUE_EnergyDisk_ElementBurst_AddLayer",
                  "target": "MPLevel"
                },

                {
                  "$type": "ApplyModifier",
                  "modifierName": "UNIQUE_EnergyDisk_ElementBurst_Layer_Reset_MPLEVEL",
                  "target": "MPLevel"
                },
                {
                  "$type": "Predicated",
                  "target": "MPLevel",
                  "targetPredicates": [
                    {
                      "$type": "ByTargetGlobalValue",
                      "key": "_ABILITY_ElementBurst_Layer",
                      "value": 4,
                      "compareType": "MoreThanAndEqual"
                    }
                  ],
                  "successActions": [
                    //满层触发效果
                    {
                      "$type": "ApplyModifier",
                      "modifierName": "UNIQUE_ElementBurst_Count3Wave_3_Extra_Effect",
                      "target": "Self"
                    }
                  ]
                }
              ]
            },

            {
              "$type": "AttachModifierToSelfGlobalValueMixin",
              "globalValueKey": "_ABILITY_ElementBurst_Layer",
              "globalValueTarget": "MPLevel",
              "valueSteps": [-0.5, 0.5, 1.5, 2.5, 100],
              "removeAppliedModifier": true, //这个置为false好像不管用
              "modifierNameSteps": [
                "UNIQUE_ElementBurst_Count3Wave_0",
                "UNIQUE_UNIQUE_ElementBurst_Count3Wave_1_Effect",
                "UNIQUE_UNIQUE_ElementBurst_Count3Wave_2_Effect",
                "UNIQUE_UNIQUE_ElementBurst_Count3Wave_3_Effect"
              ]
              //统一管理，由layer层数决定buff
            }
          ]
        },

        "UNIQUE_ElementBurst_Count3Wave_0": {
          "isUnique": true
