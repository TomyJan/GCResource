  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "ActivityAbility_HideSeek_Ghost_Bait_V3",
      "isDynamicAbility": true,
      "onAdded": [
        {
          "$type": "SetGlobalValue",
          "doOffStage": true,
          "target": "Team",
          "key": "PlaceBait_Forbid",
          "value": 0,
          "useLimitRange": true,
          "minValue": 0,
          "maxValue": 100
        }
      ],
      "onAbilityStart": [
        {
          "$type": "SetGlobalValue",
          "key": "HideAndSeek_Is_Bait",
          "value": 0,
          "target": "Team",
          "doOffStage": true
        },
        {
          "$type": "Predicated",
          "otherTargets": {
            "$type": "SelectTargetsByShape",
            "shapeName": "CircleR3H5",
            "centerBasedOn": "Self",
            "campTargetType": "All",
            "campBasedOn": "Self"
          },
          "targetPredicates": [
            {
              "$type": "ByEntityTypes",
              "entityTypes": ["Gadget", "Field", "Bullet"]
            },
            {
              "$type": "ByTargetGlobalValue",
              "key": "HideAndSeek_Seeker_Bait_Gadget",
              "value": 1,
              "compareType": "Equal"
            }
          ],
          "successActions": [
            {
              "$type": "SetGlobalValue",
              "key": "HideAndSeek_Is_Bait",
              "value": 1,
              "target": "Team",
              "doOffStage": true
            }
          ],
          "failActions": []
        },
        {
          "$type": "Predicated",
          "target": "Team",
          "targetPredicates": [
            {
              "$type": "ByTargetGlobalValue",
              "key": "HideAndSeek_Is_Bait",
              "value": 1,
              "compareType": "Equal"
            }
          ],
          "successActions": [
            {
              "$type": "DebugLog",
              "content": "不可以放技能啥也不做"
            },
            {
              "$type": "ApplyModifier",
              "modifierName": "UNIQUE_Eff_Activity_HideSeek_Bait_Disappear_V3",
              "target": "Self"
            }
          ],
          "failActions": [
            {
              "$type": "Predicated",
              "target": "Caster",
              "targetPredicates": [
                {
                  //【三期新增】只有幽灵状态下才能放技能
                  "$type": "ByTargetGlobalValue",
                  "key": "SGV_HideAndSeek_PlayerState_Play",
                  "value": 2,
                  "compareType": "Equal",
                  "target": "Team"
                }
              ],
              "successActions": [
                {
                  "$type": "AvatarSkillStart",
                  "skillID": 20522,
                  "cdRatio": 1
                },
                {
                  "$type": "DebugLog",
                  "content": "SCL:HideAndSeek_Skill_PlaceBait!!!!!!!!!!!"
                },
                {
                  "$type": "ServerLuaCall",
                  "luaCallType": "AbilityGroupSourceGroup",
                  "funcName": "SLC_HideAndSeek_Skill_PlaceBait"
                  //V3替换标记   HideAndSeek_Skill_PlaceBait
                },
                {
                  "$type": "SetGlobalValue",
                  "key": "HideAndSeek_Is_Bait",
                  "value": 0,
                  "target": "Team",
                  "doOffStage": true
                }
              ]
            }
          ]
        }
      ],
      "modifiers": {
        "UNIQUE_Eff_Activity_HideSeek_Bait_Disappear_V3": {
          "duration": 1,
          "isUnique": true,
          "onAdded": [
            {
              "$type": "FireEffect",
              "effectPattern": "Eff_Activity_HideSeek_Bait_Disappear",
              "muteRemoteAction": true,
              "born": {
                "$type": "ConfigBornBySelf",
                "offset": {
                  "x": 0.0,
                  "y": 0.0,
                  "z": 1.0
                }
              }
            }
          ]
        }
      }
    }
  },

