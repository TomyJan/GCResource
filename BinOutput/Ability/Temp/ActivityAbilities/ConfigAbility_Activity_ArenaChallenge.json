    //1、对冻结目标造成伤害提升{1}%
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "ActivityAbility_ArenaChallenge_HitFrozenTarget_DamageUp",
      "isDynamicAbility": true,
      "onAdded": [
        {
          "$type": "AttachModifier",
          "target": "Self",
          "modifierName": "UNIQUE_Activity_ArenaChallenge_HitFrozenTarget_DamageUp"
        }
      ],
      "modifiers": {
        "UNIQUE_Activity_ArenaChallenge_HitFrozenTarget_DamageUp": {
          "isUnique": true,
          "stacking": "Unique",
          "isLimitedProperties": true,
          "modifierMixins": [
            {
              "$type": "ModifyDamageMixin",
              "ignoreEventInfo": true, //不筛选攻击类型，所有攻击类型攻击力都获得加成
              "damagePercentageRatio": "%DamageUpRate_Frozen",
              "predicates": [
                {
                  "$type": "ByHasAbilityState",
                  "target": "Target",
                  "abilityState": "ElementFrozen"
                }
              ]
            }
          ]
        }
      },
      "abilitySpecials": {
        "DamageUpRate_Frozen": 0
      }
    }
  },
  {
    //2、触发冻结反应时使敌人防御力降低{1}%，持续{2}秒
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "ActivityAbility_ArenaChallenge_FrozenReact_DefenceDown",
      "isDynamicAbility": true,
      "abilityMixins": [
        {
          "$type": "AttachModifierToGlobalValueMixin",
          "globalValueTarget": "Team",
          "globalValueKey": "SGV_CHANGE_STAGE",
          "valueSteps": [0.5, 1.5],
          "modifierNameSteps": [
            "UNIQUE_Activity_ArenaChallenge_FrozenReact_DefenceDown_Decision_FrozenReact"
          ]
        }
      ],
      "modifiers": {
        "UNIQUE_Activity_ArenaChallenge_FrozenReact_DefenceDown_Decision_FrozenReact": {
          "isUnique": true,
          "stacking": "Unique",
          "modifierMixins": [
            {
              "$type": "DoActionByElementReactionMixin",
              "entityTypes": ["Monster"],
              "reactionTypes": ["Freeze"],
              "range": 40,
              "actions": [
                {
                  "$type": "ApplyModifier",
                  "target": "Other",
                  "doOffStage": true,
                  "doAfterDie": true,
                  "modifierName": "UNIQUE_Activity_ArenaChallenge_FrozenReact_DefenceDown"
                }
              ]
            }
          ]
        },
        "UNIQUE_Activity_ArenaChallenge_FrozenReact_DefenceDown": {
          "isUnique": true,
          "stacking": "Unique",
          "duration": "%FrozenDuration",
          "state": "DefenseDown",
          "isLimitedProperties": true,
          "properties": {
            "Actor_DefenceRatio": "-1*%FrozenDefenceDownRate"
          }
        }
      },
      "abilitySpecials": {
        "FrozenDefenceDownRate": 0,
        "FrozenDuration": 0
      }
    }
  },
  {
    //3、触发x次冻结反应
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "ActivityAbility_ArenaChallenge_TriggerReact_Freeze",
      "isDynamicAbility": true,
      "onAdded": [
        {
          "$type": "AttachModifier",
          "target": "Self",
          "modifierName": "UNIQUE_Activity_ArenaChallenge_TriggerReact_Freeze"
        }
      ],
      "modifiers": {
        "UNIQUE_Activity_ArenaChallenge_TriggerReact_Freeze": {
          "isUnique": true,
          "stacking": "Unique",
          "modifierMixins": [
            {
              "$type": "DoActionByElementReactionMixin",
              "range": 40,
              "entityTypes": ["Monster"],
              "reactionTypes": ["Freeze"], //判断触发元素反应
              "actions": [
                {
                  "$type": "ServerLuaCall",
                  "doOffStage": true,
                  "luaCallType": "CurGalleryControlGroup",
                  "funcName": "SLC_State_Condition_Complete",
                  "predicates": [
                    {
                      "$type": "ByTargetGlobalValue",
                      "target": "Team",
                      "key": "Freeze_CDState",
                      "value": 0,
                      "compareType": "NoneOrEqual"
                    }
                  ]
                },
                {
                  "$type": "ApplyModifier",
                  "doOffStage": true,
                  "target": "Team",
                  "modifierName": "UNIQUE_Activity_ArenaChallenge_TriggerReact_Freeze_CD"
                }
              ]
            }
          ]
        },
        "UNIQUE_Activity_ArenaChallenge_TriggerReact_Freeze_CD": {
          "isUnique": true,
          "stacking": "Unique",
          "duration": "%Freeze_CD",
          "onAdded": [
            {
              "$type": "SetGlobalValue",
              "target": "Team",
              "key": "Freeze_CDState",
              "value": -1
            }
          ],
          "onRemoved": [
            {
              "$type": "SetGlobalValue",
              "target": "Team",
              "key": "Freeze_CDState",
              "value": 0
            }
          ]
        }
      },
      "abilitySpecials": {
        "Freeze_CD": 0
      }
    }
  }
]
