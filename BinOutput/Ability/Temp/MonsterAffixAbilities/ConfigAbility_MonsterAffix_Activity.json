  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "MonsterAffix_OverdoseEnhanced_ForHunting", // Affix ID - 4068
      "isDynamicAbility": true,
      // 每次对怪物触发激化反应时，引发落雷劈怪物一次（只打单体），造成一次雷元素伤害（伤害系数①，元素含量②，内置cd③）
      // 同时降低怪物的雷抗和草抗（抗性降低④），可叠层（有叠层上限，  最多叠⑤层，每层独立计时）
      "abilitySpecials": {
        // %Value_DamageRate*%Value_DamageSystemValue
        "vulnerable_duration": 10, // 减抗持续时间
        "sub_hurt_delta": -0.1, // 减抗量
        "max_stacking": 5, // 最高减抗层数
        "damage_ratio": 6.5, // 伤害系数
        "element_level_co": 0, // 环境伤害倍率 (Ability onAdded时覆盖)
        "element_durability": 25, // 元素含量
        "strike_cooldown": 1.5, // 雷击内置CD时长
        "detection_interval": 5, // 玩家检测周期
        "player_modifier_duration": 6 // 玩家身上的元素反应主控Modifier的持续时间 比检测周期稍长一点
      },
      "onAdded": [
        {
          "$type": "AttachModifier",
          "modifierName": "HuntingBuff_OverdoseEnhanced_MonsterHandler"
        },
        {
          "$type": "SetSystemValueToOverrideMap",
          "type": "ElementLevelCo",
          "key": "element_level_co"
        }
      ],
      "modifiers": {
        // 只有被赋予这个Ability的怪物才能被触发这个反应的额外效果 (依靠GV判断)
        // Tick附近的玩家，上玩家可用的Mixin操作
        "HuntingBuff_OverdoseEnhanced_MonsterHandler": {
          "thinkInterval": "%detection_interval",
          "onThinkInterval": [
            {
              "$type": "ApplyModifier",
              "modifierName": "HuntingBuff_OverdoseEnhanced_PlayerHandler",
              "target": "AllPlayerAvatars"
            }
          ],
          "onAdded": [
            {
              "$type": "ApplyModifier",
              "modifierName": "HuntingBuff_OverdoseEnhanced_PlayerHandler",
              "target": "AllPlayerAvatars"
            },
            // 判断条件  只有这个Key为1的时候才能进行雷击
            {
              "$type": "SetGlobalValue",
              "key": "_OVERDOSEENHANCED_FORHUNTING_CAN_BE_STRUCK_",
              "value": 1
            }
          ]
        },
        // 玩家身上的反应相关控制
        "HuntingBuff_OverdoseEnhanced_PlayerHandler": {
          "stacking": "Refresh",
          "duration": "%player_modifier_duration",
          "modifierMixins": [
            {
              "$type": "DoActionByElementReactionMixin",
              "reactionTypes": [
                "Overdose",
                "OverdoseElectric",
                "OverdoseGrass"
              ],
              "entityTypes": ["Monster"],
              "range": 20,
              "actions": [
                {
                  "$type": "Predicated",
                  "target": "Other",
                  "predicates": [
                    // 判断条件  只有这个Key为1的时候才能进行雷击
                    {
                      "$type": "ByTargetGlobalValue",
                      "key": "_OVERDOSEENHANCED_FORHUNTING_CAN_BE_STRUCK_",
                      "compareType": "Equal",
                      "value": 1
                    }
                  ],
                  "successActions": [
                    {
                      "$type": "ApplyModifier",
                      "modifierName": "Thunder_Strike_From_HuntingBuff_OverdoseEnhanced",
                      "target": "Other"
                    }
                  ]
                }
              ]
            }
          ]
        },
        // 雷击效果Modifier
        "Thunder_Strike_From_HuntingBuff_OverdoseEnhanced": {
          "stacking": "Unique",
          "duration": 0.1,
          "onAdded": [
            // 雷击特效
            {
              "$type": "FireEffect",
              "effectPattern": "Eff_HuntingEvent_ThunderSrike"
            },
            {
              "$type": "DamageByAttackValue",
              "target": "Self",
              "attacker": "Caster",
              "attackInfo": {
                "attackProperty": {
                  "damageExtra": "%damage_ratio*%element_level_co",
                  "trueDamage": true,
                  "ignoreAttackerProperty": true,
                  "elementType": "Electric",
                  "elementDurability": "%element_durability",
                  "enBreak": 0
                }
              }
            },
            {
              "$type": "ApplyModifier",
              "modifierName": "MonsterAffix_HuntingBuff_InterTimer"
            },
            {
              "$type": "ApplyModifier",
              "modifierName": "Thunder_Strike_Cooldown_Timer"
            }
          ]
        },
        // 计时器，Modifier存在期间无法触发雷击效果
        "Thunder_Strike_Cooldown_Timer": {
          "duration": "%strike_cooldown",
          "onAdded": [
            {
              "$type": "SetGlobalValue",
              "key": "_OVERDOSEENHANCED_FORHUNTING_CAN_BE_STRUCK_",
              "value": 0
            }
          ],
          "onRemoved": [
            {
              "$type": "SetGlobalValue",
              "key": "_OVERDOSEENHANCED_FORHUNTING_CAN_BE_STRUCK_",
              "value": 1
            }
          ]
        },
        "MonsterAffix_HuntingBuff_InterTimer": {
          "duration": 0.1,
          "onRemoved": [
            {
              "$type": "ApplyModifier",
              "modifierName": "MonsterAffix_HuntingBuff_OverdoseEnhanced_VulnerableModifier"
            }
          ]
        },
        // 雷草抗性降低叠层效果
        "MonsterAffix_HuntingBuff_OverdoseEnhanced_VulnerableModifier": {
          "duration": "%vulnerable_duration",
          "stacking": "MultipleRefreshNoRemove",
          "stackingOption": {
            "maxModifierNumForMultipleType": "%max_stacking"
          },
          "properties": {
            "Actor_GrassSubHurtDelta": "%sub_hurt_delta",
            "Actor_ElecSubHurtDelta": "%sub_hurt_delta"
          }
        }
      }
    }
  },
