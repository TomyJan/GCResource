          "modifierNameSteps": ["EnterDeathZone"]
        }
      ],
      "modifiers": {
        "UNIQUE_FungusNormalState": {
          "isUnique": true,
          "modifierMixins": [
            {
              "$type": "OverrideMonsterMoveSpeedRatioMixin",
              "minValue": 0.0,
              "maxValue": 4.0
            },
            {
              "$type": "AttachToElementTypeMixin",
              "elementTypes": ["Electric"],
              "modifierName": "EnterOverdose"
            },
            {
              "$type": "AttachToElementTypeMixin",
              "elementTypes": ["Fire"],
              "modifierName": "EnterDecay"
            }
          ],
          "onAdded": [
            {
              "$type": "SetPoseInt",
              "intID": "FungusState",
              "value": 0
            },
            {
              "$type": "SetAnimatorInt",
              "intID": "ShaderState",
              "value": 0,
              "persistent": true
            }
          ],
          "onBeingHit": [
            {
              "$type": "Predicated",
              "targetPredicates": [
                {
                  "$type": "ByAny",
                  "predicates": [
                    {
                      "$type": "ByHitElement",
                      "element": "Electric"
                    }
                  ]
                }
              ],
              "successActions": [
                {
                  "$type": "SetGlobalValue",
                  "key": "_MONSTER_FUNGUSSTATE_",
                  "value": 1
                }
              ],
              "failActions": [
                {
                  "$type": "Predicated",
                  "targetPredicates": [
                    {
                      "$type": "ByAny",
                      "predicates": [
                        {
                          "$type": "ByHitElement",
                          "element": "Fire"
                        }
                      ]
                    }
                  ],
                  "successActions": [
                    {
                      "$type": "SetGlobalValue",
                      "key": "_MONSTER_FUNGUSSTATE_",
                      "value": 2
                    }
                  ]
                }
              ]
            }
          ]
        },
        "UNIQUE_FungusOverdoseState": {
          "isUnique": true,
          "properties": {
            "Animator_MoveSpeedRatio": 1.5
          },
          "modifierMixins": [
            {
              "$type": "OverrideMonsterMoveSpeedRatioMixin",
              "minValue": 0.0,
              "maxValue": 1.0
            }
          ],
          "onAdded": [
            {
              "$type": "SetPoseInt",
              "intID": "FungusState",
              "value": 1
            },
            {
              "$type": "SetAnimatorTrigger",
              "triggerID": "ChangeStateToOverdose"
            },
            {
              "$type": "SetAnimatorInt",
              "intID": "ShaderState",
              "value": 1,
              "persistent": true
            }
          ]
        },
        "UNIQUE_FungusDecayedState": {
          "isUnique": true,
          "properties": {
            "Animator_MoveSpeedRatio": 0.5
          },
          "modifierMixins": [
            {
              "$type": "OverrideMonsterMoveSpeedRatioMixin",
              "minValue": -0.8,
              "maxValue": -0.2
            }
          ],
          "onAdded": [
            {
              "$type": "SetPoseInt",
              "intID": "FungusState",
              "value": 2
            },
            {
              "$type": "SetAnimatorTrigger",
              "triggerID": "ChangeStateToDecayed"
            },
            {
              "$type": "SetAnimatorInt",
              "intID": "ShaderState",
              "value": 2,
              "persistent": true
            }
          ]
        },
        "EnterOverdose": {
          "onAdded": [
            {
              "$type": "SetGlobalValue",
              "key": "_MONSTER_FUNGUSSTATE_",
              "value": 1
            }
          ]
        },
        "EnterDecay": {
          "onAdded": [
            {
              "$type": "SetGlobalValue",
              "key": "_MONSTER_FUNGUSSTATE_",
              "value": 2
            }
          ]
        },
        "EnterDeathZone": {
          //如果SGV为1，则立刻进入衰亡态。
          "onAdded": [
            {
              "$type": "SetGlobalValue",
              "key": "_MONSTER_FUNGUSSTATE_",
              "value": 2
            }
          ],
          "onRemoved": [
            //移除时自杀
            {
              "$type": "SetCanDieImmediately",
              "dieImmediately": true
            },
            {
              "$type": "KillSelf",
              "banDrop": true,
              "banExp": true,
              "banHPPercentageDrop": true
            }
          ]
        }
      }
    }
  },
