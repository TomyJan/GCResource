  //  Flee
  //
  ////////////////////////////////////////////////////////////////
  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "Monster_Vulture_None_Flee",
      "abilityMixins": [
        {
          "$type": "AttachToPoseIDMixin",
          "poseIDs": [131],
          "modifierName": "FleeSkillLabel"
        },
        {
          "$type": "AttachToPoseIDMixin",
          "poseIDs": [101],
          "modifierName": "AlertSkillLabel"
        },
        {
          "$type": "AttachToStateIDMixin",
          "stateIDs": ["Flee_BS"],
          "modifierName": "Flee_Turn"
        },
        {
          "$type": "AttachToStateIDMixin",
          "stateIDs": ["Flee_Loop"],
          "modifierName": "Flee_Launch"
        },
        {
          "$type": "AttachToStateIDMixin",
          "stateIDs": ["Alert_AS"],
          "modifierName": "Flee_High"
        }
      ],
      "modifiers": {
        "FleeSkillLabel": {
          "onAdded": [
            {
              "$type": "ApplyModifier",
              "modifierName": "ChangeFleeSkillCD"
            }
          ]
        },
        "AlertSkillLabel": {
          "onAdded": [
            {
              "$type": "ApplyModifier",
              "modifierName": "ChangeAlertSkillCD"
            }
          ]
        },
        "ChangeAlertSkillCD": {
          "modifierMixins": [
            {
              "$type": "AttachToPoseIDMixin",
              "poseIDs": [201],
              "modifierName": "RealChangeAlertSkillCD"
            }
          ]
        },
        "ChangeFleeSkillCD": {
          "modifierMixins": [
            {
              "$type": "AttachToPoseIDMixin",
              "poseIDs": [201],
              "modifierName": "RealChangeFleeSkillCD"
            }
          ]
        },
        "RealChangeAlertSkillCD": {
          "onAdded": [
            {
              "$type": "SetAISkillCDAvailableNow",
              "skillIDs": [621]
            }
          ]
        },
        "RealChangeFleeSkillCD": {
          "onAdded": [
            {
              "$type": "SetAISkillCDAvailableNow",
              "skillIDs": [611]
            }
          ]
        },
        "Flee_Turn": {
          "onAdded": [
            {
              "$type": "SetGlobalValue",
              "key": "FleeVulture",
              "value": 1
            },
            {
              "$type": "SetGlobalPos",
              "key": "FleeDirection",
              "born": {
                "$type": "ConfigBornByTargetLinearPoint",
                "linearOffset": -2
              }
            },
            {
              "$type": "TurnDirectionToPos",
              "toPos": {
                "$type": "ConfigBornByGlobalValue",
                "positionKey": "FleeDirection"
              },
              "maxAngle": 0.6
            }
          ]
        },
        "Flee_Launch": {
          "duration": 4,
          "thinkInterval": 0.1,
          "onThinkInterval": [
            {
              "$type": "SetAnimatorBool",
              "boolID": "FleeLaunch",
              "value": true,
              "predicates": [
                {
                  "$type": "ByTargetPositionToSelfPosition",
                  "compareType": "XZ",
                  "target": "SelfAttackTarget",
                  "logic": "Greater",
                  "value": 8
                }
              ]
            },
            {
              "$type": "SetAnimatorBool",
              "boolID": "FleeLaunch",
              "value": true,
              "predicates": [
                {
                  "$type": "ByNot",
                  "predicates": [
                    {
                      "$type": "ByHasAttackTarget"
                    }
                  ]
                }
              ]
            }
          ],
          "onRemoved": [
            {
              "$type": "SetAnimatorBool",
              "boolID": "FleeLaunch",
              "value": true
            }
          ]
        },
        "Flee_High": {
          "onAdded": [
            {
              "$type": "SetPoseBool",
              "boolID": "Flee",
              "predicates": [
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "FleeVulture",
                  "target": "Self",
                  "compareType": "Equal",
                  "value": 1
                }
              ],
              "value": true
            }
          ],
          "onRemoved": [
            {
              "$type": "ApplyModifier",
              "modifierName": "Flee_Die",
              "predicates": [
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "FleeVulture",
                  "target": "Self",
                  "compareType": "Equal",
                  "value": 1
                }
              ]
            }
          ]
        },
        "Flee_Die": {
          "duration": 4,
          "onRemoved": [
            {
              "$type": "SetAnimatorBool",
              "boolID": "NoDieAnimation",
              "value": true
            },
            {
              "$type": "SetCanDieImmediately",
              "dieImmediately": true
            },
            {
              "$type": "StartDither",
              "duration": 2
            },
            {
              "$type": "HideUIBillBoard",
              "hide": true
            },
            {
              "$type": "KillSelf",
              "banDrop": true,
              "banExp": true,
              "killSelfType": "Disappear",
              "duration": 0.1
            }
          ]
        }
      }
    }
