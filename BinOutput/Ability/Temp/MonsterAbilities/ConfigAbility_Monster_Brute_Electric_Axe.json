      "modifiers": {
        // 这个是上在武器上的
        "ElectricAxeState": {
          "elementType": "Electric",
          "elementDurability": 75,
          "overrideWeaponElement": true,
          "onAdded": [
            {
              "$type": "SetPoseBool",
              "boolID": "ElectricAxe",
              "value": true,
              "target": "Owner"
