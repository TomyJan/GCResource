        {
          "$type": "ApplyModifier",
          "modifierName": "Pile_ValueCheck",
          "target": "Self"
        }
      ],
      "modifiers": {
        "Pile_Eff": {
          "onAdded": [
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_Monster_Effigy_Electric_Elite_Pile"
            }
          ]
        },
        "Unactived": {
          "onAdded": [
            {
              "$type": "HideUIBillBoard",
              "hide": true
            },
            {
              "$type": "EnableHitBoxByName",
              "hitBoxNames": ["CoreHitBox"],
              "setEnable": true
            },
            {
              "$type": "EnableHitBoxByName",
              "hitBoxNames": ["CoreActivateHitBox"],
              "setEnable": false
            },
            {
              "$type": "AttachModifier",
              "modifierName": "DenyLockOn",
              "target": "Self"
            },
            {
              "$type": "ApplyModifier",
              "modifierName": "BeingHitCD",
              "target": "Self"
            },
            {
              "$type": "SendEffectTrigger",
              "parameter": "Activate",
              "type": "Bool",
              "value": 0,
              "effectPattern": "Eff_Monster_Effigy_Electric_Elite_Pile",
              "canBeHandledOnRecover": true
            }
          ]
        },
        //关闭一段时间内不会被打开
        "BeingHitCD": {
          "duration": 2.0,
          "onRemoved": [
            {
              "$type": "ApplyModifier",
              "modifierName": "BeingHit",
              "target": "Self"
            }
          ]
        },
        "BeingHit": {
          "onBeingHit": [
            {
              "$type": "SetGlobalValue",
              "key": "_ELECTRICPILE_ACTIVE_",
              "value": 1,
              "predicates": [
                {
                  "$type": "ByHitElement",
                  "element": "Electric"
                }
              ]
            }
          ]
        },
        // b1130561 断线重连回来后，会根据同步的globalvalue执行abilitymixin，但是此时用来做激活状态相关的modifier，根据时序还没有全部恢复，因此导致部分modifier不能正确remove或add。改为使用modifiermixin来进行管理，同时可以控制断线重连后的时序。
        "Pile_ValueCheck": {
          "modifierMixins": [
            {
              "$type": "AttachModifierToSelfGlobalValueMixin",
              "globalValueKey": "_ELECTRICPILE_ACTIVE_", //检测物件激活状态
              "valueSteps": [-0.5, 0.5, 1.5],
              "removeAppliedModifier": true,
              "modifierNameSteps": ["Unactived", "Actived"]
            }
          ]
        },
        "Actived": {
          "onAdded": [
            {
              "$type": "RemoveModifier",
              "modifierName": "BeingHit",
              "target": "Self"
            },
            {
              "$type": "EnableHitBoxByName",
              "hitBoxNames": ["CoreHitBox"],
              "setEnable": false
            },
            {
              "$type": "EnableHitBoxByName",
              "hitBoxNames": ["CoreActivateHitBox"],
              "setEnable": true
            },
            {
              "$type": "HideUIBillBoard",
              "hide": false
            },
            {
              "$type": "SendEffectTrigger",
              "parameter": "Activate",
              "type": "Bool",
              "value": 1,
              "effectPattern": "Eff_Monster_Effigy_Electric_Elite_Pile",
              "canBeHandledOnRecover": true
            },
            {
              "$type": "ApplyModifier",
              "modifierName": "ElementShield",
              "target": "Self"
            },
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_Monster_Effigy_Electric_Elite_Pile_Alert_Loop"
            },
            {
              "$type": "ApplyModifier",
              "modifierName": "Damage",
              "target": "Self"
            },
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_Monster_Effigy_Electric_Elite_Pile_Charge"
            }
          ],
          "thinkInterval": 3.1,
          "onThinkInterval": [
            {
              "$type": "ApplyModifier",
              "modifierName": "Damage",
              "target": "Self"
            },
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_Monster_Effigy_Electric_Elite_Pile_Charge"
            }
          ]
        },
        "ElementShield": {
          "elementType": "Electric",
          "elementDurability": 50,
          "modifierMixins": [
            {
              "$type": "ShieldBarMixin"
            }
          ],
          "onRemoved": [
            {
              "$type": "SetGlobalValue",
              "key": "_ELECTRICPILE_ACTIVE_",
              "value": 0
            }
          ]
        },
        //为了防止切换状态时，强行进入remove，多执行一次伤害，所以加个判定
        "Damage": {
          "duration": 2.9,
          "onRemoved": [
            {
              "$type": "Predicated",
