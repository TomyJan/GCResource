  ////////////////////////////////////////////////////////////////
  //
  //  设置出生隐藏 冰龙蜥
  //
  ////////////////////////////////////////////////////////////////
  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "Monster_Drake_DeepSea_Ice_SetBornPos",
      "onAdded": [
        {
          "$type": "ApplyModifier",
          "modifierName": "Monster_Drake_DeepSea_TowerBorn",
          "predicates": [
            {
              "$type": "ByTargetGlobalValue",
              "key": "_DRAKE_DEEPSEA_IN_TOWER",
              "value": 1,
              "compareType": "Equal"
            }
          ]
        }
      ],
      "abilityMixins": [
        {
          "$type": "AttachToAIAlertnessMixin",
          "alertness": [0, 1],
          "modifierName": "Monster_Drake_DeepSea_Ice_HideBeforeBattle"
        },
        {
          "$type": "AttachToStateIDMixin",
          "stateIDs": ["Walk_BS", "Walk_AS", "NormalMove", "SwimWalk"],
          "modifierName": "Monster_Drake_DeepSea_Ensure_Show"
        }
      ],
      "modifiers": {
        "Monster_Drake_DeepSea_Ensure_Show": {
          "onAdded": [
            {
              "$type": "TriggerSetVisible",
              "visible": true
            },
            {
              "$type": "HideUIBillBoard",
              "hide": false
            }
          ]
        },
        "Monster_Drake_DeepSea_TowerBorn": {
          "onAdded": [
            {
              "$type": "SetAnimatorBool",
              "boolID": "InTower",
              "value": true
            }
          ]
        },
        "Monster_Drake_DeepSea_Ice_HideBeforeBattle": {
          "modifierMixins": [
            {
              "$type": "AttachActionToModifierMixin",
              "onAdded": [
                {
                  "$type": "TriggerSetVisible",
                  "visible": false,
                  "canBeHandledOnRecover": true
                },
                {
                  "$type": "HideUIBillBoard",
                  "hide": true,
                  "canBeHandledOnRecover": true
                }
              ],
              "onRemoved": [
                {
                  "$type": "TriggerSetVisible",
                  "visible": true
                },
                {
                  "$type": "HideUIBillBoard",
                  "hide": false
                }
              ]
            }
          ],
          "onAdded": [
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_Monster_Oceanid_BornBS"
            },
            {
              "$type": "Predicated",
              "targetPredicates": [
                {
                  "$type": "ByAny",
                  "predicates": [
                    {
                      "$type": "ByTargetGlobalValue",
                      "key": "_DRAKE_DEEPSEA_CHALLENGE_LEVEL",
                      "value": 0,
                      "compareType": "MoreThan"
                    },
                    {
                      "$type": "ByTargetGlobalValue",
                      "key": "_IS_WQ_DRAKE",
                      "value": 1,
                      "compareType": "Equal"
                    }
                  ]
                }
              ],
              "successActions": [
                {
                  "$type": "SetGlobalPos",
                  "key": "_DRAKE_DEEPSEA_ICE_BORN_POS",
                  "born": {
                    "$type": "ConfigBornByActionPoint",
                    "actionPointType": "StandbyPoint",
                    "selectType": {
                      "$type": "SelectActionPointByID",
                      "actionPointID": 700500001
                    }
                  }
                }
              ],
              "failActions": [
                {
                  "$type": "SetGlobalPos",
                  "key": "_DRAKE_DEEPSEA_ICE_BORN_POS",
                  "born": {
                    "$type": "ConfigBornByActionPoint",
                    "actionPointType": "StandbyPoint",
                    "selectType": {
                      "$type": "SelectActionPointByID",
                      "actionPointID": 500500002
                    }
                  }
                }
              ]
            }
          ]
        }
      }
    }
  },
