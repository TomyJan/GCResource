  ////////////////////////////////////////////////////////////////
  //
  //  Alert
  //
  ////////////////////////////////////////////////////////////////
  //由于入战后姿势差异比较大，要整体替换下pushcollider
  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "Monster_Drake_Rock_Alert",
      "abilityMixins": [
        {
          "$type": "AttachToPoseIDMixin",
          "poseIDs": [201, 211, 212, 213, 214, 221, 222, 223, 224],
          "modifierName": "UNIQUE_Monster_Drake_Rock_Alert_Reset",
          "isUnique": false
        },
        {
          "$type": "AttachModifierToSelfGlobalValueMixin",
          "globalValueKey": "_MONSTER_Drake_Rock_Enhance",
          "valueSteps": [-0.5, 0.5, 1.5],
          "modifierNameSteps": [
            "Monster_Drake_NotEnhance",
            "Monster_Drake_Enhance"
          ]
        },
        {
          "$type": "AttachToAIAlertnessMixin",
          "alertness": [0],
          "modifierName": "Monster_Drake_ExitEnhance"
        }
      ],
      "onKill": [
        {
          "$type": "EnablePushColliderName",
          "pushColliderNames": ["PushCollider_Alert"],
          "setEnable": false
        },
        {
          "$type": "EnablePushColliderName",
          "pushColliderNames": ["PushCollider_Free"],
          "setEnable": false
        },
        {
          "$type": "EnablePushColliderName",
          "pushColliderNames": ["PushCollider_Roll"],
          "setEnable": false
        }
      ],
      "modifiers": {
        "UNIQUE_Monster_Drake_Rock_Alert_Reset": {
          "isUnique": true,
          "onAdded": [
            {
              "$type": "EnablePushColliderName",
              "pushColliderNames": ["PushCollider_Alert"],
              "setEnable": true
            },
            {
              "$type": "EnablePushColliderName",
              "pushColliderNames": ["PushCollider_Free"],
              "setEnable": false
            },
            {
              "$type": "ReTriggerAISkillInitialCD",
              "skillIDs": [101, 102]
            }
          ],
          "onRemoved": [
            {
              "$type": "EnablePushColliderName",
              "pushColliderNames": ["PushCollider_Alert"],
              "setEnable": false
            },
            {
              "$type": "EnablePushColliderName",
              "pushColliderNames": ["PushCollider_Free"],
              "setEnable": true
            }
          ]
        },
        "Monster_Drake_Enhance": {
          "onAdded": [
            {
              "$type": "SetPoseBool",
              "boolID": "Enhance",
              "value": true
            }
          ]
        },
        "Monster_Drake_NotEnhance": {
          "onAdded": [
            {
              "$type": "SetPoseBool",
              "boolID": "Enhance",
              "value": false
            }
          ]
        },
        "Monster_Drake_ExitEnhance": {
          "onAdded": [
            {
              "$type": "AddGlobalValue",
              "value": -1,
              "key": "_MONSTER_Drake_Rock_Enhance",
              "useLimitRange": true,
              "maxValue": 1,
              "minValue": 0
            },
            {
              "$type": "SetAnimatorBool",
              "boolID": "ElementLayer_Fire",
              "value": false
            },
            {
              "$type": "SetAnimatorBool",
              "boolID": "ElementLayer_Ice",
              "value": false
            },
            {
              "$type": "SetAnimatorBool",
              "boolID": "ElementLayer_Elec",
              "value": false
            },
            {
              "$type": "SetAnimatorBool",
              "boolID": "ElementLayer_Water",
              "value": false
            }
          ]
        }
      }
    }
  },
