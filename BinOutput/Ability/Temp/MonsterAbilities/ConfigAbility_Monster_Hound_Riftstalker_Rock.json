  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "Monster_Hound_Riftstalker_Rock_Pose",
      "onAdded": [
        {
          "$type": "ApplyModifier",
          "modifierName": "UNIQUE_Riftstalker_BeHitCheck"
        },
        // 用于区分岩猎犬
        {
          "$type": "SetGlobalValue",
          "key": "_RIFTSTALKER_ROCK_TAG_",
          "value": 1,
          "target": "Self"
        }
      ],
      "abilityMixins": [
        // _Riftstalker_POSE_TAG_
        // 0:未激活
        // 1:未激活→激活
        // 2:激活
        {
          "$type": "AttachModifierToSelfGlobalValueMixin",
          "globalValueKey": "_Riftstalker_POSE_TAG_",
          "defaultGlobalValueOnCreate": 0,
          "removeAppliedModifier": true,
          "valueSteps": [-0.5, 0.5, 1.5, 2.5],
          "modifierNameSteps": [
            "Pose_None",
            "Pose_NormalToActivated",
            "Pose_Activated"
          ]
        },
        {
          "$type": "AttachToPoseIDMixin",
          "poseIDs": [201, 301, 401],
          "modifierName": "Riftstalker_ChargeBar_Handler"
        }
      ],
      "modifiers": {
        "Riftstalker_ChargeBar_Handler": {
          "onAdded": [
            {
              "$type": "ApplyModifier",
              "modifierName": "UNIQUE_Riftstalker_ChargeBar"
            }
          ],
          "onRemoved": [
            {
              "$type": "RemoveUniqueModifier",
              "modifierName": "UNIQUE_Riftstalker_ChargeBar"
            }
          ]
        },
        "UNIQUE_Riftstalker_ChargeBar": {
          "isUnique": true,
          "onAdded": [
            {
              "$type": "SetChargeBarValue",
              "value": 0
            }
          ],
          "modifierMixins": [
            {
              "$type": "ChargeBarMixin",
              "chargeBarElementType": "Rock",
              "chargeBarIconID": 2,
              "initialValue": 0,
              "maxValue": 100
            },
            {
              "$type": "AttachModifierToSelfGlobalValueMixin",
              "globalValueKey": "_CHARGEBAR_VALUE",
              "valueSteps": [99.5, 100.5],
              "removeAppliedModifier": true,
              "modifierNameSteps": ["ChargeActivated"]
            }
          ]
        },
        // 通过充能条来控制怪物激活/未激活的pose切换
        "ChargeActivated": {
          "onAdded": [
            {
              "$type": "SetGlobalValue",
              "key": "_Riftstalker_POSE_TAG_",
              "value": 1
            }
          ]
        },
        "Pose_None": {
          "onAdded": [
            {
              "$type": "SetPoseInt",
              "intID": "ActivatedTag",
              "value": 0
            },
            {
              "$type": "SetAnimatorBool",
              "boolID": "ActivatedTag",
              "value": false
            },
            {
              "$type": "SetAnimatorBool",
              "boolID": "ShaderActivatedTag",
              "value": false
            }
          ]
        },
        "Pose_NormalToActivated": {
          "onAdded": [
            {
              "$type": "SetPoseInt",
              "intID": "ActivatedTag",
              "value": 1
            },
            {
              "$type": "SetAISkillGCD",
              "value": 0.2
            }
          ]
        },
        "Pose_Activated": {
          "state": "DefenseDown",
          "properties": {
            "Actor_RockSubHurtDelta": -0.65
          },
          "onAdded": [
            {
              "$type": "SetPoseInt",
              "intID": "ActivatedTag",
              "value": 2
            },
            {
              "$type": "SetAnimatorBool",
              "boolID": "ActivatedTag",
              "value": true
            },
            {
              "$type": "SetAnimatorBool",
              "boolID": "ShaderActivatedTag",
              "value": true
            },
            {
              "$type": "SetAISkillGCD",
              "value": 1.4
            }
          ]
        },
        "UNIQUE_Riftstalker_BeHitCheck": {
          "isUnique": true,
          "onBeingHit": [
            {
              "$type": "Predicated",
              "target": "Target",
              "targetPredicates": [
                {
                  "$type": "ByEntityTypes",
                  "entityTypes": ["Avatar"]
                },
                {
                  "$type": "ByHitElement",
                  "element": "Rock"
                }
              ],
              "successActions": [
                {
                  "$type": "AddChargeBarValue",
                  "value": 20,
                  "target": "Self"
                }
              ]
            },
            {
              "$type": "Predicated",
              "target": "TargetOriginOwner",
              "targetPredicates": [
                {
                  "$type": "ByEntityTypes",
                  "entityTypes": ["Avatar"]
                },
                {
                  "$type": "ByHitElement",
                  "element": "Rock"
                }
              ],
              "successActions": [
                {
                  "$type": "AddChargeBarValue",
                  "value": 20,
                  "target": "Self"
                }
              ]
            }
          ]
        }
      }
    }
  },
