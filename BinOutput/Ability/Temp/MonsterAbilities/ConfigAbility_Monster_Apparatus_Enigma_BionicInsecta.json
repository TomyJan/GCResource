                  ]
                },
                {
                  "$type": "FireEffect",
                  "effectPattern": "Eff_Monster_Apparatus_Enigma_BionicInsecta_Shield_Hit",
                  "born": {
                    "$type": "ConfigBornByAttachPoint",
