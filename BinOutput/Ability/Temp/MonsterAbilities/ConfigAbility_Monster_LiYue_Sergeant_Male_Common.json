          "modifierName": "AuxWeaponTransManager"
        }
      ],
      "modifiers": {
        "AuxWeaponTransManager": {
          "modifierMixins": [
            {
              "$type": "AttachToStateIDMixin",
              "stateIDs": [
                "NormalIdle",
                "NormalMove",
                "Turn",
                "BeHit",
                "Attack01",
                "GroupAttack01",
                "BackSprint",
                "BackSprintAttack_BS",
                "BackSprintOrder_BS"
              ],
              "modifierName": "Pole_AuxWeaponTrans"
            }
          ],
          "onRemoved": [
            {
              "$type": "RemoveModifier",
              "modifierName": "Pole_AuxWeaponTrans"
            }
          ]
        },
        "Pole_AuxWeaponTrans": {
          "onAdded": [
            {
              "$type": "TriggerAuxWeaponTrans",
              "setEnable": true,
              "equipPart": "equip01"
            }
          ],
          "onRemoved": [
            {
              "$type": "TriggerAuxWeaponTrans",
              "setEnable": false,
              "equipPart": "equip01"
            }
          ]
        }
