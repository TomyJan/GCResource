              "value": 0
            },
            {
              "$type": "SetPoseBool",
              "boolID": "ElementState",
              "value": false
            },
            {
              "$type": "KillSelf",
              "predicates": [
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "_MONSTER_INVOKER_DEACON_FIRE_DISAPPEAR",
                  "value": 0,
                  "compareType": "NoneOrEqual"
                }
              ]
