            },
            "onGround": true
          }
        },
        {
          "$type": "SetGlobalDir",
          "target": "Self",
          "key": "_MONSTER_SANDWORM_STAGE02MOVEMENT_POS01_01_DIR_",
          "born": {
            "$type": "ConfigBornByTarget",
            "direction": {
              "$type": "ConfigDirectionByDefault",
              "angleOffsetCorrect": {
