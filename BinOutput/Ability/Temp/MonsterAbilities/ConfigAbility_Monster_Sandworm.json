      "modifiers": {
        "Sandworm_Stage02Movement": {
          "modifierMixins": [
            {
              "$type": "AttachToAnimatorStateIDMixin",
              "stateIDs": ["Stage02_Standby_Blink"],
              "modifierName": "Sandworm_Stage02Movement_DoBlink"
            }
          ],
          "onAdded": [
            {
              "$type": "SetGlobalValue",
              "key": "_MONSTER_SANDWORM_MOVEPOS_COUNTER_",
              "value": 0
            }
          ]
        },
        "Sandworm_Stage02Movement_DoBlink": {
          "onAdded": [
            //////////////////////Check target in which arc
            {
              "$type": "SetNearestPosIndexToGlobalValue",
              "srcTarget": "SelfAttackTarget",
              "globalValueKey": "_MONSTER_SANDWORM_MOVEPOS_POINTER_",
              "posGVList": [
                "_MONSTER_SANDWORM_STAGE02MOVEMENT_POS00_00_",
                "_MONSTER_SANDWORM_STAGE02MOVEMENT_POS01_00_",
                "_MONSTER_SANDWORM_STAGE02MOVEMENT_POS02_00_"
              ],
              "onlyXZ": true,
              "useFarthest": false
            },
            {
              "$type": "SetGlobalValueToOverrideMap",
              "target": "Self",
              "globalValueKey": "_MONSTER_SANDWORM_MOVEPOS_POINTER_",
              "overrideMapKey": "CurrentPointer"
            },
            //////////////////////CurrentPointer - LastPointer
            {
              "$type": "SetOverrideMapValue",
              "target": "Self",
              "value": "%CurrentPointer-%LastPointer",
              "overrideMapKey": "CurrentMinusLastPointer"
            },
            {
              "$type": "SetGlobalValueToOverrideMap",
              "target": "Self",
              "globalValueKey": "_MONSTER_SANDWORM_MOVEPOS_POINTER_",
              "overrideMapKey": "LastPointer"
            },
            //////////////////////Use the diff to get direction/Counter
            //0 is always -
            //1 is always C
            //-2 is always C
            //2 is always RC
            //-1 is always RC
            {
              "$type": "Predicated",
              "target": "Self",
              "targetPredicates": [
                {
                  "$type": "ByTargetOverrideMapValue",
                  "target": "Self",
                  "targetAbilityName": "Monster_Sandworm_Stage02MovementHandler",
                  "targetKey": "CurrentMinusLastPointer",
                  "targetValue": 0,
                  "logic": "Equal"
                }
              ],
              //poinerdiff 0 means stay at the last arc
              //successActions is nothing, so just loop between [0, 1], based on last counter
              "successActions": [
                //Check target position and move to nearest pos pointer
                //If need new pointer, reset counter to 0
                //If pointer remains, add Movepos counter, loops between [0, 1]
                {
                  "$type": "Predicated",
                  "target": "Self",
                  "targetPredicates": [
                    {
                      "$type": "ByTargetGlobalValue",
                      "target": "Self",
                      "key": "_MONSTER_SANDWORM_MOVEPOS_COUNTER_",
                      "value": 1,
                      "compareType": "Equal"
                    }
                  ],
                  "successActions": [
                    {
                      "$type": "SetGlobalValue",
                      "target": "Self",
                      "key": "_MONSTER_SANDWORM_MOVEPOS_COUNTER_",
                      "value": 0
                    }
                  ],
                  "failActions": [
                    {
                      "$type": "AddGlobalValue",
                      "target": "Self",
                      "key": "_MONSTER_SANDWORM_MOVEPOS_COUNTER_",
                      "value": 1,
                      "useLimitRange": true,
                      "maxValue": 1,
                      "minValue": 0
                    }
                  ]
                }
              ],
              "failActions": [
                {
                  "$type": "Predicated",
                  "target": "Self",
                  "targetPredicates": [
                    {
                      "$type": "ByAny",
                      "predicates": [
                        {
                          "$type": "ByTargetOverrideMapValue",
                          "targetAbilityName": "Monster_Sandworm_Stage02MovementHandler",
                          "targetKey": "CurrentMinusLastPointer",
                          "targetValue": 1,
                          "logic": "Equal"
                        },
                        {
                          "$type": "ByTargetOverrideMapValue",
                          "targetAbilityName": "Monster_Sandworm_Stage02MovementHandler",
                          "targetKey": "CurrentMinusLastPointer",
                          "targetValue": -2,
                          "logic": "Equal"
                        }
                      ]
                    }
                  ],
                  "successActions": [
                    //to C is 0
                    {
                      "$type": "SetGlobalValue",
                      "target": "Self",
                      "key": "_MONSTER_SANDWORM_MOVEPOS_COUNTER_",
                      "value": 0
                    }
                  ],
                  "failActions": [
                    //to RC is 1
                    {
                      "$type": "SetGlobalValue",
                      "target": "Self",
                      "key": "_MONSTER_SANDWORM_MOVEPOS_COUNTER_",
                      "value": 1
                    }
                  ]
                }
              ]
            },
            //First check pointer then check counter
            {
              "$type": "Predicated",
              "target": "Self",
              "targetPredicates": [
                {
                  "$type": "ByTargetGlobalValue",
                  "target": "Target",
                  "key": "_MONSTER_SANDWORM_MOVEPOS_POINTER_",
                  "value": 0,
                  "compareType": "Equal"
                }
              ],
              "successActions": [
                //////////////////////////////Pointer 0
                {
                  "$type": "Predicated",
                  "target": "Self",
                  "targetPredicates": [
                    {
                      "$type": "ByTargetGlobalValue",
                      "target": "Target",
                      "key": "_MONSTER_SANDWORM_MOVEPOS_COUNTER_",
                      "value": 0,
                      "compareType": "Equal"
                    }
                  ],
                  "successActions": [
                    //////////////////////////////////////Counter 0
                    {
                      "$type": "DoBlinkByGlobalPos",
                      "positionKey": "_MONSTER_SANDWORM_STAGE02MOVEMENT_POS00_00_",
                      "directionKey": "_MONSTER_SANDWORM_STAGE02MOVEMENT_POS00_00_DIR_",
                      "useGlobalUp": true
                    }
                  ],
                  "failActions": [
                    //////////////////////////////////////Counter 1
                    {
                      "$type": "DoBlinkByGlobalPos",
                      "positionKey": "_MONSTER_SANDWORM_STAGE02MOVEMENT_POS00_01_",
                      "directionKey": "_MONSTER_SANDWORM_STAGE02MOVEMENT_POS00_01_DIR_",
                      "useGlobalUp": true
                    }
                  ]
                }
              ],
              "failActions": [
                {
                  "$type": "Predicated",
                  "target": "Self",
