  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "Dvalin_S01_Enviroment",
      "onAdded": [
        {
          "$type": "ApplyModifier",
          "modifierName": "WaitForAvatarReady",
          "target": "Self"
        },
        {
          "$type": "ApplyModifier",
          "modifierName": "SpecialSkyBox",
          "target": "Self"
        },
        // 创建MovingCenter
        {
          "$type": "CreateGadget",
          "target": "Self",
          "gadgetID": 42901008, // Dvalin_S01_Moving_Center
          "byServer": true,
          "born": {
            "$type": "ConfigBornByWorld",
            "worldPos": {
              "x": 0.0,
              "y": 50.0,
              "z": -80.0
            },
            "worldFwd": {
              "x": 0.0,
              "y": 0.0,
              "z": 1.0
            }
          }
        }
      ],
      "onKill": [
        {
          "$type": "ShowReminder",
          "id": 20010017
        }
      ],
      "modifiers": {
        "SpecialSkyBox": {
          "modifierMixins": [
            {
              "$type": "EnviroFollowRotateMixin",
              "deactiveOnCutsecneName": "ART/Cutscene/Cs_MDAQ022_KaeyaArrived_Part1_Convert",
              "selfRotateOffSet": {
                "x": 0,
                "y": 0,
                "z": 0
              }
            },
            {
              "$type": "DvalinS01PathEffsMixin",
              "effectStart": 4,
              "effectEnd": 3,
              "effInfos": [
                {
                  "$type": "DvalinS01PathEffsInfo",
                  "flyState": 1,
                  "effectName": "Eff_Monster_Dvalin_S01_Cloud01",
                  "intervalMax": 0,
                  "intervalMin": 0,
                  "numMax": 1,
                  "numMin": 1,
                  "rangeMax": 0.0,
                  "rangeMin": 0.0,
                  "eularMax": 0.0,
                  "eularMin": 0.0
                }
              ]
            },
            {
              "$type": "DvalinS01PathEffsMixin",
              "effectStart": 4,
              "effectEnd": 6,
              "effInfos": [
                {
                  "$type": "DvalinS01PathEffsInfo",
                  "flyState": 0,
                  "effectName": "Eff_Monster_Dvalin_S01_Cloud02",
                  "intervalMax": 0,
                  "intervalMin": 0,
                  "numMax": 1,
                  "numMin": 1,
                  "rangeMax": 0.0,
                  "rangeMin": 0.0,
                  "eularMax": 0.0,
                  "eularMin": 0.0
                }
              ]
            }
          ]
        },
        "WaitForAvatarReady": {
          "modifierMixins": [
            {
              "$type": "StageReadyMixin",
              "onStageReady": [
                {
                  "$type": "ApplyModifier",
                  "modifierName": "AvatarControlSettings",
                  "target": "CurTeamAvatars"
                },
                {
                  "$type": "RemoveModifier",
                  "modifierName": "WaitForAvatarReady"
                }
              ]
            }
          ],
          "onAdded": [
            {
              "$type": "Predicated",
              "targetPredicates": [
                {
                  "$type": "ByStageIsReadyTemp"
                }
              ],
              "successActions": [
                {
                  "$type": "ApplyModifier",
                  "modifierName": "AvatarControlSettings",
                  "target": "CurTeamAvatars"
                },
                {
                  "$type": "RemoveModifier",
                  "modifierName": "WaitForAvatarReady"
                }
              ]
            }
          ]
        },
        "AvatarControlSettings": {
          "modifierMixins": [
            {
              // 锁定飞行面向，摇杆移动不改变面向
              "$type": "AvatarLockForwardFlyMixin",
              "worldForward": {
                "x": 0.0,
                "y": 0.0,
                "z": 1.0
              },
              "flySpeedScale": 0.75,
              "flyBackSpeedScale": 4.0,
              "eularRawInput": {
                "x": -90.0,
                "y": 0.0,
                "z": 0.0
              }
            },
            {
              "$type": "AirFlowMixin",
              "gadgetID": 42901002,
              "campTargetType": "All",
              "born": {
                "$type": "ConfigBornByTarget"
              }
            }
          ],
          "onAdded": [
            //
            // Re:Link
            //
            {
              "$type": "ChangePlayMode",
              "toPlayMode": "DvalinS01FlyState",
              "canBeHandledOnRecover": true
            }
          ],
          "thinkInterval": 0.5,
          "onThinkInterval": [
            // 风场中不受重力，不耗体力
            {
              "$type": "SetKeepInAirVelocityForce",
              "setEnable": true
            },
            // 禁用滑翔伞拖尾特效
            {
              "$type": "EnableAvatarFlyStateTrail",
              "setEnable": false
            },
            // 开伞
            {
              "$type": "ForceAirStateFly",
              "isActive": true
            }
          ],
          "onKilled": [
            // 风场中不受重力，不耗体力
            {
              "$type": "SetKeepInAirVelocityForce",
              "setEnable": false
            }
          ],
          "properties": {
            "Actor_EndureDelta": -999999
          }
        }
      }
    }
