  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "Dvalin_S01_StateCommon",
      "abilityMixins": [
        // DEFINE DVALIN GLOBAL VALUE
        // PROCESS_STATE:
        //   0 - None
        //   1 - Pursue
        //   2 - Dog Fighting
        {
          "$type": "AttachModifierToSelfGlobalValueMixin",
          "globalValueKey": "PROCESS_STATE",
          "valueSteps": [-0.5, 0.5, 1.5, 100],
          "defaultGlobalValueOnCreate": 0,
          "removeAppliedModifier": true,
          "modifierNameSteps": [
            "PROCESS_STATE_None",
            "PROCESS_STATE_DogFight",
            "PROCESS_STATE_Pursue"
          ]
        },
        // HPBAR_STATE:
        //   0 - No HpBar
        //   1 - Show HpBar No Lock
        //   2 - Show HpBar With Lock
        {
          "$type": "AttachModifierToSelfGlobalValueMixin",
          "globalValueKey": "HPBAR_STATE",
          "valueSteps": [0, 1, 2, 100],
          "defaultGlobalValueOnCreate": 0,
          "removeAppliedModifier": true,
          "modifierNameSteps": [
            "HPBAR_STATE_NoHPBar",
            "HPBAR_STATE_ShowHPBarWithLock",
            "HPBAR_STATE_ShowHPBarNoLock"
          ]
        },
        // DFM_STATE:
        //   0 - Dog Fight Fly With Steer
        //   1 - Dog Fight Fly No Steer
        //   2 - Normal Fly
        {
          "$type": "AttachModifierToSelfGlobalValueMixin",
          "globalValueKey": "DFM_STATE",
          "valueSteps": [0, 1, 2, 100],
          "defaultGlobalValueOnCreate": 0,
          "removeAppliedModifier": true,
          "modifierNameSteps": [
            "DFM_STATE_DogFightFlyNoSteer",
            "DFM_STATE_DogFightFlyWithSteer",
            "DFM_STATE_NormalFly"
          ]
        }
      ],
      "onAdded": [
        {
          "$type": "SetSelfAttackTarget",
          "target": "CurLocalAvatar"
        }
      ],
      "modifiers": {
        "PROCESS_STATE_None": {
          "onAdded": [
            // Re:Link
            {
              "$type": "PushDvalinS01Process",
              "time": 1,
              "toPercentage": 20,
              "unBreak": false,
              "setForce": true,
              "attenuation": 2.0,
              "vector": {
                "x": 0.0,
                "y": 0.0,
                "z": 50
              },
              "canBeHandledOnRecover": true
            }
          ]
        },
        "PROCESS_STATE_DogFight": {
          "onAdded": [
            // Re:Link
            {
              "$type": "PushDvalinS01Process",
              "time": 1,
              "toPercentage": 0,
              "unBreak": false,
              "setForce": true,
              "attenuation": 2.0,
              "vector": {
                "x": 0.0,
                "y": 0.0,
                "z": 50
              },
              "canBeHandledOnRecover": true
            }
          ]
        },
        "PROCESS_STATE_Pursue": {
          "onAdded": [
            {
              "$type": "PushDvalinS01Process",
              "time": 3,
              "toPercentage": 100.0,
              "unBreak": false,
              "setForce": true,
              "attenuation": 2.0,
              "vector": {
                "x": 0.0,
                "y": 0.0,
                "z": 50
              },
              "canBeHandledOnRecover": true
            }
          ]
        },
        "HPBAR_STATE_NoHPBar": {
          "onAdded": [
            // Re:Link
            {
              "$type": "ShowUICombatBar",
              "show": false,
              "fore": true,
              "canBeHandledOnRecover": true
            }
          ]
        },
        "HPBAR_STATE_ShowHPBarWithLock": {
          "modifierMixins": [
            {
              "$type": "StageReadyMixin",
              "onStageReady": [
                {
                  "$type": "ShowUICombatBar",
                  "show": true,
                  "fore": true
                }
              ]
            }
          ],
          "onAdded": [
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_Monster_Dvalin_S01_LockOn"
            },
            // Re:Link
            {
              "$type": "ShowUICombatBar",
              "show": true,
              "fore": true,
              "canBeHandledOnRecover": true,
              "predicates": [
                {
                  "$type": "ByStageIsReadyTemp"
                }
              ]
            }
          ]
        },
        "HPBAR_STATE_ShowHPBarNoLock": {
          "modifierMixins": [
            {
              "$type": "StageReadyMixin",
              "onStageReady": [
                {
                  "$type": "ShowUICombatBar",
                  "show": true,
                  "fore": true
                }
              ]
            }
          ],
          "onAdded": [
            // Re:Link
            {
              "$type": "ShowUICombatBar",
              "show": true,
              "fore": true,
              "canBeHandledOnRecover": true,
              "predicates": [
                {
                  "$type": "ByStageIsReadyTemp"
                }
              ]
            }
          ]
        },
        "DFM_STATE_DogFightFlyNoSteer": {
          "onAdded": [
            // Re:Link
            {
              "$type": "SetAnimatorBool",
              "boolID": "isDFM",
              "value": true,
              "canBeHandledOnRecover": true
            },
            {
              "$type": "SetDvalinS01FlyState",
              "toState": 0,
              "canBeHandledOnRecover": true
            }
          ]
        },
        "DFM_STATE_DogFightFlyWithSteer": {
          "onAdded": [
            // Re:Link
            {
              "$type": "SetAnimatorBool",
              "boolID": "isDFM",
              "value": true,
              "canBeHandledOnRecover": true
            },
            {
              "$type": "SetDvalinS01FlyState",
              "toState": 1,
              "canBeHandledOnRecover": true
            }
          ]
        },
        "DFM_STATE_NormalFly": {
          "onAdded": [
            // Re:Link
            {
              "$type": "SetAnimatorBool",
              "boolID": "isDFM",
              "value": false,
              "canBeHandledOnRecover": true
            },
            {
              "$type": "SetDvalinS01FlyState",
              "toState": 0,
              "canBeHandledOnRecover": true
            }
          ]
        }
      }
    }
