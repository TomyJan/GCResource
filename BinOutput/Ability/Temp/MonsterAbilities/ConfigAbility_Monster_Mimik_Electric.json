              "hitBoxNames": ["HitBox_Fruit"],
              "setEnable": false
            },
            {
              "$type": "EnableHitBoxByName",
              "hitBoxNames": ["HitBox", "HeadBox"],
              "setEnable": true
            }
          ]
        }
      }
    }
  },
  {
    "Default": {
      //技能过程中增加重量
      "$type": "ConfigAbility",
      "abilityName": "Monster_Mimik_Electric_AbsorbFruit_WeightManager",
      "abilityMixins": [
        {
          //在吸收果实期间挂载modifier
          "$type": "AttachToStateIDMixin",
