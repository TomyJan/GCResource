      ],
      "modifiers": {
        "AddWeight": {
          "properties": {
            "Entity_WeightRatio": 5.0
          }
        }
      }
    }
  },
  {
    "Default": {
      //吸收果实时,给拟态草挂载元素盾
      //元素盾挂载时,自动触发果实充能表现
      //元素盾被击破时,触发眩晕
      "$type": "ConfigAbility",
      "abilityName": "Monster_Mimik_Electric_AbsorbFruit",
      "abilityMixins": [
        {
          //在吸收果实期间挂载modifier
          "$type": "AttachToStateIDMixin",
          "stateIDs": ["AbsorbFruit_Loop"],
          "modifierName": "UNIQUE_Mimik_Electric_Fruit"
        }
      ],
      "modifiers": {
        "UNIQUE_Mimik_Electric_Fruit": {
          //实现果实相关逻辑的主要modifier
          //带有ShieldBarMixin 在Durability被反应完时触发onShieldBroken,就是眩晕之类的效果
          "stacking": "Unique",
          "isUnique": true,
          "forceSyncToRemote": true,
          "elementType": "Electric",
          "elementDurability": 10,
          "modifierMixins": [
            {
              "$type": "ShieldBarMixin",
              "onShieldBroken": [
                //盾被击破时：
                {
                  "$type": "ActCameraShake",
                  "cameraShake": {
                    "shakeType": "Center",
                    "shakeRange": 0.2,
                    "shakeTime": 0.12,
                    "shakeDistance": 30,
                    "extension": {
                      "baseOnCamera": true
                    }
                  }
                },
                {
                  //开始击晕倒计时
                  "$type": "TriggerAbility",
                  "abilityName": "Monster_Mimik_Electric_DownManager",
                  "target": "Caster"
                },
                {
                  //进入击晕Pose状态
                  "$type": "SetPoseBool",
                  "boolID": "Down",
                  "value": true,
                  "target": "Caster"
                },
                {
                  //进入击晕Animator状态
                  "$type": "SetAnimatorTrigger",
                  "triggerID": "DownTrigger",
                  "target": "Caster",
                  "MPTriggerOnRemote": true
                }
              ]
            }
          ],
          "onAdded": [
            {
              "$type": "AddGlobalValue",
              "key": "_ABILITY_MIMIK_ELECTRIC_FRUIT",
              "value": 1,
              "useLimitRange": true,
              "maxValue": 1,
              "minValue": 0
            },
            {
              //播放充能表现
              "$type": "TriggerAbility",
              "abilityName": "Monster_Mimik_Electric_Fruit_Effect",
              "target": "Self"
            },
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_Monster_Mimik_Electric_AbsorbFruit_Loop_01"
            }
          ],
          "onKill": [
            //被杀时销毁自身
            {
              "$type": "RemoveUniqueModifier",
              "modifierName": "UNIQUE_Mimik_Electric_Fruit"
            }
          ],
          "onRemoved": [
            {
              "$type": "AddGlobalValue",
              "key": "_ABILITY_MIMIK_ELECTRIC_FRUIT",
              "value": -1,
              "useLimitRange": true,
              "maxValue": 1,
              "minValue": 0
            },
            {
              "$type": "RemoveUniqueModifier",
              "modifierName": "UNIQUE_Mimik_Electric_Fruit_Effect"
            }
          ]
        }
      }
    }
  },
