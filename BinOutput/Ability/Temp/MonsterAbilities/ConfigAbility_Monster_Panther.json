            },
            {
              "$type": "HideUIBillBoard",
              "hide": true
            },
            {
              "$type": "KillSelf",
              "banDrop": true,
              "banExp": true,
              "killSelfType": "Disappear",
              "duration": 2
            }
          ]
        }
      },
      "abilitySpecials": {
        "FleeTimer": 3,
        "RandomTimer": 7.5
      }
