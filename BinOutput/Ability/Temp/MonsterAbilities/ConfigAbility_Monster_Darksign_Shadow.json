              "$type": "SetGlobalValue",
              "key": "_LIFEEFF_SWITCH",
              "value": 0
            }
          ],
          "onRemoved": [
            {
              "$type": "SetGlobalValue",
              "key": "_LIFEEFF_SWITCH",
              "value": 1
            }
          ],
          "onReconnect": [
