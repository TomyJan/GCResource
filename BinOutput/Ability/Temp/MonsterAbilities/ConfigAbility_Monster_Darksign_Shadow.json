        "Monster_Darksign_Shadow_PowerAttackTimer": {
          "duration": 30,
          "onAdded": [
            {
              "$type": "UseSkillEliteSet",
              "skillEliteSetID": 1,
              "canBeHandledOnRecover": true
            }
