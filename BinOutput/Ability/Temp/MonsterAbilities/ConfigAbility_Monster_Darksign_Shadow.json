            {
              "$type": "ApplyModifier",
              "modifierName": "Monster_Darksign_Shadow_ShowReminder_Delay"
            },
            {
              "$type": "ApplyModifier",
              "modifierName": "Monster_Darksign_Shadow_WeakTimer"
            }
