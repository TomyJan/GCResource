  {
    "Default": {
      //重置后跳技能CD
      "$type": "ConfigAbility",
      "abilityName": "Human_Male_Pole_ResetBackSprintCD",
      "abilityMixins": [
        {
          "$type": "DoActionByStateIDMixin",
          "stateIDs": ["GroupAttack01", "GroupAttack02"],
          "exitActions": [
            {
              "$type": "ReTriggerAISkillInitialCD",
              "skillIDs": [401]
            }
          ]
        }
      ]
    }
  },
