          "setPoint": true,
          "toPos": {
            "$type": "ConfigBornByGlobalValue",
            "positionKey": "_ABILITY_Millelith_Target_Pos",
            "offset": {
              "x": 0.0,
              "y": 0.0,
              "z": 4.0
            },
            "direction": {
              "$type": "ConfigDirectionByEntity",
              "angleOffset": {
                "x": 0.0,
                "y": 30.0, //角度偏移
                "z": 0.0
              }
            }
          }
        },
        {
          "$type": "SetPoseBool",
          "boolID": "Surround",
          "value": true,
          "target": "Self"
        }
      ]
    }
  },
  {
    "Default": {
      //包围位置记录清零
