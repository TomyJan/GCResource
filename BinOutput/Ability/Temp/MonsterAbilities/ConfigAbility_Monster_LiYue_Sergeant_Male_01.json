  //
  //  OrderedState
  //
  ////////////////////////////////////////////////////////////////
  {
    "Default": {
      //进入得令Pose
      "$type": "ConfigAbility",
      "abilityName": "Monster_Millelith_TurnToOrdered",
      "abilityMixins": [
        {
          "$type": "DoActionByStateIDMixin",
          "stateIDs": ["TurnToOrdered"],
          "enterActions": [
            {
              "$type": "ApplyModifier",
              "modifierName": "UNIQUE_OrderedState"
            }
          ]
        }
      ],
      "modifiers": {
        "UNIQUE_OrderedState": {
          //一段时间后结束得令状态
          "isUnique": true,
          "duration": 5.0,
          "onAdded": [
            {
              "$type": "SetPoseBool",
              "boolID": "Ordered",
              "value": true,
              "target": "Self"
            }
          ],
          "onRemoved": [
            {
              "$type": "SetPoseBool",
              "boolID": "Ordered",
              "value": false,
              "target": "Self"
            },
            {
              "$type": "ReTriggerAISkillInitialCD",
              "skillIDs": [201, 202]
            }
          ]
        }
      }
    }
  },
  {
    "Default": {
      //结束得令Pose
      "$type": "ConfigAbility",
      "abilityName": "Monster_Millelith_OutOfOrdered",
      "onAbilityStart": [
        {
          "$type": "RemoveUniqueModifier",
          "modifierName": "UNIQUE_OrderedState"
        }
      ]
