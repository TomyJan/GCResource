  {
    "Default": {
      //切换Pose，进入包围战术
      "$type": "ConfigAbility",
      "abilityName": "Human_Male_Pole_TurnToSurround",
      "abilityMixins": [
        {
          "$type": "DoActionByStateIDMixin",
