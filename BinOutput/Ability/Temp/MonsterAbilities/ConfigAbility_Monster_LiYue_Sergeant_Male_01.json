  {
    "Default": {
      //切换Pose，进入等待；等待结束时，切回正常战斗Pose
      "$type": "ConfigAbility",
      "abilityName": "Human_Male_Pole_WaitState",
      "abilityMixins": [
        {
          "$type": "DoActionByStateIDMixin",
          "stateIDs": ["Wait"],
          "enterActions": [
            {
              "$type": "TriggerAbility",
              "abilityName": "Human_Male_Pole_ResetSurroundPosition"
            },
            {
              "$type": "SetPoseBool",
              "boolID": "Wait",
              "value": true,
              "target": "Self"
            },
            {
              "$type": "SetPoseBool",
              "boolID": "Surround",
              "value": false,
              "target": "Self"
            },
            {
              "$type": "ApplyModifier",
              "modifierName": "WaitTimer"
            },
            {
              //通知周围的队长自己到达包围位置
              "$type": "AddGlobalValue",
              "key": "_ABILITY_Count_WaitingSoldiers",
              "value": 1,
              "useLimitRange": true,
              "maxValue": 4,
              "minValue": 0,
              "otherTargets": {
                "$type": "SelectTargetsByShape",
                "shapeName": "CircleR25",
                "centerBasedOn": "Self",
                "campTargetType": "AllExceptSelf",
                "campBasedOn": "Self"
              },
              "targetPredicates": [
                {
                  "$type": "ByEntityTypes",
                  "entityTypes": ["Monster"]
                },
                {
                  "$type": "ByHasFeatureTag",
                  "featureTagIDs": [4007]
                },
                {
                  "$type": "ByHasFeatureTag",
                  "featureTagIDs": [4903]
                }
              ]
            }
          ]
        }
      ],
      "modifiers": {
        "WaitTimer": {
          //最多等待5秒，超时切回Pose
          "duration": 5.0,
          "onRemoved": [
            {
              "$type": "SetPoseBool",
              "boolID": "Wait",
              "value": false,
              "target": "Self"
            }
          ]
        }
      }
    }
  },

