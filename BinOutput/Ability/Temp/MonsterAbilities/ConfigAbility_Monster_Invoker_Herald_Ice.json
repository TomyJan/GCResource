          "$type": "CreateGadget",
          "gadgetID": 42202004,
          "campID": 4001,
          "lifeByOwnerIsAlive": true,
          "target": "SelfAttackTarget",
          "born": {
            "$type": "ConfigBornByPredicatePoint",
