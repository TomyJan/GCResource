  // -----------------------------------------------
  //
  //				Debuff技能侦听功能
  //
  //		部分技能命中后会使目标的体力减少
  //
  //		只有带有Tag为【 Invoker_Herald_Ice_Special 】的攻击才会使体力减少
  //
  // -----------------------------------------------
  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "Invoker_Herald_Ice_Special_Debuff_Monitor",
      "onAdded": [
        {
          "$type": "ApplyModifier",
          "modifierName": "Special_Debuff_Monitor"
        }
      ],
      "modifiers": {
        "Special_Debuff_Monitor": {
          "onAttackLanded": [
            {
              "$type": "Predicated",
              "target": "Target",
              "targetPredicates": [
                {
                  "$type": "ByAttackTags",
                  "attackTags": ["Invoker_Herald_Ice_Special"]
                },
                {
                  "$type": "ByEntityTypes",
                  "entityTypes": ["Avatar"]
                }
              ],
              "successActions": [
                {
                  "$type": "DeductStamina",
                  "target": "Target",
                  "value": 100
                }
              ]
            }
          ]
        }
      }
    }
  },
  // --------------------------------------------------
  //
  //            霜凝爆发 Hoarfrost Blast
  //
  //		用于技能的二段击地强攻击
  //
  // 		如果进入神降状态，则额外为自己身边附上冰霜领域效果
  //		持续对进入范围内的角色造成伤害
  //
  // --------------------------------------------------
  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "Invoker_Herald_Ice_Hoarfrost_Area",
      "onAbilityStart": [
        // ----------------------------
        //         攻击效果
        // 用SelectTargetsByShape实现
        // 所以这个技能不会被闪避技能躲开
        // 用跳跃的方式可以避开
        // ----------------------------
        {
          "$type": "ApplyModifier",
          "otherTargets": {
            "$type": "SelectTargetsByShape",
            "shapeName": "CircleInvokerHeraldIceHitGround",
            "centerBasedOn": "Self",
            "campTargetType": "Enemy",
            "campBasedOn": "Self"
          },
          "modifierName": "Invoker_Herald_Ice_Blast_Damage"
        },

        // 	"$type": "ActCameraShake",     看看需不需要做震屏

        // 为自身添加冰霜领域效果
        {
          "predicates": [
            {
              "$type": "ByTargetGlobalValue",
              "key": "_MONSTER_INVOKER_HERALD_ICE_SHIELD_ADDED",
              "compareType": "Equal",
              "value": 1
            }
          ],
          "$type": "ApplyModifier",
          "modifierName": "Hoarfrost_Area_Halo"
        }
      ],
      "modifiers": {
        // 砸地的无法闪避躲开的伤害效果
        "Invoker_Herald_Ice_Blast_Damage": {
          "duration": 1.0,
          "stacking": "Unique",
          "onAdded": [
            {
              "$type": "DamageByAttackValue",
              "attacker": "Caster",
              "attackInfo": {
                "attackProperty": {
                  "damagePercentage": 2.0,
                  "strikeType": "Blunt",
                  "elementType": "Ice",
                  "elementDurability": 25,
                  "enBreak": 200
                },
                "hitPattern": {
                  "onHitEffectName": "default",
                  "hitImpulseType": "LV7",
                  "hitHaltTime": 0,
                  "hitHaltTimeScale": 0
                }
              }
            }
          ]
        },
        // 冰霜领域  (光环)
        "Hoarfrost_Area_Halo": {
          "duration": 6,
          "thinkInterval": 0.33,
          "onAdded": [
            // 冰霜领域的特效
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_Monster_Invoker_Herald_Ice_Hoarfrost_Area_Temp"
            }
          ],
          "onThinkInterval": [
            // 每跳造成一次伤害
            {
              "$type": "ApplyModifier",
              "otherTargets": {
                "$type": "SelectTargetsByShape",
                "shapeName": "CircleR6H3",
                "centerBasedOn": "Self",
                "campTargetType": "Enemy",
                "campBasedOn": "Self"
              },
              "modifierName": "Hoarfrost_Area_Halo_Damage"
            }
          ]
        },
        // 无法闪避躲开的光环的伤害效果
        "Hoarfrost_Area_Halo_Damage": {
          "duration": 0.1,
          "stacking": "Unique",
          "onAdded": [
            {
              "$type": "DamageByAttackValue",
              "attacker": "Caster",
              "attackInfo": {
                "attackProperty": {
                  "damagePercentage": 0.06,
                  "elementType": "Ice",
                  "elementDurability": 5,
                  "enBreak": 0.0
                },
                "hitPattern": {
                  // "attenuationTag": "Group1",
                  // "attenuationGroup": "default",
                  "hitImpulseType": "LV1",
                  "hitHaltTime": 0.0,
                  "hitHaltTimeScale": 0.0
                }
              }
            }
          ]
        }
      }
    }
  },
