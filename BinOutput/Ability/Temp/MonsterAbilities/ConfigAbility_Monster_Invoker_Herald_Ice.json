              "$type": "DoBlink"
            }
          ]
        },
        {
          "$type": "FireEffect",
          "effectPattern": "Eff_Monster_Invoker_Herald_Ice_Blink_Hint_Temp",
          "born": {
            "$type": "ConfigBornBySelf",
            "offset": {
