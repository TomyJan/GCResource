                  "hitPattern": {
                    "onHitEffectName": "none",
                    "hitImpulseType": "LV3",
                    "hitHaltTime": 0.0,
                    "hitHaltTimeScale": 0.0
                  }
                }
              }
            },
            {
              "$type": "KillSelf"
            }
          ]
        }
      }
    }
