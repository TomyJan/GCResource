  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "Monster_Mimik_Fel_Watcher",
      "onAdded": [
        //Init watcher
        {
          "$type": "SetGlobalValue",
          "key": "_SUMMONEDFRUIT_NOT_DESTROYED_",
          "value": 0
        },
        {
          "$type": "SetGlobalValue",
          "key": "_ULTIMATESHOOT_BROKEN_",
          "value": 0
        },
        {
          //To player
          "$type": "SetGlobalValue",
          "key": "_DIGOUTATTACK_HIT_",
          "value": 0
        },
        {
          //To player
          "$type": "SetGlobalValue",
          "key": "_SUMMONEDFRUIT_FALLINGATTACK_HIT_",
          "value": 0
        },
        {
          "$type": "SetGlobalValue",
          "key": "_ENERGYCHIT_NOT_PICKED_",
          "value": 0
        },
        {
          //To player
          "$type": "SetGlobalValue",
          "key": "_IS_IN_ULTIMATESHOOT_",
          "value": 0
        },
        {
          "$type": "ApplyModifier",
          "modifierName": "DigOutAttack_WatcherHandler"
        }
      ],
      "abilityMixins": [
        {
          "$type": "AttachToStateIDMixin",
          "stateIDs": [
            "AbsorbFruit",
            "AbsorbFruit_Loop",
            "ForwardShoot02",
            "UpperShoot01",
            "SnowBall_BS",
            "SnowBall_Shoot",
            "SnowBall_AS"
          ],
          "modifierName": "UltimateShoot_WatcherHandler"
        },
        {
          "$type": "ServerFinishWatcherMixin",
          "watcherId": 12009202,
          "predicates": [
            {
              "$type": "ByTargetGlobalValue",
              "target": "Self",
              "key": "_ULTIMATESHOOT_BROKEN_",
              "value": 1,
              "compareType": "Equal"
            },
            {
              "$type": "ByTargetGlobalValue",
              "key": "_MIMIK_GIANTSTATESHIELD_REDUCED_",
              "target": "Self",
              "value": 0,
              "compareType": "NoneOrEqual"
            }
          ]
        }
      ],
      "onKill": [
        {
          "$type": "DoWatcherSystemAction",
          "watcherId": 12009201,
          "inThreatListOnly": true,
          "predicates": [
            {
              "$type": "ByTargetGlobalValue",
              "key": "_SUMMONEDFRUIT_NOT_DESTROYED_",
              "value": 0,
              "compareType": "Equal"
            },
            {
              "$type": "ByTargetGlobalValue",
              "key": "_MIMIK_GIANTSTATESHIELD_REDUCED_",
              "value": 0,
              "compareType": "NoneOrEqual"
            }
          ]
        },
        {
          "$type": "DoWatcherSystemAction",
          "watcherId": 12009203,
          "inThreatListOnly": true,
          "predicates": [
            {
              "$type": "ByTargetGlobalValue",
              "key": "_DIGOUTATTACK_HIT_",
              "value": 0,
              "compareType": "Equal"
            },
            {
              "$type": "ByTargetGlobalValue",
              "key": "_MIMIK_GIANTSTATESHIELD_REDUCED_",
              "value": 0,
              "compareType": "NoneOrEqual"
            }
          ]
        },
        {
          "$type": "DoWatcherSystemAction",
          "watcherId": 12009204,
          "inThreatListOnly": true,
          "predicates": [
            {
              "$type": "ByTargetGlobalValue",
              "key": "_SUMMONEDFRUIT_FALLINGATTACK_HIT_",
              "value": 0,
              "compareType": "Equal"
            },
            {
              "$type": "ByTargetGlobalValue",
              "key": "_MIMIK_GIANTSTATESHIELD_REDUCED_",
              "value": 0,
              "compareType": "NoneOrEqual"
            }
          ]
        },
        {
          "$type": "DoWatcherSystemAction",
          "watcherId": 12009206,
          "inThreatListOnly": true,
          "predicates": [
            {
              "$type": "ByTargetGlobalValue",
              "key": "_ENERGYCHIT_NOT_PICKED_",
              "value": 0,
              "compareType": "Equal"
            },
            {
              "$type": "ByTargetGlobalValue",
              "key": "_MIMIK_GIANTSTATESHIELD_REDUCED_",
              "value": 0,
              "compareType": "NoneOrEqual"
            }
          ]
        }
      ],
      "modifiers": {
        "UltimateShoot_WatcherHandler": {
          "onAdded": [
            {
              "$type": "SetGlobalValue",
              "key": "_IS_IN_ULTIMATESHOOT_",
              "value": 1
            }
          ],
          "onRemoved": [
            {
              "$type": "SetGlobalValue",
              "key": "_IS_IN_ULTIMATESHOOT_",
              "value": 0
            }
          ]
        },
        "DigOutAttack_WatcherHandler": {
          "onAttackLanded": [
            {
              "$type": "Predicated",
              "target": "Target",
              "targetPredicates": [
                {
                  "$type": "ByEntityTypes",
                  "entityTypes": ["Avatar"]
                },
                {
                  "$type": "ByAttackTags",
                  "attackTags": ["Mimik_Fel_DigOutAttack"]
                }
              ],
              "successActions": [
                {
                  "$type": "SetGlobalValue",
                  "key": "_DIGOUTATTACK_HIT_",
                  "value": 1
                },
                {
                  "$type": "ApplyModifier",
                  "modifierName": "DigOutAttack_WatcherSLCHandler",
                  "target": "Target"
                }
              ]
            }
          ]
        },
        "DigOutAttack_WatcherSLCHandler": {
          "duration": 2.0,
          "onAdded": [
            //DoSLC
            {
              "$type": "ServerLuaCall",
              "luaCallType": "SpecificGroup",
              "funcName": "SLC_Watcher_OnHitBy_SkillA",
              "CallParamList": [133002066]
            }
          ]
        }
      }
    }
