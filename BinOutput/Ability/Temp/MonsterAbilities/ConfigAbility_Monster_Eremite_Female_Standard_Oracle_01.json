        //强化次数
        {
          "$type": "SetGlobalValue",
          "key": "_MONSTER_EREMITE_IMPROVED_COUNT_",
          "minValue": 0,
          "maxValue": 99,
          "useLimitRange": true,
          "value": 0
        },
        //是否入战
        {
          "$type": "SetGlobalValue",
          "key": "_MONSTER_EREMITE_ISINFIGHT_",
          "minValue": 0,
          "maxValue": 1,
          "useLimitRange": true,
          "value": 0
        },
        //虚弱状态
        {
          "$type": "SetGlobalValue",
          "key": "_MONSTER_EREMITE_WEAK_",
          "minValue": 0,
          "maxValue": 1,
          "useLimitRange": true,
          "value": 0
        },
        //是否处于重生状态
        {
          "$type": "SetGlobalValue",
          "key": "_MONSTER_EREMITE_RETURNTOBORN_",
          "value": 0,
          "useLimitRange": true,
          "maxValue": 1,
          "minValue": 0
        }
      ],
      "modifiers": {
        "Eremite_OutFightState": {
          "onAdded": [
            {
              "$type": "AddGlobalValue",
              "key": "_MONSTER_EREMITE_ISINFIGHT_",
