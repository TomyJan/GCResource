  ////////////////////////////////////////////////////////////////
  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "Monster_Oceanid_Falcon_ElementState",
      "onAdded": [
        {
          "$type": "ApplyModifier",
          "modifierName": "UNIQUE_ElementState"
        },
        {
          "$type": "AttachAbilityStateResistance",
          "resistanceBuffDebuffs": ["Struggle", "ElementWet"]
        },
        {
          "$type": "AttachEffect",
          "effectPattern": "Eff_Monster_Oceanid_Falcon_Eye"
        }
      ],
      "modifiers": {
        "UNIQUE_ElementState": {
          "isUnique": true,
          "elementType": "Water",
          "elementDurability": 100,
          "isElementDurabilityMutable": false
        }
      }
    }
  },
  ////////////////////////////////////////////////////////////////
  //
  //  Dive
  //
  ////////////////////////////////////////////////////////////////
  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "Monster_Oceanid_Falcon_Dive",
      "abilityMixins": [
        {
          "$type": "AttachToNormalizedTimeMixin",
          "stateID": "Dive",
          "normalizeStart": 0.375,
          "normalizeEnd": 0.75,
          "modifierName": "DiveState"
        },
        {
          "$type": "SteerAttackMixin",
          "steerStateIDs": ["Dive"],
          "startNormalizedTime": 0.0,
          "endNormalizedTime": 0.32,
          "angularSpeed": 180,
          "attackTrigger": "",
          "attackDistance": 1.5
        }
      ],
      "modifiers": {
        "DiveState": {
          "onAdded": [
            {
              "$type": "EnablePushColliderName",
              "pushColliderNames": ["PushCollider"],
              "setEnable": false
            }
          ],
          "onRemoved": [
            {
              "$type": "EnablePushColliderName",
              "pushColliderNames": ["PushCollider"],
              "setEnable": true
            }
          ]
        }
      }
    }
  },
  ////////////////////////////////////////////////////////////////
  //
  //  Steer
  //
  ////////////////////////////////////////////////////////////////
  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "Monster_Oceanid_Falcon_Steer",
      "abilityMixins": [
        {
          "$type": "SteerAttackMixin",
          "steerStateIDs": ["NormalIdle"],
          "startNormalizedTime": 0.0,
          "endNormalizedTime": 1.0,
          "angularSpeed": 180,
          "attackTrigger": "",
          "attackDistance": 1.5
        }
      ]
    }
  },
  ////////////////////////////////////////////////////////////////
  //
  //  Die
  //
  ////////////////////////////////////////////////////////////////
  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "Monster_Oceanid_Falcon_Die",
      "onAbilityStart": [
        {
          "$type": "FireEffect",
          "effectPattern": "Eff_Monster_Oceanid_AnimalDie",
          "born": {
            "$type": "ConfigBornByAttachPoint",
            "attachPointName": "RootNode",
            "offset": {
              "x": 0.0,
              "y": 0.0,
              "z": 0.0
            },
            "direction": {
              "$type": "ConfigDirectionByEntity"
            }
          }
        }
      ]
    }
  },
  ////////////////////////////////////////////////////////////////
  //
  //  HealHp
  //
  ////////////////////////////////////////////////////////////////
  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "Monster_Oceanid_Falcon_HealHp_Challenge",
      "abilityMixins": [
        {
          "$type": "AttachModifierToSelfGlobalValueMixin",
          "globalValueKey": "_HEALHP_STATE_",
          "defaultGlobalValueOnCreate": 1,
          "valueSteps": [-0.5, 0.5, 1.5],
          "removeAppliedModifier": true,
          "modifierNameSteps": ["Empty", "UNIQUE_Falcon_Water_HealHp"]
        }
      ],
      "modifiers": {
        "UNIQUE_Falcon_Water_HealHp": {
          "isUnique": true,
          "thinkInterval": 1.5,
          "onThinkInterval": [
            {
              "$type": "Predicated",
              "targetPredicates": [
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "SGV_MONSTERAFFIX_OCEANID_02_ANIMALHEAL_UP",
                  "value": 1,
                  "compareType": "Equal"
                }
              ],
              "successActions": [
                {
                  "$type": "HealHP",
                  "amountByCasterMaxHPRatio": "%HealHPHigh"
                },
                {
                  "$type": "FireEffect",
                  "effectPattern": "Eff_Monster_Oceanid_Falcon_Heal"
                }
              ],
              "failActions": []
            }
          ],
          "onBeingHit": [
            {
              "$type": "Predicated",
              "targetPredicates": [
                {
                  "$type": "ByTargetGlobalValue",
                  "key": "SGV_MONSTERAFFIX_OCEANID_02_ANIMALHEAL_UP",
                  "value": 1,
                  "compareType": "Equal"
                }
              ],
              "successActions": [
                {
                  "$type": "ApplyModifier",
                  "modifierName": "HealHP_Pause_AffixUp"
                }
              ],
              "failActions": [
                {
                  "$type": "ApplyModifier",
                  "modifierName": "HealHP_Pause"
                }
              ]
            }
          ]
        },
        "HealHP_Pause": {
          "duration": 3,
          "onAdded": [
            {
              "$type": "AddGlobalValue",
              "key": "_HEALHP_STATE_",
              "value": 0,
              "useLimitRange": true,
              "maxValue": 0,
              "minValue": 0
            }
          ],
          "onRemoved": [
            {
              "$type": "AddGlobalValue",
              "key": "_HEALHP_STATE_",
              "value": 1,
              "useLimitRange": true,
              "maxValue": 1,
              "minValue": 1
            }
          ]
        },
        "HealHP_Pause_AffixUp": {
          "duration": 1.5,
          "onAdded": [
            {
              "$type": "AddGlobalValue",
              "key": "_HEALHP_STATE_",
              "value": 0,
              "useLimitRange": true,
              "maxValue": 0,
              "minValue": 0
            }
          ],
          "onRemoved": [
            {
              "$type": "AddGlobalValue",
              "key": "_HEALHP_STATE_",
              "value": 1,
              "useLimitRange": true,
              "maxValue": 1,
              "minValue": 1
            }
          ]
        },
        "Empty": {}
      },
      "abilitySpecials": {
        "HealHPHigh": 0.15
      }
    }
  }
]
