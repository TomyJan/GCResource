            "CircleAtk_Loop",
            "CircleAtk_CoreOutLoop"
          ],
          "modifierName": "CoreAttackLoopEffect"
        }
      ],
      "modifiers": {
        "UNIQUE_CoreStandyEffect": {
          "isUnique": true,
          "onAdded": [
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_Monster_Effigy_Grass_CoreStandby"
            },
            {
              "$type": "SetGlobalValue",
              "key": "_EFFIGY_ISRETURNINGTOBURN_",
              "value": 0
            }
          ]
        },
        "UNIQUE_EnergyCubeEffect": {
          "isUnique": true,
          "modifierMixins": [
            {
              "$type": "AttachToPoseIDMixin",
              "poseIDs": [101, 201],
              "modifierName": "EnergyCoreStandby"
            }
          ],
          "onAdded": [
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_Monster_Effigy_Grass_EnergyCore"
            }
          ]
        },
        "UNIQUE_CoreStandyFreeEffect": {
          "isUnique": true,
          "onAdded": [
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_Monster_Effigy_Grass_CoreStandbyFree"
            },
            {
              "$type": "SetAnimatorTrigger",
              "triggerID": "ToShaderState1"
            }
          ]
        },
        "CoreAttackLoopEffect": {
          "onAdded": [
            {
              "$type": "AttachEffect",
              "effectPattern": "Eff_Monster_Effigy_Grass_CoreAttackLoop"
            }
          ]
        },
        "EnergyCoreStandby": {
          "onAdded": [
            {
              "$type": "SendEffectTrigger",
              "parameter": "Standby",
              "effectPattern": "Eff_Monster_Effigy_Grass_EnergyCore",
