  //【三期新增】隐身诱饵Gadget创建
  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "ActivityAbility_HideSeek_Seeker_InvisibleBait_Gadget",
      "isDynamicAbility": true,
      "onAdded": [
        {
          "$type": "AttachModifier",
          "modifierName": "UNIQUE_ActivityAbility_HideSeek_Hider_PlaceInvisibleBait_Team"
        },
        {
          //标记用于判断能否创建诱饵-诱饵通用
          "$type": "SetGlobalValue",
          "key": "HideAndSeek_Seeker_Bait_Gadget",
          "value": 1
        },
        {
          //标记用于判断能否创建诱饵-隐身诱饵专用
          "$type": "SetGlobalValue",
          "key": "HideAndSeek_Seeker_InvisibleBait_Gadget",
          "value": 1
        },
        {
          "$type": "AttachModifier",
          "modifierName": "UNIQUE_HideSeek_InvisibleBait_Timer"
        },
        {
          "$type": "AttachModifier",
          "modifierName": "UNIQUE_InvisibleBait_AttachEffect"
        }
      ],
      "abilityMixins": [
        {
          "$type": "AttachModifierToSelfGlobalValueNoInitMixin",
          "globalValueKey": "SGV_HideAndSeek_PlayerState_Play", //玩家阵营发生变化的时候重新确认要不要下掉特效（没有从无到有的情况）
