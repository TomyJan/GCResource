        "MAX_mutehurtbylevelentity_LV": 10, //  破碎物叠抗性最大层数
        "MAX_defencebylevelentity_LV": 10, //  破碎物叠防御最大层数
        "MAX_chargebylevelentity_LV": 10, //  破碎物叠充能最大层数
        "MAX_masterybygear_LV": 20,
        "MAX_reviveroombuff_LV": 2, //  回复房间强力BUFF最大叠层
