        {
          "$type": "ApplyModifier",
          "modifierName": "AttackByLevelEntity_Manager"
        },

        {
          "$type": "SetGlobalValueToOverrideMap",
          "globalValueKey": "SGV_ROGUE_CHARGEBYLEVELENTITY",
          "overrideMapKey": "chargebylevelentity_LV"
        },
