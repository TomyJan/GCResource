                  "targetPredicates": [
                    {
                      "$type": "ByTargetGlobalValue",
                      "target": "Team",
                      "key": "SGV_ROGUE_MUTEHURTBYLEVELENTITY",
                      "value": "%MAX_mutehurtbylevelentity_LV",
