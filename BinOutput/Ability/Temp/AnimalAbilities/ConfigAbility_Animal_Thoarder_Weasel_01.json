  //
  //  Alert
  //
  ////////////////////////////////////////////////////////////////
  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "Animal_Thoarder_Weasel_Alert",
      "onAbilityStart": [
        {
          "$type": "RemoveUniqueModifier",
          "modifierName": "UNIQUE_Invisible",
          "doOffStage": true
        },
        {
          "$type": "AttachEffect",
          "effectPattern": "Eff_Animal_Thoarder_Weasel_Jump",
          "born": {
            "$type": "ConfigBornBySelf",
            "onGround": true
          },
          "doOffStage": true
        },
        {
          "$type": "Predicated",
          "otherTargets": {
            "$type": "SelectTargetsBySelfGroup",
            "operation": "OR",
            "value": 2,
            "compareType": "Equal"
          },
          "targetPredicates": [
            {
              "$type": "ByTargetGlobalValue",
              "target": "Target",
              "key": "_IS_MOLE_HOLE",
              "value": 1,
              "compareType": "Equal"
            }
          ],
          "successActions": [
            {
              "$type": "ApplyModifier",
              "modifierName": "SetSelfAttackTarget",
              "target": "Target"
            }
          ]
        }
      ],
      "onAdded": [
        {
          "$type": "AddGlobalValue",
          "key": "_IS_WEASEL",
          "value": 1,
          "useLimitRange": true,
          "maxValue": 1,
          "minValue": 0
        }
      ],
      "modifiers": {
        "SetSelfAttackTarget": {
          "onAdded": [
            {
              "$type": "SetSelfAttackTarget",
              "target": "Caster"
            }
          ]
        }
      }
    }
  },
