  ////////////////////////////////////////////////////////////////
  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "Animal_Squirrel_Random",
      "abilityMixins": [
        {
          "$type": "AttachToAnimatorStateIDMixin",
