      "modifiers": {
        "UNIQUE_EnergyDisk_Lv1_NoShieldAtkDown_Handler_AS": {
          "isUnique": true,
          "onAdded": [
            {
              "$type": "ApplyModifier",
              "modifierName": "UNIQUE_EnergyDisk_Lv1_NoShieldAtkDown_Handler",
              "target": "Self",
              "doOffStage": true
            }
          ],
          "modifierMixins": [
            {
              "$type": "AttachModifierToSelfGlobalValueMixin",
              "globalValueKey": "_Ability_EnergyDisk_NoShieldAtkDown_Check",
              "removeAppliedModifier": true,
              "valueSteps": [-0.5, 0.5, 1.5],
              "modifierNameSteps": [
                "UNIQUE_EnergyDisk_Lv1_NoShieldAtkDown_HasDebuff",
                "UNIQUE_EnergyDisk_Lv1_NoShieldAtkDown_NoDebuff"
              ]
            }
          ]
        },
        //通过Modifier上的Interval间隔检测自身是否有护盾进行属性增减
        "UNIQUE_EnergyDisk_Lv1_NoShieldAtkDown_Handler": {
          "isUnique": true,
          "thinkInterval": 1,
          "onThinkInterval": [
            {
              "$type": "Predicated",
              "doOffStage": true,
              "target": "Self",
              "targetPredicates": [
                {
                  "$type": "ByHasShield",
                  "type": "All"
                }
              ],
              "successActions": [
                {
                  "$type": "AddGlobalValue",
                  "doOffStage": true,
                  "key": "_Ability_EnergyDisk_NoShieldAtkDown_Check",
                  "useLimitRange": true,
                  "maxValue": 1,
                  "minValue": 1
                }
              ],
              "failActions": [
                {
                  "$type": "AddGlobalValue",
                  "doOffStage": true,
                  "key": "_Ability_EnergyDisk_NoShieldAtkDown_Check",
                  "useLimitRange": true,
                  "maxValue": 0,
                  "minValue": 0
                }
              ]
            }
          ]
        },
        "UNIQUE_EnergyDisk_Lv1_NoShieldAtkDown_NoDebuff": {
          "isUnique": true,
          "onAdded": [
            {
              "$type": "RemoveUniqueModifier",
              "doOffStage": true,
              "modifierName": "UNIQUE_EffigyChallenge_NoShieldAtkDown_HasDebuff"
            }
          ]
        },
        "UNIQUE_EnergyDisk_Lv1_NoShieldAtkDown_HasDebuff": {
          "isUnique": true,
          "isLimitedProperties": true,
          "onAdded": [
            {
              "$type": "AttachEffect",
