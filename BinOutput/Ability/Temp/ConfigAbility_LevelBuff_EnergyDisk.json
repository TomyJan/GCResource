  //血量低于70%时造成的伤害降低30%，低于35%时造成的伤害降低60%
  {
    "Default": {
      "$type": "ConfigAbility",
      "abilityName": "LevelEntity_EnergyDisk_Lv2_AtkDownLowHP",
      "isDynamicAbility": true,
      "abilityMixins": [
        {
          "$type": "CurLocalAvatarMixin",
          "modifierName": "UNIQUE_EnergyDisk_Lv2_AtkDownLowHP"
        }
      ],
      "onAdded": [
        {
          "$type": "ApplyModifier",
          "modifierName": "UNIQUE_EnergyDisk_Lv2_AtkDownLowHP"
        }
      ],
      "modifiers": {
        "UNIQUE_EnergyDisk_Lv2_AtkDownLowHP": {
          "isUnique": true,
          "onAdded": [
            {
              "$type": "ApplyModifier",
              "target": "Self",
              "modifierName": "UNIQUE_EnergyDisk_AtkDownLowHP_Sub1", //在35%-70%血量之间
              "predicates": [
                {
                  "$type": "ByTargetHPRatio",
                  "HPRatio": "%HpRate_P1",
