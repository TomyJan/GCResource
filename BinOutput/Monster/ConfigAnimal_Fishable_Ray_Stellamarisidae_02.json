    "threat": {
      "feelThreatGrow": 1000,
      "threatBroadcastRange": 5.0
    },
    "fly": {
      "enable": true,
      "floatingPoses": [101, 102, 201]
    },
    "mixin": {
      "setControllerParameterOnBeHitByEntityType": [
        {
          "poseIDs": [1, 2, 101, 201, 401],
          "settings": [
            {
              "entityTypes": ["Bullet", "Avatar", "Field", "Gadget"],
              "onSuccess": {
                "setPoseBool": [
                  {
                    "boolID": "Flee_State",
                    "value": true
                  }
                ]
              },
              "onFail": {
                "setPoseBool": [
                  {
                    "boolID": "Flee_State",
                    "value": true
                  }
                ]
              }
            }
          ]
        }
      ]
    },
