      "checkInSurface": true,
      "enableCrashDamage": false
    },
    "timer": {
      "lifeInfinite": false,
      "lifeTime": 20.0 //太不准了
    },
    "move": {
      "$type": "ConfigRigidbodyMove",
      // "startCloseToGround":true,
      "velocityForce": {
        "includeForces": ["RetreatType", "RetreatAirType", "WindZoneType"]
