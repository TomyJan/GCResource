  ////////////////////////////////////////////////////////////////
  //
  //  大招扔出去的瓶子
  //
  ////////////////////////////////////////////////////////////////
  "Sucrose_DemiHurricane_Flusk": {
    "$type": "ConfigGadget",
    /*		"hasmodel": true,
	    "model":
	    {
	      "attachEffect": "Eff_Klee_ElementalArt_Mine",
	    },*/
    "common": {
      "effectAttachShape": {
        "$type": "ConfigEffectAttachBox",
        "emitFrom": "Volume",
        "scaleX": 1.0,
        "scaleY": 1.0,
        "scaleZ": 1.0
      },
      "scale": 1,
      "affectedByWorld": true,
      "checkInSurface": true,
      "mass": 5.0
    },
    //战斗参数
    "combat": {
      "property": {
        "useCreatorProperty": true,
        "weight": 50,
        "isInvincible": true,
        "isLockHP": true,
        "isGhostToAllied": true,
        "isGhostToEnemy": false,
        "combatLock": {
          "denyLockOn": true
        }
      }
    },
    "move": {
      "$type": "ConfigParabolaBulletMove",
      "speed": 10,
      "angleOffHor": 45,
      "gravityOfAcceleration": 30,
      "syncToRemote": true,
      /*			"updateAngle" : 
			{
				"$type": "ConfigBulletMoveAngleRotating",
				"angularVelocity":
				{
					"x": 0,
					"y": 0,
					"z": 0,
				},
			},*/
      /*			"correction" : 
			{
				"$type":"ConfigParabolaBulletMoveCorrection",
				"deviation": 1.0,
				"fixAngleOfVer": 15.0,
				"minSpeed": 5.0,
				"maxSpeed": 20.0,
			},*/
      "velocityForce": {
        "muteAll": true
      }
    },
    "timer": {
      "lifeInfinite": false, //如果未触发是否一致存在
      "lifeTime": 2.4 //存在时间
    },
    "gadget": {
      "$type": "ConfigBulletPattern",
      "collision": {
        "targetType": "None",
        "triggerType": "TriggerOnce"
        /*				"bornWithTriggerEnabled": false,*/
      },
      "triggerActions": [
        {
          "$type": "ConfigGadgetTriggerAbility",
          "abilityName": "Avatar_Sucrose_DemiHurricane_Flusk"
        }
      ],
      "triggerLifeOverActions": [
        {
          "$type": "ConfigGadgetTriggerAbility",
          "abilityName": "Avatar_Sucrose_DemiHurricane_Flusk"
        }
      ],
      "killByOther": true
    },
    "abilities": [
      {
        "abilityID": "DemiHurricane_Flusk",
        "abilityName": "Avatar_Sucrose_DemiHurricane_Flusk"
      }
    ]
  },
