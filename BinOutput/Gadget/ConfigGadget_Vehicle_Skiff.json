        "dieDisappearEffect": "Eff_Skiff_Normal_Die"
      }
    },
    "specialPoint": {
      "bulletAim": "BulletAim"
    },
    "gadget": {
      "$type": "ConfigGadgetPattern"
    },
    "stateLayers": {
      "defaultLayer": {
        "$type": "ConfigNormalStateLayer",
        "stateIDs": {
          "Idle": {
            "$type": "ConfigAvatarStateIDInfo",
            "animatorStates": {
              "Idle": []
            },
            "moveType": "normalMove",
            "canDoSkill": true
          },
          "Forward": {
            "$type": "ConfigAvatarStateIDInfo",
            "animatorStates": {
              "Forward": []
            },
            "moveType": "normalMove",
            "canDoSkill": true
          },
          "Backward": {
            "$type": "ConfigAvatarStateIDInfo",
            "animatorStates": {
              "Backward": []
            },
            "moveType": "normalMove",
            "canDoSkill": true
          },
          "Sprint": {
            "$type": "ConfigAvatarStateIDInfo",
            "animatorStates": {
              "Sprint": []
            },
            "moveType": "normalMove",
            "canDoSkill": true,
            "cameraParam": {
              "radiusSpringRatio": 0.6
            }
          }
        }
      }
    },
    "field": {
      "concernType": "AllAvatars",
      "shape": "CircleR4", //形状，不填为collider
      "checkInterval": 0.5, //触发间隔
      "triggerInfinite": true, //是否永远触发
      "lifeInfinite": true //如果未触发是否一致存在
    },
    "abilities": [
      {
        "abilityID": "Vehicle_Skiff_Normal_01",
