  // 2.3狗活动 烟雾发射器
  //
  ///////////////////////////////////////////////////////////
  "SceneObj_Area_Dq_Property_Prop_SmokeDevice_01": {
    "$type": "ConfigGadget",
    "hasModel": true,
    "field": {
      "concernType": "LocalAvatar",
      "shape": "CircleR2H2", //形状，不填为collider
      "checkInterval": 0.5, //触发间隔
      "triggerInfinite": true, //是否永远触发
      "lifeInfinite": true, //如果未触发是否一致存在
      "overwriteCampType": true,
      "campType": "SelfCamp"
    },
    "uiInteract": {
      "onTouch": "Default"
    },
    "intee": {
      "$type": "ConfigIntee",
      "triggerRadius": 2,
      "triggerHeight": 2
    },
    "gadgetAction": {
      "gadgetStateActionList": [
        //Default可交互
        {
          "$type": "ConfigGadgetStateEnableGadgetIntee",
          "enable": true,
          "gadgetState": "Default"
        },
        //GearStop不可交互
        {
          "$type": "ConfigGadgetStateEnableGadgetIntee",
          "enable": false,
          "gadgetState": "GearStop"
        },
        {
          "$type": "ConfigGadgetStateEnableGadgetIntee",
          "enable": false,
          "gadgetState": "GearStart"
        }
      ]
    },
    "combat": {
      "property": {
        "useCreatorProperty": false,
        "HP": 99999,
        "attack": 200,
        "defense": 0,
        "weight": 50,
        "isInvincible": false,
        "isLockHP": true,
        "endure": 99,
        "isGhostToAllied": true,
        "isGhostToEnemy": false,
        "canTriggerBullet": true
      },
      "combatLock": {
        "denyLockOn": false,
        "lockType": "Gadget_Lv1"
      },
      "beHit": {
        "hitBloodEffect": "",
        "hitAutoRedirect": false,
        "muteAllHit": false,
        "muteAllHitEffect": false,
        "muteAllHitText": true
      }
    },
    "abilities": [
      {
        "abilityID": "SceneObj_Area_Dq_Property_Prop_SmokeDevice_01",
        "abilityName": "SceneObj_Area_Dq_Property_Prop_SmokeDevice_01"
      }
    ]
  },
