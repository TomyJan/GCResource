          "$type": "ConfigGadgetStateEnableGadgetIntee",
          "enable": true,
          "predicate": {
            "$type": "ConfigPredicateByInteract",
            "isEnableInteract": true
          }
        }
      ]
    },
    "abilities": [
      {
        "abilityID": "Ability_01",
        "abilityName": "SceneObj_Gear_ElemForce_Base"
      }
    ]
  },
