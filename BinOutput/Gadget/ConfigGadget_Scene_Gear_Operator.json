        "attack": 200,
        "defense": 0,
        "weight": 50,
        "isInvincible": false,
        "isLockHP": true,
        "endure": 99,
        "isGhostToAllied": true,
        "isGhostToEnemy": true,
        "canTriggerBullet": true
      },
      "combatLock": {
        "denyLockOn": true,
        "lockType": "Gadget_Lv1"
      },
      "beHit": {
        "hitBloodEffect": "",
