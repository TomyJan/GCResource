        "muteAllHit": false,
        "muteAllHitEffect": false,
        "muteAllHitText": true
      }
    },
    "gadgetAction": {
      "gadgetStateActionList": [
        {
          "$type": "ConfigGadgetStateSetAnimInt",
          "intID": "gadgetState",
          "syncState": true
        },

        {
          "$type": "ConfigGadgetStateSendEffectTrigger",
          "parameter": "gadgetState",
          "type": "Int",
          "value": 0,
          "effectPattern": "Eff_Gear_ElementDungeon_Operator",
          "gadgetState": "Default"
        },
        {
          "$type": "ConfigGadgetStateSendEffectTrigger",
          "parameter": "gadgetState",
          "type": "Int",
          "value": 202,
          "effectPattern": "Eff_Gear_ElementDungeon_Operator",
          "gadgetState": "GearStop"
        }
      ]
    },
    "abilities": [
      {
        "abilityID": "OperatorActive",

        "abilityName": "SceneObj_Gear_Operator_ElementDungeon_Operator"
      }
    ],
    "stateLayers": {
      "defaultLayer": {
        "$type": "ConfigNormalStateLayer",
        "stateIDs": {
          "All": {
            "$type": "ConfigNormalStateIDInfo",
            "animatorStates": {
              "Start": ["Audio_Gear_ElementDungeon_Operator_Start"],
              "Loop": [],
              "End": ["Audio_Gear_ElementDungeon_Operator_End"]
            }
          }
        }
      }
    },
    "audio": {
      "$type": "ConfigGadgetAudio",
      "animAudio": {
        "onTransitionOut": [
          {
            "currentStateName": "Loop",
            "audioEvent": {
              "text": "Stop_amb_dungeon_gear_elemDungeon_operator_loop"
            },
            "otherStateNames": [],
            "usage": "Any"
          }
        ],
        "onTransitionIn": [
          {
            "currentStateName": "Loop",
            "audioEvent": {
              "text": "Play_amb_dungeon_gear_elemDungeon_operator_loop"
            },
            "otherStateNames": [],
            "usage": "Any"
          }
        ]
      },
      "disableEvent": {
        "text": "Stop_amb_dungeon_gear_elemDungeon_operator"
      },
      "destroyEvent": {
        "text": "Stop_amb_dungeon_gear_elemDungeon_operator"
      }
    }
  },
