{
  ////////////////////////////////////////////////////////////////
  //
  //  Fungus_Un_Gamma_Water_BulletNormal
  //
  ////////////////////////////////////////////////////////////////
  "Fungus_Un_Gamma_Water_BulletNormal": {
    "$type": "ConfigGadget",
    "common": {
      "effectAttachShape": {
        "$type": "ConfigEffectAttachBox",
        "emitFrom": "Volume",
        "scaleX": 1.0,
        "scaleY": 1.0,
        "scaleZ": 0.4
      }
    },
    "combat": {
      "property": {
        "useCreatorProperty": true, // 是否继承创造者的数值
        "HP": 999999,
        "attack": 15.0,
        "defense": 999999,
        "isLockHP": true,
        "isGhostToAllied": false,
        "isGhostToEnemy": false,
        "canTriggerBullet": true
      },
      "combatLock": {
        "denyLockOn": true
      },
      "beHit": {
        "hitBloodEffect": "",
        "muteAllHit": true,
        "muteAllHitEffect": true,
        "muteAllHitText": true
      },
      "animEvents": {}
    },

    // 子弹线性移动
    "move": {
      "$type": "ConfigTrackBulletMove",
      "speed": 4.8,
      "anglerVelocity": 30,
      "destroyWhenTargetDie": false,
      "traceOnYAxis": true,
      "updateAngle": {
        "$type": "ConfigBulletMoveAngleByVelocity"
      },
      "canBornInWater": true
    },
    "timer": {
      "lifeInfinite": false, //如果未触发是否一致存在
      "lifeTime": 3.0 //存在时间
    },
    // 子弹参数
    "gadget": {
      "$type": "ConfigBulletPattern",
      "collision": {
        "targetType": "All",
        "triggerType": "TriggerOnce",
        "ignoreScene": false
      },
      "triggerActions": [
        {
          "$type": "ConfigGadgetTriggerAbility",
          "abilityName": "Fungus_Un_Gamma_Water_BulletNormal_Surround"
        }
      ],
      "triggerLifeOverActions": [
        {
          "$type": "ConfigGadgetTriggerAbility",
          "abilityName": "Fungus_Un_Gamma_Water_BulletNormal_Explode"
        }
      ],
      "killByOther": true
    },
    "abilities": [
      {
        "abilityID": "Bubble_Loop",
        "abilityName": "Fungus_Un_Gamma_Water_BulletNormal_Loop"
      },
      {
        "abilityID": "Bubble_Explode",
        "abilityName": "Fungus_Un_Gamma_Water_BulletNormal_Explode"
      },
      {
        "abilityID": "Bubble_Surround",
        "abilityName": "Fungus_Un_Gamma_Water_BulletNormal_Surround"
      }
    ]
  },
