  ////////////////////////////////////////////////////////////////
  "SceneObj_Toys_ProtablePot_Bullet": {
    "$type": "ConfigGadget",
    /*		"hasmodel": true,
	    "model":
	    {
	      "attachEffect": "Eff_Klee_ElementalArt_Mine",
	    },*/
    "common": {
      "effectAttachShape": {
        "$type": "ConfigEffectAttachBox",
        "emitFrom": "Volume",
        "scaleX": 1.0,
        "scaleY": 1.0,
        "scaleZ": 1.0
      },
      "scale": 1,
      "affectedByWorld": true,
      "checkInSurface": true,
      "mass": 5.0
    },
    //战斗参数
    "combat": {
      "property": {
        "useCreatorProperty": false,
        "weight": 50,
        "HP": 99999,
        "isInvincible": true,
        "isLockHP": true,
        "isGhostToAllied": true,
        "isGhostToEnemy": false,
        "combatLock": {
          "denyLockOn": true
        }
      }
    },
    "move": {
      "$type": "ConfigParabolaBulletMove",
      "speed": 10,
      "angleOffHor": 45,
      "gravityOfAcceleration": 30,
      "syncToRemote": true,
      "velocityForce": {
        "muteAll": true
      }
    },
    "timer": {
      "lifeInfinite": false, //如果未触发是否一致存在
      "lifeTime": 2.4 //存在时间
    },
    "gadget": {
      "$type": "ConfigBulletPattern",
      "collision": {
        "targetType": "None",
        "triggerType": "TriggerOnce"
        /*				"bornWithTriggerEnabled": false,*/
      },
      "triggerActions": [
        {
          "$type": "ConfigGadgetTriggerAbility",
          "abilityName": "SceneObj_Toys_ProtablePot_Bullet"
        }
      ],
      "triggerLifeOverActions": [
        {
          "$type": "ConfigGadgetTriggerAbility",
          "abilityName": "SceneObj_Toys_ProtablePot_Bullet"
        }
      ],
      "killByOther": true
    },
    "abilities": [
      {
        "abilityID": "SceneObj_Toys_ProtablePot_Bullet",
        "abilityName": "SceneObj_Toys_ProtablePot_Bullet"
      }
    ]
  },
