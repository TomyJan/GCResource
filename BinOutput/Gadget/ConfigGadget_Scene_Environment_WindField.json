  "SceneObj_Environment_WindField_PeriodWindField_Plus": {
    "$type": "ConfigGadget",
    "combat": {
      "property": {
        "HP": 300,
        "attack": 10,
        "isLockHP": true,
        "endure": 99,
        "isGhostToAllied": true,
        "isGhostToEnemy": false,
        "canTriggerBullet": true,
        "ignorePurgeRate": true
      },
      "combatLock": {
        "denyLockOn": true,
        "lockType": "Gadget_Lv1"
      },
      "die": {
        "hasAnimatorDie": true,
        "dieEndTime": 2,
        "dieForceDisappearTime": 2
      }
    },
    /*"field":
		{
			"concernType": "AllAvatars",
			"shape": "CircleR8",
			"startCheckTime" : 0.0,		//触发开始延时
			"checkInterval" : 8,		//触发间隔
			"triggerInfinite" : true,	//是否永远触发
		},*/
    "misc": {
      "forceField": {
        "$type": "ConfigAirflowField",
        "velocity": 10,
        "scale": 1.5
      },
      "gear": {
        //"startValue": 0,
      }
    },
    "gadgetAction": {
      "gadgetStateActionList": [
        //Default时开启功能
        {
          "$type": "ConfigGadgetStateEnableAction",
          "enable": true,
          "gadgetState": "Default"
        },
        //Action01时特效渐变
        {
          "$type": "ConfigGadgetStateSetAnimInt",
          "intID": "DieFlag",
          "value": 1,
          "gadgetState": "Action01"
        },
        //Action02时暂定功能
        {
          "$type": "ConfigGadgetStateEnableAction",
          "enable": false,
          "gadgetState": "Action02"
        },
        //Action02时重置flag
        {
          "$type": "ConfigGadgetStateSetAnimInt",
          "intID": "DieFlag",
          "value": 0,
          "gadgetState": "Action02"
        }
      ]
    }
  },
