{
  ////////////////////////////////////////////////////////////////
  //
  //  Rosaria_ExtraAttack_Bullet 分支攻击 41045005
  //
  ////////////////////////////////////////////////////////////////
  "Rosaria_ExtraAttack_Bullet": {
    "$type": "ConfigGadget",
    "common": {
      "effectAttachShape": {
        "$type": "ConfigEffectAttachBox",
        "emitFrom": "Volume",
        "scaleX": 1,
        "scaleY": 1,
        "scaleZ": 1.0
      }
    },
    "combat": {
      "property": {
        "useCreatorProperty": true, // 是否继承创造者的数值
        "canTriggerBullet": false,
        "isInvincible": true
      },
      "combatLock": {
        "denyLockOn": true,
        "lockType": "Gadget_Lv1"
      },
      "beHit": {
        "hitBloodEffect": "",
        "muteAllHit": true,
        "muteAllHitEffect": true,
        "muteAllHitText": true
      }
    },
    // 生命周期管理
    "timer": {
      "lifeInfinite": false, //如果未触发是否一致存在
      "lifeTime": 0.5 //存在时间
    },
    "move": {
      "$type": "ConfigFollowMove",
      "target": "LocalAvatar",
      // "attachPoint" : "RootNode",
      "followRotation": true,
      "offset": {
        "x": 0.0,
        "y": 0.9, //和少女统一高度
        "z": 0.0
      },
      "forward": {
        "x": 0.0,
        "y": 0.0,
        "z": 1.0
      }
    },
    "gadget": {
      "$type": "ConfigBulletPattern",
      "collision": {
        "targetType": "Enemy",
        "triggerType": "TriggerNoRepeat",
        "ignoreScene": true,
        "ccd": {
          "type": "Always"
        }
      },
      "triggerActions": [
        {
          "$type": "ConfigGadgetTriggerAbility",
          "abilityName": "Avatar_Rosaria_ExtraAttack_Bullet"
        }
      ],
      "triggerLifeOverActions": []
    },
    "abilities": [
      {
        "abilityID": "Avatar_Rosaria_ExtraAttack_Bullet",
        "abilityName": "Avatar_Rosaria_ExtraAttack_Bullet"
      }
    ]
  },

  ////////////////////////////////////////////////////////////////
  //
  //  Rosaria_ElementalBurst_Bullet  41045002
  //
  ////////////////////////////////////////////////////////////////
  //落地型bullet
  "Rosaria_ElementalBurst_Bullet": {
    "$type": "ConfigGadget",
    "common": {
      "effectAttachShape": {
        "$type": "ConfigEffectAttachBox",
        "emitFrom": "Volume",
        "scaleX": 1.0,
        "scaleY": 1.0,
        "scaleZ": 1.0
      },
      "scale": 1,
      "affectedByWorld": true,
      "checkInSurface": true,
      "mass": 5.0
    },
    //战斗参数
    "combat": {
      "property": {
        "useCreatorProperty": true,
        "weight": 50,
        "isInvincible": true,
        "isLockHP": true,
        "isGhostToAllied": true,
        "isGhostToEnemy": false,
        "combatLock": {
          "denyLockOn": true
        }
      }
    },
    "move": {
      "$type": "ConfigLinerBulletMove",
      "speed": 25,
      // "accelerationRawNum":2,
      "syncToRemote": true,
      "velocityForce": {
        "muteAll": true
      },
      "canBornInWater": true,
      "delay": 0.7
    },
    "timer": {
      "lifeInfinite": false, //如果未触发是否一致存在
      "lifeTime": 5.0 //存在时间
    },
    "gadget": {
      "$type": "ConfigBulletPattern",
      "enableCollisionDelay": 0.7, //延迟开启碰撞。
      "collision": {
        "targetType": "None",
        "triggerType": "TriggerOnce",
        "ccd": {
          "type": "Always"
        }
      },
      "triggerActions": [
        {
          "$type": "ConfigGadgetTriggerAbility",
          "abilityName": "Avatar_Rosaria_ElementalBurst_Bullet_Material"
        }
      ],
      "triggerLifeOverActions": [
        {
          "$type": "ConfigGadgetTriggerAbility",
          "abilityName": "Avatar_Rosaria_ElementalBurst_Bullet_Material"
        }
      ],
      "killByOther": true
    },
    "abilities": [
      {
        "abilityID": "Avatar_Rosaria_ElementalBurst_Bullet_Material",
        "abilityName": "Avatar_Rosaria_ElementalBurst_Bullet_Material"
      }
    ]
  },

