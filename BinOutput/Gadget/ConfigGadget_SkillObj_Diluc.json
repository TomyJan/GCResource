{
  ////////////////////////////////////////////////////////////////
  //
  //  Diluc_FireCross  迪卢克充能技，打出一道刀光。
  //
  ////////////////////////////////////////////////////////////////
  //附带天赋的大招刀光
  "Diluc_FireCross": {
    "$type": "ConfigGadget",
    "common": {
      "effectAttachShape": {
        "$type": "ConfigEffectAttachBox",
        "emitFrom": "Volume",
        "scaleX": 1.0,
        "scaleY": 1.0,
        "scaleZ": 1.0
      }
    },
    "combat": {
      "property": {
        "useCreatorProperty": true, // 是否继承创造者的数值
        "isInvincible": true,
        "isGhostToEnemy": true
      },
      "combatLock": {
        "denyLockOn": true,
        "lockType": "Gadget_Lv1"
      },
      "beHit": {
        "hitBloodEffect": "",
        "muteAllHit": true,
        "muteAllHitEffect": true,
        "muteAllHitText": true
      },
      "animEvents": {}
    },
    // 子弹线性移动
    "move": {
      "$type": "ConfigLinerBulletMove",
      "speed": 14,
      "acceleration": -2,
      "accelerationTime": 1,
      "stickToGround": {
        "$type": "ConfigMoveStickToGround",
        "heightToGround": 1.6,
        "flexibleRange": 1.2,
        "maxStepHeight": 3,
        "maxSlopeAngle": 30,
        "isStickToWater": true,
        "unstickWhenDownSlide": "Keep",
        "unstickWhenUpSlide": "Keep"
        // "updateAngle":
        // {
        // 	"$type": "ConfigBulletMoveAngleByVelocity", //这个会导致特效抬头，不要配。
        // },
      },
      "velocityForce": {
        "muteAll": true
      },
      "canBornInWater": true
    },
    // 生命周期管理
    "timer": {
      "lifeInfinite": false, //如果未触发是否一致存在
      "lifeTime": 1.7 //存在时间
    }, // 子弹参数
    "gadget": {
      "$type": "ConfigBulletPattern",
      "collision": {
        "targetType": "Enemy",
        "triggerType": "TriggerNoRepeat",
        "triggerCD": 10,
        "ignoreScene": true
      },
      "triggerLifeOverActions": [
        {
          "$type": "ConfigGadgetTriggerAbility",
          "abilityName": "Avatar_Diluc_FireCross_Explode"
        }
      ]
    },
    "abilities": [
      {
        "abilityID": "Avatar_Diluc_FireCross_Material",

        "abilityName": "Avatar_Diluc_FireCross_Material"
      },
      {
        "abilityID": "Avatar_Diluc_FireCross_Explode",

        "abilityName": "Avatar_Diluc_FireCross_Explode"
      }
    ]
  }
}
