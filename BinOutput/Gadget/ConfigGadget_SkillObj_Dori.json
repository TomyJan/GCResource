{
  ////////////////////////////////////////////////////////////////
  //
  //  Dori_ElementalArt_Bullet
  //
  ////////////////////////////////////////////////////////////////
  "Dori_ElementalArt_Bullet": {
    "$type": "ConfigGadget",
    "common": {
      "effectAttachShape": {
        "$type": "ConfigEffectAttachBox",
        "emitFrom": "Volume",
        "scaleX": 0.5,
        "scaleY": 0.5,
        "scaleZ": 1.0
      },
      "affectedByWorld": true
    },
    "combat": {
      "property": {
        "useCreatorProperty": true, // 是否继承创造者的数值
        "useCreatorBuffedProperty": true, // 继承的数值是否使用创造者身上的buff带来的property进行计算，仅在useCreatorProperty为true，或useCreatorPropertyPartly有值时生效，说白了就是创生时主人的临时buff对它起不起效
        "useAbilityProperty": false, // 是否使用自己的AbilityState中的AbilityProperty进行CombatProperty的计算,说白了就是自己后续还吃不吃临时buff。
        "canTriggerBullet": false,
        "isInvincible": true
      },
      "combatLock": {
        "denyLockOn": true
      },
      "beHit": {
        "hitBloodEffect": "",
        "muteAllHit": true,
        "muteAllHitEffect": true,
        "muteAllHitText": true
      },
      "die": {
        "dieEndTime": 1.5
      }
    },
    // 子弹线性移动
    // "move" :
    // {
    // 	"$type" : "ConfigTrackBulletMove",
    // 	"speed" : 5.0,
    // 	"acceleration" : 15,
    // 	"traceOnYAxis" : true,
    // 	"anglerVelocity": 3600,
    // 	"destroyWhenTargetDie":false,
    // },
    "move": {
      "$type": "ConfigParabolaBulletMove",
      "speed": 20,
      "angleOffHor": 40,
      "gravityOfAcceleration": 50,
      "syncToRemote": true,
      "velocityForce": {
        "muteAll": true
      },
      "correction": {
        "$type": "ConfigParabolaBulletMoveCorrection",
        "fixAngleOfVer": 15.0,
        "minSpeed": 4.0,
        "maxSpeed": 20.0
      }
    },
    // 生命周期管理
    "timer": {
      "lifeInfinite": false, //如果未触发是否一致存在
      "lifeTime": 3.0 //存在时间
    },
    // 子弹参数
    "gadget": {
      "$type": "ConfigBulletPattern",
      "collision": {
        "targetType": "Enemy",
        "triggerType": "TriggerOnce",
        "triggerCD": 0.5,
        "ccd": {
          "type": "Always"
        }
      },
      "triggerActions": [
        {
          "$type": "ConfigGadgetTriggerAbility",
          "abilityName": "Avatar_Dori_ElementalArt_Bullet_Material"
        }
      ],
      "fireAISoundEvent": true
    },
    "abilities": [
      {
        "abilityName": "Avatar_Dori_ElementalArt_Bullet_Material"
      }
    ]
  },

  "Dori_ElementalArt_SubBullet": {
    "$type": "ConfigGadget",
    "common": {
      "effectAttachShape": {
        "$type": "ConfigEffectAttachBox",
        "emitFrom": "Volume",
        "scaleX": 0.5,
        "scaleY": 0.5,
        "scaleZ": 1.0
      },
      "affectedByWorld": true
    },
    "combat": {
      "property": {
        "useCreatorProperty": true, // 是否继承创造者的数值
        "useCreatorBuffedProperty": true, // 继承的数值是否使用创造者身上的buff带来的property进行计算，仅在useCreatorProperty为true，或useCreatorPropertyPartly有值时生效，说白了就是创生时主人的临时buff对它起不起效
        "useAbilityProperty": false, // 是否使用自己的AbilityState中的AbilityProperty进行CombatProperty的计算,说白了就是自己后续还吃不吃临时buff。
        "canTriggerBullet": false,
        "isInvincible": true
      },
      "combatLock": {
        "denyLockOn": true
      },
      "beHit": {
        "hitBloodEffect": "",
        "muteAllHit": true,
        "muteAllHitEffect": true,
        "muteAllHitText": true
      }
    },
    "move": {
      "$type": "ConfigTrackBulletMove",
      "delay": 0.3,
      "speed": 10.0,
      "acceleration": 20,
      "maxSpeed": 20,
      "minSpeed": 10,
      "traceOnYAxis": true,
      "anglerVelocity": 90.0,
      "canBornInWater": true,
      "updateAngle": {
        "$type": "ConfigBulletMoveAngleByVelocity"
      },
      "whenNoTarget": {
        "defaultTarget": {
          "$type": "ConfigBornBySelf",
          "offset": {
            "x": 0,
            "y": 0,
            "z": 0.01 //不可以完全一致，会导致认为到达而停止。
          }
        },
        "detectDelay": 0.3, //防止还没飞出就自毁了。
        "$type": "ConfigDestroyByTargetSphere",
        "radius": 0.5 //这个自毁机制很有趣，如果此时子弹有碰撞盒会触发trigger，没有则会触发lifeOver。
      }
    },
    // "move" :
    // {
    // 	"$type" : "ConfigItanoCircusBulletMove",
    // 	"speed" : 5.0,
    // 	"acceleration": 20,
    // 	"maxSpeed" : 10,
    // 	"minSpeed" : 5,
    // 	"guidanceSpeedChange": true,
    // 	"itanoCircusStartAngler": 150,
    // 	"guidanceDelay": 0.8,
    // 	"anglerVelocity": 360.0,
    // 	"guidanceDuration": 100.0,
    // 	"guidanceMinAnglerVelocity": 5.0,
    // 	"canBornInWater": true,
    // 	// "destroyWhenTargetDie":true,
    // },
    // 生命周期管理
    "timer": {
      "lifeInfinite": false, //如果未触发是否一致存在
      "lifeTime": 2.0 //存在时间
    },
    // 子弹参数
    "gadget": {
      "$type": "ConfigBulletPattern",
      "collision": {
        "targetType": "Enemy",
        "triggerType": "TriggerOnce",
        "triggerCD": 0.1,
        "ccd": {
          "type": "Always"
        }
      },
      "triggerActions": [
        {
          "$type": "ConfigGadgetTriggerAbility",
          "abilityName": "Avatar_Dori_ElementalArt_SubBullet_Material"
        }
      ],
      "triggerLifeOverActions": [
        {
          "$type": "ConfigGadgetTriggerAbility",
          "abilityName": "Avatar_Dori_ElementalArt_SubBullet_Material"
        }
      ],
      "fireAISoundEvent": true,
      "enableCollisionDelay": 0.8,
      "resetLastPosOnDelayEnd": true,
      "reTriggerOnDelayEnd": true
    },
    "abilities": [
      {
        "abilityName": "Avatar_Dori_ElementalArt_SubBullet_Material"
      }
    ]
  },

