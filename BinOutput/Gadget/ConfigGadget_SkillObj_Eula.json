      },
      "beHit": {
        "hitBloodEffect": "",
        "muteAllHit": true,
        "muteAllHitEffect": true,
        "muteAllHitText": true
      }
    },
    // 生命周期管理
    "timer": {
      "lifeInfinite": false, //如果未触发是否一致存在
      "lifeTime": 3.5 //存在时间
    },
    "abilities": []
  },

