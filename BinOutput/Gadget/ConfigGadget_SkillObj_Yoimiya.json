      "$type": "ConfigBulletPattern",
      "collision": {
        "targetType": "Enemy",
        "triggerType": "TriggerOnce"
      },
      "triggerActions": [
        {
          "$type": "ConfigGadgetDoAttackEvent",
          "attackEvent": "Arrow_ATK01"
        },
        {
          "$type": "ConfigGadgetTriggerAbility",
          "abilityName": "Bullet_Yoimiya_Arrow_Material_Small"
        }
      ],
      "fireAISoundEvent": true
    },
    "abilities": [
