  "ChongyunTest_TestBlade": {
    "$type": "ConfigGadget",
    "common": {
      "effectAttachShape": {
        "$type": "ConfigEffectAttachBox",
        "emitFrom": "Volume",
        "scaleX": 1.0,
        "scaleY": 1.0,
        "scaleZ": 1.0
      }
    },
    "combat": {
      "property": {
        "useCreatorProperty": true, // 是否继承创造者的数值
        "isInvincible": true,
        "canTriggerBullet": false
      },
      "combatLock": {
        "denyLockOn": true
      },
      "beHit": {
        "hitBloodEffect": "",
        "muteAllHit": true,
        "muteAllHitEffect": true,
        "muteAllHitText": true
      }
      // "die":
      // {
      // 	"dieDisappearEffect":"Eff_Avatar_Male_Pole_Zhongli_RockStand_Die",
      // },
    },
    "move": {
      "$type": "ConfigLinerBulletMove",
      "speed": 20,
      "stickToGround": {
        "$type": "ConfigMoveStickToGround",
        "heightToGround": 0.8,
        "flexibleRange": 0.8,
        "maxStepHeight": 1.5,
        "maxSlopeAngle": 40,
        "isStickToWater": true,
        "unstickWhenDownSlide": "Keep",
        "unstickWhenUpSlide": "Keep"
      },
      "canBornInWater": true,
      "velocityForce": {
        "muteAll": true
      }
    },
    // 生命周期管理
    "timer": {
      "lifeInfinite": false, //如果未触发是否一致存在
      "lifeTime": 0.6 //存在时间
    },
    // 寻路阻挡
    // "navigation":
    // {
    // 	"type" : "Obstacle",
    // 	"sizeX" : 1.0,
    // 	"sizeY" : 4.0,
    // 	"sizeZ" : 1.0,
    // },
    "gadget": {
      "$type": "ConfigBulletPattern",
      "collision": {
        "targetType": "Enemy",
        "triggerType": "TriggerAlways",
        "triggerCD": 1.0,
        "ignoreScene": true
      },
      "triggerActions": [
        // {
        // 	"$type" : "ConfigGadgetTriggerAbility",
        // 	"abilityName" : "ChongyunTest_TestBlade_Hit",
        // },
        {
          "$type": "ConfigGadgetDamageByAttackValue",

          // "target":"Caster",
          // "targetType": "Enemy",
          "attackInfo": {
            "canBeModifiedBy": "OriginOwner",
            "attackTag": "IceBlades",
            "attackProperty": {
              "damagePercentage": 0,
              "strikeType": "Default",
              "elementDurability": 25,
              "elementType": "Ice",
              "attackType": "Default",
              "enBreak": 36
            },
            "hitPattern": {
              "onHitEffectName": "default",
              "overrideHitImpulse": {
                "hitLevel": "Heavy", //hitlevel = 2
                "hitImpulseX": 200, //hitlevel = 2
                "hitImpulseY": 0 //hitlevel = 2
              },
              "reboundRatio": 0,
              "canBeDefenceHalt": false,
              "hitHaltTime": 0.0,
              "hitHaltTimeScale": 0.05,
              "muteHitText": false
            },
            "forceCameraShake": false
          }
        }
      ]
    },
    "abilities": [
      {
        "abilityID": "ChongyunTest_TestBlade_Hit",
        "abilityName": "ChongyunTest_TestBlade_Hit"
      }
    ]
  }
