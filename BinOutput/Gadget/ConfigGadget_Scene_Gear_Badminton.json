      "$type": "ConfigParabolaBulletMove",
      "speed": 6,
      "angleOffHor": 45,
      "gravityOfAcceleration": 2.8
    },
    "abilities": [
      {
        "abilityID": "Ability_01",

        "abilityName": "SceneObj_Gear_Other_Badminton_Bullet_Crash"
      }
    ]
  },
