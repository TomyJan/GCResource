{
  //躲猫猫V2，物件JSON配置 音频第一套
  "SkillObj_Activity_HideSeekV2_Type_01": {
    "$type": "ConfigGadget",
    "model": {},
    "hasAbility": true,
    "common": {
      "effectAttachShape": {
        "$type": "ConfigEffectAttachMesh",
        "mesh": "EffectMesh"
      },
      "affectedByWorld": true
    },
    "combat": {
      "property": {
        "useCreatorProperty": false,
        "HP": 5000,
        "canTriggerBullet": false,
        "isGhostToAllied": true,
        "isGhostToEnemy": false,
        "isLockHP": true
      },
      "combatLock": {
        "denyLockOn": true
      },
      "beHit": {
        "hitBloodEffect": "",
        "muteAllHit": false,
        "muteAllHitEffect": false,
        "muteAllHitText": false
      }
    },
    // 子弹线性移动
    "move": {
      "$type": "ConfigFollowMove",
      "target": "LocalAvatar",
      "followRotation": true,
      "offset": {
        "x": 0.0,
        "y": 0.0,
        "z": 0.0
      },
      "forward": {
        "x": 0.0,
        "y": 0.0,
        "z": 1.0
      },
      "ignoreCollision": {
        "selfColliders": [
          {
            "type": "ScenePropCollider"
          }
        ],
        "targetColliders": [
          {
            "type": "MoveCollider"
          }
        ]
      },
      "followOwnerInvisible": false,
      //移动关闭碰撞配置
      "moveDisableCollision": {
        "selfColliders": [
          {
            "type": "ScenePropCollider"
          }
        ],
        "delayEnableTime": 0.5
      },

      //音效配置
      "moveAudio": {
        "startEvent": { "text": "Play_sfx_char_activity_hideSeek_wood_move" },
        //开始移动
        "stopEvent": { "text": "Stop_sfx_char_activity_hideSeek_wood_move" },
        //停止移动
        "moveStateParam": { "text": "Param_hideSeek" },
        //移动状态
        "fallOnGroundEvent": {
          "text": "Play_sfx_char_activity_hideSeek_wood_land"
        }
        //落地音频
      }
    },
    "timer": {
      "lifeInfinite": true //如果未触发是否一致存在
    },
    "abilities": [
      {
        "abilityID": "ActivityAbility_HideSeek_kong",
        "abilityName": "ActivityAbility_HideSeek_kong"
      }
    ]
  },
