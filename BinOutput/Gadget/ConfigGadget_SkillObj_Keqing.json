  ////////////////////////////////////////////////////////////////
  //
  //  41042007 Keqing_Bullet_NoTarget
  //
  ////////////////////////////////////////////////////////////////
  "Keqing_Bullet_NoTarget": {
    "$type": "ConfigGadget",
    "common": {
      "effectAttachShape": {
        "$type": "ConfigEffectAttachBox",
        "emitFrom": "Volume",
        "scaleX": 1,
        "scaleY": 1,
        "scaleZ": 1
      },
      "affectedByWorld": true
    },
    "combat": {
      "property": {
        "useCreatorProperty": true,
        "canTriggerBullet": false,
        "isGhostToAllied": true,
        "isGhostToEnemy": true
      },
      "combatLock": {
        "denyLockOn": true,
        "lockType": "Gadget_Lv1"
      },
      "beHit": {
        "hitBloodEffect": "",
        "muteAllHit": true,
        "muteAllHitEffect": true,
        "muteAllHitText": true
      }
    },
    // 子弹线性移动
    "move": {
      "$type": "ConfigTrackBulletMove",
      // "speed" : 40.0,
      "speed": 50.0,
      "traceOnYAxis": true,
      // "anglerVelocityRawNum":600,"stickToGround":
      "stickToGround": {
        "$type": "ConfigMoveStickToGround",
        "heightToGround": 0.45,
        "flexibleRange": 1.6,
        "maxStepHeight": 3,
        "maxSlopeAngle": 45,
        "isStickToWater": true,
        "unstickWhenDownSlide": "Keep",
        "unstickWhenUpSlide": "Keep"
        // "updateAngle":
        // {
        // 	"$type": "ConfigBulletMoveAngleByVelocity", //这个会导致特效抬头，不要配。
        // },
      }
    },
    // 生命周期管理
    "timer": {
      "lifeInfinite": false, //如果未触发是否一致存在
      "lifeTime": 0.16 //存在时间
    },
    // 子弹参数
    "gadget": {
      "$type": "ConfigBulletPattern",
      "collision": {
        "targetType": "Enemy",
        "triggerType": "TriggerOnce",
        "ccd": {
          "type": "Always"
        }
      },
      "triggerActions": [
        {
          "$type": "ConfigGadgetTriggerAbility",
          "abilityName": "Keqing_SkillBullet_Material"
        }
      ],
      "triggerLifeOverActions": [
        {
          "$type": "ConfigGadgetTriggerAbility",
          "abilityName": "Keqing_SkillBullet_Material"
        }
      ],
      "dieDelayTime": 1,
      "fireAISoundEvent": true
    },
    "abilities": [
      {
        "abilityID": "Keqing_SkillBullet_Material",
        "abilityName": "Keqing_SkillBullet_Material"
      }
    ]
  },
