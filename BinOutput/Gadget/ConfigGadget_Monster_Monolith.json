  "Monolith_Starchild_Lance": {
    "$type": "ConfigGadget",
    "hasModel": true,
    "model": {
      "hasCharacterRenderering": true
    },
    "common": {
      "effectAttachShape": {
        "$type": "ConfigEffectAttachBox",
        "emitFrom": "Volume",
        "scaleX": 1,
        "scaleY": 1,
        "scaleZ": 1
      },
      "mass": 100000,
      "disableTickDistance": 10000
    },
    "billboard": {
      "$type": "ConfigCombatBillboard",
      "offsetType": "Character",
      "offset": {
        "x": 0,
        "y": 0,
        "z": 0
      },
      "radiusOffset": 0,
      "showHPBar": true,
      "shieldBarOnly": true,
      "size": "Grand",
      "forceShowDistance": 50
    },
    "specialPoint": {
      "bulletAim": "BulletAimPoint"
    },
    "combat": {
      "property": {
        "useCreatorProperty": true, // 是否继承创造者的数值，
        "useAbilityProperty": true,
        "isLockHP": false,
        "canTriggerBullet": true,
        "isGhostToEnemy": false
      },
      "combatLock": {
        "denyLockOn": false,
        "lockType": "Avatar&Monster_Lv4"
      },
      "beHit": {
        "hitBloodEffect": "",
        "muteAllHit": true,
        "muteAllHitEffect": true,
        "muteAllHitText": false
      }
    },
    // 子弹线性移动
    "move": {
      "$type": "ConfigRigidbodyMove",
      "enableSyncTransToServer": true
    },
    "timer": {
      "lifeInfinite": true, //如果未触发是否一致存在
      "lifeTime": 8.0 //存在时间
    },
    "abilities": [
      {
        "abilityName": "Monster_Monolith_Starchild_LanceLife"
      },
      {
        "abilityName": "Monster_Monolith_Starchild_PartGeneral"
      }
    ]
  },
