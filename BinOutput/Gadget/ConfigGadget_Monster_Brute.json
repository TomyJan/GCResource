        "scaleY": 1,
        "scaleZ": 1
      },
      "affectedByWorld": true,
      "checkInSurface": true
    },
    "timer": {
      "lifeTime": 10
    },
    "combat": {
      "property": {
        "useCreatorProperty": true,
        "HP": 999999,
        "attack": 15.0,
        "defense": 999999,
        "weight": 60,
        "isLockHP": true,
        "canTriggerBullet": false
      },
      "combatLock": {
        "denyLockOn": true
      }
    },
    // 子弹线性移动
    "move": {
      "$type": "ConfigParabolaBulletMove",
      "canBornInWater": true,
      "speed": 40,
      "angleOffHor": 60,
      "gravityOfAcceleration": 40,
      "syncToRemote": true,
