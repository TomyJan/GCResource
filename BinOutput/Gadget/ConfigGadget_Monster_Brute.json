      },
      "triggerActions": [
        {
          "$type": "ConfigGadgetTriggerAbility",
          "abilityName": "Brute_Rock_BigSlime_Explode"
        }
      ]
    },
    "abilities": [
      {
        "abilityID": "RockSlime_Explode",
        "abilityName": "Brute_Rock_BigSlime_Explode"
      }
    ]
  },

