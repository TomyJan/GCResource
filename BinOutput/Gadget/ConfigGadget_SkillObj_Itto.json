        "scaleZ": 1
      }
    },
    "combat": {
      "property": {
        "useCreatorProperty": true,
        "canTriggerBullet": false,
        "isGhostToAllied": true,
        "isGhostToEnemy": true
      },
      "combatLock": {
        "denyLockOn": true,
        "lockType": "Gadget_Lv1"
      },
      "beHit": {
        "hitBloodEffect": "",
        "muteAllHit": true,
        "muteAllHitEffect": true,
        "muteAllHitText": true
      }
