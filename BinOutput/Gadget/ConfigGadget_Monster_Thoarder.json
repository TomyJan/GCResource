      },
      "combatLock": {
        "denyLockOn": true
      },
      "beHit": {
        "hitBloodEffect": "",
        "muteAllHit": true,
        "muteAllHitEffect": true,
        "muteAllHitText": true
      }
    },
    // 子弹抛物线移动修正
    "move": {
      "$type": "ConfigParabolaBulletMove",
      "speed": 15.0,
      "angleOffHor": 50,
