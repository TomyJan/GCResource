  "SceneObj_Chest_Flora": {
    "$type": "ConfigGadget",
    "hasModel": true,
    "common": {
      "disableTickDistance": 3000
    },
    "combat": {
      "property": {
        "useCreatorProperty": false,
        "HP": 99999,
        "attack": 0,
        "defense": 0,
        "weight": 50,
        "isInvincible": false,
        "isLockHP": true,
        "isGhostToAllied": false,
        "isGhostToEnemy": false,
        "canTriggerBullet": false
      },
      "combatLock": {
        "denyLockOn": true,
        "lockType": "Gadget_Lv1"
      },
      "beHit": {
        "hitBloodEffect": "",
        "muteAllHit": true,
        "muteAllHitEffect": false,
        "muteAllHitText": true
      },
      "die": {
        "dieEndTime": 6.8,
        "dieForceDisappearTime": 6.8,
        "dieShaderData": "GadgetDie",
        "dieModelFadeDelay": 4
      }
    },
    "uiInteract": {
      "onTouch": "FloraChest",
      "openRewardId": 1
    },
    "misc": {
      "chest": {
        "openEffect": "Eff_SceneObj_FloraChest_Active"
      },
      "pickup": {
        "$type": "ConfigPickup"
        //"dropPointMinSpeed": 4,
        //"dropPointMaxSpeed": 5,
      },
      "targetIndicatorYOffset": 1.35
    },
    "stateLayers": {
      "defaultLayer": {
        "$type": "ConfigNormalStateLayer",
        "stateIDs": {
          "Open": {
            "$type": "ConfigNormalStateIDInfo",
            "animatorStates": {
              "ChestOpen": []
            }
          },
          "InIdle": {
            "$type": "ConfigNormalStateIDInfo",
            "animatorStates": {
              "Idle": []
            }
          },

          "FloraBorn": {
            "$type": "ConfigNormalStateIDInfo",
            "animatorStates": {
              "Born": []
            }
          },
          "InVisible": {
            "$type": "ConfigNormalStateIDInfo",
            "animatorStates": {
              "Buffer": []
            }
          }
        }
      }
    },
    "gadgetAction": {
      "gadgetStateActionList": [
        {
          "$type": "ConfigGadgetStateSetAnimInt",
          "intID": "gadgetState",
          "syncState": true
        }
      ]
    },
    "abilities": [
      {
        "abilityID": "Ability_01",
        "abilityName": "SceneObj_Chest_Flora"
      }
    ]
  },
