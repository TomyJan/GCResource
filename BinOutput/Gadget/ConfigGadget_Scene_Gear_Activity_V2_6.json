      "property": {
        "HP": 10000,
        "attack": 0,
        "isLockHP": true,
        "isGhostToAllied": true,
        "isGhostToEnemy": true,
        "canTriggerBullet": false,
        "ignorePurgeRate": true
      },
      "combatLock": {
        "denyLockOn": true,
        "lockType": "Gadget_Lv1"
      },
      "beHit": {
        "hitBloodEffect": "",
        "hitAutoRedirect": false,
        "muteAllHit": false,
        "muteAllHitEffect": false,
        "muteAllHitText": true
      }
    },
    "gadgetAction": {
      "gadgetStateActionList": [
        //Default时可以交互
        {
          "$type": "ConfigGadgetStateEnableGadgetIntee",
          "enable": true,
          "gadgetState": "Default"
        },
        //201时不可以交互
        {
          "$type": "ConfigGadgetStateEnableGadgetIntee",
          "enable": false,
          "gadgetState": "GearStart"
        }
      ]
    },
    "abilities": [
      {
        "abilityID": "SceneObj_Area_Common_Property_Tree_CameraActivityOperator_01",
        "abilityName": "SceneObj_Area_Common_Property_Tree_CameraActivityOperator_01"
      }
    ]
  },

