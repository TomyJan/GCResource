{
  ////////////////////////////////////////////////////////////////
  //
  //  Layla_ElementalArt_Gadget  41074001 小技能gadget
  //
  ////////////////////////////////////////////////////////////////
  "Layla_ElementalArt_Gadget": {
    "$type": "ConfigGadget",
    "common": {
      "effectAttachShape": {
        "$type": "ConfigEffectAttachBox",
        "emitFrom": "Volume",
        "scaleX": 1.0,
        "scaleY": 1.0,
        "scaleZ": 1.0
      },
      "affectedByWorld": false,
      "checkInSurface": false
    },
    "combat": {
      //完全不会和世界交互
      "property": {
        "useCreatorProperty": true, // 是否继承创造者的数值
        "canTriggerBullet": false,
        "isGhostToAllied": true,
        "isGhostToEnemy": true,
        "isInvincible": true
      },
      "combatLock": {
        "denyLockOn": true,
        "lockType": "Gadget_Lv1",
        "overrideRange": -1,
        "overrideNormalPri": -1,
        "overrideCombatPri": -1
      },
      "beHit": {
        "muteAllHitText": true
      }
    },
    // 无子弹功能的配置，不要配置bulltetPattern，会导致默认配置里有ccd，有collison+triggerOnce，有撞墙判定，gadget会撞墙而亡。建议整个gadget配置都干掉。
    // "gadget": {
    // 	"$type": "ConfigBulletPattern",
    // 	"killByOther": true,
    // },
    "move": {
      "$type": "ConfigFollowMove",
      "target": "LocalAvatar",
      "attachPoint": "RootNode",
      // "forward": { //forward会影响朝向镜头的转向。
      // 	"x": 0.0,
      // 	"y": 0.0,
      // 	"z": 1.0,
      // },
      "followOwnerInvisible": true,
      "velocityForce": {
        "muteAll": true
      }
    },
    "timer": {
      "lifeInfinite": false, //如果未触发是否一致存在
      "lifeTime": 4.0 //存在时间
    },
    "abilities": [
      {
        "abilityID": "Avatar_Layla_ElementalArt_Gadget",
        "abilityName": "Avatar_Layla_ElementalArt_Gadget"
      }
    ]
  },
  ////////////////////////////////////////////////////////////////
  //
  //  Layla_ElementalArt_Bullet1  41074002 小技能盾发射子弹相关的bullet，
  //
  ////////////////////////////////////////////////////////////////
  "Layla_ElementalArt_Bullet1": {
    "$type": "ConfigGadget",
    "common": {
      "effectAttachShape": {
        "$type": "ConfigEffectAttachBox",
        "emitFrom": "Volume",
        "scaleX": 1,
        "scaleY": 1,
        "scaleZ": 1.0
      },
      "affectedByWorld": true
    },
    "combat": {
      "property": {
        "useCreatorProperty": true, // 是否继承创造者的数值
        "canTriggerBullet": false,
        "isGhostToAllied": true,
        "isGhostToEnemy": true
      },
      "combatLock": {
        "denyLockOn": true,
        "lockType": "Gadget_Lv1"
      },
      "beHit": {
        "hitBloodEffect": "",
        "muteAllHit": true,
        "muteAllHitEffect": true,
        "muteAllHitText": true
      }
    },
    "move": {
      "$type": "ConfigTrackBulletMove",
      "speed": 8, //原8
      "maxSpeed": 15,
      "acceleration": 5, //原50，
      "traceOnYAxis": true,
      "anglerVelocity": 180, //太低靠random的话随机性太大了
      "canBornInWater": true,
      "updateAngle": {
        "$type": "ConfigBulletMoveAngleByVelocity",
        "traceLerpCoef": 3600
      },
      "whenNoTarget": {
        //没怪的时候有一个默认飞行方向
        "defaultTarget": {
          "$type": "ConfigBornBySelfOwner",
          "offset": {
            "x": 0,
            "y": 0,
            "z": 10
          },
          "onGround": true //尽量贴地。
        },
        "$type": "ConfigDestroyByTargetSphere",
        "radius": 0.1
      }
    },
    "timer": {
      "lifeInfinite": false, //如果未触发是否一致存在
      "lifeTime": 1.5 //存在时间 1.5秒可以飞15米左右
    },
    // 子弹参数
    "gadget": {
      "$type": "ConfigBulletPattern",
      "collision": {
        "targetType": "Enemy",
        "triggerType": "TriggerOnce",
        "ignoreScene": false
      },
      "triggerActions": [
        {
          "$type": "ConfigGadgetTriggerAbility",
          "abilityName": "Avatar_Layla_ElementalArt_Bullet_Material"
        }
      ],
      "triggerLifeOverActions": [
        {
          "$type": "ConfigGadgetTriggerAbility",
          "abilityName": "Avatar_Layla_ElementalArt_Bullet_Material"
        }
      ],
      "killByOther": false
    },
    "abilities": [
      {
        "abilityID": "Avatar_Layla_ElementalArt_Bullet_Material",
        "abilityName": "Avatar_Layla_ElementalArt_Bullet_Material"
      }
    ]
  },
