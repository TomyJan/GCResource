  ///////////////////////////////////////////////////
  //
  // 	试玩活动——追踪机关 火 子弹
  //
  ///////////////////////////////////////////////////
  "SceneObj_Level_Common_Property_Prop_TrackingOrganBullet_01_Fire": {
    "$type": "ConfigGadget",
    "model": {
      "bornEffect": "Eff_SceneObj_TowerFire_01_Fire"
    },
    //该注释用于探索元素视野的最小集合设置
    "combat": {
      "property": {
        "useCreatorProperty": true,
        "HP": 99999,
        "attack": 300,
        "isInvincible": true,
        "isLockHP": true,
        "isGhostToAllied": true,
        "isGhostToEnemy": true
      }
    },
    "gadget": {
      "$type": "ConfigBulletPattern",
      "collision": {
        "targetType": "Enemy",
        "triggerType": "TriggerOnce",
        "ignoreScene": false
      },
      "triggerActions": [
        {
          "$type": "ConfigGadgetTriggerAbility",
          "abilityName": "SceneObj_Level_Common_Property_Prop_TrackingOrganBullet_01_Fire_Ability"
        }
      ],
      "triggerLifeOverActions": [
        {
          "$type": "ConfigGadgetTriggerAbility",
          "abilityName": "SceneObj_Level_Common_Property_Prop_TrackingOrganBullet_01_Fire_Ability"
        }
      ],
      "enableCollisionDelay": 0.3
    },
    "timer": {
      "lifeInfinite": false, //如果未触发是否一致存在
      "lifeTime": 5 //存在时间
    },
    // 子弹线性移动
    "move": {
      "$type": "ConfigParabolaBulletMove",
      "speed": 35,
      "angleOffHor": 15,
      "gravityOfAcceleration": 10,
      "updateAngle": {
        "$type": "ConfigBulletMoveAngleRotating",
        "angularVelocity": {
          "x": -900,
          "y": 0,
          "z": 0
        }
      },
      "correction": {
        "$type": "ConfigParabolaBulletMoveCorrection",
        "deviation": 0.5,
        "fixAngleOfVer": 15,
        "minSpeed": 5,
        "maxSpeed": 20
      }
    },
    "abilities": [
      {
        "abilityID": "Ability_01",

        "abilityName": "SceneObj_Level_Common_Property_Prop_TrackingOrganBullet_01_Fire_Ability"
      }
    ]
  },
